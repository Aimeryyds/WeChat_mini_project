<template name="blankStatus">
    <view class="blank-status">
        <view class="status-image">
            <image backgroundSize="cover" class="img" src="{{img}}"></image>
        </view>
        <view class="txt-wrapper">
            <text class="title">
				{{title}}
			</text>
            <text class="sub-title" style="display:{{subTitle===''?'none':''}}">
				{{subTitle}}
			</text>
        </view>
    </view>
</template>
