.toast-wrapper {
    width: 100vw;
    height: 88rpx;
    display: flex;
    z-index: 9999;
    background: #F2F2F2;
    flex-direction: row;
    align-items: center;
    float: left;
    position: absolute;
}

.icon {
    width: 40rpx;
    height: 40rpx;
    margin-right: 30rpx;
    margin-left: 30rpx;
}

.message {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #000000;
    line-height: 28rpx;
}