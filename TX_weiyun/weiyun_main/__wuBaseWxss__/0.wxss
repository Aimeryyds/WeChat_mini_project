.blank-status {
    margin-top: 132px;
    color: #9B9B9B;
    text-align: center;
}

.blank-status .status-image {
    display: block;
    width: 163px;
    height: 150px;
    margin: 0 auto;
}

.blank-status .status-image .img {
    width: 100%;
    height: 100%;
}

.blank-status .title,.blank-status .sub-title {
    display: block;
    padding: 0 30px;
}

.blank-status .title {
    display: block;
    font-size: 18px;
}

.blank-status .sub-title {
    font-size: 14px;
    margin-top: 10px;
}

.dialog-wrapper,.dialog-wrapper .mask {
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
}

.dialog-wrapper {
    position: fixed;
    z-index: 1000;
}

.dialog-wrapper .mask {
    position: absolute;
    z-index: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.8);
}

.dialog-wrapper .dialog-content {
    z-index: 1;
    width: 80%;
    height: auto;
    margin: 0 auto;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    border-radius: 5px;
    background-color: #fff;
    font-size: 14pt;
    text-align: center;
}

.dialog-wrapper .dialog-content .txt {
    padding: 30px 40px;
    color: #353535;
    border-bottom: 1px solid #d2d3d5;
}

.dialog-wrapper .dialog-content .btn-wrapper {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
}

.dialog-wrapper .dialog-content .btn {
    width: 100%;
    height: 32px;
    color: #09bb07;
    font-weight: bold;
    padding: 12px;
}