.privilege-ovewview-title {
    margin-bottom: 40rpx;
    padding: 0 40rpx;
    position: relative;
}

.privilege-ovewview-title .title-note {
    font-weight: bold;
    font-size: 34rpx;
    color: #212121;
}

.privilege-ovewview-title .privilege-contrast-door {
    font-weight: bold;
    font-size: 24rpx;
    color: #9E9E9E;
    text-align: right;
    position: absolute;
    right: 40rpx;
    bottom: 10rpx;
}

.privilege-ovewview-title .privilege-contrast-door .door-icon {
    width: 10rpx;
    height: 18rpx;
    margin-left: 12rpx;
}

.privilege-contrast-img {
    width: 100%;
}

.hide {
    display: none;
}