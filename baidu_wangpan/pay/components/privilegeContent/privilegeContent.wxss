.privilege-content {
    font-size: 32rpx;
    border-top-left-radius: 30rpx;
    border-top-right-radius: 30rpx;
    background-color: white;
    overflow: hidden;
    box-sizing: border-box;
    padding: 30rpx;
    position: absolute;
    bottom: 0rpx;
    left: 0;
    right: 0;
    overflow: scroll;
    padding-bottom: 160rpx;
}

.privilege-content.no-padding-bottom {
    padding-bottom: 20rpx;
}

.privilege-image {
    width: 100%;
}