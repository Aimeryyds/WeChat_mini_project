.privilege-button-box {
    position: fixed;
    bottom: 0;
    height: 140rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx;
    background-color: white;
}

.privilege-button {
    border-radius: 100rpx;
    overflow: hidden;
    width: 100%;
    height: 100%;
    font-size: 34rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-weight: bold;
}

.privilege-button.vip {
    background-image: linear-gradient(90deg,#FF8888 0%,#F76161 100%);
    color: #FFFFFF;
}

.privilege-button.svip {
    background-image: linear-gradient(90deg,#EED9AE 0%,#E5C585 100%);
    color: #532B07;
}