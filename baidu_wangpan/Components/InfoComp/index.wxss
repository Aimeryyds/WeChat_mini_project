.info-comp {
    text-align: center;
}

.info-comp-icon {
    line-height: 1;
}

.info-comp-icon-img {
    width: 280rpx;
    height: 241rpx;
}

.info-comp-title {
    font-family: PingFangSC-Semibold;
    font-size: 40rpx;
    color: #212121;
    margin-top: 55rpx;
}

.info-comp-tip text {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #BDBDBD;
    letter-spacing: 1.33rpx;
    margin-top: 26rpx;
}

.info-comp-tip {
    margin-top: 45rpx;
}

.info-comp-tip.has-title {
    margin-top: 8rpx;
}

.info-comp-action-text {
    margin-top: 178rpx;
    margin-left: auto;
    margin-right: auto;
}

.info-comp-action-text-body {
    border-radius: 41rpx!important;
    font-family: PingFangSC-Semibold;
    color: #FFFFFF;
    font-size: 28rpx!important;
    box-shadow: 0 0 0 0!important;
    height: 80rpx!important;
    line-height: 40rpx!important;
    padding: 21rpx 56rpx;
}

.info-comp-action-text-body:after {
    border: 0;
}

.info-comp-btn {
    width: 280rpx;
    margin-top: 148rpx;
    margin-left: 235rpx;
}

.info-comp-u-btn {
    border-radius: 41rpx!important;
    font-family: PingFangSC-Semibold;
    font-size: 28rpx!important;
    box-shadow: 0 0 0 0!important;
    height: 82rpx!important;
    line-height: 82rpx!important;
}

.error-icon-img {
    width: 120rpx;
    height: 120rpx;
}