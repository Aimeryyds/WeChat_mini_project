.fixed-bottom {
    width: 100%;
    position: fixed;
    bottom: 0;
    z-index: 99;
}

.fixed-bottom .quesition-btn {
    display: flex;
    width: 100%;
    height: 128rpx;
    margin-top: 20rpx;
    background: #FCFCFC;
    border: none;
    outline: none;
    text-align: inherit;
    line-height: 128rpx;
    box-sizing: border-box;
    flex-direction: row;
    align-items: center;
    border-radius: 0rpx;
    justify-content: space-around;
    color: #212121;
    font-size: 30rpx;
    font-weight: bold;
}

.fixed-bottom button {
    padding: 0;
}

.quesition-btn::after {
    content: none;
}

.fixed-bottom .quesition-btn .icon-message {
    margin-right: 24rpx;
    width: 36rpx;
    height: 34rpx;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_feedback.png') no-repeat;
    background-size: 36rpx 34rpx;
    display: inline-block;
}

.my,.contact {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    width: 100%;
    text-align: center;
}

.my::after {
    display: block;
    content: '';
    position: absolute;
    left: auto;
    bottom: auto;
    right: 0;
    top: 25%;
    height: 58rpx;
    width: 1px;
    transform: scaleX(0.5);
    background-color: #BDBDBD;
    transform-origin: 50% 100%;
    opacity: .5;
}

.contact-icon {
    display: inline-block;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_feedback_v2.png') no-repeat;
    margin-right: 24rpx;
    width: 36rpx;
    height: 34rpx;
    background-size: 36rpx 34rpx;
}