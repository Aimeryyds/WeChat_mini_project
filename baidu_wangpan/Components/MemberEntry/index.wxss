.entry-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 686rpx;
    margin: -35rpx auto 20rpx auto;
    color: #333333;
    background-image: linear-gradient(90deg,#F8E6BE 0%,#ECD6AA 100%);
    border-radius: 16rpx;
    height: 110rpx;
    padding-left: 32rpx;
    padding-right: 32rpx;
    box-sizing: border-box;
    line-height: 1;
}

.entry-vip-icon {
    width: 45rpx;
    height: auto;
    vertical-align: middle;
}

.entry-vip-title {
    margin-left: 20rpx;
    display: inline-block;
    vertical-align: middle;
}

.entry-vip-title-text {
    font-weight: bold;
    font-size: 28rpx;
    line-height: 28rpx;
}

.entry-vip-title-text.no-sub {
    font-size: 30rpx;
    line-height: 42rpx;
}

.entry-vip-title-desc {
    font-family: PingFangSC-Regular;
    font-size: 20rpx;
    line-height: 20rpx;
    margin-top: 10rpx;
}

.entry-vip-desc {
    opacity: 0.92;
    font-weight: bold;
    font-size: 24rpx;
    line-height: 33rpx;
    display: inline-block;
    vertical-align: middle;
}

.entry-nav-icon {
    width: 26rpx;
    height: auto;
    margin-left: 20rpx;
    vertical-align: middle;
}