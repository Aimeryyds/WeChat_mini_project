<view bindtouchend="_touchEnd" bindtouchstart="_touchStart" class="story-item-wrap" data-index="{{index}}" style="width:{{width}}rpx;height:{{height}}rpx;margin-left:{{space*ownIndex}}rpx;">
    <view bindtap="_click" class="file-item-main" data-index="{{index}}">
        <view class="image" style="background-image: url({{thumbnail}});"></view>
    </view>
</view>
