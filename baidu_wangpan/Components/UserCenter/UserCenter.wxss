.usercenter-wrap {
    width: 100%;
    background: #FFF;
    height: 100%;
    margin-top: 28rpx;
    overflow-y: auto;
    color: #212121;
}

.userinfo-wrap {
    width: 100%;
    height: 324rpx;
    background: #fff;
    padding: 98rpx 32rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
}

.bind-button-wrap {
    background: #fff;
    margin-top: -20rpx;
    padding: 0 32rpx;
    padding-bottom: 58rpx;
}

.userinfo-wrap .avatar-wrap {
    width: 128rpx;
    height: 128rpx;
    border-radius: 50%;
    margin-right: 32rpx;
    background-position: center;
    background-repeat: no-repeat;
    position: relative;
    background-size: 128rpx 128rpx;
    margin-top: -6rpx;
    background-color: #ebebeb;
}

.userinfo-wrap .avatar-wrap .avatar-logo {
    width: 56rpx;
    height: 56rpx;
    background-image: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_avatar_logo.png');
    background-repeat: no-repeat;
    background-size: 56rpx 56rpx;
    border-radius: 50%;
    position: absolute;
    bottom: 0;
    right: -8rpx;
}

.userinfo-wrap .avatar-wrap .avatar-wx-logo {
    width: 56rpx;
    height: 56rpx;
    background-image: url('https://staticwx.cdn.bcebos.com/mini-program/images/wx_logo.png');
    background-repeat: no-repeat;
    background-size: 56rpx 56rpx;
    border-radius: 50%;
    position: absolute;
    bottom: 0;
    right: -8rpx;
}

.userinfo-wrap .userinfo {
    width: calc(100% - 160rpx);
    height: 128rpx;
    line-height: 100%;
}

.userinfo-wrap .userinfo .userinfo-title {
    display: flex;
    align-items: center;
    align-content: center;
}

.userinfo-wrap .userinfo .userinfo-title .userinfo-name {
    color: #212121;
    font-size: 32rpx;
    font-weight: bold;
}

.userinfo-wrap .userinfo .userinfo-title .userinfo-typeIcon {
    width: 40rpx;
    height: 40rpx;
    margin-left: 16rpx;
    background-repeat: no-repeat;
    background-position: center;
}

.userinfo-wrap .userinfo .userinfo-title .userinfo-typeIcon0 {
    background-image: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_non_member_v2.png');
    background-size: 40rpx 40rpx;
}

.userinfo-wrap .userinfo .userinfo-title .userinfo-typeIcon1 {
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_member_v2.png');
    background-size: 40rpx 40rpx;
}

.userinfo-wrap .userinfo .userinfo-title .userinfo-typeIcon2 {
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_super_member_v2.png');
    background-size: 40rpx 40rpx;
}

.userinfo-wrap .userinfo .quota-des {
    margin-top: 20rpx;
    color: #212121;
    font-size: 24rpx;
}

.userinfo-wrap .userinfo .quota-wrap {
    margin: 20rpx 0;
}

.userinfo-wrap .userinfo .quota-wrap .quota-default {
    width: 100%;
    height: 10rpx;
    background-color: #F9F9F9;
    border-radius: 10rpx;
}

.userinfo-wrap .userinfo .quota-wrap .quota-e {
    width: 0.01%;
    height: 10rpx;
    background-color: #4ce33c;
    border-radius: 10rpx;
    transition: width ease;
}

.userinfo-wrap .userinfo .quota-wrap .quota-n {
    width: 0.01%;
    height: 10rpx;
    background-color: #fd9600;
    border-radius: 10rpx;
    transition: width ease;
}

.userinfo-wrap .userinfo .quota-wrap .quota-t {
    width: 0.01%;
    height: 10rpx;
    background-color: #ff5455;
    border-radius: 10rpx;
    transition: width ease;
}

.usercenter-wrap .feedbackWrap {
    display: flex;
    width: 100%;
    height: 128rpx;
    padding: 0 32rpx;
    margin-top: 20rpx;
    background: #fff;
    border: none;
    outline: none;
    border-color: #fff;
    text-align: inherit;
    line-height: 128rpx;
    box-sizing: border-box;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
    font-weight: bold;
}

.usercenter-wrap .feedbackWrap::after {
    content: none;
}

.usercenter-wrap .unbindWrap {
    display: flex;
    width: 100%;
    height: 128rpx;
    margin-top: 20rpx;
    padding: 0 32rpx;
    background: #fff;
    text-align: inherit;
    line-height: 128rpx;
    box-sizing: border-box;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
    font-weight: bold;
    color: #212121;
}

.usercenter-wrap .unbindWrap::after {
    content: none;
}

.usercenter-wrap .feedbackWrap .feedbackMore {
    display: inline-block;
    width: 16rpx;
    height: 28rpx;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_mine_more_v2.png') no-repeat;
    background-size: 16rpx 28rpx;
}

.usercenter-wrap .userCenterSlogen {
    width: 100%;
    height: 75rpx;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/logo_slogan.png') no-repeat;
    background-position: center;
    background-size: 231rpx 75rpx;
    position: fixed;
    bottom: 32rpx;
}

.usercenter-guide {
    margin-top: 12rpx;
}

.blank {
    height: 110rpx;
}