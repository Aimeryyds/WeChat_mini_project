<view bindtouchend="createShareTouchE" bindtouchstart="createShareTouchS" catchtap="tap" class="share-button-item {{isShow?'active':''}}" style="width: {{width}}rpx">
    <view class="share-button-bg {{isTouchStart?'touch-start':''}} {{disabled?'disabled':''}}">
        <view class="op-icon"></view>
        <text class="text">{{content}}</text>
    </view>
</view>
