.seckill-container {
    width: 100%;
    position: relative;
}

.seckill-wrap {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
}

.sec-already {
    position: absolute;
    top: 154rpx;
    text-align: center;
    width: 100%;
    font-size: 26rpx;
    line-height: 26rpx;
}

.sec-already span {
    font-weight: 700;
}

.sec-btn {
    position: absolute;
    top: 184rpx;
    width: 100%;
    text-align: center;
    background: rgba(0,0,0,0);
}

.sec-btn::after {
    border: none;
}

.sec-floating {
    position: absolute;
    top: 2rpx;
    right: 127rpx;
    width: 324rpx;
    height: 61rpx;
}

.sec-btnBg {
    width: 596rpx;
    height: 122rpx;
    margin: 38rpx auto;
}

.sec-btnText {
    position: absolute;
    top: 38rpx;
    left: 0;
    right: 0;
    bottom: 0;
    width: 611rpx;
    height: 97rpx;
    line-height: 97rpx;
    text-align: center;
    box-sizing: border-box;
    overflow: hidden;
    font-size: 42rpx;
    color: #fff4c9;
}

.sec-time {
    width: 60rpx;
    display: inline-block;
    text-align: center;
}

.sec-time-min {
    width: 30rpx;
    display: inline-block;
    text-align: center;
}

.dialog-content {
    color: #fff5d3;
    font-size: 27rpx;
    line-height: 40rpx;
    margin-top: 75rpx;
}

.seckill-image {
    width: 100%;
    height: 100%;
    display: block;
}