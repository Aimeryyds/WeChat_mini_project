<view catchtap="radioChange" class="u-class u-radio">
    <u-cell uClass="u-radio-cell">
        <label>
            <radio checked="{{checked}}" class="u-radio-radio {{positionCls}}" color="{{checked?color:''}}" disabled="{{disabled}}" value="{{value}}"></radio>
            <view class="u-radio-title">{{value}}</view>
        </label>
    </u-cell>
</view>
