.wrap-container {
    position: relative;
}

.wrap-container-mask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
}

.wrap-container .bacth-bound {
    display: flex;
    padding: 0 32rpx;
    height: 112rpx;
    align-items: center;
    justify-content: space-between;
    color: #9E9E9E;
    font-size: 28rpx;
    font-weight: bold;
}

.wrap-container .bacth-bound.is-trans {
    justify-content: center;
}

.wrap-container .batch-header {
    position: relative;
    z-index: 2;
}

.wrap-container .batch-bottom {
    height: 128rpx;
    background-color: #FCFCFC;
    font-size: 30rpx;
}

.wrap-container .bacth-bound .info-tab:last-child,.wrap-container .bottom-menu {
    color: #06A7FF;
}

.wrap-container .info-tab {
    color: #212121;
}

.wrap-container .info-tab.tab1 {
    width: 560rpx;
    font-weight: bold;
    font-size: 34rpx;
}

.ellipsis {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}