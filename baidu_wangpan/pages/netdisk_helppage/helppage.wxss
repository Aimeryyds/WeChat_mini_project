.wrap-container {
    background: #fff;
}

.box-shadow {
}

.help-list {
}

.help-list button.button-hover {
    background-color: #F9F9FA;
}

.quesition-btn {
    display: flex;
    width: 100%;
    height: 128rpx;
    padding: 0 32rpx;
    border: none;
    outline: none;
    border-color: #fff;
    background: #fff;
    text-align: inherit;
    line-height: 128rpx;
    box-sizing: border-box;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    font-size: 30rpx;
}

.quesition-btn::after {
    content: none;
}

.help-list .quesition-btn .quesition-btn-detail {
    width: 16rpx;
    height: 28rpx;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_mine_more_v2.png') no-repeat;
    background-size: 16rpx 28rpx;
    display: inline-block;
}