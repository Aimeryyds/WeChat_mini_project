.launch {
    padding: 0 32rpx;
    color: #212121;
    font-size: 28rpx;
}

.launch-logo {
    height: 74rpx;
    padding: 98rpx 0 128rpx 0;
    background: url(https://staticwx.cdn.bcebos.com/mini-program/images/netdisk_logo.png) no-repeat;
    background-size: 340rpx 74rpx;
    background-position: left center;
}

.launch-title {
    padding-bottom: 32rpx;
    font-size: 32rpx;
    font-weight: bold;
}

.launch-content {
    padding: 32rpx;
    margin-bottom: 72rpx;
    word-break: break-all;
    border-radius: 8rpx;
    color: #9e9e9e;
    background-color: #f9f9f9;
}