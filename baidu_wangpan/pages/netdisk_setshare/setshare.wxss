page {
    min-height: auto;
    background-color: #FFF;
}

.setshare-wrap {
    width: 100%;
}

.setshare-wrap .setshare-top .avator {
    position: relative;
    z-index: 20;
    padding: 34rpx 25rpx 0;
    background-color: #FFF;
}

.setshare-wrap .setshare-top .avator .avator-row {
    height: 218rpx;
}

.setshare-wrap .setshare-top .avator .avator-row.last-row {
    height: 212rpx;
}

.setshare-wrap .setshare-top .avator .avator-row .avator-item {
    width: 20%;
    height: 100%;
    float: left;
}

.setshare-wrap .setshare-top .avator .avator-row .avator-item .avator-item-img {
    margin: 0 20rpx;
    width: 100rpx;
    height: 100rpx;
    padding: 0;
    background-repeat: no-repeat;
    background-size: 100%;
    border-radius: 100%;
}

.setshare-wrap .setshare-top .avator .avator-row .avator-item button,.setshare-wrap .setshare-top .avator .avator-row .avator-item button.button-hover {
    color: rgba(0,0,0,0);
    background-color: rgba(0,0,0,0);
}

.setshare-wrap .setshare-top .avator .avator-row .avator-item button::after {
    content: none;
}

.setshare-wrap .setshare-top .avator .avator-row .avator-item.avator-add .avator-item-img {
    background-image: url(https://staticwx.cdn.bcebos.com/mini-program/images/ic_add_people_big_v3.png);
}

.avator-item.avator-del .avator-item-img {
    background-image: url(https://staticwx.cdn.bcebos.com/mini-program/images/ic_del_people_big_v3.png);
}

.setshare-wrap .setshare-top .avator .avator-row .avator-item .avator-item-name {
    text-align: center;
    width: 110rpx;
    margin: 0 auto;
    font-size: 24rpx;
    line-height: 26rpx;
    margin-top: 26rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #212121;
}

.setshare-wrap .setshare-top .avator .avator-more {
    width: 100%;
    height: 26rpx;
    font-size: 26rpx;
    line-height: 26rpx;
    color: #999;
    padding-bottom: 50rpx;
    display: flex;
    flex-flow: row nowrap;
    justify-content: center;
    align-items: center;
}

.setshare-wrap .setshare-top .avator .avator-more .more-icon {
    width: 20rpx;
    height: 22rpx;
    margin-left: 6rpx;
    background: url(https://staticwx.cdn.bcebos.com/mini-program/images/ic_see_more.png) no-repeat center center;
    background-size: contain;
}

.setshare-wrap .setshare-top .group-name {
    display: flex;
    flex-flow: row nowrap;
    justify-content: flex-start;
    align-items: center;
    height: 124rpx;
    background-color: #FFF;
    margin-top: 10rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: #212121;
}

.setshare-wrap .setshare-top .group-name .group-name-title {
    height: 100%;
    margin: 0 38rpx;
    flex-grow: 0;
    flex-shrink: 0;
    display: flex;
    align-items: center;
}

.setshare-wrap .setshare-top .group-name .group-name-text {
    width: auto;
    height: 100%;
    margin-right: 32rpx;
    flex-grow: 1;
    line-height: 50rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-align: end;
    -webkit-box-pack: center;
    -webkit-line-clamp: 2;
}

.setshare-wrap .setshare-top .group-name .group-name-text .file-name {
    margin-right: 28rpx;
    color: #9E9E9E;
}

.setshare-wrap .setshare-top .group-name .group-name-text .icon-sharedir-rename {
    width: 16rpx;
    height: 28rpx;
    background-size: 16rpx 24rpx;
    background-repeat: no-repeat;
    background-position: center;
    display: inline-block;
    vertical-align: middle;
    margin-top: -8rpx;
}

.setshare-bottom {
    width: 100%;
    height: 128rpx;
    line-height: 128rpx;
    text-align: center;
}

.setshare-bottom.fixed {
    position: fixed;
    left: 0;
    bottom: 0;
}

.setshare-bottom .bottom-button {
    height: 100%;
    border: none;
    color: #FE436A;
    font-size: 32rpx;
    font-weight: bold;
}

.op-select-wrap {
    display: flex;
    margin-top: 14rpx;
    font-size: 26rpx;
    color: #999;
}

.op-select-wrap .op-select {
    position: relative;
    top: 5rpx;
    width: 34rpx;
    height: 34rpx;
    margin-right: 12rpx;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_select_normal_new.png') no-repeat center;
    background-size: 34rpx;
}

.op-select-wrap .op-select.active {
    width: 34rpx;
    height: 34rpx;
    background: url('https://staticwx.cdn.bcebos.com/mini-program/images/ic_select_pressed.png') no-repeat center;
    background-size: 34rpx;
}