<template name="gamePlayerStats">
    <view class="player-stat-table">
        <view class="player-stat-data" wx:for="{{playerStatsArr}}" wx:for-index="playerIdx" wx:for-item="playerItem" wx:key="playerIdx">
            <text class="player-item" wx:for="{{playerItem}}" wx:for-index="playerItemIdx" wx:for-item="playerItemItem" wx:key="playerItemIdx">{{playerItemItem}}</text>
        </view>
    </view>
</template>
