.empty {
    padding-top: 64rpx;
    text-align: center;
    color: #adb4be;
    font-size: 32rpx;
    line-height: 1.5em;
}

.loading {
    text-align: center;
    color: #93989f;
    background-color: #f3f3f7;
    font-size: 26rpx;
    line-height: 50px;
}

.one-day {
    padding-bottom: 40rpx;
    color: #212121;
    background-color: #f3f3f7;
    font-size: 28rpx;
}

.one-day.versus {
    padding-bottom: 0;
}

.one-day.versus .day-title:first-of-type {
    padding-top: 40rpx;
}

.one-day .day-title {
    padding-left: 24rpx;
    background-color: #f3f3f7;
    font-size: 24rpx;
    line-height: 64rpx;
}

.one-day .day-title .calendar-icon {
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    margin-left: 12rpx;
    vertical-align: text-bottom;
    background-size: cover;
}