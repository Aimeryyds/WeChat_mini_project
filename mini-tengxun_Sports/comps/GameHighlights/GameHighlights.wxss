.video-list-container {
    position: relative;
    padding-bottom: 20px;
    border-bottom: 8px solid #ebedf0;
}

.video-list-tab {
    position: absolute;
    top: 0;
    left: 0;
    width: 32px;
    height: 88px;
    font-size: 12px;
}

.video-list-tab .tab1,.video-list-tab .tab2 {
    width: 12px;
    height: 34px;
    padding: 5px 8px;
    text-align: center;
    letter-spacing: 30px;
    background-color: #f4f5f7;
    font-size: 12px;
    line-height: 16px;
}

.video-list-tab .tab1 {
    border-radius: 0 4px 0 0;
}

.video-list-tab .tab2 {
    border-radius: 0 0 4px 0;
}

.video-list-tab .tab1.on {
    width: 16px;
    color: white;
    background: url(data:image/png;base64,);
    background-size: 32px 44px;
}

.video-list-tab .tab2.on {
    width: 16px;
    color: white;
    background: url(data:image/png;base64,);
    background-size: 32px 44px;
}

.video-list-item {
    position: relative;
    display: -webkit-box;
    float: left;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    width: 132px;
    height: 60px;
    margin-left: 16px;
    padding: 14px 16px;
    border-radius: 4px;
    background-color: #F3F3F7;
    font-size: 14px;
}

.video-list-text {
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
}

.video-list-item.on {
    color: #4177F6;
}

.video-highlight-icon {
    position: absolute;
    top: 0;
    right: 0;
    height: 14px;
    padding: 0 2px;
    text-align: center;
    color: white;
    border-radius: 0 3px 0 3px;
    background-color: #2B58E0;
    font-size: 10px;
    line-height: 14px;
}

.video-scoll-wrap {
    width: 100%;
    height: 88px;
    margin-left: 32px;
}

.video-vip-icon {
    display: inline-block;
    width: 14px;
    height: 14px;
    background: url(data:image/png;base64,);
    background-size: 14px 14px;
}