<view bindtap="goPostList" class="city">
    <image class="icon icon-kemp" src="/assets/FIBA/icon_theone@3x.png" wx:if="{{kemp}}"></image>
    <view class="circle">
        <view class="filter" style="{{filterStyle}}">
            <image lazyLoad class="img-grey-city" mode="aspectFit" src="{{cityInfo.greyIcon}}"></image>
        </view>
        <image class="img-city" src="{{cityInfo.icon}}"></image>
    </view>
</view>
