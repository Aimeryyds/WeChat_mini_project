.city {
    position: relative;
    width: 280rpx;
    height: 280rpx;
}

.city .icon-kemp {
    width: 51rpx;
    height: 51rpx;
    position: absolute;
    right: 0;
    top: 0;
}

.city .circle {
    width: 100%;
    height: 100%;
    border-radius: 50%;
    overflow: hidden;
    position: relative;
    border: 8rpx solid #c1ddf4;
}

.city .circle .filter {
    height: 0;
    overflow: hidden;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    z-index: 1;
}

.city .circle .img-grey-city {
    width: 280rpx;
    height: 280rpx;
}

.city .circle .img-city {
    width: 280rpx;
    height: 280rpx;
}