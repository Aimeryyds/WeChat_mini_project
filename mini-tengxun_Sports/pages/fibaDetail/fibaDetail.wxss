page {
    background: #f3f3f7;
}

page .content {
    margin-top: -20rpx;
    padding-bottom: 100rpx;
}

page .content .title {
    padding: 0 20rpx;
    background: #fff;
    margin-top: 20rpx;
    border-bottom: 1px solid #EAEAEA;
    height: 68rpx;
    line-height: 68rpx;
    font-size: 28rpx;
    color: #333333;
}

page .content .empty {
    background: #fff;
    text-align: center;
    padding: 40rpx 0;
}

page .content .empty .img {
    display: inline-block;
    width: 140rpx;
    height: 108rpx;
}

page .content .empty .txt {
    font-size: 28rpx;
    color: #ADAFB5;
    text-align: center;
    line-height: 28rpx;
    margin-top: 16rpx;
}

page .m-home {
    position: fixed;
    right: 30rpx;
    bottom: 150rpx;
    z-index: 1001;
    width: 80rpx;
    height: 80rpx;
}