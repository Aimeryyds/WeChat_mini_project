html,body {
    margin: 0;
    background-color: #fff;
}

.date-nav {
    width: 100%;
    height: 50px;
    color: #576b95;
    border-bottom: 1px solid #D6D7DC;
    background-color: #fff;
    font-size: 14px;
    line-height: 50px;
}

.icon-base {
    width: 50px;
    height: 40px;
    text-indent: -10000px;
}

.pre-day.disable {
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgAgMAAAAOFJJnAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAJUExURUxpcdfX3dbX3Ba2E+MAAAACdFJOUwCAmytOGAAAADBJREFUGNNjYCASMDpAGWwToAwpGCMLKsW4EqZkCQVKEAy4FFwxXDvZiiage4cAAABouQ733DGGOgAAAABJRU5ErkJggg==);
}

.next-day.disable {
    background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgAgMAAAAOFJJnAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAJUExURUxpcdfX3dbX3Ba2E+MAAAACdFJOUwCAmytOGAAAADBJREFUGNNjYCAKMDpAGWwTYIwlMKmVMEVZMEVSFCqCM+BScMUw7WQpgTse7h2CAABlyw737F65+gAAAABJRU5ErkJggg==);
}

.pre-day {
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgAgMAAAAOFJJnAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAJUExURUxpcVdrlVdrlSQW+8sAAAACdFJOUwCAmytOGAAAADBJREFUGNNjYCASMDpAGWwToAwpGCMLKsW4EqZkCQVKEAy4FFwxXDvZiiage4cAAABouQ733DGGOgAAAABJRU5ErkJggg==) no-repeat;
    background-position: 15px 16px;
    background-size: 16px 16px;
}

.next-day {
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAACAAAAAgAgMAAAAOFJJnAAAABGdBTUEAALGPC/xhBQAAAAFzUkdCAK7OHOkAAAAJUExURUxpcVdrlVdrlSQW+8sAAAACdFJOUwCAmytOGAAAADBJREFUGNNjYCAKMDpAGWwTYIwlMKmVMEVZMEVSFCqCM+BScMUw7WQpgTse7h2CAABlyw737F65+gAAAABJRU5ErkJggg==) no-repeat;
    background-position: 20px 16px;
    background-size: 16px 16px;
}

.pre-day {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 19%;
}

.cur-day {
    text-align: center;
}

.next-day {
    position: absolute;
    top: 0;
    right: 19%;
}

.calendar-icon {
    display: inline-block;
    width: 20px;
    height: 20px;
    margin-left: 12px;
    vertical-align: -5px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 20px 20px;
}

.gif-preview {
    display: block;
    width: 100%;
}

.play-btn {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 52px;
    height: 52px;
    margin-top: -26px;
    margin-left: -26px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 52px 52px;
}

.gif-title {
    position: absolute;
    bottom: 0;
    width: 100%;
    padding: 0 12px 12px 12px;
    color: #fff;
    font-size: 16px;
    line-height: 20px;
}

.gif-background {
    position: absolute;
    bottom: 0;
    width: 100%;
    height: 72px;
    background-image: linear-gradient(-180deg,rgba(0,0,0,0) 0%,rgba(0,0,0,0.6) 100%);
}

.gif-item {
    position: relative;
    margin-bottom: 4px;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.content {
    min-height: 350px;
}

.more-match {
    position: fixed;
    right: 16px;
    bottom: 16px;
    width: 52px;
    height: 52px;
    -webkit-transform: translateZ(0);
    opacity: 0.8;
    border-radius: 26px;
    background-image: url(data:image/png;base64,);
    background-size: 52px 52px;
    box-shadow: 0 2px 4px 0 rgba(0,0,0,0.3);
}

image>div {
    background-size: 100% 100%;
}

.match-info-list {
    height: 1px;
    margin-top: 51px;
}

.no-match {
    margin-top: 80px;
    text-align: center;
    color: #adb4be;
    font-size: 16px;
}

.loading {
    text-align: center;
    color: #93989f;
    background-color: #f3f3f7;
    font-size: 26rpx;
    line-height: 50px;
}