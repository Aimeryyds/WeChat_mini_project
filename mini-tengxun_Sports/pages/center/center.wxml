<button bindtap="onClickHeader" class="center-header">
    <TuaImage class="avatar" errSrc="{{defaultAvatarUrl}}" height="120rpx" src="{{avatarUrl}}" styleStr="border-radius: 50%;" width="120rpx"></TuaImage>
    <view class="nick-name">{{nickName}}</view>
</button>
<view class="center-body">
    <view bindtap="navigateToAttendList" class="attend-list">
        <image class="icon" src="/assets/attend_list@3x.png"></image>
        <text class="desc">我的预约</text>
        <image class="arrow" src="/assets/attend_list_arrow@3x.png"></image>
    </view>
</view>
<view class="center-body">
    <view bindtap="myFibaPublish" class="attend-list">
        <image class="icon" src="/assets/FIBA/icon_wdfb@3x.png"></image>
        <text class="desc">我的发布</text>
        <image class="arrow" src="/assets/attend_list_arrow@3x.png"></image>
    </view>
</view>
