html,body {
    margin: 0;
    padding: 0;
    width: 100%;
    height: 100%;
}

#container {
    height: 100%;
}

.match-type {
    overflow: hidden;
    letter-spacing: -0.34px;
    border-bottom: 8px solid #f3f3f7;
}

.match-type:last-of-type {
    border-bottom: 0;
}

.match-item {
    width: 33%;
    float: left;
    text-align: center;
    padding: 16px 0;
    height: 66px;
}

.match-type-name {
    font-size: 14px;
    color: #9C9C9C;
    text-align: center;
    margin-top: 10px;
}

.match-icon {
    width: 44px;
    height: 44px;
    margin: 0 auto;
}

.navigator-hover {
    background-color: #F7F9FC;
}

image>div {
    background-size: 100% 100%;
}