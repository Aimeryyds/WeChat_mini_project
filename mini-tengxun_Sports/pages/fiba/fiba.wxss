.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

page {
    background: #f3f3f7;
}

page .fixed {
    position: sticky;
    left: 0;
    top: 0;
    right: 0;
    z-index: 29;
}

page .login {
    height: 148rpx;
    width: 100%;
    padding: 20rpx;
    position: relative;
    box-sizing: border-box;
}

page .login .bg {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
}

page .login .content {
    position: relative;
    z-index: 2;
    font-size: 36rpx;
    color: #FFFFFF;
    line-height: 36rpx;
    top: 15rpx;
}

page .login .content .user {
    position: relative;
    display: inline-block;
}

page .login .content .user .photo {
    width: 84rpx;
    height: 84rpx;
    border-radius: 50%;
    vertical-align: middle;
    margin-right: 24rpx;
}

page .login .content .user .name {
    display: inline-block;
}

page .login .yy-btn {
    position: absolute;
    top: 22rpx;
    right: 0;
    z-index: 3;
    width: 280rpx;
    height: 100rpx;
    padding-right: 28rpx;
    box-sizing: border-box;
}

page .login .yy-btn .yy-bg {
    display: inline-block;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
}

page .login .yy-btn .yy-txt {
    position: relative;
    z-index: 3;
    font-size: 32rpx;
    color: #FFF;
    line-height: 32rpx;
    top: 30rpx;
    text-align: right;
}

page .login .yy-btn .yy-txt .icon {
    display: inline-block;
    width: 44rpx;
    height: 44rpx;
    vertical-align: middle;
    position: relative;
    top: -3rpx;
}

page .nav {
    background: #fff;
    height: 84rpx;
    box-sizing: border-box;
}

page .nav .li {
    position: relative;
    display: inline-block;
    width: 33.33333%;
    text-align: center;
    font-size: 30rpx;
    color: #999;
    line-height: 84rpx;
}

page .nav .li .text {
    width: 80rpx;
    height: 4rpx;
    background: #fff;
    border-radius: 1px;
    position: absolute;
    z-index: 3;
    left: 0;
    right: 0;
    margin: 0 auto;
    bottom: 10rpx;
}

page .nav .li.active {
    color: #2B7FFB;
}

page .nav .li.active .text {
    background: #2B7FFB;
}

page .tabcon {
    display: none;
}

page .tabcon.square .square-empty {
    text-align: center;
    margin-top: 150rpx;
}

page .tabcon.square .square-empty .img {
    width: 208rpx;
    height: 200rpx;
    display: inline-block;
}

page .tabcon.square .square-empty .txt {
    margin: 20rpx 0 36rpx 0;
    font-size: 32rpx;
    color: #ADAFB5;
    text-align: center;
    line-height: 32rpx;
}

page .tabcon.rank {
    position: relative;
    padding-top: 125rpx;
}

page .tabcon.rank .rank-bg {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    height: 532rpx;
    width: 100%;
    z-index: 1;
}

page .tabcon.rank .citytabs {
    position: relative;
    z-index: 2;
    white-space: nowrap;
    word-break: keep-all;
    text-align: center;
    padding: 0 5rpx;
    overflow-x: auto;
    border-top: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
    border-image: linear-gradient(left,#E3F2FF,#85b5de,#E3F2FF) 20 20;
    margin-top: 20rpx;
}

page .tabcon.rank .citytabs .li {
    margin: 0 20rpx;
    padding: 30rpx 0rpx;
    display: inline-block;
    font-size: 26rpx;
    opacity: 0.96;
    color: #006BB6;
    line-height: 26rpx;
    font-weight: bold;
}

page .tabcon.rank .citytabs .li.active {
    opacity: 1;
    font-size: 32rpx;
}

page .tabcon.rank .content {
    position: relative;
    z-index: 2;
    background: #E3F2FF;
}

page .tabcon.rank .tips {
    font-size: 24rpx;
    color: #006BB6;
    line-height: 24rpx;
    position: relative;
    z-index: 2;
    padding-left: 200rpx;
}

page .tabcon.rank .tips .supporter {
    position: absolute;
    left: 24rpx;
    top: -3rpx;
    font-weight: normal;
}

page .tabcon.rank .tips .supporter .supporter-logo {
    width: 100rpx;
    height: 36rpx;
    position: relative;
    top: -3rpx;
    vertical-align: middle;
}

page .tabcon.rank .citybox {
    position: relative;
    background: #E3F2FF;
    padding-top: 120rpx;
}

page .tabcon.rank .citybox .city-bg {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 532rpx;
    width: 100%;
    z-index: 1;
}

page .tabcon.rank .citybox .intro {
    font-size: 26rpx;
    color: #006BB6;
    line-height: 20rpx;
    margin-top: 20rpx;
    margin-left: 200rpx;
    position: relative;
    z-index: 2;
}

page .tabcon.rank .citybox .intro .blue-txt {
    font-size: 32rpx;
}

page .tabcon.rank .citybox .city {
    padding: 20rpx 0;
    position: relative;
    z-index: 2;
}

page .tabcon.rank .citybox .city .li {
    width: 50%;
    display: inline-block;
    text-align: center;
    margin: 20rpx 0;
}

page .tabcon.rank .citybox .city .li .compscity {
    display: inline-block;
}

page .tabcon.rank .citybox .city .li .label {
    background: rgba(0,107,182,0.1);
    display: inline-block;
    padding: 5rpx 10rpx;
    font-size: 28rpx;
    color: #2B7FFB;
    text-align: center;
    line-height: 28rpx;
    font-weight: bold;
}

page .tabcon.samecity .auth-location {
    text-align: center;
    margin-top: 100rpx;
}

page .tabcon.samecity .auth-location .img {
    width: 208rpx;
    height: 200rpx;
    display: inline-block;
}

page .tabcon.samecity .auth-location .txt {
    margin: 20rpx 0 36rpx 0;
    font-size: 32rpx;
    color: #ADAFB5;
    text-align: center;
    line-height: 32rpx;
}

page .tabcon.samecity .auth-location .btn {
    position: relative;
    width: 200rpx;
    border: 1px solid #2B7FFB;
    border-radius: 4rpx;
    font-size: 32rpx;
    color: #2B7FFB;
    line-height: 32rpx;
    display: inline-block;
    padding: 16rpx 0;
    text-align: center;
}

page .show {
    display: block!important;
}

page .f-fix-btn {
    position: fixed;
    right: 30rpx;
    bottom: 70rpx;
    background: #2d74df;
    width: 80rpx;
    height: 80rpx;
    font-size: 24rpx;
    color: #FFFFFF;
    line-height: 24rpx;
    text-align: center;
    border-radius: 50%;
    box-sizing: border-box;
    padding: 16rpx;
    z-index: 11;
}

page .f-no-data {
    text-align: center;
    position: relative;
    padding-top: 150rpx;
}

page .f-no-data .img {
    width: 208rpx;
    height: 200rpx;
    display: inline-block;
}

page .f-no-data .txt {
    font-size: 32rpx;
    color: #ADAFB5;
    text-align: center;
    line-height: 32rpx;
    margin-top: 28rpx;
}

page .f-no-data .btn {
    width: 200rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    border: 1px solid #2B7FFB;
    border-radius: 4rpx;
    font-size: 32rpx;
    color: #2B7FFB;
    display: inline-block;
    margin-top: 36rpx;
}

page .hot-topic {
    padding: 18rpx 20rpx 5rpx;
    background: #fff;
    position: relative;
    display: none;
    max-height: 137rpx;
    overflow: hidden;
}

page .hot-topic .bg {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
    width: 100%;
    height: 100%;
}

page .hot-topic .top {
    position: relative;
    z-index: 2;
    font-size: 24rpx;
    line-height: 24rpx;
}

page .hot-topic .top .title {
    float: left;
    color: #666;
}

page .hot-topic .top .more {
    position: absolute;
    z-index: 3;
    right: 0;
    top: -2rpx;
    color: #0061B8;
    background: #CADFFD;
    border-radius: 18rpx;
    padding: 6rpx 12rpx;
}

page .hot-topic .con {
    position: relative;
    z-index: 2;
    margin-top: 10rpx;
}

page .hot-topic .con .li {
    display: inline-block;
    font-size: 32rpx;
    color: #71BCFF;
    height: 32rpx;
    line-height: 32rpx;
    margin-right: 50rpx;
}

page .hot-topic .con .li.active {
    color: #0061B8;
}

page .layer-banner {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: -10;
    opacity: 0;
    background: rgba(0,0,0,0.6);
    padding: 36rpx 60rpx 50rpx 60rpx;
}

page .layer-banner .content {
    position: absolute;
    width: 594rpx;
    height: 960rpx;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

page .layer-banner .content .close {
    position: absolute;
    top: 0;
    right: 0;
    width: 60rpx;
    height: 60rpx;
    z-index: 11;
}

page .layer-banner .content .img {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    right: 0;
    z-index: 1;
    top: 0;
    bottom: 0;
}

page .layer-banner .content .mask {
    height: 144rpx;
    position: absolute;
    z-index: 2;
    left: 0;
    right: 0;
    bottom: 0;
    width: 594rpx;
    overflow: hidden;
}

page .layer-banner .content .btn {
    text-align: center;
    margin: 0 auto;
    width: 440rpx;
    height: 64rpx;
    line-height: 64rpx;
    font-size: 32rpx;
    color: #FFFFFF;
    position: absolute;
    bottom: 112rpx;
    left: 0;
    right: 0;
    z-index: 3;
}

page .layer-banner .content .btn .btn-bg {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

page .layer-banner .content .btn .btn-txt {
    position: relative;
    z-index: 4;
}

page .layer-banner .content .rules {
    position: absolute;
    z-index: 3;
    bottom: 40rpx;
    text-align: center;
    left: 0;
    right: 0;
    font-size: 32rpx;
    color: #FFFFFF;
    letter-spacing: 4rpx;
    line-height: 32px;
    text-shadow: 0 0 10rpx #000000;
}

page .animated {
    z-index: 40;
    animation: scalediv 1s linear 0.1s 1 forwards;
}

@-webkit-keyframes scalediv {
    from {
        opacity: 1;
        transform: scale(0,0);
    }

    to {
        opacity: 1;
        transform: scale(1,1);
    }
}

@keyframes scalediv {
    from {
        opacity: 1;
        transform: scale(0,0);
    }

    to {
        opacity: 1;
        transform: scale(1,1);
    }
}

page .animatedredheart {
    animation: redheart 0.3s ease;
}

@-webkit-keyframes redheart {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}

@keyframes redheart {
    from {
        opacity: 1;
    }

    to {
        opacity: 0;
    }
}