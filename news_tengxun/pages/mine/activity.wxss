.empty-wrapper {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    flex-direction: column;
    height: 100%;
    width: 100%;
}

.empty-icon {
    width: 362rpx;
    height: 226rpx;
    margin-top: 400rpx;
}

.empty-text {
    font-family: PingFangSC;
    font-size: 28rpx;
    font-weight: 400;
    font-style: normal;
    font-stretch: normal;
    line-height: 1.57;
    letter-spacing: normal;
    text-align: center;
    color: #888;
}

.bottom-loading-bar {
    display: block;
}