.attentionbox {
    width: 100rpx;
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 20rpx 0;
}

.attentionbox image {
    width: 40rpx;
    height: 40rpx;
}

.fundcode {
    width: 160rpx;
}

.bar-color {
    background-color: #FBA52F!important;
    border-top: 1rpx solid #FBA52F!important;
    border-bottom: 1rpx solid #FBA52F!important;
}

.search-cancel {
    margin-left: 10px;
    line-height: 28px;
    color: #FFFFFF;
    white-space: nowrap;
    font-size: 16px;
}

.weui-panel {
    margin-top: 0px;
}

.weui-panel__hd {
    padding: 6px 15px 5px;
    background-color: #F2F2F2;
    color: #333333;
}

.weui-panel__hd::after {
    border-bottom: none;
}

.weui-cells {
    font-size: 14px;
}

.weui-cell {
    padding: 0px 15px 0px 0px;
    color: #333333;
    border-top: 1px solid #E5E5E5;
    text-align: left;
    font-size: 14px;
    line-height: 88rpx;
}

.weui-cell_nodata {
    text-align: center;
    color: #999999;
    padding: 12px 15px;
}

.clear-history {
    text-align: center;
    display: inline-block;
    height: 43px;
    line-height: 40px;
}

.clear-history image {
    width: 14px;
    height: 14px;
    vertical-align: middle;
}

.clear-history text {
    font-size: 14px;
    color: #999999;
    vertical-align: middle;
}