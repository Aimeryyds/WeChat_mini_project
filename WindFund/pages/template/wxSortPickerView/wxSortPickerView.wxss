.wxSortPickerViewTemTags {
    width: 30px;
    height: 405px;
    position: fixed;
    top: 0;
    right: 0;
    background-color: rgba(0,0,0,0);
    color: #FBA52F;
    bottom: 0;
    margin: auto;
    z-index: 10;
    text-align: center;
}

.wxSortPickerViewTemTag {
    text-align: right;
    padding-right: 10rpx;
    font-size: 12px;
    line-height: 15px;
}

.wxSortPickerViewFixedTag {
    position: fixed;
    width: 100%;
}

.wxSortPickerViewTag {
    background-color: #eee;
    font-size: 16px;
    font-weight: bold;
    color: #666;
    height: 30px;
    line-height: 30px;
    padding: 0 5px;
    padding-left: 10px;
}

.wxSortPickerViewItem {
    height: 40px;
    line-height: 40px;
    border-bottom: 1px solid #eee;
    font-size: 14px;
    padding-left: 10px;
    color: #333;
}