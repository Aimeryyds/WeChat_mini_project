.conBox {
    width: 100%;
    height: 45px;
    overflow: hidden;
}

.swiper-tab {
    width: 100%;
    height: 45px;
    line-height: 43px;
    background: #fff;
    position: fixed;
    z-index: 999;
    overflow: hidden;
    border-bottom: 1px solid #f1f1f1;
}

.swiper-tab-list {
    display: inline-block;
    width: 14.28%;
    color: #333;
    text-align: center;
    font-size: 17px;
}

.on {
    color: #000;
    border-bottom: 2px solid #f04141;
}

.swiper-box {
    display: block;
    height: auto;
    width: 100%;
    overflow: hidden;
}

.navImg {
    width: 100%;
    height: 89.6px;
}

.main {
    width: 93%;
    margin: 0 3.5%;
    padding: 15px 0;
    border-bottom: 1px solid #ddd;
    overflow: hidden;
}

.leftBox {
    width: 65%;
    float: left;
}

.rigBox {
    float: right;
    width: 32%;
    height: 4.375rem;
}

.rigBox image {
    width: 100%;
    height: 100%;
    display: block;
}

.main_scroll {
    width: 100%;
    margin: 0;
    padding: 5px 0;
    border-bottom: 0;
}

.borders {
    width: 92%;
    margin: 0 auto;
    padding-top: 5px;
    border-bottom: 1px solid #ddd;
}

.scroll_roll {
    width: 100%;
    height: 10000px;
}

.solid {
    width: 1px;
    height: 100%;
    position: fixed;
    left: 3.25%;
    border-left: 2px solid #fde7e7;
}

.main_scroll .timeBox {
    width: 90%;
    padding: 0 5%;
}

.main_scroll .content {
    width: 90%;
    padding: 0 5%;
}

.title {
    max-height: 48px;
    font-size: 16px;
    line-height: 24px;
    overflow: hidden;
    color: #333;
    font-weight: 600;
    display: block;
}

.content {
    margin: 5px 0;
    font-size: 13px;
    line-height: 18px;
    max-height: 37px;
    overflow: hidden;
    color: #000;
    display: block;
}

.timeBox {
    width: 100%;
    max-height: 37px;
    font-size: 12px;
    line-height: 18px;
    overflow: hidden;
}

.times {
    margin: 5px 0;
    color: #999;
    line-height: 18px;
    padding-right: 10px;
    max-height: 37px;
    font-size: 12px;
    float: left;
    position: relative;
}

.circle:after {
    content: '';
    display: table;
    width: 4px;
    height: 4px;
    background: #f04141;
    border-radius: 50%;
    position: absolute;
    left: -0.42rem;
    top: 8px;
}

.times2 {
    margin: 5px 0;
    color: #999;
    float: right;
    padding-right: 10px;
}

.from {
    margin: 5px 0;
    font-size: 12px;
    line-height: 18px;
    max-height: 37px;
    overflow: hidden;
    color: #999;
    float: left;
}

.layerBoxs {
    width: auto;
    height: 26px;
    line-height: 26px;
    position: fixed;
    right: 0px;
    bottom: 100px;
    color: #fff;
    background: rgba(0,0,0,0.7);
    font-size: 12px;
    text-align: center;
    border-top-left-radius: 100px;
    border-bottom-left-radius: 100px;
}

.layerBoxs view {
    padding: 0 10px;
}

.tuiBg {
    width: 100%;
    height: 0;
    color: #fff;
    text-align: center;
    font-size: 0.75rem;
    background: #a7d1ee;
    opacity: 0;
    transition: all .5s ease-in;
}

.moves {
    opacity: 1;
    height: 1.3rem;
    line-height: 1.3rem;
}

.again-box {
    width: 100%;
    height: 2.3rem;
    font-size: 0.75rem;
    color: #6facec;
    background: #f0f0f0;
}

.again-click {
    width: 60%;
    height: 1.5rem;
    margin: 0.4rem auto;
    background: #fff;
    line-height: 1.5rem;
    text-align: center;
    border-radius: 5rem;
}

.load-pic {
    width: 0.8rem;
    height: 0.8rem;
    vertical-align: middle;
}

.layerBoxs {
    width: auto;
    height: 26px;
    line-height: 26px;
    position: fixed;
    right: 0px;
    bottom: 100px;
    color: #fff;
    background: rgba(0,0,0,0.7);
    font-size: 12px;
    text-align: center;
    border-top-left-radius: 100px;
    border-bottom-left-radius: 100px;
}

.layerBoxs navigator {
    padding: 0 10px;
}

.zhi-ding {
    border: 1px solid #4990E2;
    color: #4990E2;
    float: left;
    font-size: 0.6rem;
    margin: 5px 5px 0 0px;
    padding: 2px 5px;
}

.re-ding {
    border: 1px solid #F04141;
    color: #F04141;
    float: left;
    font-size: 0.6rem;
    margin: 5px 2px 0 0px;
    padding: 2px 2px;
}

.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.6;
}

.supply-modal {
    width: 100%;
    position: fixed;
    top: 300rpx;
    z-index: 9999;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.supply-modal-body {
    text-align: center;
    display: block;
    color: white;
    margin: 0rpx 100rpx;
    border-radius: 20rpx;
}

.supply-modal-body .supply-title {
    font-size: 38rpx;
    line-height: 100%;
    padding: 20rpx;
}

.supply-modal-body .supply-content {
    border-radius: 20rpx;
    background: white;
    padding: 50rpx 20rpx;
}

.supply-modal-body .supply-step {
    color: #333333;
    font-size: 32rpx;
    line-height: 100%;
    padding: 20rpx;
}

.btn-save-supply-image {
    display: inline-block;
    font-size: 38rpx;
    line-height: 100%;
    padding: 22rpx 60rpx;
    background: #f03f3f;
    border-radius: 50px;
    margin-top: 30rpx;
}

.btn-supply-contact {
    background: transparent;
    padding: 0px;
    margin: 0px;
    font-size: 30rpx;
    line-height: 100%;
    color: white;
}

.btn-supply-contact::after {
    border: none;
}

.ads-box {
    position: fixed;
    z-index: 999999;
    width: 100%;
}

.ads-box image {
    width: 100%;
    display: block;
}

.ads-inner {
    position: relative;
}

.ads-close {
    background: url("data:image/png;base64,");
    background-size: 100% 100%;
    background-repeat: repeat;
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    border-radius: 50%;
    z-index: 1000000;
}

.ads-close-icon {
    background: url("data:image/png;base64,");
    width: 50rpx;
    height: 50rpx;
    background-size: 60% 60%;
    background-repeat: no-repeat;
    background-position: center center;
}

.ads-contact-btn {
    border: 0px;
    background: transparent;
    padding: 0rpx;
    margin: 0rpx;
    width: 100%;
    height: 100%;
    border-radius: 0px;
    position: absolute;
    left: 0px;
    top: 0px;
}

.ads-contact-btn::after {
    border: none;
}

.ads-contact-btn:active {
}