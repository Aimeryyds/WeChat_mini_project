.navImg {
    width: 100%;
    height: 89.6px;
    position: fixed;
    bottom: 0;
    border-top: 1px solid #f0f0f0;
    background: #fff;
}

.contentBox {
    width: 100%;
    height: auto;
}

.contentTitle {
    width: 90%;
    margin: 0 auto;
    font-weight: 600;
    font-size: 18px;
    line-height: 25px;
    display: block;
    overflow: hidden;
    padding: 15px 0;
}

.contentMsg {
    width: 90%;
    margin: 0 auto;
    font-size: 12px;
    color: #999;
    line-height: 25px;
    display: block;
    overflow: hidden;
}

.conTime {
    float: left;
}

.conCheck {
    float: right;
}

.contentMsg image {
    width: 16px;
    height: 16px;
    vertical-align: middle;
    padding-right: 5px;
}

.conMain {
    width: 90%;
    margin: 15px auto;
    display: block;
    font-size: 14px;
    line-height: 25px;
    margin-bottom: 90px;
}

.conMain .wxParse-p {
    padding: 8px 0;
    font-size: 16px;
}

.ads-box {
    position: fixed;
    z-index: 999999;
    width: 100%;
}

.ads-box image {
    width: 100%;
    display: block;
}

.ads-inner {
    position: relative;
}

.ads-close {
    background: url("data:image/png;base64,");
    background-size: 100% 100%;
    background-repeat: repeat;
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    border-radius: 50%;
    z-index: 1000000;
}

.ads-close-icon {
    background: url("data:image/png;base64,");
    width: 50rpx;
    height: 50rpx;
    background-size: 60% 60%;
    background-repeat: no-repeat;
    background-position: center center;
}

.ads-contact-btn {
    border: 0px;
    background: transparent;
    padding: 0rpx;
    margin: 0rpx;
    width: 100%;
    height: 100%;
    border-radius: 0px;
    position: absolute;
    left: 0px;
    top: 0px;
}

.ads-contact-btn::after {
    border: none;
}

.ads-contact-btn:active {
}