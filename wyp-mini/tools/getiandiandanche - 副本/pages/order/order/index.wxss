}

.add-btn26 {
    height: 40px;
    line-height: 40px;
    color: #fff;
    background: #fccf00;
    margin-top: 20px;
}

.modal-mask {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 9000;
    color: #fff;
}