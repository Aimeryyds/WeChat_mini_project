.box {
}

.top {
    position: relative;
}

.topimg {
    width: 750rpx;
    height: 350rpx;
}

.top text {
    position: absolute;
    left: auto;
    top: auto;
    right: 32rpx;
    bottom: 16rpx;
    color: #fff;
    font-size: 15px;
}

.btn {
    padding: 53px 16px 0 16px;
}

.btn .box {
    background: #01D00D;
    border-radius: 24.5px;
    height: 49px;
    text-align: center;
    position: relative;
}

.btn image {
    width: 34px;
    height: 28px;
    vertical-align: top;
    margin-top: 11px;
}

.btn text {
    color: #fff;
    font-size: 18px;
    margin-left: 16px;
    line-height: 49px;
}

.change {
    padding: 10px 36px 0 36px;
    font-size: 15px;
}

.change .t1 {
    color: #999999;
}

.change .t2 {
    color: #01A9FF;
    float: right;
}

.butbox {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.butbox .button {
    width: 100%;
    height: 100%;
}