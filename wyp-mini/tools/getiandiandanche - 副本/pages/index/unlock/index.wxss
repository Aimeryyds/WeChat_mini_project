page {
    background: #f2f2f2;
}

.page {
    padding-bottom: 58px;
    position: relative;
    height: 100vh;
    overflow-y: auto;
}

.uk-cont {
    padding-top: 20rpx;
}

.uk-top {
    text-align: center;
    margin: auto;
    height: 80rpx;
}

.uk-top .pre {
    font-size: 28rpx;
    color: #fccf00;
    margin-right: 3px;
}

.uk-top .pre.on {
    color: red;
}

.dcimg {
    width: 50rpx;
    height: 26rpx;
    display: inline-block;
    vertical-align: middle;
    margin-right: 15rpx;
}

.longkm {
    font-size: 28rpx;
    color: #fccf00;
    display: inline-block;
    vertical-align: middle;
}

.carimg {
    width: 298rpx;
    height: 218rpx;
    display: block;
    margin: auto;
}

.carno {
    font-size: 26rpx;
    color: #333;
    text-align: center;
    padding: 20rpx;
    padding-bottom: 0;
}

.spicer {
    font-size: 24rpx;
    padding: 30rpx;
}

.fot-btn {
    padding: 30rpx 0 30rpx 0;
    position: fixed;
    left: 0;
    top: auto;
    bottom: 0;
    width: 100%;
}

.loginBtn {
    margin: 0 auto;
    display: block;
    font-size: 32rpx;
    color: white;
    background: #fccf00;
    border-radius: 40rpx;
    padding: 2rpx 0;
    width: 70%;
}

.nk-lod {
    margin-top: 20%;
}

.lod-nk10 {
    margin: 80rpx auto;
    margin-bottom: 25%;
}

.progress {
    width: 80%;
    margin: auto;
}

.ul-li {
    margin: 0;
    padding: 0;
}

.ul-li .li {
    height: 200rpx;
    border-bottom: 2rpx solid #DCDCDC;
    overflow: hidden;
}

.ul-li .img {
    display: block;
    float: left;
    width: 35%;
    height: 100%;
}

.ul-li .txt {
    float: right;
    width: 65%;
    padding-top: 20rpx;
}

.ul-li .txt .p {
    font-size: 34rpx;
    line-height: 42rpx;
    color: #000;
    margin-top: 30rpx;
    font-weight: bold;
}

.ul-li .txt .span {
    font-size: 28rpx;
    margin-top: 0;
    margin-bottom: 20rpx;
    color: #8f8f94;
}

.img-list {
    display: flex;
    border-bottom: 2rpx solid #e4e4e4;
    background: #fff;
    padding-top: 14rpx;
}

.img-list .list-item.info {
    flex: 1;
}

.img-list .img {
    width: 160rpx;
    height: 150rpx;
}

.img-list .img-cen {
    padding-left: 30rpx;
    padding-right: 30rpx;
    text-align: center;
}

.img-list .l-tit {
    font-size: 30rpx;
    font-weight: bold;
}

.img-list .l-tit2 {
    font-size: 28rpx;
    color: #666666;
}

.red {
    color: red;
}

.jft {
    text-align: center;
    font-size: 28rpx;
}

.blue {
    color: #00A9FF;
}

.jifei {
    border-radius: 10px;
    background: #fff;
    box-shadow: 0 0 8px #dbe7fa;
    font-size: 14px;
    padding-top: 13px;
    padding-bottom: 13px;
}

.jifei .t1 {
}

.jifei .t1 image {
    width: 15px;
    height: 21px;
    vertical-align: top;
    margin-left: 7px;
}

.jifei.zhuyi {
    margin-top: 12px;
}

.jifei.zhuyi .t1 image {
    width: 20px;
}

.jifei.zhuyi .t2 .item .dian {
    background: #FFC136;
}

.jifei .t1 text {
    display: inline-block;
    line-height: 21px;
    font-size: 15px;
    font-weight: bold;
    margin-left: 6px;
}

.jifei .t2 {
    padding-left: 14px;
    margin-top: 2px;
}

.jifei .t2 .item {
    display: flex;
}

.jifei .t2 .item .dian {
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background: #00A9FF;
    vertical-align: top;
    margin-top: 6px;
}

.jifei .t2 .item .txt {
    flex: 1;
    vertical-align: top;
    padding-left: 5px;
    padding-right: 10px;
    line-height: 21px;
}