.page {
    overflow-y: auto;
}

.h1-title {
    text-align: center;
    line-height: 70px;
}

.page {
    background: #f1f2f2;
    position: absolute;
    width: 100%;
    height: 100%;
}

.content {
    position: relative;
    width: 100%;
    height: 259rpx;
}

.content image {
    width: 685rpx;
    height: 259rpx;
    margin: 42rpx 33rpx 32rpx;
    display: block;
    z-index: -999;
}

.content .left {
    position: absolute;
    height: 100%;
    width: 50%;
    color: #765600;
    bottom: 0;
    left: 67rpx;
    top: 0;
    padding-top: 47rpx;
    box-sizing: border-box;
}

.down {
    height: 58rpx;
    display: flex;
    flex-direction: column;
    line-height: 58rpx;
    color: #333;
    position: absolute;
    bottom: 13rpx;
    left: 0;
    font-size: 30rpx;
}

.isL {
    font-size: 24rpx;
    line-height: 32rpx;
}

.daoqi {
    color: rgba(102,102,102,1);
}

.content .right {
    position: absolute;
    height: 100%;
    width: 50%;
    bottom: 0;
    right: 0;
}

.area {
    display: flex;
    font-size: 28rpx;
    font-weight: 500;
    color: #765600;
    position: absolute;
    top: 53rpx;
    right: 59rpx;
}

.discount {
    color: #765600;
    font-size: 36rpx;
    position: absolute;
    right: 59rpx;
    top: 90rpx;
    font-weight: bold;
}

.btn {
    text-align: center;
    line-height: 58rpx;
    color: #333;
    position: absolute;
    bottom: 13rpx;
    right: 56rpx;
    font-size: 30rpx;
}

.no {
    text-align: center;
    color: #999;
}

.no .noqi {
    width: 80%;
    margin-top: 30%;
}