page {
    height: 100%;
    overflow: hidden;
    background: rgba(243,244,246,1);
}

.page {
    display: flex;
    flex-direction: column;
    height: 81%;
}

.prompt {
    font-size: 24rpx;
    flex: auto;
    overflow-y: scroll;
    width: 672rpx;
    margin: 25rpx auto;
    color: #333333;
}

.prompt>view:nth-child(1) {
    font-size: 28rpx;
    color: #333333;
    font-weight: Bold;
}

.prompt>view:nth-child(2) {
    margin: 10rpx 0;
}

.btn {
    width: 640rpx;
    height: 88rpx;
    border-radius: 14rpx;
    font-size: 36rpx;
    font-weight: 500;
    color: rgba(255,255,255,1);
    text-align: center;
    line-height: 88rpx;
    position: fixed;
    bottom: 30rpx;
    left: 55rpx;
}

.btnBtn {
    background: #fccf00!important;
    color: rgba(255,255,255,1);
}

.section {
    margin: 10rpx auto;
    width: 100%;
    display: flex;
    width: 686rpx;
    height: 90rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 3px 9px 0px rgba(58,162,252,0.2);
    border-radius: 10rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    padding: 0 0 0 22rpx;
}

.input {
    width: 550rpx;
}

.section__title {
    font-weight: 550;
    line-height: 23rpx;
}