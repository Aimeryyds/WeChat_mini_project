page {
    height: 100%;
    overflow: hidden;
}

.page {
    width: 100%;
    height: 82.5%;
    display: flex;
    flex-direction: column;
    background: rgba(243,244,246,);
}

.touBu {
    width: 100%;
    height: 280rpx;
    background: #03A9F4;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(255,255,255,1);
    line-height: 60rpx;
}

.touBu view:nth-child(2) {
    font-size: 60rpx;
    line-height: 60rpx;
    font-family: PingFang-SC-Medium;
    margin-bottom: 80rpx;
}

.card {
    width: 686rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 3px 9px 0px rgba(58,162,252,0.2);
    border-radius: 10rpx;
    margin: 0 auto;
    margin-top: -10%;
}

.card>view {
    padding: 20rpx 20rpx 0 20rpx;
    display: flex;
    justify-content: space-between;
}

.card>view:nth-child(4) {
    padding-bottom: 20rpx;
}

.card>view view:nth-child(1) {
    font-size: 28rpx;
    font-weight: bold;
    color: rgba(51,51,51,1);
}

.card>view view:nth-child(2) {
    font-size: 28rpx;
    font-weight: 500;
    color: rgba(128,128,128,1);
}

.progress {
    width: 686rpx;
    margin: 0 auto;
}

.progress .main {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20rpx 0;
}

.progress .left {
    width: 22rpx;
    height: 22rpx;
    box-shadow: 2rpx 7rpx 10rpx 0rpx rgba(172,172,172,0.65);
    border-radius: 11rpx;
    margin-left: 20rpx;
}

.yes {
    background: linear-gradient(130deg,rgba(65,212,243,1) 0%,#fccf00 100%);
}

.yesTwo {
    color: rgba(51,51,51,1)!important;
}

.noTwo {
    color: rgba(128,128,128,1)!important;
}

.no {
    background: #ACACAC;
}

.sueccesNo {
    color: rgba(244,94,94,1)!important;
}

.progress .right {
    width: 584rpx;
    height: 80rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 3rpx 9rpx 0rpx rgba(58,162,252,0.2);
    border-radius: 10rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20rpx;
}

.progress .right>view:nth-child(1) {
    font-size: 28rpx;
    font-weight: bold;
    color: rgba(51,51,51,1);
}

.isSucceedTwo {
    position: fixed;
    bottom: 148rpx;
    left: 32rpx;
    font-size: 24rpx;
    font-weight: 500;
    color: rgba(153,153,153,1);
}

.progress .right>view:nth-child(2) {
    font-size: 22rpx;
    font-weight: 500;
    color: rgba(128,128,128,1);
}

.prompt {
    font-size: 24rpx;
    width: 672rpx;
    margin: 40rpx auto;
    color: #333333;
    flex: auto;
    overflow-y: scroll;
}

.prompt>view:nth-child(1) {
    font-size: 28rpx;
    color: #333333;
    font-weight: Bold;
}

.prompt>view:nth-child(2) {
    margin: 10rpx 0;
}

.btn {
    width: 672rpx;
    display: flex;
    justify-content: space-between;
    position: fixed;
    bottom: 30rpx;
    left: 32rpx;
}

.isSucceed {
    width: 672rpx;
    position: fixed;
    bottom: 30rpx;
    left: 32rpx;
    font-size: 24rpx;
    font-weight: 500;
    color: rgba(153,153,153,1);
}

.btn view {
    width: 333rpx;
    height: 88rpx;
    background: rgba(153,153,153,1);
    border-radius: 14rpx;
    font-size: 36rpx;
    font-weight: bold;
    text-align: center;
    line-height: 88rpx;
    color: rgba(255,255,255,1);
}

.btn view:nth-child(2) {
    background: #fccf00;
    margin-left: 20rpx;
}