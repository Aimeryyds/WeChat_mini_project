.lost-wrapper {
    display: flex;
    height: 100vh;
    justify-content: center;
    align-items: center;
}

.lost-content {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.lost-img {
    margin-bottom: 40rpx;
    width: 202rpx;
    height: 284rpx;
}

.lost-text {
    margin-bottom: 20rpx;
    text-align: center;
    color: #f3c267;
    font-weight: bold;
}

.lost-note {
    text-align: center;
    color: #d4d4d4;
}