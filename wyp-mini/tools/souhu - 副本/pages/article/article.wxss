.article-main {
    padding: 30rpx 30rpx;
}

.article-title {
    height: 46rpx;
    line-height: 64rpx;
    text-align: justify;
    font-size: 46rpx;
    font-weight: bold;
    color: #3C3C3D;
}

.article-detail {
    margin: 30rpx 0;
    display: flex;
}

.content-border {
    width: 100%;
    height: 8rpx;
    background: #F5F6F7;
    margin-bottom: 6rpx;
}

.fixMargin {
    margin-bottom: 4rpx;
}

.article-detail .avatar {
    width: 44rpx;
    height: 44rpx;
    border-radius: 100%;
    border: solid 1rpx #efeef3;
}

.author-detail {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 14rpx;
}

.author-name {
    height: 44rpx;
    margin-right: 18rpx;
    line-height: 44rpx;
    font-size: 24rpx;
    color: #656565;
}

.ad-item {
    padding-bottom: 6rpx;
    border-bottom: 2rpx solid #F5F6F7;
    padding: 8rpx 0;
}

.border-zero {
    border: 0;
}

.padding-fix {
    padding-top: 0;
    padding-bottom: 8rpx;
}

.ad-cut-line {
    padding-bottom: 6rpx;
    padding-top: 6rpx;
    border-top: 8rpx solid #F5F6F7;
}

.rec-title {
    padding-bottom: 14rpx;
}

.cut-line {
    display: block;
    border-top: 8rpx solid #F5F6F7;
}

.rec-img {
    display: block;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-size: contain;
    width: 238rpx;
    height: 34rpx;
    margin: auto;
}

.public-time {
    height: 44rpx;
    line-height: 44rpx;
    font-size: 24rpx;
    color: #A4A4A4;
}

.rich-text {
    font-size: 36rpx;
    text-align: justify;
    line-height: 62rpx;
    color: #3C3C3D;
    letter-spacing: 0.4rpx;
    padding-bottom: 49rpx;
}

p:last-child {
    margin-bottom: 0;
}

.show-display {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 42rpx 0 16rpx;
    font-size: 28rpx;
    line-height: 28rpx;
    text-align: center;
    color: #6D98D4;
    font-weight: bold;
}

.ic-more {
    display: block;
    margin-left: 8rpx;
    width: 32rpx;
    height: 32rpx;
    background-size: cover;
    background-image: url(data:image/png;base64,);
}

.title-zw {
    height: 110rpx;
    background: #f5f5f5;
}

.detail-zw {
    margin-top: 30rpx;
    height: 36rpx;
    background: #f5f5f5;
}

.content-zw {
    margin-top: 30rpx;
    height: 600rpx;
    background: #f5f5f5;
}

.ad-middle {
    position: relative;
    margin-top: 26rpx;
}

.ad-middle::after {
    content: '';
    position: absolute;
    top: -13rpx;
    left: 0;
    box-sizing: border-box;
    width: 100%;
    height: 1rpx;
    border-bottom: 1rpx solid rgba(232,232,232,0.8);
}

img {
    display: block;
    padding: 0;
    margin: 0;
}

.back-home {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 336rpx;
    height: 74rpx;
    margin: 20rpx auto 0rpx auto;
    background: #F5F5F5;
    border-radius: 18.5px;
}

.bottomSpace {
    width: 100%;
    height: 128rpx;
}

.bottomSpaceFix {
    padding-bottom: 68rpx;
}

.back-home-text {
    font-size: 30rpx;
    font-weight: bold;
    line-height: 30rpx;
    color: #6D98D4;
}

.back-home-logo {
    width: 16rpx;
    height: 26rpx;
    margin-left: 4rpx;
}

.pClass,.hidden-PClass {
    display: block;
    margin-bottom: 48rpx;
}

.hidden-PClass:last-child {
    margin-bottom: 0;
}

.content-image {
    display: block;
    border: 0;
    margin: 0;
}

.custom-feed .no-pic-wrapper {
    padding: 24rpx 26rpx 26rpx;
    color: #333333;
    border-bottom: 1rpx solid rgba(232,232,232,0.8);
}

.custom-feed .no-pic-wrapper .no-border {
    border: none;
}

.custom-feed .no-pic-wrapper .article {
    position: relative;
    padding-bottom: 40rpx;
}

.custom-feed .no-pic-wrapper .title {
    font-size: 34rpx;
    line-height: 48rpx;
}

.custom-feed .single-pic-wrapper {
    display: flex;
    padding: 26rpx 26rpx 22rpx 26rpx;
    color: #333333;
    border-bottom: 1rpx solid rgba(232,232,232,0.8);
}

.custom-feed .single-pic-wrapper .article {
    position: relative;
    flex: 1;
    margin-right: 20rpx;
}

.custom-feed .single-pic-wrapper .title {
    display: -webkit-box;
    max-height: 96rpx;
    line-height: 48rpx;
    font-size: 34rpx;
    overflow: hidden;
    -webkit-line-clamp: 2;
    word-break: break-all;
    -webkit-box-orient: vertical;
}

.custom-feed .single-pic-wrapper .img-container {
    line-height: 0;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.custom-feed .single-pic-wrapper .img-wrapper {
    width: 228rpx;
    height: 148rpx;
    border-radius: 8rpx;
}

.custom-feed .big-pic-wrapper {
    position: relative;
    width: 100%;
    border-bottom: 1rpx solid rgba(232,232,232,0.8);
}

.custom-feed .big-pic-wrapper .picture {
    position: relative;
    height: 420rpx;
}

.custom-feed .big-pic-wrapper .img-wrapper {
    width: 100%;
    height: 100%;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.custom-feed .big-pic-wrapper .title {
    position: absolute;
    top: 0;
    left: 0;
    box-sizing: border-box;
    padding: 20rpx 30rpx;
    width: 100%;
    height: 140rpx;
    font-size: 34rpx;
    background: linear-gradient(0deg,rgba(0,0,0,0) 0%,rgba(0,0,0,0.7) 100%);
    color: #ffffff;
    z-index: 100;
}

.custom-feed .big-pic-wrapper .play-icon {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 140rpx;
    height: 140rpx;
    transform: translate(-50%,-50%);
    border-radius: 50%;
    background: url('http://statics.itc.cn/mobile/uav2019/play-icon.png');
    background-size: 100%;
}

.custom-feed .big-pic-wrapper .author {
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    height: 104rpx;
}

.custom-feed .big-pic-wrapper .author-avatar {
    margin-right: 10rpx;
    height: 40rpx;
    width: 40rpx;
    border-radius: 50%;
}

.custom-feed .big-pic-wrapper .author-name {
    font-size: 28rpx;
    color: #A4A4A4;
}

.custom-feed .big-pic-wrapper .video {
    margin: 0;
    width: 100%;
    height: 420rpx;
}

.custom-feed .footer {
    position: absolute;
    bottom: 0;
    display: flex;
    align-items: center;
    height: 28rpx;
    margin-top: 16rpx;
    font-size: 24rpx;
    color: #aaaaaa;
}

.custom-feed .top-tag {
    margin-right: 16rpx;
    padding: 2rpx;
    border: 2rpx solid #ffaeb0;
    border-radius: 4rpx;
    line-height: 24rpx;
    color: #ff3240;
    box-sizing: border-box;
}

.custom-feed .author {
    max-width: 120rpx;
    margin-right: 14rpx;
    line-height: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.custom-feed .time {
    margin-right: 14rpx;
    line-height: 24rpx;
}

.custom-feed .article-pv {
    display: flex;
    align-items: center;
}

.custom-feed .icon-eye {
    width: 22rpx;
    height: 18rpx;
    margin-right: 4rpx;
}