.feed-list {
    column-count: 2;
    column-gap: 0;
    padding: 0 5rpx;
}

.feed-item {
    box-sizing: border-box;
    -webkit-column-break-inside: avoid;
    break-inside: avoid;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    margin: 10rpx 5rpx;
    height: auto;
    font-size: 20px;
    border-radius: 6rpx;
    overflow: hidden;
    box-shadow: 0 0 10rpx #ccc;
}