.feed-item-wrapper {
    display: flex;
    height: 148rpx;
    padding: 26rpx 26rpx 22rpx 26rpx;
    color: #333333;
    border-bottom: 1rpx solid rgba(232,232,232,0.8);
}

.article {
    position: relative;
    flex: 1;
    margin-right: 20rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.title {
    display: -webkit-box;
    max-height: 96rpx;
    line-height: 48rpx;
    font-size: 34rpx;
    overflow: hidden;
    -webkit-line-clamp: 2;
    word-break: break-all;
    -webkit-box-orient: vertical;
}

.img-container {
    line-height: 0;
    background-image: url(data:image/png;base64,);
    background-size: 100% 100%;
}

.img-wrapper {
    width: 228rpx;
    height: 148rpx;
    border-radius: 8rpx;
}

.in-rec .title {
    line-height: 44rpx;
    font-size: 32rpx;
    color: #000000;
}

.in-rec .img-wrapper {
    width: 224rpx;
    height: 150rpx;
    border-radius: 0rpx;
}

.in-rec .feed-item-wrapper {
    margin: 18rpx 26rpx;
}