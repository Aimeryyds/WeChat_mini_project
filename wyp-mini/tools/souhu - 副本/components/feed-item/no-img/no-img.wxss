.feed-item-wrapper {
    padding: 24rpx 26rpx 26rpx;
    color: #333333;
    border-bottom: 1rpx solid rgba(232,232,232,0.8);
}

.no-border {
    border: none;
}

.article {
    position: relative;
    display: flex;
    flex-direction: column;
}

.title {
    font-size: 34rpx;
    line-height: 48rpx;
}

.footer {
    margin-top: 16rpx;
}