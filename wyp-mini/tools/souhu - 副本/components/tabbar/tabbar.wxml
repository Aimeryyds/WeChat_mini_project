<view class="container" style="padding-top:{{paddingTop}}px;height:{{statusBarHeight}}px;">
    <view class="left-bar-items">
        <navigator class="navigator" openType="navigateBack">
            <image class="back" src="/images/back.svg"></image>
        </navigator>
    </view>
    <view class="content">
        <text class="title" style="color:{{titleColor}};">{{title}}</text>
    </view>
    <view class="right-bar-items"></view>
</view>
