.custom-tab-bar {
    position: fixed;
    display: flex;
    z-index: 999;
    bottom: 0;
    left: 0;
    width: 100vw;
    height: 94rpx;
    border-top: 2rpx solid #F5F5F5;
    background: #FFFFFF;
}

.setBottomBar {
    padding-bottom: 68rpx;
}

.tab-bar-item {
    flex: 1;
    height: 100%;
    border-right: 2rpx solid #F5F5F5;
}

.tab-bar-item:nth-last-child(1) {
    border-right: 0;
}

.removeButtonStyle {
    display: block;
    background: #FFFFFF;
}

.removeButtonStyle::after {
    border: none;
}

.tab-bar-item-content {
    width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.share-btn {
    width: 180rpx;
    height: 72rpx;
    margin: 11rpx calc(50% - 90rpx);
    border-radius: 36rpx;
}

.emphasizeShare {
    background: #FFD200;
}

.fixWidth {
    width: 200rpx;
}

.tab-bar-logo {
    width: 40rpx;
    height: 40rpx;
    margin-right: 14rpx;
}

.tab-bar-share {
    font-size: 34rpx;
    line-height: 34rpx;
    white-space: nowrap;
    color: #3C3C3D;
}

.mask {
    position: fixed;
    display: flex;
    justify-content: center;
    align-items: center;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 1000;
    background: rgba(0,0,0,0.8);
}

.container {
    width: 580rpx;
    border-radius: 12rpx;
    margin-bottom: 30%;
    background: white;
}

.title {
    display: flex;
    align-items: center;
    justify-content: center;
    padding: 60rpx 0;
}

.title image {
    width: 34rpx;
    height: 34rpx;
    margin-right: 10rpx;
}

.title text {
    font-size: 34rpx;
    color: #2b2b2b;
}

.content {
    border-radius: 6rpx;
    padding: 34rpx 28rpx;
    margin-bottom: 40rpx;
    margin: 0 24rpx 50rpx;
    font-size: 30rpx;
    word-break: break-all;
    background: #F0F1F2;
    color: #656566;
}

.content-title {
    font-weight: 600;
    color: #29292A;
}

.confirm-button {
    display: flex;
    align-items: center;
    justify-content: center;
    border-top: solid 1rpx #F0F1F2;
    height: 100rpx;
    font-size: 32rpx;
    color: #FFD200;
}

.confirm-button::after {
    display: none;
}