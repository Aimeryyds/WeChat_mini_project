.search-header {
    display: flex;
    align-items: center;
    padding: 20rpx 20rpx 26rpx 30rpx;
}

.search-input {
    flex: 1;
    background-color: #fff;
    border-radius: 10rpx;
    padding: 10rpx 20rpx;
    margin-right: 20rpx;
    font-size: 26rpx;
    display: flex;
}

.suggest-item {
    border-bottom: 2rpx solid #d0d0d0;
    padding: 20rpx 0;
    display: flex;
}