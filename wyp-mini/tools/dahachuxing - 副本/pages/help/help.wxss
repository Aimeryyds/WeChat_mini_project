.container {
    display: flex;
    flex-direction: column;
    background: #fff;
    height: auto;
    font-family: 微软雅黑;
}

.helpTitle {
    height: 120rpx;
    padding-left: 30rpx;
    background: #f5f5f5;
    line-height: 120rpx;
    font-size: 30rpx;
}

.container .title {
    height: 120rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx #ededed solid;
}

.title text {
    padding-left: 30rpx;
}

.container .contents {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    background: #f5f5f5;
    font-size: 30rpx;
}

.container item image {
    width: 50rpx;
    float: right;
    height: 50rpx;
    padding-right: 30rpx;
}