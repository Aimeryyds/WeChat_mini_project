.target-content {
    background-color: #fff;
}

.area-out {
    padding-top: 40rpx;
    text-align: center;
}

.area-in {
    padding: 40rpx 30rpx 0 30rpx;
    display: flex;
    justify-content: space-between;
}

.target-content>view:last-child {
    display: flex;
    justify-content: center;
    padding: 40rpx 0 60rpx 0;
}

.target-btn {
    padding: 20rpx 0;
    text-align: center;
    border-radius: 50rpx;
    width: 70%;
}