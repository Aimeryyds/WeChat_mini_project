.historyOrder {
    background: #edf0f2;
    font-family: 微软雅黑;
    font-size: 16px;
    overflow: hidden;
}

.noRecord {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    color: #888;
    font-size: 36rpx;
}

.historyRecord {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    background: #fff;
    margin: 20rpx 15rpx;
    padding: 20rpx 30rpx;
    box-shadow: 0 0 5rpx rgba(0,0,0,.3);
    border-radius: 10rpx;
}

