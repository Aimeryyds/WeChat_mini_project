}

.label {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.label1 text {
    font-size: 14px;
    color: #888;
}

.label1 {
    display: flex;
    flex-direction: column;
    align-items: start;
    padding-left: 10rpx;
}