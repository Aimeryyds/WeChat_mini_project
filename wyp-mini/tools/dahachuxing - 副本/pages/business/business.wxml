<view style="background:#edf0f2;height:{{height}}px;">
    <view class="business">
        <view>
            <text>{{business}}</text>
            <text>{{address}}</text>
        </view>
        <view bindtap="navigation" class="navigation">
            <image src="/images/navigation.png"></image>
            <text>去这</text>
        </view>
    </view>
    <view class="available">
        <view>
            <image src="/images/star.png"></image>
            <text>共{{available}}个可用</text>
        </view>
        <view bindtap="tel" class="tel">
            <image src="/images/tel.png"></image>
            <text style="color:#0099CC;">联系商家</text>
        </view>
    </view>
</view>
