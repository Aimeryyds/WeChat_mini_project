    margin: 10px;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    height: 100px;
    box-sizing: border-box;
    position: relative;
}

.box {
    position: absolute;
    width: 100%;
}

image {
    width: 100%;
    height: 100px;
}

.out-list-12 {
