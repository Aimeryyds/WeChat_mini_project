.search {
    padding: 40rpx 40rpx 0;
}

.s_total_found {
    font-size: 20rpx;
    text-align: center;
    color: #666;
    padding: 20rpx 0 0;
}

.rlist {
    padding-top: 0;
}

.s_note {
    font-size: 20rpx;
    color: #ff6767;
    padding: 0 6rpx;
    display: inline-block;
    border: 1px solid #ff6767;
    border-radius: 3px;
    vertical-align: top;
    margin-top: 6rpx;
    line-height: 24rpx;
}

.loading_none {
    text-align: center;
    padding: 20px 0;
    height: 22rpx;
    line-height: 22rpx;
    font-size: 24rpx;
    color: #999;
}

.sq_tip {
    font-size: 24rpx;
    color: #666;
    padding: 10px;
}

.sq_tip_item {
    display: inline-block;
    font-size: 26rpx;
    height: 40rpx;
    line-height: 40rpx;
    padding: 6rpx 30rpx;
    border-radius: 26rpx;
    margin: 0 0 0 10px;
    background: #999;
    color: #fff;
}

.hotkey {
    padding: 60rpx 0 0 40rpx;
}

.hotkey navigator {
    text-align: center;
    display: inline-block;
    width: 143rpx;
    margin: 0 30rpx 20rpx 0;
    font-size: 28rpx;
    color: #666;
    border-radius: 33rpx;
    height: 66rpx;
    line-height: 66rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    border: 1rpx solid #ddd;
}

.hot500 {
    text-align: center;
    padding: 30rpx 0 0 0;
}

.hot500 navigator {
    border-radius: 40rpx;
    height: 80rpx;
    line-height: 80rpx;
    display: inline-block;
    padding: 0 40rpx;
    font-size: 30rpx;
    border: 1rpx solid #ddd;
}