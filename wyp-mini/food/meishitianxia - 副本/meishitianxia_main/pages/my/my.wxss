.userinfo {
    background: #fafafa;
    width: 100%;
    position: fixed;
    z-index: 3;
    height: 240rpx;
}

.avatar {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    margin: 0 20rpx 0 40rpx;
    vertical-align: top;
}

.userinfo_lv {
    font-size: 24rpx;
    color: #fff;
    display: inline-block;
    border: 1rpx solid #fff;
    vertical-align: bottom;
    padding: 0 14rpx;
    border-radius: 6rpx;
}

.my_btn {
    color: #fff;
    border: 1px solid #ff6767;
    background: #ff6767;
}

.isshow {
    position: fixed;
    padding: 10rpx;
    left: 20rpx;
    bottom: 20rpx;
    font-size: 26rpx;
    background: rgba(0,128,1,.7);
    border-radius: 10rpx;
    text-align: center;
    color: #fff;
}

.tip {
    padding: 40rpx 40rpx 0;
    font-size: 24rpx;
    color: #999;
    text-align: left;
}

.labe {
    font-size: 28rpx;
    margin: 60rpx 0 30rpx 0;
}

.no_user {
    color: #999;
    position: absolute;
    font-size: 28rpx;
    width: 100%;
    text-align: center;
    bottom: 30rpx;
}

.no_user_a {
    padding: 20rpx 40rpx;
    display: inline-block;
}

.userbtn {
    font-size: 34rpx;
    height: 80rpx;
    line-height: 80rpx;
    width: 40%;
    margin: 60rpx auto 0;
}

.nav {
    top: 240rpx;
}

.rlist {
    padding: 301rpx 0 0 40rpx;
}

.feed_list {
    padding: 301rpx 0 0 40rpx;
}

.plist {
    padding-top: 301rpx;
}

.loading_nodata {
    padding: 50rpx 0;
    color: #999;
}

.userdata {
    display: inline-block;
    width: 490rpx;
}

.username {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-weight: bold;
    font-size: 44rpx;
}

.usersign {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    line-height: 36rpx;
    height: 36rpx;
    font-size: 24rpx;
    color: #777;
}

.useritem {
    width: 33.3%;
    display: inline-block;
    font-size: 22rpx;
    color: #777;
}

.useritem text {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    font-size: 32rpx;
    display: block;
    color: #111;
    padding: 10rpx 0 0;
}

.vip_icon {
    width: 60rpx;
    height: 60rpx;
    position: absolute;
    top: 110rpx;
    left: 150rpx;
}

.mask_login {
    position: fixed;
    background: rgba(0,0,0,.7);
    z-index: 2;
    left: 0;
    top: 0;
    text-align: center;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.dialog_login {
    padding: 40rpx 0;
    height: 560rpx;
    border-radius: 20rpx;
    position: fixed;
    background: #fff;
    z-index: 3;
    left: 75rpx;
    text-align: center;
    width: 600rpx;
    overflow: hidden;
}

.btn_no {
    display: inline-block;
    background: #fff;
    color: #ff6767;
    border-radius: 40rpx;
    border: 1px solid #ff6767;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    paddingg: 0;
    margin: 0 20rpx;
    width: 230rpx;
}

.btn_in {
    display: inline-block;
    background: #ff6767;
    color: #fff;
    border-radius: 40rpx;
    border: 1px solid #ff6767;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    margin: 0 20rpx;
    width: 230rpx;
}

.icon_goods {
    position: fixed;
    color: #fff;
    background: rgba(0,0,0,0.6);
    text-align: center;
    border-radius: 50%;
    right: 40rpx;
    bottom: 36rpx;
    font-size: 20rpx;
    width: 80rpx;
    height: 80rpx;
    z-index: 10;
}

.icon_goods image {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin: 4rpx auto 0;
    line-height: 100%;
}

.icon_goods text {
    line-height: 100%;
}

.setting {
    float: right;
    font-size: 24rpx;
    border: 1rpx solid #777;
    color: #777;
    border-radius: 6rpx;
    padding: 2rpx 10rpx;
    margin-top: -6rpx;
}

.loginerror {
    text-align: center;
    color: #f00;
    font-size: 24rpx;
    min-height: 40rpx;
    margin-top: 35%;
}

.btn_add {
    position: fixed;
    text-align: center;
    bottom: 36rpx;
    z-index: 9;
    width: 100%;
}

.btn_add navigator {
    display: inline-block;
    color: #fff;
    background: rgba(255,103,103,0.8);
    font-size: 32rpx;
    height: 78rpx;
    line-height: 78rpx;
    padding: 0 50rpx;
    border-radius: 40rpx;
}

.refresh {
    text-align: center;
    color: #999;
    font-size: 28rpx;
    height: 60rpx;
    line-height: 60rpx;
    overflow: hidden;
}

.out {
    height: 0;
    transition: height .5s ease-in-out;
}