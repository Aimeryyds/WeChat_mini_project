.r_info_mainpic {
    width: 100%;
    height: 750rpx;
    position: relative;
}

.r_info_mainpic:after {
    background: rgba(0,0,0,.3);
    position: absolute;
    right: 0;
    bottom: 0;
    padding: 10rpx;
    color: #ddd;
    font-size: 20rpx;
    content: '点击查看大图';
    display: block;
}

.r_info_u {
    color: #111;
    font-size: 26rpx;
    padding: 24rpx 40rpx 0;
}

.r_info_u_a {
    display: inline-block;
    line-height: 40rpx;
    position: relative;
}

.r_info_u_a:hover {
    background: rgba(0,0,0,0);
    opacity: 1;
}

.r_info_u_img {
    border-radius: 50%;
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin: 0 10rpx 0 0;
    background: url(http://static.meishichina.com/u2/res/n.jpg) no-repeat center/80rpx 80rpx;
}

.r_info_txt {
    text-align: justify;
    font-size: 32rpx;
    line-height: 60rpx;
    padding: 0 40rpx;
    margin-top: 20rpx;
}

.r_info_u_a .vip_icon {
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    top: 26rpx;
    left: 24rpx;
}

.c2list {
    padding-left: 40rpx;
    margin-bottom: 50rpx;
}

.gray {
    color: #bbb;
}

.r_info_steps_t {
    padding: 20rpx 40rpx 40rpx;
    font-size: 32rpx;
    line-height: 170%;
    text-align: justify;
}

.r_info_steps_t2 {
    padding: 0 0 34rpx 40rpx;
    font-size: 32rpx;
    line-height: 170%;
    text-align: justify;
}

.r_info_steps_t text {
    color: #999;
}

.r_info_steps_t2 text:nth-child(1) {
    color: #999;
}

.r_info_tips {
    padding: 0 40rpx 40rpx;
    text-align: justify;
    font-size: 32rpx;
    line-height: 170%;
}

.rlist {
    padding: 0 0 0 40rpx;
}

.steppic_span {
    font-size: 32rpx;
    display: inline-block;
    width: 40rpx;
    text-align: center;
    color: #888;
}

.steppic_span2 {
    font-size: 32rpx;
    float: left;
    width: 40rpx;
    text-align: center;
    color: #888;
    line-height: 60rpx;
}

.steppic {
    display: inline-block;
    position: relative;
    border-radius: 6rpx;
}

.steppic:after {
    background: rgba(0,0,0,.3);
    position: absolute;
    right: 0;
    bottom: 0;
    padding: 10rpx;
    color: #ddd;
    font-size: 20rpx;
    content: '点击查看大图';
    display: block;
    border-radius: 6rpx;
}

.group_name {
    font-weight: 700;
    font-size: 28rpx;
    padding: 0 0 20rpx 40rpx;
    color: #666;
}

.mask_poster {
    position: fixed;
    background: rgba(0,0,0,.7);
    z-index: 2;
    left: 0;
    top: 0;
    text-align: center;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.myCanvas {
    margin: 10px auto 0;
    background: #fff;
    overflow: hidden;
}

.mask_share {
    position: fixed;
    background: rgba(0,0,0,.7);
    z-index: 2;
    left: 0;
    top: 0;
    text-align: center;
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.share_dialog {
    position: fixed;
    background: #fff;
    z-index: 3;
    left: 0;
    bottom: 0px;
    text-align: center;
    width: 80%;
    padding: 0 10%;
    overflow: hidden;
}

.share_dialog view {
    padding: 20rpx 0 0 0;
    font-size: 24rpx;
}

.share_dialog image {
    width: 60rpx;
    height: 60rpx;
}

.share_dialog button[plain] {
    width: 50%;
    float: left;
    margin: auto;
    border: none;
}

.share_dialog text {
    display: block;
    font-size: 24rpx;
    height: 30rpx;
    line-height: 30rpx;
    padding: 0 0 60rpx 0;
}

.mask_poster_close {
    padding: 10rpx 0;
    text-align: center;
}

.mask_poster_close image {
    width: 60rpx;
    height: 60rpx;
}

.mask_poster_tip {
    text-align: center;
    color: #fff;
    padding: 10rpx 0 0 0;
    font-size: 26rpx;
    height: 34rpx;
}

.r_info_4 {
    text-align: center;
}

.r_info_4item {
    width: 120rpx;
    color: #999;
    font-size: 24rpx;
    display: inline-block;
}

.r_info_4item image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 20rpx auto 10rpx;
}

.steps {
    position: relative;
}

.step_down {
    position: absolute;
    bottom: -70rpx;
    width: 100%;
    text-align: center;
    font-size: 36rpx;
    color: #ff6767;
    line-height: 100%;
}

.step_down view {
    height: 50rpx;
    background: linear-gradient(top,rgba(250,250,250,.55),rgba(250,250,250,1));
}

.step_down image {
    width: 40rpx;
    height: 40rpx;
    margin: 20rpx auto 10rpx;
    display: block;
}

.search {
    padding: 80rpx 40rpx 40rpx;
}

.search_item {
    border: 1px solid #ff6767;
}

.videobox {
    overflow: hidden;
    width: 750rpx;
    height: 422rpx;
    position: relative;
}

.videobox image {
    width: 750rpx;
    height: 422rpx;
}

.videobox .play {
    position: absolute;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 422rpx;
    background: url(http://static.meishichina.com/v6/img/lib/play.png) no-repeat scroll center center/100rpx 100rpx;
}

.recipeVideo {
    position: fixed;
    width: 750rpx;
    height: 422rpx;
    z-index: 100;
    top: 0;
    left: 0;
    overflow: hidden;
}

#recipeVideo {
    width: 750rpx;
    height: 422rpx;
}

.tplist {
    padding: 0 20rpx 30rpx;
    font-size: 0;
}

.tplist button {
    border: none;
    display: inline-block;
    width: 210rpx;
    height: 98rpx;
    padding: 0;
    margin: 10rpx 0 10rpx 20rpx;
}

.tplist button image {
    width: 100%;
    height: 100%;
}

.event_show {
    padding: 0 40rpx 20rpx;
    font-size: 30rpx;
    color: #ccc;
    text-align: right;
}

.event_show text {
    vertical-align: middle;
}

.event_show image {
    height: 48rpx;
    width: 48rpx;
}

.height1 {
    overflow: hidden;
    height: 300rpx;
}

.refresh {
    text-align: center;
    color: #999;
    font-size: 28rpx;
    height: 0rpx;
    overflow: hidden;
    opacity: 0;
    line-height: 38rpx;
    width: 100%;
    position: absolute;
    left: 0;
}

.refresh_show {
    height: 80rpx;
    opacity: 1;
}

.refresh_show_padding {
    padding-top: 80rpx;
}

.main {
    position: relative;
}

.icon_goods {
    position: fixed;
    color: #fff;
    background: rgba(0,0,0,0.6);
    text-align: center;
    border-radius: 50%;
    right: 40rpx;
    bottom: 180rpx;
    font-size: 20rpx;
    width: 80rpx;
    height: 80rpx;
    z-index: 10;
}

.icon_goods image {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin: 4rpx auto 0;
    line-height: 100%;
}

.icon_goods text {
    line-height: 100%;
}

.recipe_swiper {
    width: 100%;
    transition: all .15s ease-in-out .15s;
}

.recipe_swiper_view {
    width: 100%;
    position: relative;
    transition: all .15s ease-in-out .15s;
}

.recipe_swiper_num {
    position: absolute;
    left: 0;
    bottom: 20rpx;
    width: 100%;
    text-align: center;
}

.recipe_swiper_num text {
    background: rgba(0,0,0,.5);
    color: #fff;
    font-size: 20rpx;
    padding: 4rpx 10rpx;
    display: inline-block;
    border-radius: 6rpx;
}

.recipe_swiper image {
    width: 100%;
}

.mask_share {
    opacity: 1;
    transform: scale3d(1,1,1);
    transition: all .3s;
    position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.6);
}

.mask_dialog {
    width: 100%;
    position: fixed;
    z-index: 1001;
    left: 0;
    bottom: 0;
}

.mask_dialog>text {
    border-radius: 12px 12px 0 0;
    text-align: center;
    display: block;
    font-size: 24rpx;
    padding: 20rpx 0;
    background: #fff;
}

.mask_bottom {
    margin: 8px 0 0 0;
    font-size: 0;
    text-align: center;
    background: #fff;
}

.mask_bottom view {
    font-size: 34rpx;
    width: 374rpx;
    display: inline-block;
    color: #999;
    height: 88rpx;
    line-height: 88rpx;
}

.mclist_item {
    height: 100rpx;
    padding: 10rpx 0 10rpx 40rpx;
}

.mclist_item image {
    float: right;
    width: 40rpx;
    height: 40rpx;
    margin: 30rpx;
}

.mclist_item view {
    line-height: 36rpx;
    padding: 12rpx 0 0 0;
    font-size: 32rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.mclist_item text {
    font-size: 24rpx;
    color: #777;
}