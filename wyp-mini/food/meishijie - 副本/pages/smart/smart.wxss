.smart_w1 {
    height: 100%;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    box-sizing: border-box;
    background: url(https://s1.st.meishij.net/p2/20180517/20180517114618_166.jpg) left 546rpx no-repeat;
    background-size: 100% auto;
    overflow: hidden;
    padding-top: 40rpx;
    z-index: 100;
}

.desw {
}

.desw .num {
    height: 48rpx;
    width: 48rpx;
    border-radius: 24rpx;
    background: #e86a58;
    color: #fff;
    font-weight: 500;
    font-size: 32rpx;
    line-height: 48rpx;
    text-align: center;
    margin: 40rpx auto 12rpx;
}

.desw .p {
    color: #1a1a1a;
    line-height: 48rpx;
    font-size: 32rpx;
    text-align: center;
}

.addsc_btnw {
    width: 100%;
    margin-top: 72rpx;
}

.addsc_btnw .btn {
    display: block;
    height: 292rpx;
    width: 200rpx;
    margin: 0rpx auto;
}

.addsc_btnw .btn .icon {
    display: block;
    height: 200rpx;
    width: 200rpx;
    background: #f5f5f5;
    border-radius: 100rpx;
    position: relative;
}

.addsc_btnw .btn .icon .i1 {
    height: 6rpx;
    width: 64rpx;
    background: rgba(0,0,0,0.1);
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.addsc_btnw .btn .icon .i2 {
    height: 64rpx;
    width: 6rpx;
    background: rgba(0,0,0,0.1);
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
}

.addsc_btnw .btn text {
    font-size: 32rpx;
    line-height: 92rpx;
    color: #999;
    text-align: center;
    display: block;
    height: 92rpx;
}

.smart_w2 {
    height: 100%;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    box-sizing: border-box;
    background: #fff;
    overflow: hidden;
    z-index: 200;
}

.smart_w2_tw {
    padding: 24rpx 30rpx 0px;
    height: 120rpx;
    line-height: 96rpx;
    position: relative;
    width: 100%;
    box-sizing: border-box;
}

.smart_w2_tw .t1 {
    color: #1a1a1a;
    font-size: 32rpx;
    font-weight: 700;
}

.smart_w2_tw .t2 {
    color: #666;
    font-size: 28rpx;
    font-weight: 300;
    position: absolute;
    right: 30rpx;
    bottom: 0rpx;
    line-height: 96rpx;
    height: 96rpx;
    display: block;
}

.sc_checked_list {
    padding: 0rpx 20rpx;
    font-size: 0px;
}

.sc_checked_item {
    width: 335rpx;
    display: inline-block;
    vertical-align: top;
    margin: 0rpx 10rpx 20rpx;
    border: 1rpx solid #eee;
    box-sizing: border-box;
    padding-left: 123rpx;
    position: relative;
    border-radius: 4rpx;
    padding-right: 88rpx;
}

.sc_checked_item .imgw {
    height: 88rpx;
    width: 88rpx;
    border-radius: 2rpx;
    overflow: hidden;
    position: absolute;
    left: 16rpx;
    top: 16rpx;
}

.sc_checked_item .imgw image {
    display: block;
    height: 88rpx;
    width: 88rpx;
}

.sc_checked_item .t {
    font-size: 32rpx;
    font-weight: 700;
    color: #1a1a1a;
    line-height: 118rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.sc_checked_item .x {
    height: 118rpx;
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: 88rpx;
    background: url(https://s1.st.meishij.net/p2/20190227/20190227151726_542.png) center no-repeat;
    background-size: 24rpx 24rpx;
}

.zc_bigbtn {
    height: 100rpx;
    width: 100%;
    text-align: center;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    line-height: 100rpx;
    font-size: 32rpx;
    color: #fff;
    font-weight: 700;
    background: #ef4848;
    background-image: linear-gradient(to right,#ff4d83,#ef4848);
}

.smart_w3 {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0px;
    top: 100%;
    transition: all ease 0.3s;
    background: #fff;
    z-index: 300;
}

.smart_w3.show {
    top: 0%;
}

.sw3_inputw {
    height: 144rpx;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    z-index: 350;
    width: 100%;
}

.sw3_inputw_it {
    height: 143rpx;
    border-bottom: 1rpx solid #f5f5f5;
    line-height: 143rpx;
    font-size: 44rpx;
    color: #1a1a1a;
    font-weight: 700;
    margin: 0rpx 136rpx 0rpx 30rpx;
}

.sw3_inputw_it_ph {
    color: #ddd;
}

.sw3_exitbtn {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: 136rpx;
    height: 144rpx;
    line-height: 144rpx;
    font-size: 28rpx;
    text-align: center;
    color: #ef4848;
    font-weight: 700;
}

.scTipslist {
    height: 100%;
    box-sizing: border-box;
    padding: 144rpx 20rpx 0rpx;
    font-size: 0px;
}

.scTipsitemw {
    padding: 30rpx 0rpx;
}

.scTipsitem {
    width: 216rpx;
    height: 216rpx;
    position: relative;
    margin: 10rpx;
    display: inline-block;
    vertical-align: top;
    border-radius: 4rpx;
    overflow: hidden;
}

.scTipsitem .imgw {
    height: 216rpx;
    width: 216rpx;
}

.scTipsitem .imgw image {
    height: 100%;
    width: 100%;
}

.scTipsitem .n {
    font-size: 28rpx;
    font-weight: 700;
    color: #fff;
    line-height: 68rpx;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    text-shadow: 2rpx 2rpx 2rpx rgba(0,0,0,0.6);
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    width: 100%;
    padding: 0rpx 20rpx;
    box-sizing: border-box;
}

.scTipsitem .checker {
    height: 44rpx;
    width: 44rpx;
    border-radius: 22rpx;
    border: 2rpx solid #fff;
    background: rgba(0,0,0,0.5);
    position: absolute;
    right: 20rpx;
    top: 20rpx;
    box-sizing: border-box;
}

.scTipsitem .checker.checked {
    height: 44rpx;
    width: 44rpx;
    border-radius: 22rpx;
    border: 2rpx solid #fff;
    background: #09bb07 url(https://s1.st.meishij.net/p2/20180517/20180517155253_763.png) center no-repeat;
    background-size: 100% 100%;
}

.bottombar {
    position: fixed;
    left: 0rpx;
    bottom: 0rpx;
    height: 81rpx;
    width: 100%;
    border-top: 1rpx solid rgba(0,0,0,0.05);
    background: #fff;
    text-align: center;
    box-sizing: border-box;
}

.bottombar .share_bottom {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    background: #fff;
    z-index: 200;
    border: 0rpx;
    border-radius: 0px;
    position: fixed;
    left: 0px;
    bottom: 0rpx;
    opacity: 0;
}

.bottombar image {
    display: inline-block;
    vertical-align: top;
    height: 60rpx;
    width: 60rpx;
    margin-top: 10rpx;
}

.bottombar text {
    font-size: 28rpx;
    color: #787878;
    display: inline-block;
    vertical-align: top;
    line-height: 80rpx;
    height: 80rpx;
    font-weight: 400;
}