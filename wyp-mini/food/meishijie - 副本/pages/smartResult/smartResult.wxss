.cplistw {
    width: 100%;
    height: 100%;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}

.recipeitem {
    width: 100%;
    box-sizing: border-box;
    padding: 0rpx 30rpx;
}

.recipeitemw {
    height: 341rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 30rpx 0rpx 30rpx 280rpx;
    position: relative;
    border-bottom: 1rpx solid rgba(0,0,0,0.05);
}

.cpimgw {
    height: 280rpx;
    width: 280rpx;
    position: absolute;
    left: 0rpx;
    top: 30rpx;
}

.cpimgw image {
    display: block;
    height: 100%;
    width: 100%;
    border-radius: 4rpx;
}

.cpinfow {
    padding-left: 30rpx;
    height: 280rpx;
}

.cpinfow .cpnamew {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 300rpx;
}

.cpinfow .cpname {
    font-size: 36rpx;
    color: #1a1a1a;
    font-weight: 700;
    line-height: 56rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpinfow .ratew {
    height: 100rpx;
    position: absolute;
    right: 0rpx;
    top: 30rpx;
    text-align: right;
}

.cpinfow .ratew .ppd {
    color: #ff9f2e;
}

.cpinfow .ratew .ppd .big {
    font-size: 42rpx;
    line-height: 62rpx;
    display: inline-block;
    vertical-align: top;
}

.cpinfow .ratew .ppd .small {
    font-size: 24rpx;
    line-height: 44rpx;
    display: inline-block;
    vertical-align: top;
}

.cpinfow .ratew .p {
    font-size: 24rpx;
    color: #ff9f2e;
    line-height: 34rpx;
}

.cpinfow .auw text {
    font-size: 24rpx;
    padding-left: 16rpx;
    line-height: 32rpx;
    color: #787878;
    display: block;
    white-space: nowrap;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpinfow .starw {
    height: 32rpx;
    font-size: 0px;
    margin-top: 20rpx;
}

.cpinfow .starw view {
    height: 32rpx;
    width: 32rpx;
    display: inline-block;
    vertical-align: top;
    margin-right: 8rpx;
}

.cpinfow .starw view image {
    height: 32rpx;
    width: 32rpx;
    display: block;
}

.cpinfow .foods {
    height: 88rpx;
    line-height: 44rpx;
    margin-top: 22rpx;
    font-size: 28rpx;
    color: #bbb;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.bottomline {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 40rpx 0rpx;
    line-height: 32rpx;
}

.tagsw {
    width: 100%;
    height: 100rpx;
    position: relative;
    padding-left: 120rpx;
    box-sizing: border-box;
}

.tagsw .t {
    font-size: 28rpx;
    color: #666;
    line-height: 100rpx;
    display: block;
    position: absolute;
    left: 30rpx;
    top: 0rpx;
}

.tags {
    height: 100rpx;
    width: 630rpx;
    white-space: nowrap;
    overflow: hidden;
    font-size: 0px;
}

.tags .tag {
    height: 60rpx;
    line-height: 60rpx;
    display: inline-block;
    vertical-align: top;
    font-size: 28rpx;
    background: #f5f5f5;
    color: #757575;
    padding: 0px 28rpx;
    border-radius: 30rpx;
    margin: 20rpx 10rpx 0rpx;
}

.tags .tag:last-child {
    margin-right: 30rpx;
}

.tags .tag.tag_checked {
    background: rgba(242,95,82,0.34);
    color: rgba(242,95,82,1);
}

.zctip {
    color: #999;
    height: 40rpx;
    line-height: 40rpx;
    padding: 0px 30rpx;
    font-size: 28rpx;
    padding-bottom: 14rpx;
}