.nui_tip {
    font-size: 32rpx;
    color: #000;
    font-weight: 300;
    text-align: center;
    line-height: 64rpx;
    width: 100%;
    z-index: 200;
    margin-top: 440rpx;
}

.nui_button {
    font-size: 32rpx;
    color: #fff;
    font-weight: 300;
    text-align: center;
    line-height: 88rpx;
    height: 88rpx;
    width: 300rpx;
    background: #ff7859;
    border-radius: 150rpx;
    margin-top: 32rpx;
    z-index: 200;
}

.nui_button:after {
    border: 0px;
}

.v_icon {
    position: absolute;
    border-radius: 100%;
    height: 80rpx;
    width: 80rpx;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background: rgba(0,0,0,0.5) url(https://s1.st.meishij.net/p2/20181011/20181011165920_645.png) center no-repeat;
    background-size: 48rpx 48rpx;
}