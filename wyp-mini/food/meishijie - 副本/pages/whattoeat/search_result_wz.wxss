.csm_dataitem {
    width: 100%;
    padding: 0rpx 32rpx;
    box-sizing: border-box;
}

.csm_dataitem_link {
    width: 100%;
    display: block;
    border-bottom: 1rpx solid #eee;
    min-height: 140rpx;
    padding: 32rpx 0rpx 32rpx 160rpx;
    box-sizing: border-box;
    position: relative;
}

.csm_dataitem_link .imgw {
    height: 140rpx;
    width: 140rpx;
    border-radius: 16rpx;
    position: absolute;
    left: 0rpx;
    top: 32rpx;
    overflow: hidden;
}

.csm_dataitem_link .imgw .img {
    height: 140rpx;
    width: 140rpx;
}

.csm_dataitem_link .imgw .vicon {
    height: 24rpx;
    width: 24rpx;
    display: block;
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    background: url(https://s1.st.meishij.net/p2/20181206/20181206143142_460.png) 0rpx 0rpx no-repeat;
    background-size: 24rpx 48rpx;
}

.csm_dataitem_link .info {
    min-height: 140rpx;
}

.csm_dataitem_link .info .t {
    font-size: 32rpx;
    line-height: 44rpx;
    height: 88rpx;
    color: #000;
    font-weight: 500;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.csm_dataitem_link .info .tagsw {
    height: 36rpx;
    font-size: 0rpx;
    margin-top: 16rpx;
}

.csm_dataitem_link .info .tagsw .tag {
    display: inline-block;
    vertical-align: top;
    padding: 0rpx 12rpx;
    border: 1rpx solid #ff9292;
    color: #ff9292;
    font-size: 22rpx;
    line-height: 34rpx;
    box-sizing: border-box;
    border-radius: 18rpx;
    margin-right: 20rpx;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
    padding-top: 200rpx;
}