.tabw {
    height: 48px;
    position: fixed;
    left: 0px;
    width: 100%;
    background: #fff;
    text-align: center;
    font-size: 0px;
    z-index: 100;
}

.tabw .tabitem {
    width: 186rpx;
    display: inline-block;
    vertical-align: top;
    height: 48px;
}

.tabw .tabitem .c {
    margin: 10px 32rpx;
    height: 28px;
    border-radius: 14px;
    background: #f2f2f2;
    color: #666;
    font-size: 26rpx;
    font-weight: 400;
    line-height: 28px;
}

.tabw .tabitem.current .c {
    color: #fff;
    background: #ff9292;
}

.sc_listw {
    width: 100%;
}

.sc_listitem {
    height: 244rpx;
    width: 100%;
}

.sc_listitem_link {
    display: block;
    padding: 32rpx 32rpx 32rpx 244rpx;
    box-sizing: border-box;
    width: 100%;
    position: relative;
}

.sc_listitem_link .imgw {
    height: 180rpx;
    width: 180rpx;
    position: absolute;
    left: 32rpx;
    top: 32rpx;
    border-radius: 16rpx;
    overflow: hidden;
}

.sc_listitem_link .infow {
    height: 180rpx;
}

.sc_listitem_link .infow .t {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
    color: #000;
    font-weight: 600;
}

.sc_listitem_link .infow .des {
    border-radius: 16rpx;
    overflow: hidden;
    height: 120rpx;
    padding: 0rpx 16rpx;
    box-sizing: border-box;
}

.sc_listitem_link .infow .des .des_t {
    height: 44rpx;
    line-height: 44rpx;
    color: #000;
    font-size: 24rpx;
    font-weight: 600;
    margin-top: 4rpx;
}

.sc_listitem_link .infow .des .des_t .i {
    display: inline-block;
    vertical-align: top;
    width: 20rpx;
    height: 22rpx;
    margin-top: 13rpx;
    margin-right: 12rpx;
}

.sc_listitem_link .infow .des.des1 {
    background: #f6fafe;
}

.sc_listitem_link .infow .des.des2 {
    background: #fef9ed;
}

.sc_listitem_link .infow .des.des3 {
    background: #fef4f6;
}

.sc_listitem_link .infow .des .des_c {
    line-height: 32rpx;
    color: #000;
    font-size: 24rpx;
    font-weight: 400;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.sc_listitem .line {
    height: 1rpx;
    margin: 0rpx 32rpx;
    background: #eee;
}

.csmi_line {
    width: 100%;
    height: 10rpx;
    background: #f5f5f5;
}

.bottomline {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 40rpx 0rpx 120rpx;
    line-height: 32rpx;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}