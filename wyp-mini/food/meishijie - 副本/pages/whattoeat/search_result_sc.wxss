.sc_listitem {
    height: 244rpx;
    width: 100%;
}

.sc_listitem_link {
    display: block;
    padding: 32rpx 32rpx 32rpx 244rpx;
    box-sizing: border-box;
    width: 100%;
    position: relative;
}

.sc_listitem_link .imgw {
    height: 180rpx;
    width: 180rpx;
    position: absolute;
    left: 32rpx;
    top: 32rpx;
    border-radius: 16rpx;
    overflow: hidden;
}

.sc_listitem_link .infow {
    height: 180rpx;
}

.sc_listitem_link .infow .t {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
    color: #000;
    font-weight: 600;
}

.sc_listitem_link .infow .des {
    border-radius: 16rpx;
    overflow: hidden;
    height: 120rpx;
    padding: 0rpx 16rpx;
    box-sizing: border-box;
}

.sc_listitem_link .infow .des .des_t {
    height: 44rpx;
    line-height: 44rpx;
    color: #000;
    font-size: 24rpx;
    font-weight: 600;
    margin-top: 4rpx;
}

.sc_listitem_link .infow .des .des_t .i {
    display: inline-block;
    vertical-align: top;
    width: 20rpx;
    height: 22rpx;
    margin-top: 13rpx;
    margin-right: 12rpx;
}

.sc_listitem_link .infow .des.des1 {
    background: #f6fafe;
}

.sc_listitem_link .infow .des.des2 {
    background: #fef9ed;
}

.sc_listitem_link .infow .des.des3 {
    background: #fef4f6;
}

.sc_listitem_link .infow .des .des_c {
    line-height: 32rpx;
    color: #000;
    font-size: 24rpx;
    font-weight: 400;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.sc_listitem .line {
    height: 1rpx;
    margin: 0rpx 32rpx;
    background: #eee;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
    padding-top: 200rpx;
}