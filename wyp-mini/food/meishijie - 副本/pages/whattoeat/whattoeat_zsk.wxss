.leftbar {
    width: 150rpx;
}

.leftbar_scroll {
    width: 150rpx;
    position: fixed;
    left: 0px;
    bottom: 0px;
    background: #fff;
}

.leftbar_item {
    width: 150rpx;
    height: 170rpx;
    background: #fff;
    position: relative;
}

.leftbar_item.current {
    background: #f5f5f5;
}

.leftbar_item .link {
    display: block;
    height: 170rpx;
    width: 150rpx;
    padding-top: 40rpx;
    box-sizing: border-box;
}

.leftbar_item .img {
    display: block;
    height: 52rpx;
    width: 50rpx;
    margin: 0rpx auto 0rpx;
}

.leftbar_item .t {
    font-size: 24rpx;
    color: #666;
    font-weight: 400;
    height: 48rpx;
    line-height: 48rpx;
    text-align: center;
}

.leftbar_item .line {
    height: 48rpx;
    width: 10rpx;
    position: absolute;
    left: 0rpx;
    top: 50%;
    margin-top: -24rpx;
    display: none;
    background: #ff9292;
    border-radius: 6rpx;
}

.leftbar_item.current .t {
    color: #ff9292;
}

.leftbar_item.current .line {
    display: block;
}

.rightcon {
    width: 600rpx;
    background: #f5f5f5;
}

.rightcon_scroll {
    width: 600rpx;
    background: #f5f5f5;
    position: fixed;
    right: 0px;
    bottom: 0px;
    box-sizing: border-box;
}

.zsk_item {
    width: 536rpx;
    margin: 0rpx auto 32rpx;
    height: 188rpx;
    background: #fff;
    border-radius: 16rpx;
    position: relative;
    top: 40rpx;
}

.zsk_item:first-child {
}

.zsk_item .link {
    display: block;
    height: 188rpx;
    width: 536rpx;
    box-sizing: border-box;
    padding: 22rpx 184rpx 22rpx 24rpx;
}

.zsk_item .link .imgw {
    height: 140rpx;
    width: 140rpx;
    right: 24rpx;
    top: 22rpx;
    border-radius: 16rpx;
    position: absolute;
    overflow: hidden;
}

.zsk_item .link .imgw .img {
    display: block;
    height: 100%;
    width: 100%;
}

.zsk_item .link .imgw .vicon {
    height: 24rpx;
    width: 24rpx;
    display: block;
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    background: url(https://s1.st.meishij.net/p2/20181206/20181206143142_460.png) 0rpx 0rpx no-repeat;
    background-size: 24rpx 48rpx;
}

.zsk_item .link .t {
    font-size: 32rpx;
    font-weight: 600;
    line-height: 44rpx;
    height: 88rpx;
    color: #000;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.zsk_item .link .c {
    font-size: 20rpx;
    line-height: 32rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #666;
    margin-top: 20rpx;
}

.zsk_item .link .c .name {
    display: inline-block;
    vertical-align: top;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.loadtip {
    display: block;
    height: 60rpx;
    width: 340rpx;
    background: rgba(0,0,0,0.7);
    color: #fff;
    text-align: center;
    border-radius: 30rpx;
    font-weight: 600;
    position: fixed;
    z-index: 100;
    top: 200rpx;
    left: 50%;
    margin-left: -170rpx;
    font-size: 28rpx;
    line-height: 60rpx;
    transition: all ease 1s;
    opacity: 1;
}

.loadtip.hide {
    top: -60rpx;
    opacity: 0;
}

.csmi_line {
    width: 100%;
    height: 10rpx;
    background: #f5f5f5;
}

.bottomline {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 40rpx 0rpx 120rpx;
    line-height: 32rpx;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}