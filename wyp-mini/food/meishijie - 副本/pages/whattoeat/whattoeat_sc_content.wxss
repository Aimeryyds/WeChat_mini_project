.backbtn {
    height: 32px;
    width: 32px;
    box-sizing: border-box;
    border: 1rpx solid rgba(214,214,214,0.5);
    background: rgba(255,255,255,0.5);
    border-radius: 16px;
    position: fixed;
    left: 7px;
    z-index: 9000;
}

.backbtnicon {
    display: block;
    height: 17px;
    width: 10px;
    margin: 7px 0px 0px 9px;
}

.sc_top {
    width: 100%;
    padding-top: 100%;
    overflow: hidden;
    position: relative;
}

.sc_titlew {
    width: 100%;
    padding: 48rpx 32rpx;
    box-sizing: border-box;
}

.sc_titlec {
    width: 686rpx;
    position: relative;
}

.sc_titlew .line {
    width: 8rpx;
    height: 100%;
    position: absolute;
    border-radius: 4rpx;
    background: #ffafaf;
    background-image: linear-gradient(to bottom,#ffafaf,#ffa1a1);
}

.sc_titlew .title {
    font-size: 56rpx;
    font-weight: 600;
    color: #000;
    line-height: 56rpx;
    padding-left: 36rpx;
    margin-bottom: 8rpx;
}

.sc_titlew .othernamesw {
    padding-left: 36rpx;
    font-size: 0px;
    width: 80%;
}

.sc_titlew .othernamesw .othername {
    display: inline-block;
    max-width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    vertical-align: top;
    height: 44rpx;
    line-height: 44rpx;
    padding: 0rpx 14rpx;
    font-size: 24rpx;
    font-weight: 400rpx;
    border-radius: 8rpx;
    background: #f2f2f2;
    margin: 24rpx 10rpx 0rpx 0rpx;
    color: #666;
}

.sc_content {
    width: 100%;
}

.sc_content_con1 {
    margin: 0rpx 32rpx;
    border-bottom: 1rpx solid #f5f5f5;
    padding-bottom: 40rpx;
}

.sc_content_con1:last-child {
    border-bottom: 0px;
}

.sc_content_con1 .tw {
    padding-top: 36rpx;
    height: 44rpx;
    line-height: 44rpx;
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
}

.sc_content_con1 .tw .i {
    height: 44rpx;
    width: 44rpx;
    margin-right: 20rpx;
    display: inline-block;
    vertical-align: top;
}

.sc_content_con1 .c {
    font-size: 28rpx;
    font-weight: 400;
    color: #999;
    line-height: 44rpx;
    padding-top: 22rpx;
}

.sc_cpw {
    width: 100%;
}

.sc_cpw .tip {
    font-size: 24rpx;
    color: #999;
    line-height: 88rpx;
    padding: 0rpx 32rpx;
}

.sc_cpw .title {
    height: 86rpx;
    line-height: 86rpx;
    padding: 0rpx 32rpx 0rpx 54rpx;
    background: url(https://s1.st.meishij.net/p2/20190605/20190605110255_219.png) left center no-repeat;
    background-size: auto 100%;
    font-size: 32rpx;
    font-weight: 600;
    color: #000;
}

.recipeitem {
    width: 686rpx;
    margin: 22rpx 32rpx 32rpx 32rpx;
    border-radius: 32rpx;
    background: #fff;
    box-shadow: 0rpx 2rpx 16rpx rgba(0,0,0,0.1);
    height: 244rpx;
    padding: 32rpx 32rpx 32rpx 244rpx;
    box-sizing: border-box;
    position: relative;
}

.recipeitem .cpimgw {
    width: 180rpx;
    height: 180rpx;
    position: absolute;
    left: 32rpx;
    top: 32rpx;
    border-radius: 16rpx;
    overflow: hidden;
}

.recipeitem .cpimgw .vicon {
    height: 24rpx;
    width: 24rpx;
    display: block;
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    background: url(https://s1.st.meishij.net/p2/20181206/20181206143142_460.png) 0rpx 0rpx no-repeat;
    background-size: 24rpx 48rpx;
}

.recipeitem .cpinfow {
    height: 180rpx;
    overflow: hidden;
    position: relative;
}

.recipeitem .cpinfow .cpnamew {
    height: 60rpx;
}

.recipeitem .cpinfow .cpnamew .cpname {
    font-size: 32rpx;
    color: #000;
    font-weight: 400;
    line-height: 60rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.recipeitem .cpinfow .starw {
    height: 48rpx;
    font-size: 0px;
}

.recipeitem .cpinfow .starw .img {
    display: inline-block;
    vertical-align: top;
    margin-top: 8rpx;
    height: 24rpx;
    width: 24rpx;
    margin-right: 6rpx;
}

.recipeitem .cpinfow .viewdata {
    height: 32rpx;
    position: absolute;
    left: 0rpx;
    width: 100%;
    text-align: right;
    bottom: 6px;
}

.recipeitem .cpinfow .viewdata .p {
    display: inline-block;
    vertical-align: top;
    line-height: 32rpx;
    font-size: 20rpx;
    color: #666;
    font-weight: 400;
    margin-left: 10rpx;
}

.recipeitem .cpinfow .viewdata .i {
    display: inline-block;
    vertical-align: top;
    height: 24rpx;
    width: 24rpx;
    margin-top: 4rpx;
    margin-left: 20rpx;
}

.recipeitem .cpinfow .viewdata .avatarw {
    height: 32rpx;
    position: absolute;
    left: 0px;
    top: 0px;
    max-width: 50%;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.recipeitem .cpinfow .viewdata .avatarw .ava {
    height: 32rpx;
    width: 32rpx;
    border-radius: 16rpx;
    overflow: hidden;
    position: absolute;
    left: 0rpx;
    top: 0rpx;
}

.recipeitem .cpinfow .viewdata .avatarw .name {
    font-size: 20rpx;
    color: #666;
    line-height: 32rpx;
    padding-left: 8rpx;
    display: inline-block;
    vertical-align: top;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    max-width: 100%;
    padding-left: 40rpx;
    box-sizing: border-box;
}

.sc_cpw .allbtn {
    height: 88rpx;
    text-align: center;
    width: 100%;
    line-height: 88rpx;
    font-size: 32rpx;
    color: #000;
    font-weight: 600;
    margin: 40rpx 0rpx;
}

.sc_cpw .allbtn .i {
    display: inline-block;
    vertical-align: top;
    height: 20rpx;
    width: 20rpx;
    margin-top: 34rpx;
    margin-left: 10rpx;
}

.sc_cpw_tip {
    font-size: 28rpx;
    text-align: center;
    color: #999;
    padding: 0rpx 0rpx 40rpx;
    line-height: 32rpx;
}

.csmi_line {
    width: 100%;
    height: 10rpx;
    background: #f5f5f5;
}