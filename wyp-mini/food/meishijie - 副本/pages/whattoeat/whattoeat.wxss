.csmi_top {
    background: #7babff url(https://s1.st.meishij.net/p2/20190710/20190710185831_210.jpg) center bottom no-repeat;
    background-size: 100% auto;
    width: 100%;
    height: 250rpx;
    position: relative;
}

.csmi_top .title {
    height: 44px;
    text-align: center;
    color: #fff;
    font-size: 18px;
    font-weight: 600;
    line-height: 44px;
}

.csmi_top .searchw {
    margin: 4px 114px 4px 16px;
    height: 32px;
    border-radius: 16px;
    background: rgba(255,255,255,0.6);
    font-size: 14px;
    color: #555;
    line-height: 31px;
    border: 1rpx solid rgba(0,0,0,0.08);
    box-sizing: border-box;
}

.csmi_top .searchw .img {
    display: inline-block;
    vertical-align: top;
    height: 16px;
    width: 16px;
    margin: 7px 10px 0rpx 16px;
}

.csmi_top_t1 {
    font-size: 44rpx;
    line-height: 64rpx;
    color: #fff;
    font-weight: 600;
    position: absolute;
    left: 32rpx;
    bottom: 60rpx;
    text-shadow: 0rpx 2rpx 4rpx rgba(0,0,0,0.2);
}

.csmi_top_t2 {
    font-size: 24rpx;
    line-height: 44rpx;
    color: #fff;
    font-weight: 400;
    position: absolute;
    left: 32rpx;
    bottom: 22rpx;
    text-shadow: 0rpx 2rpx 4rpx rgba(0,0,0,0.2);
}

.csmi_content {
    width: 100%;
    background: #fff;
}

.csmi_item {
    padding: 48rpx 0rpx 16rpx;
}

.csmi_item .t {
    height: 56rpx;
    line-height: 56rpx;
    font-size: 28rpx;
    font-weight: 600;
    color: #fff;
    padding: 0rpx 16rpx 0rpx;
    display: inline-block;
    border-radius: 28rpx;
    background: #ff7267;
    margin: 32rpx 0rpx 0rpx 32rpx;
}

.csmi_item .t .arrow {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710194337_374.png) center no-repeat;
    background-size: 100% 100%;
    height: 26rpx;
    width: 26rpx;
    display: inline-block;
    vertical-align: top;
    margin: 15rpx 0rpx 0rpx 20rpx;
}

.csmi_item .t .i {
    display: inline-block;
    vertical-align: middle;
    height: 54rpx;
    width: 44rpx;
    margin-right: 16rpx;
}

.csmi_item .link {
    padding: 0rpx 0rpx 0rpx;
    margin: 0rpx 32rpx 32rpx;
}

.csmi_item .link .c {
    height: 386rpx;
    width: 686rpx;
    border-radius: 5rpx;
    position: relative;
    margin: 0rpx auto;
    box-shadow: 0rpx 0rpx 0rpx rgba(0,0,0,0.2);
}

.csmi_item .link .t1 {
    font-size: 48rpx;
    line-height: 68rpx;
    color: #fff;
    font-weight: 600;
    text-shadow: 0rpx 0rpx 50rpx rgba(0,0,0,0.05);
    margin: 22rpx 0rpx 0rpx 32rpx;
}

.csmi_item .link .c .info {
    position: absolute;
    bottom: 28rpx;
    left: 32rpx;
}

.csmi_item .link .c .tags {
    font-size: 0px;
}

.csmi_item .link .c .tags .tag {
    height: 44rpx;
    line-height: 44rpx;
    color: #363636;
    font-size: 28rpx;
    font-weight: 400;
    padding: 0rpx 0rpx;
    display: block;
}

.csmi_item .link .c .tags .tag .dot {
    display: inline-block;
    vertical-align: top;
    height: 16rpx;
    width: 16rpx;
    margin-top: 14rpx;
    margin-right: 10rpx;
    background: url(https://s1.st.meishij.net/p2/20190710/20190710194337_588.png) center no-repeat;
    background-size: 100% 100%;
}

.csmi_line {
    width: 100%;
    height: 10rpx;
    background: #f5f5f5;
}

.more_cj_tip {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 0rpx 0rpx 40rpx;
    line-height: 32rpx;
}

.more_show {
    width: 100%;
    padding-bottom: 48rpx;
}

.more_show .t {
    height: 132rpx;
    padding-top: 8rpx;
    line-height: 132rpx;
    font-size: 40rpx;
    color: #363636;
    padding: 0px 32rpx;
    font-weight: 500;
}

.more_show .t .i {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200123_387.png) center no-repeat;
    background-size: 100% 100%;
    height: 36rpx;
    width: 38rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 48rpx;
    margin-right: 12rpx;
}

.more_show .c {
    padding: 0px 0rpx 0rpx 24rpx;
    font-size: 0px;
}

.more_show .c .item {
    height: 220rpx;
    width: 220rpx;
    display: inline-block;
    vertical-align: top;
    margin: 0rpx 8rpx 16rpx;
    border-radius: 4rpx;
}

.more_show .c .item .t1 {
    font-size: 32rpx;
    line-height: 44rpx;
    height: 44rpx;
    margin-left: 20rpx;
    font-weight: 500;
    color: #363636;
    margin-top: 24rpx;
}

.more_show .c .item .des {
    font-size: 24rpx;
    line-height: 36rpx;
    height: 36rpx;
    margin-left: 20rpx;
    font-weight: 400;
    color: #363636;
}

.more_show .c .item1 {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200124_725.png) center no-repeat;
    background-size: 100% 100%;
}

.more_show .c .item2 {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200124_826.png) center no-repeat;
    background-size: 100% 100%;
}

.more_show .c .item3 {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200125_360.png) center no-repeat;
    background-size: 100% 100%;
}

.more_show .c .item4 {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200125_298.png) center no-repeat;
    background-size: 100% 100%;
}

.more_show .c .item5 {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200125_527.png) center no-repeat;
    background-size: 100% 100%;
}

.more_show .c .item6 {
    background: url(https://s1.st.meishij.net/p2/20190710/20190710200125_790.png) center no-repeat;
    background-size: 100% 100%;
}

.bottomline {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 40rpx 0rpx 120rpx;
    line-height: 32rpx;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}