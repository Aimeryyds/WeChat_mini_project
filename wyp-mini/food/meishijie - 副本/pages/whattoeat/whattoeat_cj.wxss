.container {
    position: relative;
}

.cj_top {
    background: #fff url(https://s1.st.meishij.net/p2/20190710/20190710204415_305.jpg) center top no-repeat;
    background-size: 100% auto;
    width: 100%;
}

.csmi_top .title {
    height: 44px;
    text-align: center;
    color: #fff;
    font-size: 18px;
    font-weight: 600;
    line-height: 44px;
}

.topbar {
    position: fixed;
    left: 0px;
    height: 44px;
    width: 100%;
    z-index: 2000;
    background: #fff;
}

.topbar_backicon {
    position: absolute;
    left: 0px;
    bottom: 0px;
    box-sizing: border-box;
    height: 44px;
}

.topbar_backicon .nav-icon {
    width: 12px;
    height: 20px;
    padding: 12px 12px;
}

.topbar .searchw {
    margin: 4px 114px 4px 55px;
    height: 32px;
    border-radius: 16px;
    background: rgba(0,0,0,0.05);
    font-size: 14px;
    color: #555;
    line-height: 31px;
    border: 1rpx solid rgba(0,0,0,0.08);
    box-sizing: border-box;
}

.topbar .searchw .img {
    display: inline-block;
    vertical-align: top;
    height: 16px;
    width: 16px;
    margin: 7px 10px 0rpx 16px;
}

.backbtn {
    height: 32px;
    width: 32px;
    box-sizing: border-box;
    border: 1rpx solid rgba(214,214,214,0.5);
    background: rgba(255,255,255,0.5);
    border-radius: 16px;
    position: absolute;
    left: 7px;
    z-index: 9000;
}

.backbtnicon {
    display: block;
    height: 17px;
    width: 10px;
    margin: 7px 0px 0px 9px;
}

.cj_top .searchw {
    margin: 4px 114px 4px 55px;
    height: 32px;
    border-radius: 16px;
    background: rgba(255,255,255,0.6);
    font-size: 14px;
    color: #555;
    line-height: 31px;
    border: 1rpx solid rgba(0,0,0,0.08);
    box-sizing: border-box;
}

.cj_top .searchw .img {
    display: inline-block;
    vertical-align: top;
    height: 16px;
    width: 16px;
    margin: 7px 10px 0rpx 16px;
}

.cj_swiperw {
    width: 100%;
    font-size: 0px;
    height: 462rpx;
    padding: 52rpx 0rpx 0rpx;
}

.cj_swiper {
    width: 100%;
    font-size: 0px;
    height: 462rpx;
    font-size: 0px;
}

.cj_swiper_item {
    width: 100%;
    height: 462rpx;
    overflow: hidden;
    position: relative;
    box-sizing: border-box;
    padding: 0px 10rpx;
    transform: scale(1);
    transition: all ease 0.2s;
}

.cj_swiper_item.current {
    transform: scale(1);
}

.cj_swiper_item .link {
    display: block;
    width: 658rpx;
    height: 462rpx;
}

.cj_swiper_item .img {
    display: block;
    width: 658rpx;
    height: 370rpx;
    overflow: hidden;
    border-radius: 12rpx;
    margin: 0px auto;
}

.cj_swiper_item .img .top_vitem {
    width: 658rpx;
    height: 370rpx;
    border-radius: 12rpx;
}

.cj_swiper_item .muted_btn {
    position: absolute;
    right: 10rpx;
    top: 390rpx;
    height: 50rpx;
    width: 50rpx;
    z-index: 100;
}

.cj_swiper_item .infos {
    position: absolute;
    bottom: 0px;
    left: 10rpx;
    width: 580rpx;
    line-height: 92rpx;
    height: 92rpx;
    white-space: nowrap;
    box-sizing: border-box;
    overflow: hidden;
    text-overflow: ellipsis;
}

.cj_swiper_item .infos .title {
    font-size: 32rpx;
    line-height: 92rpx;
    color: #333;
    font-weight: 600;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cj_swiper_item .infos .time {
    font-size: 20rpx;
    color: #fff;
    line-height: 60rpx;
}

.cj_swiper_item .infos .vicon {
    height: 34rpx;
    width: 72rpx;
    position: absolute;
    right: 24rpx;
    bottom: 21rpx;
}

.cj_swiper_item .infos .vicon .i {
    height: 34rpx;
    width: 72rpx;
    display: block;
}

.cj_linksw {
    margin-top: 20rpx;
    width: 100%;
    text-align: center;
    font-size: 0px;
    padding-bottom: 20rpx;
}

.cj_links_item {
    width: 160rpx;
    height: 210rpx;
    display: inline-block;
    vertical-align: top;
    margin: 0rpx 36rpx;
}

.cj_links_item .img {
    display: block;
    height: 100rpx;
    width: 100rpx;
    margin: 0px auto;
}

.cj_links_item .t {
    font-size: 30rpx;
    color: #000;
    line-height: 92rpx;
    font-weight: 600;
}

.csm_datalist {
    width: 100%;
}

.csm_titlew {
    height: 76rpx;
    line-height: 76rpx;
    padding: 38rpx 32rpx 0rpx;
    position: relative;
}

.csm_titlew .t {
    font-weight: 600;
    color: #000;
    line-height: 76rpx;
    font-size: 40rpx;
}

.csm_titlew .more {
    height: 76rpx;
    right: 0rpx;
    top: 38rpx;
    padding: 0rpx 32rpx 0rpx;
    line-height: 76rpx;
    color: #777;
    font-size: 28rpx;
    position: absolute;
    display: block;
}

.csm_titlew .more .i {
    height: 30rpx;
    width: 18rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 23rpx;
    margin-left: 14rpx;
}

.csm_dataitem {
    width: 100%;
    padding: 0rpx 32rpx;
    box-sizing: border-box;
}

.csm_dataitem_link {
    width: 100%;
    display: block;
    border-bottom: 1rpx solid #eee;
    min-height: 140rpx;
    padding: 32rpx 160rpx 32rpx 0rpx;
    box-sizing: border-box;
    position: relative;
}

.csm_dataitem_link .imgw {
    height: 140rpx;
    width: 140rpx;
    border-radius: 16rpx;
    position: absolute;
    right: 0rpx;
    top: 32rpx;
    overflow: hidden;
}

.csm_dataitem_link .imgw .img {
    height: 140rpx;
    width: 140rpx;
}

.csm_dataitem_link .imgw .vicon {
    height: 24rpx;
    width: 24rpx;
    display: block;
    position: absolute;
    right: 12rpx;
    top: 12rpx;
    background: url(https://s1.st.meishij.net/p2/20181206/20181206143142_460.png) 0rpx 0rpx no-repeat;
    background-size: 24rpx 48rpx;
}

.csm_dataitem_link .info {
    min-height: 140rpx;
}

.csm_dataitem_link .info .t {
    font-size: 32rpx;
    line-height: 44rpx;
    height: 88rpx;
    color: #000;
    font-weight: 500;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.csm_dataitem_link .info .tagsw {
    height: 36rpx;
    font-size: 0rpx;
    margin-top: 16rpx;
}

.csm_dataitem_link .info .tagsw .tag {
    display: inline-block;
    vertical-align: top;
    padding: 0rpx 12rpx;
    border: 1rpx solid #ff9292;
    color: #ff9292;
    font-size: 22rpx;
    line-height: 34rpx;
    box-sizing: border-box;
    border-radius: 18rpx;
    margin-right: 20rpx;
}

.cj_adw {
    padding: 32rpx 32rpx;
}

.csm_cj_zqw {
    width: 100%;
    padding-bottom: 60rpx;
}

.csm_cj_zq_list {
    padding: 32rpx 0rpx;
    white-space: nowrap;
    font-size: 0px;
}

.csm_cj_zq_item {
    height: 270rpx;
    width: 480rpx;
    margin-right: 32rpx;
    position: relative;
    display: inline-block;
    vertical-align: top;
    border-radius: 16rpx;
    overflow: hidden;
}

.csm_cj_zq_item:first-child {
    margin-left: 32rpx;
}

.csm_cj_zq_item .img {
    height: 270rpx;
    width: 480rpx;
    border-radius: 16rpx;
    margin-right: 32rpx;
}

.csm_cj_zq_item .infos {
    position: absolute;
    bottom: 0px;
    left: 0px;
    width: 100%;
    padding: 32rpx 32rpx 8rpx;
    box-sizing: border-box;
}

.csm_cj_zq_item .infos .title {
    font-size: 32rpx;
    line-height: 44rpx;
    font-weight: 600;
    color: #fff;
    text-shadow: 0rpx 2rpx 2rpx rgba(0,0,0,0.2);
    white-space: normal;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.csm_cj_zq_item .infos .des {
    font-size: 20rpx;
    line-height: 48rpx;
    font-weight: 400;
    color: #fff;
    text-shadow: 0rpx 2rpx 2rpx rgba(0,0,0,0.2);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.csm_cj_zq_item .infos .des .name {
    max-width: 160rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    display: inline-block;
    vertical-align: top;
}

.csm_cj_zq_item .vicon {
    height: 44rpx;
    width: 44rpx;
    display: block;
    position: absolute;
    right: 24rpx;
    top: 24rpx;
    background: url(https://s1.st.meishij.net/p2/20181206/20181206143142_460.png) 0rpx 0rpx no-repeat;
    background-size: 44rpx 88rpx;
}

.csmi_line {
    width: 100%;
    height: 10rpx;
    background: #f5f5f5;
}

.bottomline {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 40rpx 0rpx 120rpx;
    line-height: 32rpx;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}