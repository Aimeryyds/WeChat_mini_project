.top {
    width: 100%;
    padding-top: 50%;
    position: relative;
    overflow: hidden;
}

.top .h1 {
    position: absolute;
    left: 30rpx;
    bottom: 120rpx;
    line-height: 80rpx;
    font-size: 0px;
    z-index: 10;
}

.top .h1 .em {
    display: inline-block;
    vertical-align: top;
    background: linear-gradient(to right,#ffffff,#ffd7ac);
    -webkit-background-clip: text;
    color: transparent;
    font-weight: 600;
    font-size: 68rpx;
}

.top .des {
    position: absolute;
    left: 32rpx;
    bottom: 44rpx;
    line-height: 44rpx;
    font-size: 32rpx;
    color: #fff;
    font-weight: 500;
    z-index: 10;
}

.top .topimg {
    display: block;
    height: 100%;
    width: 100%;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    z-index: 1;
}

.top .topimg_cover {
    height: 100%;
    width: 100%;
    background: url(https://s1.st.meishij.net/p2/20180912/20180912174143_971.png) center no-repeat;
    background-size: 100% 100%;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    z-index: 5;
}

.cpitemlist {
    width: 100%;
    padding: 24rpx 0;
}

.recipeitem {
    width: 100%;
    box-sizing: border-box;
    padding: 0rpx 30rpx;
}

.recipeitemw {
    height: 298rpx;
    width: 100%;
    box-sizing: border-box;
    padding: 24rpx 0rpx 24rpx 250rpx;
    position: relative;
}

.cpimgw {
    height: 250rpx;
    width: 250rpx;
    position: absolute;
    left: 0rpx;
    top: 24rpx;
    background: #f8f8f8;
}

.cpimgw .rank {
    display: block;
    height: 33px;
    width: 124rpx;
    position: absolute;
    left: 4rpx;
    top: -6rpx;
    background: url(https://s1.st.meishij.net/p2/20180912/20180912174144_667.png) no-repeat;
    background-size: 124rpx 330px;
}

.cpimgw .rank .em {
    text-align: center;
    font-size: 18rpx;
    line-height: 26rpx;
    margin: 17px 5% 0;
    color: #985e0e;
    font-weight: 400;
    display: block;
    width: 90%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpimgw .rank0 {
    background-position: 0rpx 0rpx;
}

.cpimgw .rank1 {
    background-position: 0rpx -33px;
}

.cpimgw .rank2 {
    background-position: 0rpx -66px;
}

.cpimgw .rank3 {
    background-position: 0rpx -99px;
}

.cpimgw .rank4 {
    background-position: 0rpx -132px;
}

.cpimgw .rank5 {
    background-position: 0rpx -165px;
}

.cpimgw .rank6 {
    background-position: 0rpx -198px;
}

.cpimgw .rank7 {
    background-position: 0rpx -231px;
}

.cpimgw .rank8 {
    background-position: 0rpx -264px;
}

.cpimgw .rank9 {
    background-position: 0rpx -297px;
}

.cpimgw image {
    display: block;
    height: 100%;
    width: 100%;
    border-radius: 2rpx;
}

.cpinfow {
    padding-left: 30rpx;
    height: 250rpx;
}

.cpinfow .cpnamew {
    padding-top: 0rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpinfow .cpname {
    font-size: 36rpx;
    color: #333;
    font-weight: 400;
    line-height: 88rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpinfow .wznamew {
    padding-top: 0rpx;
}

.cpinfow .wzname {
    font-size: 36rpx;
    color: #333;
    font-weight: 400;
    line-height: 54rpx;
    height: 162rpx;
    padding: 16rpx 0rpx 0rpx;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
}

.cpinfow .auw {
    height: 32rpx;
    padding-left: 32rpx;
    position: relative;
}

.cpinfow .auw image {
    display: block;
    height: 32rpx;
    width: 32rpx;
    border-radius: 100%;
    position: absolute;
    left: 0px;
    top: 0px;
}

.cpinfow .auw text {
    font-size: 24rpx;
    padding-left: 16rpx;
    line-height: 32rpx;
    color: #787878;
    display: block;
    white-space: nowrap;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpinfow .tagw {
    height: 40rpx;
}

.cpinfow .tag {
    height: 40rpx;
    background-image: linear-gradient(to bottom,#fc9b8b,#fb7887);
    color: #fff;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    border-radius: 4rpx;
    display: inline-block;
}

.cpinfow .tagw .t {
    height: 36rpx;
    line-height: 36rpx;
    display: inline-block;
    vertical-align: top;
    color: #fff;
    font-size: 24rpx;
    padding: 0rpx 20rpx 0rpx 20rpx;
    font-weight: 400;
    margin: 2rpx 0rpx 0rpx 0rpx;
}

.cpinfow .tagw .c {
    height: 36rpx;
    line-height: 36rpx;
    display: inline-block;
    vertical-align: top;
    font-weight: 400;
    font-size: 24rpx;
    padding: 0rpx 20rpx 0rpx 20rpx;
    background: #fff;
    color: #fc7b88;
    border-top-right-radius: 2rpx;
    border-bottom-right-radius: 2rpx;
    margin: 2rpx 2rpx 0rpx 0rpx;
    max-width: 230rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cpinfow .starw {
    height: 20rpx;
    font-size: 0px;
    margin-top: 28rpx;
}

.cpinfow .starw view {
    height: 20rpx;
    width: 20rpx;
    display: inline-block;
    vertical-align: top;
    margin-right: 8rpx;
}

.cpinfow .starw view image {
    height: 20rpx;
    width: 20rpx;
    display: block;
}

.cpinfow .viewdata {
    height: 32rpx;
    line-height: 32rpx;
    margin-top: 26rpx;
}

.cpinfow .viewdata image {
    display: inline-block;
    vertical-align: top;
    height: 26rpx;
    width: 26rpx;
    margin-top: 3rpx;
}

.cpinfow .viewdata text {
    display: inline-block;
    vertical-align: top;
    font-size: 18rpx;
    color: #666;
    margin: 0 30rpx 0 6rpx;
    font-weight: 400;
}

.bottomline {
    font-size: 28rpx;
    text-align: center;
    color: #bbb;
    padding: 40rpx 0rpx 120rpx;
    line-height: 32rpx;
}

.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}