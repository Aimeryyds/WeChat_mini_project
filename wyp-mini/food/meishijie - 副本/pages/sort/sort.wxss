.cplist_nodata {
    font-size: 36rpx;
    text-align: center;
    line-height: 400rpx;
    color: #787878;
}

.sort_w {
    padding: 0rpx 30rpx 60rpx;
}

.sort_item {
    padding: 20rpx 0rpx 0rpx;
    font-size: 0rpx;
}

.sort_t {
    font-size: 36rpx;
    color: #000;
    font-weight: 600;
    line-height: 88rpx;
    line-height: 88rpx;
}

.sort_icon {
    height: 56rpx;
    width: 56rpx;
    margin-top: 16rpx;
    display: inline-block;
    vertical-align: top;
    margin-right: 8rpx;
}

.sort_c {
}

.sort_c navigator {
    display: inline-block;
    vertical-align: top;
    margin: 0rpx 16rpx 16rpx 0rpx;
    border: 1rpx solid #eee;
    color: #555;
    font-size: 28rpx;
    padding: 0rpx 18rpx;
    line-height: 60rpx;
    height: 60rpx;
    border-radius: 2rpx;
    box-sizing: border-box;
}

.sort_c navigator[data-class=yes] {
    color: #000;
    display: block;
    margin: 0px 0px 0px 0px;
    border: 0px;
    font-size: 28rpx;
    font-weight: 600;
    line-height: 60rpx;
    height: 60rpx;
    padding: 0px;
    position: relative;
}

.sort_c navigator[data-class=yes] view {
    position: relative;
    z-index: 100;
    background: #fff;
    display: inline-block;
    vertical-align: top;
}

.sort_c navigator[data-class=yes]:after {
    content: " ";
    height: 2rpx;
    width: 100%;
    background: #f5f5f5;
    position: absolute;
    top: 50%;
    margin-top: -1rpx;
    left: 0rpx;
    z-index: 50;
}

.sort_iw {
    display: inline;
}