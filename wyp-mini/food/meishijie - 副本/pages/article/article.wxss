.loaderw {
    position: relative;
    z-index: 2000;
}

.loader {
    position: fixed;
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    background: rgba(255,255,255,0.5);
}

.loader .icon {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    display: block;
    background: #000;
    height: 80px;
    width: 80px;
}

.c_ad1 {
    margin: 30rpx 32rpx 30rpx;
    overflow: hidden;
}

.addXccBtn {
    width: 686rpx;
    height: 88rpx;
    background: #f25f52;
    color: #fff;
    line-height: 88rpx;
    text-align: center;
    border-radius: 4rpx;
    font-size: 32rpx;
    margin: 30rpx auto 60rpx;
}

.addXccbox {
    z-index: 99999;
    background: rgba(0,0,0,0.85);
    position: fixed;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 100%;
}

.hideXccboxbtn {
    background-color: transparent;
    height: 88rpx;
    width: 240rpx;
    color: #fff;
    border: 1px solid #fff;
    border-radius: 8rpx;
    line-height: 88rpx;
    text-align: center;
    font-size: 36rpx;
    position: absolute;
    left: 50%;
    top: 70%;
    transform: translate(-50%,-50%);
}

.favbtn_bottom_w {
    position: relative;
    height: 120rpx;
    width: 100%;
}

.favbtn_bottom {
    width: 200rpx;
    height: 88rpx;
    line-height: 86rpx;
    background: rgba(255,255,255,0.9);
    z-index: 200;
    border: 1rpx solid rgba(0,0,0,0.05);
    border-radius: 44rpx;
    position: fixed;
    text-align: right;
    left: 32rpx;
    bottom: 32rpx;
    box-sizing: border-box;
    box-shadow: 0rpx 16rpx 32rpx rgba(0,0,0,0.1);
}

.favbtn_bottom image {
    display: inline-block;
    vertical-align: top;
    height: 60rpx;
    width: 60rpx;
    position: absolute;
    left: 21rpx;
    top: 13rpx;
}

.favbtn_bottom text {
    font-size: 28rpx;
    color: #333;
    display: block;
    line-height: 86rpx;
    height: 86rpx;
    font-weight: 600;
    text-align: center;
    padding-left: 64rpx;
    box-sizing: border-box;
}

.favbtn_bottom::after {
    border: 0px;
}

