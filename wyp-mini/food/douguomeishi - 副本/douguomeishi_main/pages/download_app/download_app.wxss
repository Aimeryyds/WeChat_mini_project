.download-app-alert-container {
    background: transparent;
    background-color: white;
    width: 662rpx;
    position: fixed;
    left: 50%;
    top: 50%;
    z-index: 999;
    transform: translate(-50%,-50%);
    border-radius: 24rpx;
}

.download-app-alert-title {
    margin-top: 60rpx;
    font-size: 36rpx;
    color: #333;
    text-align: center;
}

.download-app-contact-button {
    margin: 40rpx 66rpx auto 66rpx;
    background-color: #27B451;
    height: 88rpx;
    line-height: 88rpx;
    border-radius: 44rpx;
    font-size: 32rpx;
    color: white;
}

.download-app-cancel-view {
    margin: 10rpx 66rpx 10rpx 66rpx;
    height: 88rpx;
    color: #999;
    font-size: 32rpx;
    text-align: center;
    line-height: 88rpx;
}