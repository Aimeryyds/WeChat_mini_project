.recipe-purchase {
    background-color: #ffce6b;
    position: fixed;
    display: flex;
    align-items: center;
    justify-content: center;
    top: 0px;
    width: 100%;
    height: 86rpx;
    z-index: 10000;
}

.recipe-purchase-text {
    color: #fff;
    font-size: 30rpx;
    flex: 1;
    font-weight: bold;
    padding-left: 40rpx;
}

.recipe-purchase-apply {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 180rpx;
    height: 60rpx;
    background-color: white;
    border-radius: 30rpx;
    font-size: 26rpx;
    font-weight: bold;
    color: #ffb31a;
    margin-right: 40rpx;
}

.recipe-purchase-view {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 160rpx;
    height: 60rpx;
    background-color: rgba(255,255,255,0.2);
    border-radius: 30rpx;
    font-size: 26rpx;
    font-weight: bold;
    color: #fff;
    margin-right: 40rpx;
}

.headline-container {
    width: 100%;
    height: 600rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center;
}

.video-play-button {
    width: 100rpx;
    height: 100rpx;
}

.headline-video {
    width: 100%;
    height: 600rpx;
    background-color: #ebebeb;
}

.blue_box {
    width: 100rpx;
    height: 100rpx;
    background: blue;
    align-self: center;
}

.title-label {
    text-align: center;
    font-size: 48rpx;
    font-weight: bold;
    line-height: 1.8;
    padding-top: 30rpx;
    margin: 30rpx 70rpx auto 100rpx;
    color: #333;
}

.brief-label {
    text-align: center;
    color: #999;
    font-size: 28rpx;
    margin: 20rpx 40rpx auto 40rpx;
}

.story-label {
    margin: 50rpx 40rpx auto 40rpx;
    font-size: 34rpx;
    line-height: 1.8;
    color: #333;
}

.author-view {
    display: flex;
    margin: 40rpx 40rpx 0 40rpx;
    align-items: center;
}

.author-view image {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
}

.author-view view {
    margin-left: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    flex: 1;
    font-size: 28rpx;
}

.ingredients-section-header,.directions-section-header,.tips-section-header,.comments-section-header,.recommends-section-header {
    text-align: center;
    font-size: 34rpx;
    font-weight: bold;
    height: 88rpx;
    line-height: 88rpx;
    vertical-align: middle;
    margin: 24rpx 40rpx 40rpx 40rpx;
}

.ingredient-cell {
    overflow: hidden;
    height: 100rpx;
}

.ingredient-cell-content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 40rpx;
    font-size: 34rpx;
    height: 98rpx;
    color: #333;
}

.ingredients-cell-seperator {
    height: 1rpx;
    background-color: #e5e3df;
