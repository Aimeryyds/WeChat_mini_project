.optional {
    padding-bottom: 20rpx;
}

.btn {
    border-radius: 36rpx;
    box-shadow: 0 2rpx 8rpx 4rpx #f5f5f5;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 24rpx;
    background-color: #fff;
}

.searchWrap {
    margin: 32rpx auto 30rpx;
    width: 686rpx;
    height: 72rpx;
}

.searchWrap .searchIcon {
    width: 32rpx;
    height: 32rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin-right: 25rpx;
}

.infoText {
    font-size: 20rpx;
    margin-left: 32rpx;
}

.btmBtn {
    margin: 32rpx auto;
    width: 343rpx;
    height: 72rpx;
}

.btmBtn .login {
    width: 100%;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}

.btmBtn .noLogin {
    height: 100%;
    box-sizing: border-box;
}

.btmBtn .noLogin #btnLogin {
    font-size: 24rpx;
}

.btmBtn .noLogin #btnLogin::after {
    border: none;
}