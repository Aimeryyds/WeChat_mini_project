.btmWrap {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 98rpx;
    background-color: #fff;
    z-index: 99;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    padding: 0 32rpx;
    box-sizing: border-box;
}

.btmWrap.iPhoneX {
    height: 156rpx;
    padding: 0 32rpx 58rpx;
}

.btmWrap .deal {
    width: 206rpx;
    height: 72rpx;
    border-radius: 36rpx;
    color: #fff;
    font-size: 30rpx;
    margin-right: 60rpx;
    position: relative;
    overflow: hidden;
}

.btmWrap .deal .text {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
    z-index: 5;
}

.btmWrap .deal .btnBg {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    z-index: 2;
}

.btmWrap .item {
    height: 72rpx;
    width: 75rpx;
    position: relative;
    margin: 0 80rpx;
}

.btmWrap .item .icon {
    width: 30rpx;
    height: 30rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    position: absolute;
    top: 5rpx;
    left: 50%;
    transform: translateX(-50%);
}

.btmWrap .item .text {
    font-size: 20rpx;
    position: absolute;
    bottom: 5rpx;
    left: 50%;
    transform: translateX(-50%);
}

.btmWrap .item #idLogin {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
}

.btmWrap .item .artBotShareBtn {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: initial;
    z-index: 2;
    color: transparent;
}