.btn {
    border-radius: 36rpx;
    box-shadow: 0 2rpx 8rpx 4rpx #f5f5f5;
    background-color: #fff;
}

.paddingBox {
    margin: 32rpx auto 48rpx;
    height: 72rpx;
}

.searchBox {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 152rpx;
    background-color: #fff;
    z-index: 10;
}

.searchWrap {
    margin: 32rpx auto 48rpx;
    width: 686rpx;
    height: 72rpx;
    position: relative;
}

.searchWrap .iconWrap {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 24rpx;
}

.searchWrap .iconWrap .searchIcon {
    width: 32rpx;
    height: 32rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin-right: 25rpx;
}

.searchWrap .inputSearch {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    padding-left: 32rpx;
    font-size: 24rpx;
}

.searchWrap .inputSearch input {
    width: 620rpx;
}

.hotSrarch .hotSearchWrap .tit {
    font-size: 40rpx;
    margin-bottom: 12rpx;
    padding-left: 32rpx;
    font-weight: 700;
}

.hotSrarch .hotSearchWrap .hotItem {
    font-family: THS;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    height: 96rpx;
    margin: 0 32rpx;
}

.hotSrarch .hotSearchWrap .hotItem .info {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.hotSrarch .hotSearchWrap .hotItem .info .rank {
    width: 32rpx;
    height: 32rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    background-color: #999;
    color: #fff;
    margin-right: 16rpx;
    font-size: 24rpx;
    border-radius: 8rpx;
}

.hotSrarch .hotSearchWrap .hotItem .info .stockName {
    font-family: PingFang Medium,PingFang SC,Source Han Sans;
    font-size: 28rpx;
    margin-right: 24rpx;
}

.hotSrarch .hotSearchWrap .hotItem .info .precent {
    font-size: 28rpx;
}

.hotSrarch .hotSearchWrap .hotItem .info .precent.up {
    color: #e93030;
}

.hotSrarch .hotSearchWrap .hotItem .info .precent.down {
    color: #090;
}

.hotSrarch .hotSearchWrap .hotItem .btn {
    width: 128rpx;
    height: 56rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    box-shadow: 0 2rpx 8rpx 0 rgba(233,48,48,.2),inset 0 0 4rpx 0 rgba(233,48,48,.3);
    border-radius: 28rpx;
    font-size: 24rpx;
    color: #f27451;
}

.hotSrarch .hotSearchWrap .hotItem .btn #btnLogin {
    height: 100%;
    box-sizing: border-box;
    font-size: 24rpx;
    color: #f27451;
    padding: 0;
}

.hotSrarch .hotSearchWrap .hotItem .btn #btnLogin::after {
    border: none;
}

.hotSrarch .hotSearchWrap .hotItem:nth-child(2) .info .rank {
    background-color: #e93030;
}

.hotSrarch .hotSearchWrap .hotItem:nth-child(3) .info .rank {
    background-color: #e98630;
}

.hotSrarch .hotSearchWrap .hotItem:nth-child(4) .info .rank {
    background-color: #e9b630;
}

.searchContent .searchItem {
    font-family: THS;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    height: 96rpx;
    margin: 0 32rpx;
}

.searchContent .searchItem .leftWrap {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.searchContent .searchItem .leftWrap .stockName {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-pack: center;
    justify-content: center;
    width: 256rpx;
}

.searchContent .searchItem .leftWrap .stockName .name {
    font-family: PingFang Medium,PingFang SC,Source Han Sans;
    font-size: 32rpx;
}

.searchContent .searchItem .leftWrap .stockName .code {
    font-size: 24rpx;
    color: #999;
}

.searchContent .searchItem .leftWrap .price {
    width: 155rpx;
    font-size: 32rpx;
}

.searchContent .searchItem .leftWrap .price.up {
    color: #e93030;
}

.searchContent .searchItem .leftWrap .price.down {
    color: #090;
}

.searchContent .searchItem .leftWrap .precent {
    -webkit-box-flex: 1;
    flex: 1;
    font-size: 32rpx;
}

.searchContent .searchItem .leftWrap .precent.up {
    color: #e93030;
}

.searchContent .searchItem .leftWrap .precent.down {
    color: #090;
}

.searchContent .searchItem .option {
    width: 128rpx;
    height: 56rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    box-shadow: 0 2rpx 8rpx 4rpx #f5f5f5;
    border-radius: 28rpx;
    font-size: 24rpx;
    color: #323232;
}

.searchContent .searchItem .option.del {
    color: #999;
}

.searchContent .searchItem .option #btnLogin {
    height: 100%;
    box-sizing: border-box;
    font-size: 24rpx;
    color: #323232;
    padding: 0;
}

.searchContent .searchItem .option #btnLogin::after {
    border: none;
}

.searchContent .noContent {
    color: #999;
    font-size: 24rpx;
    text-align: center;
}