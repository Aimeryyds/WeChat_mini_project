.tit {
    margin: 32rpx;
    font-size: 34rpx;
    font-weight: 700;
}

.logoWrap {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    padding: 80rpx 0 110rpx;
}

.logoWrap .thsLogo {
    width: 200rpx;
    height: 282rpx;
}

.btnWrap {
    height: 88rpx;
    width: 684rpx;
    position: relative;
    margin: 0 auto;
    box-sizing: border-box;
}

.btnWrap .btn {
    position: absolute;
    top: 0;
    left: 0;
    height: 88rpx;
    width: 100%;
    background: linear-gradient(left,#f26312,#e93030);
    text-align: center;
    line-height: 88rpx;
    border-radius: 44rpx;
    font-size: 26rpx;
    color: #fff;
    z-index: 1;
}

.btnWrap .jumpService {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 5;
    height: 100%;
    width: 100%;
    color: transparent;
    height: 88rpx;
    padding: 0;
    background-color: initial;
}

.btnWrap .jumpService::after {
    border: none;
}

.stepWrap {
    margin: 100rpx 32rpx;
}

.stepWrap .stepItem {
    height: 40rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.stepWrap .stepItem .num {
    height: 32rpx;
    width: 32rpx;
    background-color: #ccc;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 24rpx;
    color: #fff;
    margin-right: 16rpx;
    border-radius: 50%;
}

.stepWrap .stepItem .text {
    font-size: 28rpx;
}

.stepWrap .stepLine {
    width: 1rpx;
    height: 56rpx;
    background-color: #eee;
    margin-left: 16rpx;
}