.paddingBlock {
    width: 100%;
}

.navWrap .accountBg {
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 1000;
}

.navWrap .guidanceBg {
    position: fixed;
    top: 158rpx;
    right: 0;
    width: 380rpx;
    height: 136rpx;
    z-index: 10000;
}

.navWrap .accountWrap {
    position: fixed;
    left: 32rpx;
    top: 150rpx;
    width: 248rpx;
    height: 192rpx;
    background-color: #5b5b5b;
    border-radius: 16rpx;
    box-shadow: 0 2rpx 8rpx 4rpx rgba(85,85,85,.1);
    border: 1px solid rgba(85,85,85,.1);
    z-index: 1000;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    color: #fff;
}

.navWrap .accountWrap .accountItem {
    -webkit-box-flex: 1;
    flex: 1;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 30rpx;
}

.navWrap .accountWrap .accountItem .icon {
    width: 38rpx;
    height: 25rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    margin-right: 25rpx;
}

.navBar {
    position: fixed!important;
    top: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    background-color: initial;
    z-index: 999;
}

.navBar .contentWrap {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.navBar .contentWrap .leftContent {
    width: 100rpx;
    margin-left: 32rpx;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.navBar .contentWrap .leftContent .backBtn {
    width: 67rpx;
    height: 48rpx;
}

.navBar .contentWrap .leftContent .userBtn {
    width: 67rpx;
    height: 48rpx;
    marign-left: 32rpx;
}

.navBar .contentWrap .leftContent .userBtn .login #btnLogin.ios {
    font-weight: 700;
}

.navBar .contentWrap .leftContent .userBtn .login #btnLogin::after {
    border: none;
}

.navBar .contentWrap .leftContent .headBtn {
    width: 64rpx;
    height: 64rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    border-radius: 50%;
}

.navBar .contentWrap .leftContent .login {
    font-size: 30rpx;
    color: #000;
}

.navBar .contentWrap .middleContent {
    font-size: 34rpx;
}

.navBar .contentWrap .middleContent.ios {
    font-weight: 700;
}

.navBar .contentWrap .wxBtn {
    width: 87rpx;
    height: 32rpx;
}

.cornerWrap {
    height: 16rpx;
    width: 24rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
    position: absolute;
    top: 0;
    right: 138rpx;
}

.addGuidance {
    position: absolute;
    top: 15rpx;
    right: 45rpx;
    height: 121rpx;
    width: 320rpx;
    background-color: #5b5b5b;
    padding: 24rpx 0 24rpx 24rpx;
    box-sizing: border-box;
    color: #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    border-radius: 16rpx;
}

.addGuidance::after {
    content: "";
    border: 24rpx solid transparent;
    border-bottom-color: #5b5b5b;
    position: absolute;
    top: -23rpx;
    right: 110rpx;
}

.addGuidance .contentText {
    -webkit-box-flex: 1;
    flex: 1;
    position: relative;
}

.addGuidance .contentText .text_one {
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    margin-bottom: 4rpx;
}

.addGuidance .contentText .text_two {
    height: 33rpx;
    line-height: 33rpx;
    font-size: 24rpx;
}

.addGuidance .contentText .corner {
    position: absolute;
    top: -59rpx;
    right: 0;
    width: 24rpx;
    height: 24rpx;
    background-color: red;
    border: 24rpx solid transparent;
    border-bottom-color: #5b5b5b;
}

.addGuidance .closeWrap {
    width: 64.5rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}

.addGuidance .closeWrap .close {
    width: 20rpx;
    height: 20rpx;
    background-size: 100% 100%;
    background-repeat: no-repeat;
}