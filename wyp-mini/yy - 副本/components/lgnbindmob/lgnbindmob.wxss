.radio-group {
    line-height: 84rpx;
    margin-left: 32rpx;
}

.radio-label {
    margin-right: 20rpx;
    position: relative;
    padding-left: 56rpx;
    cursor: pointer;
    vertical-align: middle;
}

.radio {
    display: none;
}

.radio-label:before {
    position: absolute;
    top: 0;
    left: 0;
    border-radius: 50%;
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    content: '';
    border: 1px solid #c0c0c0;
}

.radio-label:after {
    position: absolute;
    display: none;
    content: '';
    top: 10rpx;
    left: 10rpx;
    width: 24rpx;
    height: 24rpx;
    border-radius: 50%;
    background: #fac200;
}

.radio-label.active:after {
    display: block;
}

.radio-label.active:before {
    border: 1px solid #fac200;
}

.m-textbox {
    border: none;
    height: 84rpx;
    outline: none;
    box-sizing: border-box;
    width: 100%;
    padding: 0 10rpx;
    -webkit-appearance: none;
    color: #333;
    font-size: 32rpx;
}

.m-button {
    height: 84rpx;
    font-size: 36rpx;
    outline: none;
    border-radius: 6rpx;
    box-sizing: border-box;
    text-align: center;
    display: inline-block;
    background-color: #ffdd00;
    color: #333;
    text-decoration: none;
    line-height: 84rpx;
    padding: 0 24rpx;
}

.m-button .extraIcon {
    display: none;
}

.m-button-disabled {
    background-color: #e8e8e8;
    color: #666;
}

.m-link {
    text-decoration: none;
    outline: none;
    color: #999;
}

.mobile-mask {
