}

.m-xTextbox {
    position: relative;
    display: inline-block;
    width: 100%;
    box-sizing: border-box;
    border: none;
    padding-right: 26rpx;
    background-color: white;
    height: 84rpx;
}

.m-xTextbox .m-textbox {
    border-width: 0rpx;
    background-color: transparent;
    display: block;
}

