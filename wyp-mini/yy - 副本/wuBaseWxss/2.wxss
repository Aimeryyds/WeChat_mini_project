.no-data {
    width: 100%;
    height: 100%;
    background: #f5f5f5;
}

.no-data-main {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.no-data-main image {
    display: block;
    width: 228rpx;
    height: 228rpx;
    border-radius: 50%;
    margin: 0 auto;
}

.no-data-main .tips {
    width: 750rpx;
    font-size: 28rpx;
    color: #999;
    text-align: center;
    padding-top: 30rpx;
    padding-bottom: 15rpx;
}