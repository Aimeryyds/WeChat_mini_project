.cover-wrap {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 999;
    background: rgba(0,0,0,0.7);
}

.cover-wrap .tx_high {
    color: #FFAA0A;
}

.cover-wrap .confirm-cover {
    width: 558rpx;
    box-sizing: border-box;
    border-radius: 8rpx;
    position: fixed;
    left: 50%;
    top: 378rpx;
    margin-left: -279rpx;
    background: #fff;
    line-height: 1;
    padding-top: 48rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
}

.cover-wrap .confirm-cover .confirm-title {
    font-size: 32rpx;
    font-family: PingFangSC-Medium,PingFang SC;
    font-weight: 600;
    text-align: center;
    padding-bottom: 24rpx;
}

.cover-wrap .confirm-cover .confirm-desc {
    min-height: 126rpx;
    font-size: 28rpx;
    color: rgba(0,0,0,0.6);
    line-height: 40rpx;
    padding: 0 32rpx;
    text-align: center;
    -webkit-box-flex: 1;
    flex: 1;
    word-wrap: break-word;
}

.cover-wrap .confirm-cover .confirm-btns {
    border-top: .6px solid rgba(0,0,0,0.1);
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    height: 96rpx;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 32rpx;
    text-align: center;
}

.cover-wrap .confirm-cover .confirm-btns .cancel {
    flex-basis: 50%;
    color: rgba(0,0,0,0.6);
    padding: 32rpx 0;
    border-right: 0.6rpx solid rgba(0,0,0,0.1);
}

.cover-wrap .confirm-cover .confirm-btns .submit {
    flex-basis: 50%;
    color: #FFAA0A;
    padding: 32rpx 0;
}

.cover-wrap .confirm-cover .confirm-btns-third {
    font-size: 32rpx;
}

.cover-wrap .confirm-cover .confirm-btns-third view {
    width: 100%;
    height: 96rpx;
    border-top: .6px solid rgba(0,0,0,0.1);
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
}

.cover-wrap .confirm-cover .confirm-btns-third .submit {
    color: #FFAA0A;
}