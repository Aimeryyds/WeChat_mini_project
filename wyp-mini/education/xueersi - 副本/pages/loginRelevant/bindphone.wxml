<view catchtouchmove="false" class="container">
    <view class="phone-number">
        <text>{{phoneText}}</text>
        <input bindinput="userValue" maxlength="11" placeholder="{{inputNumMes}}" placeholderClass="holderText" type="number" value="{{userAccount}}"></input>
    </view>
    <view class="getbtn">
        <button bindtap="jumpPage" class="button" disabled="{{numDisable}}" style="opacity: {{opacity}}">{{getCodeText}}</button>
    </view>
</view>
