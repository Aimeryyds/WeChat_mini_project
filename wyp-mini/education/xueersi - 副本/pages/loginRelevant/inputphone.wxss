@import "../../wuBaseWxss/12.wxss";

.button {
    font-size: 36rpx;
    width: 566rpx;
    height: 84rpx;
    border-radius: 50rpx;
    background: -webkit-gradient(linear,left top,right top,from(#6DCAE0),to(#339EDC));
    background: -webkit-linear-gradient(left,#6DCAE0,#339EDC);
    background: linear-gradient(to right,#6DCAE0,#339EDC);
    color: #fff;
}

button[disabled] {
    border: none;
}

page {
    background-color: #3F4460;
    font-size: 32rpx;
    color: #FFF;
}

.container {
    height: 1200rpx;
}

.phone-number {
    padding: 148rpx 92rpx 0 92rpx;
}

.phone-number text {
    font-size: 48rpx;
    font-weight: 400;
}

.phone-number input {
    margin-top: 60rpx;
    padding-bottom: 40rpx;
    border-bottom: 1rpx solid #ADB0C1;
}

.getbtn {
    margin: 100rpx 92rpx 0 92rpx;
}

.getbtn button {
    color: #fff;
}

.login {
    margin-top: 80rpx;
    text-align: center;
    font-size: 28rpx;
}

.login text {
    color: #ADB0C1;
    border-bottom: 1rpx solid #ADB0C1;
}

.holderText {
    color: #ADB0C1;
    font-size: 32rpx;
}

.toast {
    z-index: 99999999;
}