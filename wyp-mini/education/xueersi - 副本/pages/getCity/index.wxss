page {
    background: #F4F5F6;
    height: 100%;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

view,text {
    font-family: PingFang SC;
}

view[hidden],text[hidden],cover-view[hidden] {
    display: none!important;
}

button[disabled] {
    border: 1px solid rgba(109,86,86,0);
    outline: none;
}

button {
    background-color: #4aa4fd;
    color: #fff;
}

.icon-home_classdetailpage4 {
    font-size: 26rpx;
    color: #A1A5BB;
}

.icon-home_classdetail_clo {
    font-size: 38rpx;
    color: #BABDC9;
}

.icon-home_classdetail_cal,.icon-home_classdetailpage,.icon-home_classdetailpage2,.icon-home_classdetail_coa,.icon-home_classdetail_mai {
    font-size: 28rpx;
    color: #C8CBD9;
}

page {
    background-color: #fff;
    height: 100%;
    position: relative;
}

.index-center .center-logo {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    padding-top: 240rpx;
    -webkit-box-pack: center;
    justify-content: center;
}

.index-center .center-logo .logo {
    height: 141rpx;
    width: 356rpx;
}

.index-center .center-button {
    position: absolute;
    bottom: 180rpx;
    width: 100%;
}

.index-center .center-button.power-button {
    bottom: 150rpx;
}

.index-center .center-button .submission {
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.index-center .center-button .submission .button-view {
    height: 82rpx;
    width: 342rpx;
    color: #0750f9;
    font-size: 36rpx;
    border-radius: 50rpx;
    border: 4rpx solid #0750f9;
    font-family: 'PingFang-SC-Bold';
    font-weight: bold;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
}

.index-center .center-button .submission .userInfoBtn {
    position: absolute;
    height: 82rpx;
    width: 348rpx;
    z-index: 1;
    border-radius: 50rpx;
    left: 50%;
    transform: translateX(-50%);
    top: 5rpx;
    opacity: 0;
}

.index-center .center-button .test {
    font-size: 24rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    padding-top: 30rpx;
    color: #b7bbc0;
    font-family: 'SourceHanSansCN-Regula';
}

.index-center .center-button .test.authorization {
    padding: 20rpx 40rpx 0;
    text-align: center;
    color: #ED2F35;
}

.index-center .center-button .test.authorization:last-child {
    padding-top: 0;
}