page {
    background: #fff;
}

.my-account {
    padding: 16rpx 32rpx 16rpx 32rpx;
}

.my-card {
    width: 686rpx;
    background-image: url(https://static-xesapi.speiyou.cn/xesFeProject/xesFeSource/xesImagesLibrary/ic_cardtwo.png);
    box-shadow: 0 4rpx 32rpx 0 rgba(0,0,0,0.08);
    background-size: 100% 100%;
    border-radius: 8rpx;
}

.head-img {
    width: 92rpx;
    border-radius: 50%;
    height: 92rpx;
    margin-top: 50rpx;
    margin-left: 48rpx;
    border: 4rpx solid rgba(255,255,255,0.4);
}

.my-name {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 338rpx;
    display: inline-block;
    margin-top: 8rpx;
    margin-left: 24rpx;
    font-size: 40rpx;
    font-family: PingFangSC-Medium,PingFang SC;
    font-weight: 500;
    vertical-align: top;
    color: #000;
    line-height: 40rpx;
}

.my-box {
    margin-top: 50rpx;
    vertical-align: top;
    display: inline-block;
}

.my-city {
    height: 24rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #000;
    line-height: 24rpx;
    margin-top: 16rpx;
    margin-left: 26rpx;
    display: inline-block;
}

.my-grade {
    height: 24rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #000;
    line-height: 24rpx;
    margin-top: 16rpx;
    margin-left: 18rpx;
    display: inline-block;
}

.num-box {
    margin-top: 80rpx;
    margin-left: 50rpx;
}

.num-first {
    height: 44rpx;
    display: inline-block;
    font-size: 48rpx;
    font-family: DIN-Medium,DIN;
    font-weight: 500;
    color: #a66300;
    line-height: 44rpx;
    letter-spacing: 2rpx;
}

.num-second {
    height: 44rpx;
    margin-left: 20rpx;
    margin-bottom: 64rpx;
    display: inline-block;
    font-size: 48rpx;
    font-family: DIN-Medium,DIN;
    font-weight: 500;
    color: #a66300;
    line-height: 44rpx;
    letter-spacing: 2rpx;
}

.other {
    width: 120rpx;
    height: 28rpx;
    font-size: 28rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #81858e;
    line-height: 28rpx;
    margin-top: 64rpx;
}

.copy {
    display: inline-block;
    width: 80rpx;
    height: 32rpx;
    background: #fff;
    border-radius: 76rpx;
    opacity: .79;
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #a66300;
    line-height: 24rpx;
    text-align: center;
    vertical-align: top;
    padding-top: 10rpx;
    margin-left: 8rpx;
}

.some-box {
    background: #fff;
    box-shadow: 0 4rpx 24rpx 0 rgba(0,0,0,0.05);
    border-radius: 8rpx;
    border: 1rpx solid rgba(0,0,0,0.05);
    margin-top: 32rpx;
}

.head-imglist {
    width: 68rpx;
    display: inline-block;
    height: 68rpx;
    background: #ffb16d;
    border-radius: 50%;
    margin-top: 50rpx;
    margin-left: 40rpx;
}

.boxlist {
    width: 172rpx;
    margin-top: 48rpx;
    margin-left: 24rpx;
    display: inline-block;
    margin-bottom: 48rpx;
}

.namelist {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 338rpx;
    height: 36rpx;
    font-size: 32rpx;
    font-family: PingFangSC-Medium,PingFang SC;
    font-weight: 500;
    color: #000;
    line-height: 36rpx;
}

.gradelist {
    margin-top: 16rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: rgba(0,0,0,0.6);
    line-height: 26rpx;
}

.change-btn {
    display: inline-block;
    width: 160rpx;
    height: 60rpx;
    border-radius: 30rpx;
    border: 1px solid #FFAA0A;
    text-align: center;
    margin-top: 54rpx;
    margin-left: 180rpx;
    vertical-align: top;
}

.change-font {
    font-size: 24rpx;
    font-family: PingFang-SC-Regular,PingFang-SC;
    font-weight: 400;
    color: #FFAA0A;
    line-height: 24rpx;
    margin-top: 18rpx;
}

.forlist {
    border-bottom: 1rpx solid #F0F0F0;
}

.addlist {
    padding-bottom: 48rpx;
    padding-top: 38rpx;
    text-align: center;
}

.add-btn {
    width: 284rpx;
    height: 68rpx;
    border-radius: 80rpx;
    border: 1px solid #FFAA0A;
    text-align: center;
    margin: 0 auto;
    margin-bottom: 22rpx;
}

.add-font {
    margin-top: 22rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: #FFAA0A;
    line-height: 24rpx;
}

.tips {
    width: 328rpx;
    height: 24rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: rgba(0,0,0,0.4);
    line-height: 24rpx;
    margin: 0 auto;
}