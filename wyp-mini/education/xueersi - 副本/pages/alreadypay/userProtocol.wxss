page {
    background: #eceff4;
}

.userprotocol .box {
    padding-left: 20rpx;
    padding-right: 60rpx;
    margin-top: 100rpx;
}

.userprotocol .title {
    color: #7bb4d6;
    font-size: 34rpx;
    padding-left: 40rpx;
    margin-bottom: 10rpx;
}

.userprotocol .content {
    position: relative;
    margin-bottom: 24rpx;
}

.userprotocol .circle {
    width: 28rpx;
    height: 28rpx;
    background-color: #7bb4d6;
    border-radius: 50%;
    position: absolute;
    top: 10rpx;
}

.userprotocol .item {
    margin-left: 42rpx;
    font-size: 28rpx;
    color: #565656;
}

.userprotocol .fl {
    float: left;
}