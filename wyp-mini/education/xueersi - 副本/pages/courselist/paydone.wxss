page {
    background-color: #fff;
}

.box {
    width: 100%;
    height: 100%;
    text-align: center;
}

.main-wrap {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin-top: 42rpx;
    width: 100%;
    color: #2f2f2f;
    line-height: 1;
}

.main-wrap image {
    width: 48rpx;
    height: 48rpx;
}

.main-wrap .congratulations {
    font-family: PingFangSC-Medium,PingFang SC;
    font-size: 42rpx;
    margin-left: 8rpx;
}

.btn-wrap {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    margin-top: 80rpx;
}

.btn-wrap .btn-common {
    width: 240rpx;
    height: 80rpx;
    border-radius: 40rpx;
    color: #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-family: PingFangSC-Medium,PingFang SC;
    font-weight: 500;
    line-height: 1;
}

.btn-wrap .btn-common.btn-left {
    background-color: #ffffff;
    color: #FFAA0A;
    border: .7px solid #FFAA0A;
    margin-right: 48rpx;
}

.btn-wrap .btn-common.btn-right {
    background: #FFAA0A;
}

.buy-count {
    width: 100%;
    height: 86rpx;
    background: #ed6237;
    position: absolute;
    bottom: 0;
    color: #fff;
    line-height: 86rpx;
}

.notice-wrap {
    width: 590rpx;
    margin-top: 260rpx;
    height: 199rpx;
    margin-left: 80rpx;
    background: rgba(255,170,10,0.06);
    border-radius: 8rpx;
    position: relative;
}

.notice-wrap .icon1 {
    position: absolute;
    width: 40rpx;
    height: 36rpx;
    left: 20rpx;
    top: 20rpx;
}

.notice-wrap .icon2 {
    position: absolute;
    width: 40rpx;
    height: 36rpx;
    right: 20rpx;
    bottom: 20rpx;
}

.notice-wrap .text1 {
    margin-left: 82rpx;
    padding-top: 40rpx;
    display: block;
    text-align: left;
    font-size: 30rpx;
    color: #000;
    font-family: PingFangSC-Medium,PingFang SC;
    font-weight: 500;
}

.notice-wrap .text2 {
    margin-left: 82rpx;
    display: block;
    text-align: left;
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(0,0,0,0.4);
}

.notice-wrap .color1 {
    color: #FFAA0A;
}