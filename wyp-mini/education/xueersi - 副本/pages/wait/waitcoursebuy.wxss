@import "../../wuBaseWxss/11.wxss";

@import "../../wuBaseWxss/8.wxss";

@import "../../wuBaseWxss/17.wxss";

@import "../../wuBaseWxss/15.wxss";

@import "../../wuBaseWxss/12.wxss";

page {
    background: #fff;
}

.class-card .no-pay-course {
    text-align: center;
    margin-top: 242rpx;
}

.class-card .no-pay-course image {
    width: 220rpx;
    height: 170rpx;
}

.class-card .no-pay-course .info {
    color: #797979;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.class-card .box-wrap {
    padding: 24rpx 32rpx;
}

.class-card .big-box {
    background: #fff;
    margin-bottom: 20rpx;
    box-shadow: 0 4rpx 24rpx 0 rgba(0,0,0,0.05);
    border-radius: 8rpx;
    border: .7px solid rgba(151,151,151,0.1);
}

.class-card .countdown {
    width: 100%;
    height: 60rpx;
    font-size: 24rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    background: #FFFAE6;
    color: #FF7925;
    padding-left: 32rpx;
}

.class-card .countdown .expireTime {
    color: #ff5252;
    width: 110rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
}

.class-card .countdown image {
    width: 24rpx;
    height: 24rpx;
    margin-right: 16rpx;
}

.class-card .btn-box {
    position: relative;
    overflow: hidden;
    font-size: 24rpx;
    padding: 40rpx;
    text-align: center;
}

.class-card .btn-box .btn-common {
    width: 160rpx;
    height: 60rpx;
    border-radius: 30rpx;
    color: #fff;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    font-size: 28rpx;
    font-family: PingFangSC-Regular,PingFang SC;
    line-height: 1;
    background: #FFAA0A;
    border: none;
}

.class-card .btn-box .btn-common.cancel-order {
    background: #fff;
    color: rgba(0,0,0,0.8);
    border: .7px solid rgba(0,0,0,0.2);
    margin-right: 40rpx;
}

.class-card .pay-course {
    padding: 0 32rpx;
}

.class-card .pay-course .price {
    color: #333333;
    font-size: 24rpx;
    margin-top: 40rpx;
}

.class-card .pay-course .price span {
    color: #FA4250;
    font-size: 40rpx;
    font-weight: bold;
}

.class-card .fr {
    float: right;
}

.class-card .pay-btn {
    border: 1px solid #FF7D0C;
    background: #FF7D0C;
    color: #FFFFFF;
}

.card {
    background-color: #fff;
    position: relative;
}

.card .desc {
    padding: 16rpx;
    border-bottom: 1px solid #f1f1f1;
}

.card .fl {
    float: left;
}

.card .fr {
    float: right;
}

.card .clearfix:after {
    content: "";
    display: block;
    visibility: hidden;
    height: 0;
    line-height: 0;
    clear: both;
}

.card .teachers .teacher {
    line-height: 64rpx;
    width: 204px;
    height: 64rpx;
    overflow-x: scroll;
    white-space: nowrap;
}

.card .teachers .teacher::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.card .teachers .one-teacher {
    display: inline-block;
    padding-right: 20rpx;
}

.card .teachers .avatar {
    width: 64rpx;
    height: 64rpx;
    display: inline-block;
    vertical-align: middle;
    background-image: url(http://static-xesapi.speiyou.cn/1524642003100.png);
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;
}

.card .teachers .teacher-name {
    font-size: 24rpx;
    line-height: 64rpx;
    margin-left: 18rpx;
}

.card .teachers .price {
    line-height: 64rpx;
    color: #ff5d5d;
    font-size: 24rpx;
    font-family: "Hiragino Sans GB";
    margin-right: 80rpx;
}

.card .teachers .price-number {
    font-weight: bold;
    font-family: "Georgia";
    font-size: 21.333rpx;
}

.card .people {
    margin: 22rpx 0 0 0;
    line-height: 22rpx;
}

.card .people .icon-teacher {
    width: 20rpx;
    height: 25rpx;
    vertical-align: middle;
    display: inline-block;
    margin-left: 12rpx;
}

.card .people .lecture {
    color: #666;
    margin-left: 12rpx;
    font-size: 22rpx;
    display: inline-block;
}

.card .class-title {
    font-size: 30rpx;
    margin: 16rpx 0 0 14rpx;
    max-width: 600rpx;
    line-height: 38rpx;
}

.card .time {
    margin: 22rpx 0 0 14rpx;
    line-height: 22rpx;
    color: #666;
    overflow: hidden;
}

.card .icon-time {
    width: 20rpx;
    height: 20rpx;
    vertical-align: middle;
    display: inline-block;
    line-height: 22rpx;
}

.card .date {
    font-size: 22rpx;
    line-height: 30rpx;
    margin-left: 12rpx;
}

.card .datas {
    margin-left: 31rpx;
}

.card .date-time {
    margin-left: 15rpx;
    font-size: 22rpx;
    max-width: 300rpx;
    line-height: 30rpx;
}

.card .area {
    background-color: #f6f6f6;
    border-radius: 40rpx;
    line-height: 22rpx;
    padding: 6rpx 24rpx 6rpx 6rpx;
    margin-top: 24rpx;
    display: inline-block;
}

.card .address {
    max-width: 500rpx;
    color: #666;
    margin-left: 12rpx;
    font-size: 22rpx;
    display: inline-block;
    line-height: 30rpx;
    vertical-align: middle;
}

.card .icon-area {
    width: 18rpx;
    height: 25rpx;
    vertical-align: middle;
    display: inline-block;
    margin-left: 12rpx;
}

.card .host-teacher {
    margin: 22rpx 0 0 14rpx;
    -webkit-box-align: center;
    align-items: center;
    overflow: hidden;
}

.card .host-teacher image {
    margin-right: 12rpx;
    width: 20rpx;
    height: 20rpx;
}

.card .host-teacher text {
    font-size: 22rpx;
    color: #666;
}

.card .bottom {
    height: 80rpx;
    padding: 0 15rpx 0;
    border-top: 1px solid #e8e8e8;
}

.card .bottom .case {
    margin-top: 23rpx;
    width: 80rpx;
    height: 34rpx;
    border-radius: 50rpx;
    border: 1px solid #ff6000;
    font-size: 20rpx;
    text-align: center;
    line-height: 34rpx;
}

.card .bottom .case-color {
    color: #ff6000;
}

.card .bottom .sign {
    margin-right: 10rpx;
}

.card .bottom .sign-word {
    color: #666;
    font-size: 23rpx;
    line-height: 80rpx;
}

.card .bottom .sign-up {
    border-radius: 0;
    width: 128rpx;
    height: 48rpx;
    font-size: 24rpx;
    display: inline-block;
    line-height: 48rpx;
    margin-top: 16rpx;
}

.card .class-type {
    position: absolute;
    right: 0;
    top: 0;
    width: 98rpx;
    height: 98rpx;
}

.course-period-item {
    position: relative;
    overflow: hidden;
    color: #333333;
    padding: 20rpx 0!important;
    border-top: 2rpx solid #EBECF1;
}

.course-period-item:last-child {
    border-bottom: 2rpx solid #EBECF1;
}

.course-period-item .info-left {
    float: left;
    width: 80%;
    font-size: 28rpx;
}

.course-period-item .info-left .period-name {
    font-size: 36rpx;
    margin-bottom: 10rpx;
}

.course-period-item .info-left .period-name .tips {
    color: #FF7B00;
    font-size: 24rpx;
    margin-left: 20rpx;
}

.course-period-item .info-left .period-name text {
    vertical-align: middle;
}

.course-period-item .info-left .date-bottom .icon {
    width: 24rpx;
    height: 22rpx;
}

.course-period-item .info-left .date-bottom .text {
    font-size: 28rpx;
}

.course-period-item .price-right {
    position: absolute;
    transform: translateY(-50%);
    top: 50%;
    right: 0;
    float: right;
    color: #FA4250;
    font-family: HelveticaNeue-CondensedBold;
    font-size: 40rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: center;
    justify-content: center;
    font-weight: bold;
}

.course-period-item .price-right:before {
    content: "Â¥";
    font-weight: normal;
    font-size: 24rpx;
    margin-right: 10rpx;
}