@import "pages/article/main.wxss";

.pos-vc {
    top: 50%;
    transform: translateY(-50%);
}

.pos-hc,.pos-vc {
    position: absolute;
}

.pos-hc {
    left: 50%;
    transform: translateX(-50%);
}

.flex-vhc {
    display: flex;
    justify-content: center;
    align-items: center;
}

.fix-iphonex {
    margin-bottom: 68rpx!important;
}

.fix-iphonex:after {
    content: " ";
    position: fixed;
    bottom: 0!important;
    height: 68rpx!important;
    width: 100%;
    background-color: #fff;
}

.flex-between {
    display: flex;
    justify-content: space-between;
}

.bookplan-wrapper {
    width: 100vw;
    min-height: 100vh;
    background-color: #f4f5f6;
    background-size: 100vw 392rpx;
    background-repeat: no-repeat;
    padding: 120rpx 0 28rpx;
}

