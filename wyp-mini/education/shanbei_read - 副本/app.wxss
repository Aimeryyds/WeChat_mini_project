.ib {
    display: inline-block;
    font: normal normal normal 28rpx/1 iconbay;
    font-size: inherit;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.ib-chevron-left:before {
    content: "\E02B";
}

.ib-chevron-right:before {
    content: "\E02E";
}

.ib-check:before {
    content: "\E025";
}

.ib-times:before {
    content: "\E0C0";
}

.ib-caret-down:before {
    content: "\E018";
}

.ib-caret-up:before {
    content: "\E01A";
}

.ib-clock-circle-o:before {
    content: "\E032";
}

.ib-share-o:before {
    content: "\E0A3";
}

.ib-search:before {
    content: "\E09F";
}

.ib-times-circle:before {
    content: "\E0BF";
}

.ib-times-circle-o:before {
    content: "\E0BE";
}

.ib-wechat-moments:before {
    content: "\E0D6";
}

.ib-wechat:before {
    content: "\E0D7";
}

.ib-volume:before {
    content: "\E0D5";
}

.ib-lock-o:before {
    content: "\E076";
}

.ib-fa-lightbulb-o:before {
    content: "\E052";
}

.ib-info-circle-o:before {
    content: "\E06E";
}

._button {
    line-height: unset;
    text-align: unset;
    padding: unset;
    margin: unset;
    background-color: unset;
    display: inline-block;
}

._button,._button:after {
    border: none;
    border-radius: 0;
}