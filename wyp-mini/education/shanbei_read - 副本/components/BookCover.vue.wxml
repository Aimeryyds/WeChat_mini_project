<template name="9afe1ab4">
    <view class="_div data-v-54eb1df1 book-cover {{type}}">
        <view class="_div data-v-54eb1df1 cover">
            <image alt="cover" class="_img data-v-54eb1df1 cover-img" src="{{cover}}"></image>
            <view class="_div data-v-54eb1df1 progress" wx:if="{{resolvedProgress}}">
                <label class="_span data-v-54eb1df1 progress-number">{{resolvedProgress}}</label>
                <label class="_span data-v-54eb1df1 progress-unit">%</label>
            </view>
            <view class="_div data-v-54eb1df1 finish" wx:if="{{isFinished}}">已读完</view>
            <view class="_div data-v-54eb1df1 trial" wx:if="{{isTrial}}">试读中</view>
        </view>
        <view class="_div data-v-54eb1df1 shadow"></view>
    </view>
</template>
