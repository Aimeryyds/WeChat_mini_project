@font-face {
    font-family: oswald;
    src: url("https://static.baydn.com/insurance/images/Oswald-Medium.ttf");
}

.container {
    position: absolute;
    overflow: hidden;
    width: 100vw;
    padding-left: 40rpx;
    box-sizing: border-box;
}

.container .title {
    margin-left: 40rpx;
    margin-bottom: 20rpx;
    height: 60rpx;
    font-size: 44rpx;
    font-weight: 600;
    text-align: left;
    color: #333;
}

.container .title ._span:nth-child(2) {
    margin-left: 10rpx;
    font-size: 26rpx;
    color: #999;
    font-weight: 400;
}

._button:after {
    border: none;
}