.blank_one {
    height: 100rpx;
}

.phone {
    border-bottom: 1px solid #f1f1f1;
    height: 120rpx;
    line-height: 120rpx;
}

.phone view input {
    margin-top: 40rpx;
    margin-left: 40rpx;
    font-size: 26rpx;
}

.time_out {
    border-radius: 10rpx;
    height: 70rpx;
    line-height: 70rpx;
    width: 200rpx;
    font-size: 32rpx;
    text-align: center;
    margin-top: 30rpx;
    margin-left: 100rpx;
    background-color: #dfdfdf;
    color: #666;
}

.time_out_change {
    border-radius: 10rpx;
    height: 70rpx;
    line-height: 70rpx;
    width: 200rpx;
    font-size: 32rpx;
    text-align: center;
    margin-top: 30rpx;
    margin-left: 100rpx;
    background-color: #ff9a00;
    color: #fff;
}

.phone view {
    float: left;
}

.login {
    font-size: 32rpx;
    width: 80%;
    margin: 50rpx auto;
    background-color: #ff9a00;
    height: 70rpx;
    line-height: 70rpx;
    border-radius: 10rpx;
    color: #fff;
    text-align: center;
}

.login_err {
    width: 240rpx;
    height: 240rpx;
    position: absolute;
    top: 30%;
    left: 50%;
    transform: translate(-120rpx,-120rpx);
}