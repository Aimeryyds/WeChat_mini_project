.header {
    width: 100%;
    height: 350rpx;
    border-bottom: 2rpx solid #f1f1f1;
    text-align: center;
}

.header image {
    width: 128rpx;
    height: 128rpx;
    margin: 70rpx auto 20rpx;
}

.button {
    width: 100%;
    margin: 80rpx 0;
    height: 300rpx;
}

.button button {
    width: 90%;
    margin: 0 auto;
    text-align: center;
    color: #666666;
}