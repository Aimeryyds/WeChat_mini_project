.header {
    width: 100%;
    height: 200rpx;
    background-color: #fff;
    display: flex;
}

.login {
    margin: auto;
    height: 60rpx;
    width: 260rpx;
    background-color: #fd9704;
    border-radius: 10rpx;
    text-align: center;
    line-height: 60rpx;
    color: #fff;
    font-size: 32rpx;
}

.logout {
    margin: auto;
    justify-content: space-around;
    width: 90%;
    height: 200rpx;
    vertical-align: middle;
}

.logout view {
    line-height: 200rpx;
}

.headerImg {
    float: left;
    background-color: #fff;
    border-radius: 50%;
    width: 125rpx;
    height: 125rpx;
    margin: 40rpx 20rpx;
}

.phone {
    float: left;
    color: #333333;
    font-size: 28rpx;
    font-weight: 600;
    text-indent: 15rpx;
}

.main {
    text-indent: 50rpx;
    color: #333333;
    background-color: #fff;
    font-size: 28rpx;
}

.main view {
    height: 100rpx;
    line-height: 100rpx;
    position: relative;
}

.serve {
    width: 100%;
    text-indent: 30rpx;
    height: 100rpx;
    background-color: #f9f9f9;
    font-size: 24rpx;
    color: #999;
}

.footer {
    height: 100rpx;
    width: 100%;
    position: fixed;
    bottom: 0rpx;
    display: flex;
    justify-content: space-around;
    color: #666;
    border-top: 1rpx solid #ccc;
    font-size: 28rpx;
    text-align: center;
    background-color: #fff;
}

.footer image {
    margin-top: 10rpx;
}

.border {
    border-top: 1px solid #f1f1f1;
    border-bottom: 1px solid #f1f1f1;
}

.footer>view {
    width: 50%;
}

.footer>view>view {
    height: 52rpx;
    line-height: 62rpx;
}

.nav_icon {
    background-color: #999999;
    width: 32rpx;
    height: 32rpx;
    display: block;
    position: relative;
    left: 671rpx;
    top: -63rpx;
    transform: rotate(45deg);
}

.nav_icon_span {
    background-color: #fff;
    width: 32rpx;
    height: 32rpx;
    display: block;
    position: relative;
    left: 666rpx;
    top: -93rpx;
    transform: rotate(45deg);
}

.out {
    height: 100rpx;
    width: 100%;
    background-color: #fff;
    text-align: center;
    line-height: 100rpx;
    font-size: 28rpx;
    color: #666666;
}

.blank {
    height: 20rpx;
    width: 100%;
}

.nav {
    width: 15rpx;
    height: 28rpx;
    position: absolute;
    left: 690rpx;
    top: 37rpx;
}