.topSearch {
    background: #efeff4;
    padding: 15rpx;
    border-bottom: 1rpx #d6d6db solid;
    height: 56rpx;
}

.seachBox {
    background: #fff;
    border-radius: 3rpx;
    border: 1rpx #e2e2e2 solid;
    height: 54rpx;
    font-size: 0;
    padding: 0 15rpx;
}

.seachBox image {
    display: inline-block;
    width: 28rpx;
    height: 26rpx;
    vertical-align: top;
    margin-top: 14rpx;
}

.seachBox input {
    display: inline-block;
    height: 54rpx;
    line-height: 54rpx;
    font-size: 24rpx;
    color: #666;
    width: 618rpx;
    margin-left: 10rpx;
    text-align: left;
}

.recently_detele {
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    vertical-align: top;
}

.recently_detele image {
    width: 30rpx;
    height: 30rpx;
    vertical-align: top;
}

.resultWrap {
    padding: 0 20rpx;
    background: #fff;
}

.resultList {
    border-bottom: 1px #efefef solid;
    height: 88rpx;
    line-height: 88rpx;
    font-size: 24rpx;
    color: #999;
    padding-left: 48rpx;
    width: 660rpx;
    display: inline-block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.resultList_title {
    font-size: 28rpx;
    color: #666;
    display: inline-block;
}

.resultList_title .yellow {
    color: #f90;
}

.dataBox_img {
    padding-top: 175rpx;
    text-align: center;
}

.dataBox_img image {
    display: inline-block;
    width: 519rpx;
    height: 378rpx;
}

.dataBox_text {
    display: block;
    width: 100%;
    height: 36rpx;
    line-height: 36rpx;
    font-size: 28rpx;
    padding: 55rpx 0 40rpx;
    text-align: center;
}

.dataBox_btn {
    display: block;
    width: 300rpx;
    height: 60rpx;
    text-align: center;
    line-height: 60rpx;
    border: 0;
    border-radius: 5rpx;
    background: #fd9827;
    color: #fff;
    font-size: 24rpx;
    margin: 0 auto;
}