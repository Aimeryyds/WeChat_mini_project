}

.index_logo {
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    width: 132rpx;
    height: 57rpx;
}

.index_logo image {
    width: 132rpx;
    height: 57rpx;
}

.index_phone {
    position: absolute;
    right: 20rpx;
