    height: 80rpx;
    line-height: 80rpx;
    font-size: 24rpx;
    color: #666;
    text-align: center;
    position: relative;
    float: left;
}

.selectCity_down {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 10rpx solid #ccc;
    font-size: 0;
    line-height: 0;
    margin: 0 0 0 15rpx;
    vertical-align: 3rpx;
}

.selectCity_line {
    width: 1rpx;
    height: 30rpx;
    border-left: 1rpx #ccc solid;
    position: absolute;
    right: -1rpx;
    top: 25rpx;
    font-size: 0;
}

.iptHeader {
    width: 542rpx;
    height: 80rpx;
    float: left;
    font-size: 0;
}

.iptHeader image {
    width: 28rpx;
    height: 26rpx;
    display: inline-block;
    margin: 27rpx 0 0 22rpx;
    vertical-align: top;
}

.iptHeader input {
    height: 80rpx;
    width: 464rpx;
    line-height: 80rpx;
    border: 0;
    font-size: 24rpx;
    padding: 0 10rpx;
    display: inline-block;
}

.navList {
    background: #fff;
    padding: 40rpx 0 30rpx;
    font-size: 0;
}

.navList navigator {
    width: 33%;
    display: inline-block;
    text-align: center;
}

.navList image {
    display: inline-block;
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
}

.navList text {
    font-size: 24rpx;
    color: #666;
    line-height: 36rpx;
    display: block;
    width: 100%;
    padding-top: 10rpx;
}

.spaceComm {
    background: #f5f6fb;
    height: 20rpx;
}

.entrustWrap {
    padding: 20rpx;
    background: #fff;
    height: 88rpx;
}

.entrustForm {
    background: #f5f6fb;
    padding: 7rpx 7rpx 7rpx 0;
    height: 74rpx;
    border-radius: 5rpx;
    font-size: 0;
    position: relative;
}

.entrustForm .iptEntrust {
    padding: 0 25rpx;
    width: 470rpx;
    height: 74rpx;
    line-height: 74rpx;
    font-size: 24rpx;
    color: #666;
    float: left;
}

.entrustForm .btSubEntrust {
    width: 180rpx;
    height: 74rpx;
    line-height: 74rpx;
    text-align: center;
    background: #ffcc5e;
    font-size: 28rpx;
    color: #fff;
    border-radius: 10rpx;
    border: 0;
    float: left;
}

.topCore {
    padding: 5rpx 30rpx 0;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 34rpx;
    color: #666;
    position: relative;
}

.listbox {
    padding: 40rpx 30rpx;
    border-bottom: 1rpx #f1f1f1 solid;
    display: block;
}

.listbox:first-child {
    padding-top: 0;
}

.listbox:last-child {
    border-bottom: 0;
}

.listbox::after {
    content: "";
    display: block;
    clear: both;
}

.listProLeft {
    display: inline-block;
    float: left;
    width: 216rpx;
    height: 162rpx;
    overflow: hidden;
    position: relative;
}

.listProLeft image {
    display: block;
    width: 216rpx;
    height: 162rpx;
}

.listProRight {
    display: inline-block;
    float: left;
    padding-left: 20rpx;
    width: 454rpx;
    position: relative;
}

.listProRight_title {
    font-size: 28rpx;
    color: #333;
    height: 42rpx;
    line-height: 42rpx;
    width: 474rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-bottom: 5rpx;
    font-weight: bold;
}

.listProRight_about {
    line-height: 42rpx;
    color: #999;
    font-size: 24rpx;
    display: block;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.listProRight_price {
    position: absolute;
    right: 0;
    top: 50rpx;
    line-height: 36rpx;
    font-size: 24rpx;
    color: #999;
}

.listProRight_price text {
    font-size: 30rpx;
    color: #f90;
}

.corner {
    width: 140rpx;
    background: rgba(0,0,0,.5);
    color: #fff;
    display: block;
    transform: rotate(-45deg);
    position: absolute;
    top: -50rpx;
    left: -62rpx;
    font-size: 18rpx;
    text-align: center;
    padding: 82rpx 0 8rpx;
}

.labelPro {
    font-size: 0;
    padding-top: 5rpx;
}

.labelPro .tag {
    display: inline-block;
    height: 36rpx;
    line-height: 36rpx;
    padding: 0 10rpx;
    font-size: 24rpx;
    margin-right: 15rpx;
    border-radius: 5rpx;
}

.labelPro .t1 {
    color: #a6c5c1;
    background: #eff6f5;
}

.labelPro .t2 {
    color: #eccb72;
    background: #f5f2e9;
}

.labelPro .t3 {
    color: #cca99e;
    background: #f7f0ee;
}

.labelPro .t4 {
    color: #b6b4b7;
    background: #f0f0f1;
}

.labelPro .t5 {
    color: #a2afc8;
    background: #eef2f8;
}

.listPro-more {
    display: block;
    height: 70rpx;
    margin: 0 20rpx;
    border-radius: 10rpx;
    background: #f9f9f9;
    text-align: center;
    line-height: 70rpx;
    color: #999;
    font-size: 24rpx;
    padding-bottom: 130rpx;
    border-bottom: 1rpx #fff solid;
}

.footer {
    height: 100rpx;
    width: 100%;
    position: fixed;
    bottom: 0;
    display: flex;
    justify-content: space-around;
    color: #666;
    border-top: 1rpx solid #ccc;
    background-color: #fff;
    font-size: 0;
}

.footer .viewTab {
    display: inline-block;
    width: 50%;
    height: 96rpx;
    vertical-align: top;
    text-align: center;
}

.footerimg {
    height: 54rpx;
    width: 100%;
    display: inline-block;
    overflow: hidden;
    padding-top: 10rpx;
}

.footerimg image {
    display: inline-block;
    height: 54rpx;
    width: 54rpx;
    vertical-align: top;
}

.footerword {
    font-size: 28rpx;
    color: #666;
    line-height: 36rpx;
    padding-top: 2rpx;
}

.footerword.cur {
    color: #f90;
}

.data-yuekan {
