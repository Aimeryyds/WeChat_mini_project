@import "search_list.wxss";

.topSearch {
    background: #efeff4;
    padding: 15rpx;
    border-bottom: 1rpx #d6d6db solid;
    height: 56rpx;
}

.recentlyWrap {
    background: #fff;
    padding: 0 20rpx;
    position: relative;
}

.recently_title {
    font-size: 24rpx;
    color: #999;
    line-height: 30rpx;
    height: 30rpx;
    padding-top: 36rpx;
}

.listRecently {
    border-bottom: 1rpx #efefef solid;
    padding: 36rpx 0 20rpx;
    font-size: 0;
}

.listRecently navigator {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 32rpx;
    font-size: 24rpx;
    color: #9396ad;
    background: #f5f8ff;
    border-radius: 30rpx;
    margin: 0 20rpx 20rpx 0;
}

.listLookFor {
    border-bottom: 0;
}

.listLookFor navigator {
    background: #f9f9f9;
    color: #999;
}

.recently_detele {
    display: inline-block;
    width: 20rpx;
    height: 22rpx;
    position: absolute;
    right: 20rpx;
    top: 36rpx;
}

.recently_detele image {
    display: inline-block;
    width: 20rpx;
    height: 22rpx;
    vertical-align: top;
}