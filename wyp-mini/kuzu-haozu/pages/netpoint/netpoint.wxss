}

.data-service {
    font-size: 0;
    padding: 38rpx 30rpx 40rpx 30rpx;
}

.data-service .service-title {
    font-size: 32rpx;
    color: #666;
    font-weight: 900;
    margin-bottom: 27rpx;
}

.data-service .service-content {
    font-size: 0;
}

.service-content .service-item {
    width: 120rpx;
    height: 120rpx;
    display: inline-block;
    vertical-align: top;
    text-align: center;
    padding-top: 12rpx;
    box-sizing: border-box;
}

.service-content .mar {
    margin-right: 22.5rpx;
}

.service-item image {
    width: 50rpx;
    height: 50rpx;
}

.service-item text {
    font-size: 24rpx;
    color: #999;
    display: block;
}

