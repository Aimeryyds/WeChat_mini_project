.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.clearfix::after {
    content: '.';
    font-size: 0;
    clear: both;
    display: block;
}

.goTag_home {
    left: 30rpx;
    bottom: 184rpx;
    z-index: 10;
    width: 100rpx;
    height: 100rpx;
    background-color: #ffb515;
    position: fixed;
    border-radius: 50%;
    text-align: center;
    font-size: 0;
}

.goTag_home .icon_home {
    width: 56rpx;
    height: 50rpx;
    display: block;
    margin: 10rpx auto 0;
}

.goTag_home text {
    font-size: 20rpx;
    color: #fff;
}