<view class="house-traffic _div data-v-9f8d47ca" wx:if="{{resblock}}">
    <view class="block-title _div data-v-9f8d47ca">周边及交通</view>
    <view class="content _div data-v-9f8d47ca">
        <map-img bind:__l="__l" class="data-v-9f8d47ca" height="320" lat="{{resblock.lat}}" lng="{{resblock.lng}}" vueId="1"></map-img>
        <view class="subway-list _div data-v-9f8d47ca" wx:if="{{resblock.subway}}">
            <view class="subway _p data-v-9f8d47ca" wx:for="{{resblock.subway}}" wx:for-index="i" wx:key="i">{{'· '+item}}</view>
        </view>
    </view>
    <view class="tips _p data-v-9f8d47ca">* 数据来源于百度地图，仅供参考</view>
</view>
