@import "../sale_detail/sale_detail.wxss";

.map_icon_img {
    width: 40rpx;
    height: 40rpx;
    float: right;
    margin-top: 7rpx;
}

.scroll_view {
    width: 100%;
    overflow: hidden;
}

.header_scroll {
    width: 100%;
    height: 60rpx;
    background-color: #fff;
    position: fixed;
    top: 0;
    z-index: 99999;
    padding: 0 10rpx;
    overflow: hidden;
    white-space: nowrap;
}

.header_title {
    display: inline-block;
    margin-right: 30rpx;
    font-size: 24rpx;
    text-align: center;
    color: #bababa;
    padding: 8rpx;
}

.header_title .line {
    width: 25rpx;
    background-color: orange;
    height: 4rpx;
    margin: 0 auto;
    margin-top: 2rpx;
}

.cur {
    color: #000;
}

.qaa {
    border-bottom: 1px solid #eaeaea;
}

.qaa text {
    padding-left: 40rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    align-content: center;
}

.qaa_question {
    font-size: 30rpx;
    font-weight: bold;
    margin-top: 30rpx;
}

.qaa_answer {
    font-size: 28rpx;
    color: #bababa;
    margin-top: 30rpx;
}

.qaa_bottom {
    margin-top: 50rpx;
    font-size: 20rpx;
    color: #bababa;
    margin-bottom: 20rpx;
}

.qaa_bl {
    float: left;
    width: 50%;
    padding-left: 40rpx;
}

.qaa_br {
    float: right;
    width: 30%;
    text-align: right;
    padding-right: 10rpx;
}

.clear {
    clear: both;
}

.q_icon {
    width: 20rpx;
    height: 20rpx;
    background-color: #FFC30E;
    border-radius: 5rpx;
    color: #fff;
    font-size: 16rpx;
    position: relative;
    top: 31rpx;
    padding: 6rpx;
    line-height: 19rpx;
    font-weight: normal;
    text-align: center;
}

.a_icon {
    width: 20rpx;
    height: 20rpx;
    background-color: #FF7E00;
    border-radius: 5rpx;
    color: #fff;
    font-size: 16rpx;
    position: relative;
    top: 31rpx;
    padding: 6rpx;
    line-height: 19rpx;
    font-weight: normal;
    text-align: center;
}

.house_resource_title {
    font-size: 32rpx;
    font-weight: bold;
    padding: 0 0 25rpx 40rpx;
}

.textmore {
    float: right;
}

.textmore image {
    width: 30rpx;
    height: 30rpx;
    margin-top: 2rpx;
    margin-right: 8rpx;
}

.bk_title {
    font-size: 30rpx;
    font-weight: bold;
    color: #000;
}

.bk_subtitle {
    font-size: 32rpx;
    color: #000;
    padding-bottom: 35rpx;
    font-weight: bold;
}

.bk_showing {
    color: #bababa;
    padding-left: 20rpx;
    font-weight: normal;
}

.t_broker_text_ view {
    line-height: 6rpx;
    margin-top: 25rpx;
}

.circum {
    padding: 20rpx 60rpx;
}

.circum_title {
    font-size: 28rpx;
    padding-bottom: 20rpx;
    font-weight: bold;
}

.circum_title text {
    font-size: 26rpx;
    color: #bababa;
    font-weight: normal;
}

.circum_con text {
    font-size: 30rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}

.fixed_foot {
    z-index: 999;
    padding-top: 0;
}

.foot_con {
    padding-top: 10rpx;
}

.foot-placeholder {
    width: 100%;
    height: 120rpx;
}

.foot_bottom_text {
    position: relative;
    top: 65rpx;
    left: -195rpx;
    font-size: 30rpx;
    z-index: 99;
    width: 100rpx;
}

.foot_bro {
    display: inline;
}

.foot_main {
    width: 55%;
}

.foot_tel {
    height: 60rpx;
    line-height: 60rpx;
    padding: 15rpx;
    text-align: center;
    color: #fff;
    background-color: #FFB102;
    border-radius: 20rpx;
    font-size: 30rpx;
}

.shadow {
    width: 100%;
    height: 7rpx;
    background: #cccccc;
    opacity: 0.1;
}

.detail_price .jiage {
    color: #FF522F;
}

.jiage text {
    display: inline-block;
}

.junjia {
    font-size: 26rpx;
    color: #BABABA;
    margin-top: 10rpx;
}

.detail_price .jiage .yuan-nuit {
    font-size: 26rpx;
    color: #FF522F;
}