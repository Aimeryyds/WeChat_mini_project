.mflex {
    display: flex;
}

.mfauto {
    flex: 1;
}

.mfrow {
    flex-direction: row;
}

.pdrf {
    padding: 0 40rpx;
}

.padb60 {
    padding-bottom: 60rpx;
}

.tag_box image {
    width: 536rpx;
    height: 840rpx;
    margin: 30rpx auto;
    display: flex;
    justify-content: center;
}

.paddb30 {
    padding-bottom: 30rpx;
}

.tese {
    padding-bottom: 45rpx;
    border-bottom: 1px solid #EAEAEA;
    margin-bottom: 45rpx;
}

.tese text {
    font-size: 30rpx;
    display: block;
}

.tese text:nth-child(1) {
    color: #7D7E82;
    margin-bottom: 20rpx;
}

.tese text:nth-child(2) {
    color: #292C34;
}

.more_btn {
    color: #596dfe;
    font-size: 28rpx;
    text-align: center;
    padding-bottom: 54rpx;
    border-bottom: 1px solid #EAEAEA;
}

.bor-top {
    border-top: 1rpx solid #ebebeb;
    margin-top: 100rpx;
    padding-bottom: 60rpx;
}

.bor-top .fp_item_box:last-child {
    border-bottom: none;
}

.fp_item_box {
    border-bottom: 1rpx solid #ebebeb;
    padding: 40rpx;
}

.fp_top {
    overflow: hidden;
    margin-bottom: 40rpx;
}

.fp_head,.fp_count,.fp_lian {
    float: left;
}

.fp_head {
    width: 102rpx;
    height: 102rpx;
    border-radius: 51rpx;
    overflow: hidden;
}

.fp_head image {
    width: 102rpx;
    height: auto;
}

.fp_lian {
    width: 200rpx;
}

.fp_count {
    width: calc(100% - 320rpx);
    margin-left: 12rpx;
}

.cishu {
    margin-bottom: 10rpx;
}

.cishu text:nth-child(1) {
    color: #0a0c0b;
    font-size: 30rpx;
}

.cishu text:nth-child(2) {
    color: #929292;
    font-size: 24rpx;
}

.shijian {
    font-size: 24rpx;
    color: #929292;
}

.fp_con {
    color: #2c2d2f;
    font-size: 28rpx;
}

.tel_icon,.liao_icon {
    float: right;
    width: 50rpx;
}

.tel_icon image,.liao_icon image {
    width: 48rpx;
    height: 48rpx;
    float: right;
    margin-top: 10rpx;
}

.tab_fixed {
    width: 100%;
    height: 100rpx;
    background: #fff;
    position: fixed;
    top: 0;
    left: 0%;
}

.tab_fp {
    width: 345rpx;
    height: 52rpx;
    border-radius: 9rpx;
    margin: 20rpx auto 10rpx auto;
}

.tab_fp text {
    color: #7f7e83;
    font-size: 30rpx;
    float: Left;
    border: 1rpx solid #fdc911;
    width: 168rpx;
    height: 50rpx;
    line-height: 50rpx;
    background: #fff;
    text-align: center;
}

.tab_fp .active {
    background: #fdc911;
    color: #18230d;
}

.dk_box {
    padding: 40rpx 0;
}

.dk_title {
    flex: 1;
}

.dk_title text {
    display: block;
    color: #98989a;
    font-size: 28rpx;
    text-align: center;
    padding-bottom: 10rpx;
}

.dk_title text:nth-child(2) {
    font-size: 34rpx;
    color: #ffbf00;
}

.ci:after {
    content: 'æ¬¡';
    font-size: 26rpx;
}

.dk_list_tit {
    height: 80rpx;
    line-height: 80rpx;
    background: #f7f7f9;
    margin-top: 30rpx;
}

.dk_list_tit view {
    flex: 1;
    text-align: center;
    font-size: 24rpx;
    color: #9c9c9e;
}

.dk_list_con {
    border-bottom: 1rpx solid #ececec;
    padding: 25rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.dk_item {
    flex: 1;
    text-align: center;
    font-size: 28rpx;
    line-height: 50rpx;
}

.tel_icon,.liao_icon {
    float: right;
    margin-left: 30rpx;
}

.dk_item .liao_icon,.dk_item .tel_icon {
    margin-left: 0;
    margin-right: 30px;
}

.tel_icon image,.liao_icon image {
    width: 48rpx;
    height: 48rpx;
}

.more_box .listhide {
    display: none;
}

.dklistmore {
    text-align: center;
    font-size: 24rpx;
    height: 100rpx;
    line-height: 100rpx;
}