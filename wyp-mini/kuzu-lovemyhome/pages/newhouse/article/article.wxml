<view>
    <view class="dt-ctn">
        <view class="dt-setborder" wx:for="{{articledata}}">
            <view class="time">{{item.publishDate}}</view>
            <view class="dt-title">{{item.title}}</view>
            <image class="article-image" src="{{item.thumbnailImageName}}"></image>
            <view class="dt-inner">{{item.content}}</view>
            <view class="border-bottom"></view>
            <view class="circle"></view>
        </view>
    </view>
</view>
