.info-title,.info-list {
    width: 670rpx;
    margin: 0 auto;
    font-size: 32rpx;
    color: #292B33;
    border-radius: 1px solid #BABABA;
    line-height: 96rpx;
}

.info-list {
    border: none;
    line-height: 62rpx;
    font-size: 28rpx;
    color: #7D7E82;
    overflow: hidden;
}

.info-list view {
    float: left;
}

.list-title {
    width: 168rpx;
}

.list-ctn {
    color: #292B33;
    width: calc(100% - 168rpx);
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.info-wrap {
    margin-bottom: 30rpx;
}