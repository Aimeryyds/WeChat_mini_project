@import "../list/list.wxss";

.list-ctn:nth-child(1) {
    padding-top: 0rpx;
}

.detail-wrap {
    padding-bottom: 40rpx;
}

.left {
    float: left;
}

.right {
    float: right;
}

.loop-wrap {
    width: 100%;
    height: 499rpx;
    position: relative;
}

.loop-wrap swiper,.loop-wrap swiper-item,.loop-wrap swiper-item image {
    width: 100%;
    height: 100%;
}

.swiper-inner {
    height: 100%;
    width: 100%;
}

.albumname,.albumcount {
    padding: 5rpx 20rpx;
    background: rgba(0,0,0,0.5);
    font-size: 12px;
    color: #fff;
    position: absolute;
    bottom: 20rpx;
    border-radius: 20rpx;
}

.albumname {
    left: 40rpx;
}

.albumcount {
    right: 40rpx;
}

.xf-info {
    padding: 0 40rpx;
}

.title {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 50rpx;
}

.title view:nth-child(1) {
    font-size: 42rpx;
    color: #292b33;
    font-weight: 600;
}

.title view:nth-child(2) {
    font-size: 12px;
    color: #7d7e82;
}

.title view:nth-child(2) image,.more image {
    width: 15rpx;
    height: 23rpx;
    display: inline-block;
    margin-left: 10rpx;
}

.more image {
    margin-left: 0;
}

.salestatus,.tags {
    padding: 25rpx 0;
    overflow: hidden;
    border-bottom: 1px solid #eaeaea;
}

.salestatus .salestatus_desc {
    color: #0491FE;
}

.salestatus .salestatus_childname {
    background: rgba(255,82,47,0.1);
    color: #FF522F;
}

.salestatus view,.tags view {
    float: left;
    padding: 5rpx 10rpx;
    border-radius: 10rpx;
    font-size: 12px;
    background: #e8effd;
    margin-right: 10rpx;
    color: #69909d;
}

.info-item {
    margin-top: 20rpx;
}

.info-address {
    display: flex;
    align-items: flex-start;
    position: relative;
}

.info-item text,.info-address text,.info-open text {
    display: inline-block;
}

.info-item text:nth-child(1) {
    font-size: 30rpx;
    color: #7d7e82;
}

.info-item text:nth-child(2) {
    font-size: 30rpx;
    color: #ff522f;
    margin-left: 20rpx;
}

.info-item text:nth-child(3) {
    font-size: 22rpx;
    color: #7d7e82;
    margin-left: 20rpx;
}

.info-address text:nth-child(2),.info-open text:nth-child(2) {
    font-size: 30rpx;
    color: #292b33;
    margin-left: 20rpx;
}

.info-address text:nth-child(2) {
    width: 550rpx;
}

.info-address image {
    width: 15rpx;
    height: 23rpx;
    float: right;
    position: absolute;
    top: 50%;
    margin-top: -11rpx;
    right: 0;
}

.item-title {
    padding: 0 40rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 38rpx;
    color: #292b33;
    height: 100rpx;
    line-height: 100rpx;
    font-weight: 600;
    margin-top: 35rpx;
}

.item-title .more {
    font-size: 24rpx;
    color: #7d7e82;
}

.hx-scroll {
    width: 100%;
    height: 384rpx;
}

.hx-scroll scroll-view {
    width: 100%;
    height: 100%;
    white-space: nowrap;
}

.housetype {
    width: 324rpx;
    height: 380rpx;
    display: inline-block;
    border: 1px solid #c0c0c0;
    border-radius: 16rpx;
    margin-right: 24rpx;
}

.housetype:nth-child(1) {
    margin-left: 40rpx;
}

.housetype image {
    width: 100%;
    display: block;
    height: 214rpx;
    border-top-left-radius: 16rpx;
    border-top-right-radius: 16rpx;
}

.hx-item,.hx-area,.hx-price {
    padding-left: 28rpx;
    overflow: hidden;
}

.hx-area {
    font-size: 12px;
    color: #7d7e82;
    line-height: 50rpx;
}

.hx-price {
    font-size: 28rpx;
    color: #ff522f;
}

.hx-item-type {
    margin-top: 28rpx;
}

.hx-item view:nth-child(1) {
    font-size: 30rpx;
    color: #292b33;
    float: left;
}

.hx-item view:nth-child(2) {
    font-size: 24rpx;
    color: #292b33;
    background: #e8effd;
    border-radius: 10rpx;
    padding: 5rpx 10rpx;
    color: #2fa3fe;
    float: left;
    margin-left: 10rpx;
}

.dt-ctn {
    position: relative;
    padding: 0 40rpx;
}

.dt-setborder {
    width: 100%;
    height: 100%;
    position: relative;
    border-left: 1px solid #eaeaea;
}

.dt-setborder view {
    margin-bottom: 28rpx;
    margin-left: 20rpx;
}

.dt-ctn .time {
    font-size: 24rpx;
    color: #7d7e82;
}

.dt-title {
    font-size: 30rpx;
    color: #292b33;
}

.dt-content {
    font-size: 30rpx;
    color: #7d7e82;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
}

.border-bottom {
    border-bottom: 1px solid #eaeaea;
    margin-top: 28rpx;
    margin-bottom: 28rpx;
}

.dt-ctn .dt-setborder:nth-last-child(1) .border-bottom {
    border: none;
}

.dt-setborder .circle {
    width: 10rpx;
    height: 10rpx;
    background: #bababa;
    position: absolute;
    border-radius: 50%;
    top: 0;
    z-index: 10;
    left: -5rpx;
    margin: 0;
}

.super-agency {
    padding: 0 40rpx;
    overflow: hidden;
    line-height: 100rpx;
    font-size: 30rpx;
    color: #292b33;
}

.super-agency .headimg {
    height: 80rpx;
    width: 80rpx;
    border-radius: 50%;
    display: block;
    margin-right: 20rpx;
    margin-top: 10rpx;
}

.super-agency .name {
    line-height: 100rpx;
}

.tel {
    margin-left: 20rpx;
}

.message,.tel {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    box-shadow: 0px 0px 10px 0px rgba(0,0,0,0.4);
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 10rpx;
}

.message image,.tel image {
    width: 42rpx;
    height: 42rpx;
}

.map-around {
    padding: 0 40rpx;
    height: 300rpx;
}

#myMap {
    width: 100%;
    height: 100%;
}

.location {
    padding: 0 40rpx;
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    font-size: 12px;
    margin-bottom: 30rpx;
}

.location view:nth-child(1) {
    width: 100rpx;
    color: #7d7e82;
}

.location view:nth-last-child(1) {
    width: calc(100% - 100rpx);
    color: #292b33;
}

.album-cover {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    bottom: 0;
    background: #000;
    z-index: 1000;
}

.album-title {
    color: #fff;
    text-align: center;
    font-size: 34rpx;
    line-height: 100rpx;
    position: relative;
}

.all_album {
    position: absolute;
    right: 40rpx;
    color: #fff;
    font-size: 28rpx;
    top: 0;
}

.album-title image {
    width: 35rpx;
    height: 35rpx;
    position: absolute;
    top: 32rpx;
    left: 32rpx;
}

.album-loop {
    width: 100%;
    height: 377rpx;
    margin-top: 258rpx;
    position: relative;
}

.album-cover swiper {
    width: 100%;
    height: 100%;
}

.album-cover swiper swiper-item {
    width: 100%;
    height: 100%;
}

.album-cover swiper .swiper-inner {
    padding: 0;
    width: 100%;
    height: 100%;
    position: relative;
}

.swiper-inner .icon_3d {
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -50rpx;
    margin-top: -50rpx;
    animation: move 5s linear 0s infinite;
}

@-webkit-keyframes move {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes move {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(360deg);
    }
}

.album-cover swiper image {
    width: 100%;
    height: 100%;
}

.album-cover .count {
    width: 100%;
    line-height: 200rpx;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    position: absolute;
}

.cover-bottom {
    width: 100%;
    padding: 42rpx 0;
    position: absolute;
    bottom: 0;
}

.cover-bottom scroll-view {
    width: 100%;
    height: 100%;
    white-space: nowrap;
}

.cover-bottom scroll-view view {
    display: inline-block;
    font-size: 24rpx;
    padding: 8rpx 18rpx;
    border-radius: 33rpx;
    background: #fff;
    margin: 0 20rpx;
}

.cover-bottom scroll-view view.selected {
    background: linear-gradient(to right,#ffc30e,#ff9d00);
    color: #fff;
}

.recommend {
    padding-bottom: 0;
}