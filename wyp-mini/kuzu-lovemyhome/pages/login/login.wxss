.login-box {
    padding: 110rpx 40rpx 0 40rpx;
}

.login-img {
    display: block;
    width: 286rpx;
    height: 207rpx;
    margin: 0 auto;
}

.logincontent button {
    width: 100%;
    height: 94rpx;
    line-height: 94rpx;
    border-radius: 16rpx;
    font-size: 32rpx;
}

.logincontent button::after {
    border: none;
}

.wxloginbtn {
    margin: 20% auto 40rpx auto;
    color: #fff;
    background: linear-gradient(to right,#ffc30e,#ffad00);
}

.phoneloginbtn {
    margin: 0 auto;
    background: #fff;
    border: 1rpx solid #ffbb09;
    color: #292b33;
}