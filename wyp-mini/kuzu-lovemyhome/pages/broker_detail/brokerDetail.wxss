.mflex {
    display: flex;
}

.mfauto {
    flex: 1;
}

.mfrow {
    flex-direction: row;
}

button {
    background-color: #fff;
}

button::after {
    border: none;
}

.button-hover {
    background: #fff;
}

.clearfix:after {
    display: block;
    visibility: hidden;
    clear: both;
    height: 0;
    content: ".";
}

.bothpadding {
    padding-left: 40rpx;
    padding-right: 40rpx;
}

.mbottom40 {
    margin-bottom: 40rpx;
}

.brokerIfon_box {
    position: relative;
    height: 466rpx;
    width: 100%;
    margin-bottom: 30rpx;
}

.brokerBg {
    width: 100%;
    height: 306rpx;
    background: linear-gradient(-45deg,#FFC30E,#FFAD00);
}

.brokerIfon_box_bg {
    height: 280rpx;
    width: calc(100% - 120rpx);
    padding: 0 20rpx;
    background: #fff;
    border-radius: 15rpx;
    box-shadow: 0rpx 5rpx 15rpx 2rpx rgba(170,170,170,0.2);
    position: absolute;
    left: 40rpx;
    bottom: 0;
    z-index: 2;
}

.broker_header_box {
    padding: 20rpx;
}

.bHeader {
    float: left;
    background: #fff;
    width: 130rpx;
    height: 130rpx;
    margin-right: 20rpx;
    border-radius: 65rpx;
    overflow: hidden;
    margin-top: -65rpx;
}

.bHeader view {
    width: 114rpx;
    height: 114rpx;
    border-radius: 57rpx;
    overflow: hidden;
    margin-top: 8rpx;
    margin-left: 8rpx;
}

.bHeader image {
    width: 114rpx;
    height: auto;
}

.broMain {
    float: left;
    width: calc(100% - 150rpx);
}

.broStar,.broFens {
    overflow: hidden;
}

.broLeibie,.broFens {
    float: left;
    width: 50%;
}

.broFens {
    margin-top: 10rpx;
}

.broNames {
    font-size: 32rpx;
    font-weight: bold;
}

.broTags {
    padding: 5rpx 10rpx;
    margin-left: 10rpx;
    font-size: 24rpx;
    border-radius: 34rpx;
    color: #fff;
    background: linear-gradient(45deg,#FB7937,#FF6751);
}

.fen_item {
    float: right;
    margin-left: 15rpx;
    font-size: 24rpx;
    color: #7D7E82;
    text-align: right;
}

.fen_item text {
    color: #292B33;
}

.broShop {
    color: #9D9DA1;
    font-size: 24rpx;
    margin-top: 10rpx;
}

.broker_number_box {
    overflow: hidden;
    margin-top: 15rpx;
}

.bro_item {
    text-align: center;
    float: left;
    width: 25%;
}

.bro_item text {
    display: block;
}

.text_weight {
    color: #292B33;
    font-size: 34rpx;
    font-weight: bold;
}

.text_qian {
    color: #BABABA;
    font-size: 26rpx;
    margin: 10rpx 0;
}

.house_scroll {
    white-space: nowrap;
    height: 410rpx;
}

.house_scroll2 {
    white-space: nowrap;
    height: 450rpx;
}

.tit_box {
    font-size: 38rpx;
    color: #292B33;
    font-weight: bold;
    padding-top: 40rpx;
    padding-bottom: 40rpx;
}

.textmore {
    margin-top: 10rpx;
    color: #7D7E82;
    font-size: 24rpx;
    font-weight: 1rpx;
}

.textmore image {
    width: 30rpx;
    height: 30rpx;
    margin-left: 10rpx;
    margin-top: 3rpx;
    float: right;
}

.house_tags {
    width: 320rpx;
    height: 400rpx;
    display: inline-block;
    margin-left: 24rpx;
    border-radius: 16rpx;
    overflow: hidden;
    box-shadow: 0rpx 5rpx 15rpx 2rpx rgba(170,170,170,0.2);
}

.house_scroll2 .house_tags {
    height: 440rpx;
}

.house_tags:last-child {
    margin-right: 50rpx;
}

.house_tags:nth-child(1) {
    margin-left: 40rpx;
}

.house_tags image {
    width: 320rpx;
    height: 213rpx;
}

.tag_title,.tag_ting,.tag_price {
    padding: 0 20rpx;
}

.tag_title {
    font-size: 30rpx;
    color: #292B33;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-top: 15rpx;
}

.tag_ting {
    font-size: 28rpx;
    color: #7D7E82;
    margin-top: 5rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.tag_price .red {
    font-size: 34rpx;
    color: #FF522F;
    margin-right: 10rpx;
    font-weight: bold;
}

.tag_price .hui {
    font-size: 26rpx;
    color: #7D7E82;
}

.tag_price .yuanyue {
    font-size: 26rpx;
    color: #FF522F;
}

.titmt20 {
    margin-top: 20rpx;
}

.mmList {
    border-bottom: 1rpx solid #EDEDED;
    padding: 40rpx 0;
}

.mmList:nth-child(1) {
    padding-top: 0;
}

.mmTitles_box {
    overflow: hidden;
    line-height: 35rpx;
    height: 35rpx;
}

.mmTags {
    float: left;
    color: #fff;
    font-size: 20rpx;
    text-align: center;
    width: 60rpx;
    height: 35rpx;
    line-height: 35rpx;
    border-radius: 5rpx;
    margin-right: 10rpx;
    background: linear-gradient(45deg,#FFC30E,#FFAD00);
}

.yizu .mmTags {
    background: linear-gradient(45deg,#FF8E00,#FF7F00);
}

.mmTitles {
    float: left;
    font-size: 30rpx;
    color: #292B33;
    font-weight: bold;
}

.mmTitles text {
    padding-right: 20rpx;
}

.mmAres {
    font-size: 26rpx;
    color: #BABABA;
    margin: 20rpx 0;
}

.mmjia_data {
    overflow: hidden;
    line-height: 36rpx;
}

.mmJiage,.mmTime {
    float: left;
    width: 50%;
}

.mmJiage .red {
    color: #FF522F;
    font-size: 36rpx;
    font-weight: bold;
    margin-right: 10rpx;
}

.mmJiage .hui {
    font-size: 26rpx;
    color: #7D7E82;
}

.mmTime {
    font-size: 26rpx;
    color: #292B33;
}

.customer_main {
    font-size: 30rpx;
    color: #292B33;
    margin: 20rpx 0 10rpx 0;
}

.customer_tags text {
    background: #F7F7F7;
    font-size: 24rpx;
    color: #A8A8A8;
    margin-right: 10rpx;
    margin-bottom: 5rpx;
    padding: 5rpx 10rpx;
}

.customer_list {
    padding: 25rpx 0;
    border-bottom: 1rpx solid #EDEDED;
}

.customer_list:nth-child(1) {
    padding-top: 10rpx;
}

.peoBox {
    overflow: hidden;
}

.peo_head {
    float: left;
    margin-right: 24rpx;
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    border-radius: 40rpx;
}

.peo_head image {
    width: 80rpx;
}

.peo_info,.peoPingfen,.fen,.star_icon,.peoTimes {
    float: left;
}

.peoTel {
    font-size: 30rpx;
    color: #292B33;
    font-weight: bold;
}

.peoPingfen {
    overflow: hidden;
    width: 425rpx;
}

.fen {
    color: #FFB000;
}

.star_icon image {
    width: 20rpx;
    height: 20rpx;
}

.peoTimes {
    width: calc(100% - 530rpx);
    font-size: 24rpx;
    color: #BABABA;
    text-align: right;
}

.question_list {
    padding: 30rpx 0;
    border-bottom: 1rpx solid #EDEDED;
    font-size: 30rpx;
    color: #292C34;
}

.question_list:nth-child(1) {
    padding-top: 10rpx;
}

.dkrecord_list_box {
    position: relative;
    padding-left: 15rpx;
}

.dkrecord_list_box:after {
    content: '';
    position: absolute;
    top: -5rpx;
    left: -10rpx;
    width: 40rpx;
    height: 32rpx;
    background: #fff;
}

.dkItem {
    padding-left: 20rpx;
    border-left: 1px solid #e5e5e5;
    position: relative;
    padding-bottom: 36rpx;
}

.quan {
    position: absolute;
    left: -18rpx;
    top: 28rpx;
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    border: 3px solid #e5e5e5;
    background: #fff;
}

.name_box {
    width: 30%;
    float: left;
    padding: 24rpx;
}

.dkItem .name,.dkItem .xiaoqu {
    color: #292b33;
    font-size: 28rpx;
    font-weight: bold;
}

.dkItem .data {
    color: #bababa;
    font-size: 24rpx;
}

.xiaoqu_box {
    width: 50%;
    float: left;
    padding: 24rpx;
    background: #f7f7f7;
    border-radius: 8rpx;
}

.huxing {
    font-size: 26rpx;
    color: #9d9da1;
}

.footer_box {
    width: calc(100% - 80rpx);
    height: 115rpx;
    padding-top: 20rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 10;
    background: #fff;
    box-shadow: 0rpx -2rpx 8rpx 3rpx rgba(170,170,170,0.2);
}

.share_box,.ailiao_box,.tel_box {
    float: left;
    text-align: center;
}

.share_box {
