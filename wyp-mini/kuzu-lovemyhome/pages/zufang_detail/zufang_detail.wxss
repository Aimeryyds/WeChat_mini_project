@import "../sale_detail/sale_detail.wxss";

.more_btn {
    border-bottom: 0;
    padding-bottom: 10rpx;
}

.qy_time {
    color: #7D7E82;
    font-size: 26rpx;
    margin-top: 10rpx;
}

.jiage {
    color: #FF522F;
    font-size: 36rpx;
    font-weight: bold;
    text-align: right;
}

.equipments_box {
    overflow: hidden;
    padding: 50rpx 40rpx 0 40rpx;
}

.eqlist {
    width: 20%;
    float: left;
    margin-bottom: 50rpx;
}

.eq_icon {
    width: 48rpx;
    height: 48rpx;
    margin: 0 auto;
}

.text {
    color: #292B33;
    font-size: 30rpx;
    text-align: center;
    margin-top: 10rpx;
}

.text_h {
    color: #7D7E82;
}

.eq_icon image {
    width: 48rpx;
    height: 48rpx;
}

.xystyle,.xytags {
    float: left;
}

.xystyle image {
    width: 72rpx;
    height: 36rpx;
}

.xytags {
    background: #6C45AF;
    color: #fff;
    font-size: 28rpx;
}

.xyBox {
    overflow: hidden;
    padding-top: 10rpx;
}

.xy_kuai {
    width: 210rpx;
    height: 200rpx;
    background: #F7F7F7;
    border-radius: 8rpx;
    float: left;
    margin-right: 18rpx;
}

.xyBox .xy_kuai:last-child {
    margin-right: 0;
}

.xy_kuai {
    text-align: center;
}

.xy_kuai image {
    width: 58rpx;
    height: 58rpx;
    margin-top: 20rpx;
}

.xy_name {
    color: #292B33;
    font-size: 30rpx;
    margin-bottom: 10rpx;
}

.xy_tit {
    color: #7D7E82;
    font-size: 26rpx;
}

.xy_more {
    color: #8D6FC9;
    font-size: 30rpx;
    text-align: center;
    padding: 60rpx 0;
}

.h_info_box {
    overflow: hidden;
    margin-bottom: 45rpx;
}

.houseimg,.houseifons,.status {
    float: left;
}

.houseimg {
    width: 132rpx;
    height: 88rpx;
    border-radius: 12rpx;
    overflow: hidden;
}

.houseimg image {
    width: 132rpx;
    height: 88rpx;
}

.houseifons {
    width: 320rpx;
    margin-left: 40rpx;
}

.status {
    width: 170rpx;
    text-align: right;
    color: #7D7E82;
    font-size: 26rpx;
    margin-top: 20rpx;
}

.huxing {
    margin-top: 10rpx;
}

.huxing,.area {
    color: #292B33;
    font-size: 26rpx;
}

.cur .status {
    color: #FF522F;
}

.status image {
    width: 13rpx;
    height: 21rpx;
    margin-left: 10rpx;
}