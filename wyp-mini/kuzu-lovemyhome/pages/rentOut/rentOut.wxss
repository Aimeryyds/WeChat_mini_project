page {
    background: #F7F7F7;
}

input[type="hidden"] {
    display: none;
}

button::after {
    border: none;
}

.form-box {
    font-size: 30rpx;
    padding: 0 42rpx;
    color: #292b33;
    background: #fff;
}

.form-item {
    height: 68rpx;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #eee;
}

.form-item:last-child {
    border-bottom: none;
}

.name {
    float: left;
    width: 166rpx;
    height: 68rpx;
    line-height: 68rpx;
}

.form-item input {
    font-size: 30rpx;
    height: 68rpx;
    line-height: 68rpx;
    color: #292b33;
}

.mid-cells {
    float: left;
    width: 350rpx;
    height: 68rpx;
    line-height: 68rpx;
}

.right-cells {
    float: right;
    line-height: 68rpx;
}

.arrow {
    width: 13rpx;
    height: 21rpx;
    margin-top: 27rpx;
    margin-left: 16rpx;
}

.shuru-box {
    margin-top: 20rpx;
}

.placeholder-inp {
    color: #bababa;
}

.building .name {
    width: 82rpx;
}

.building input {
    float: left;
    width: 120rpx;
    margin-right: 20rpx;
}

.codebtn {
    float: left;
    width: 148rpx;
    background: #fff;
    border: 1rpx solid #bababa;
    color: #7d7e82;
    font-size: 22rpx;
    line-height: 60rpx;
    padding: 0;
    margin-top: 4rpx;
}

.addcode {
    background: #e5e7ec;
    border: 1rpx solid #e5e7ec;
    color: #999;
}

.bottom-box {
    width: 100%;
    height: 142rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    padding-top: 24rpx;
    box-shadow: 2rpx 2rpx 20rpx 0 rgba(0,0,0,0.08);
    background: #fff;
    font-size: 32rpx;
    z-index: 2;
}

.telrent,.released {
    float: left;
    width: 50%;
    height: 94rpx;
    line-height: 94rpx;
    color: #fff;
    text-align: center;
}

.telrent text,.released button {
    display: block;
    height: 95rpx;
    line-height: 95rpx;
    font-size: 32rpx;
    border-radius: 14rpx;
}

.telrent {
    padding: 0 25rpx 0 42rpx;
}

.released {
    padding: 0 42rpx 0 25rpx;
}

.telrent text {
    background: linear-gradient(to right,#ffc30e,#ffad00);
}

.released button {
    font-size: 32rpx;
    color: #fff;
    border: none;
    background: linear-gradient(to right,#ff9000,#ff7e00);
}

.bottom-note-box {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 3;
}

.bottom-note {
    position: absolute;
    right: 42rpx;
    bottom: 160rpx;
    color: #fff;
    font-size: 24rpx;
    line-height: 32rpx;
    padding: 34rpx 46rpx;
    background: rgba(0,0,0,0.6);
    border-radius: 30rpx;
}

.bottom-note text {
    display: block;
}

.bot-arrow {
    width: 20rpx;
    height: 13rpx;
    position: absolute;
    left: 50%;
    bottom: -12rpx;
    margin-left: -10rpx;
}

.radio-layer-box {
    width: 100%;
    height: 100%;
    position: fixed;
    left: top;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 2;
}

.radio-layer {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    padding: 0 42rpx;
    background: #fff;
    font-size: 30rpx;
    text-align: center;
    color: #666;
}

.radio-item {
    line-height: 110rpx;
    border-bottom: 1rpx solid #eee;
}