.clearfix:after {
    display: block;
    visibility: hidden;
    clear: both;
    height: 0;
    content: ".";
}

image {
    display: inline-block;
}

page {
    background: #f7f7f7;
}

.bottomBox {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    z-index: 50;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.lf {
    float: left;
}

.rf {
    float: right;
}

.main_box {
    padding: 40rpx;
    margin-bottom: 50rpx;
}

.tag_box {
    margin-top: 35rpx;
}

.talk_head {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50rpx;
    overflow: hidden;
}

.talk_head image {
    width: 100rpx;
    height: auto;
}

.talk_head.lf image {
    height: auto;
}

.houseTag {
    margin-left: 40rpx;
    width: 320rpx;
    border-radius: 15rpx;
    overflow: hidden;
}

.wordTag,.houseText {
    max-width: calc(100% - 200rpx);
}

.wordTag image,.houseText image {
    max-width: 100%;
}

.right40 {
    margin-right: 40rpx;
}

.houseText {
    margin-left: 40rpx;
    border-radius: 15rpx;
    overflow: hidden;
    background: #fff;
    color: #292b33;
    padding: 30rpx;
    margin-top: 10rpx;
    font-size: 26rpx;
}

.wordTag {
    margin-right: 40rpx;
    border-radius: 15rpx;
    overflow: hidden;
    background: #ffb000;
    color: #fff;
    padding: 30rpx;
    margin-top: 10rpx;
    max-width: calc(100% - 200rpx);
    font-size: 26rpx;
}

.houseText text,.wordTag text {
    font-size: 26rpx;
    display: block;
    word-wrap: break-word;
    word-break: break-all;
}

.houseImg {
    height: 213rpx;
}

.houseImg image {
    width: 100%;
    height: 213rpx;
}

.houseInfos {
    padding: 5rpx 25rpx 20rpx 25rpx;
    background: #fff;
}

.housetitles {
    width: 100%;
    color: #292b33;
    font-size: 30rpx;
    font-weight: bold;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-top: 15rpx;
}

.house_x {
    margin-top: 10rpx;
}

.house_x,.house_x text {
    font-size: 24rpx;
    color: #7d7e82;
}

.house_price {
    overflow: hidden;
    margin-top: 5rpx;
}

.wan {
    color: #ff522f;
    font-size: 34rpx;
    float: left;
}

.wan text {
    font-weight: bold;
}

.ping {
    float: left;
    margin-top: 8rpx;
    margin-left: 10rpx;
    font-size: 26rpx;
    color: #7d7e82;
}

.img {
    width: 100%;
}

.timeBox {
    text-align: center;
    margin-top: 35rpx;
}

.timeBox text {
    display: inline-block;
    background: #e5e5e5;
    font-size: 22rpx;
    padding: 10rpx 20rpx;
    border-radius: 7rpx;
    color: #fff;
}

.talk_box {
    height: 114rpx;
    width: calc(100% - 80rpx);
    padding: 0 40rpx;
    background: #fff;
}

.phrase_box,.input_box,.tool_box {
    float: left;
}

.phrase_box {
    width: 110rpx;
}

.tool_box {
    width: 80rpx;
}

.input_box {
    width: calc(100% - 190rpx);
}

.input_box.bigSty {
    width: calc(100% - 220rpx);
    margin-right: 30rpx;
}

.phrase_box text {
    color: #408fff;
    font-size: 22rpx;
    border: 1rpx solid #408fff;
    border-radius: 9rpx;
    width: 84rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    margin-top: 32rpx;
    display: block;
}

.addBtn {
    width: 47rpx;
    height: 47rpx;
    margin-top: 32rpx;
    float: right;
}

.addBtn image {
    width: 47rpx;
    height: 47rpx;
}

.inputSty {
    margin-top: 20rpx;
    padding: 10rpx 20rpx;
    width: calc(100% - 40rpx);
    height: 52rpx;
    line-height: 52rpx;
    border: 1rpx solid #eee;
    border-radius: 36rpx;
}

.inputSty input {
    font-size: 26rpx;
    width: 100%;
    background: #fff;
}

.phcolor {
    color: #9d9da1;
}

.phrase_list {
    border-top: 1rpx solid #eee;
    background: #fff;
}

.phraseItem {
    color: #292b33;
    font-size: 28rpx;
    border-bottom: 1rpx solid #eee;
    height: 97rpx;
    line-height: 97rpx;
    padding: 0 40rpx;
}

.tool_main {
    background: #fff;
    overflow: hidden;
    padding: 20rpx 40rpx;
}

.xcBtn,.psBtn,.fzBtn {
    float: left;
    width: 112rpx;
    margin: 0 20rpx;
    text-align: center;
}

.btnIcon {
    margin-bottom: 10rpx;
}

.btnIcon image {
    width: 112rpx;
    height: 112rpx;
}

.btnText {
    color: #292b33;
    font-size: 28rpx;
    padding-bottom: 20rpx;
}

.fsbutton {
    color: #fff;
    font-size: 24rpx;
    width: 100rpx;
    height: 50rpx;
    line-height: 50rpx;
    text-align: center;
    border-radius: 9rpx;
    margin-top: 32rpx;
    background: linear-gradient(145deg,#FF8E00,#FF7F00);
}