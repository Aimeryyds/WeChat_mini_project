input[type="hidden"] {
    display: none;
}

.joinBox {
    width: 100%;
    height: 100%;
}

.joinBanner,.xiansty {
    width: 100%;
}

.joinBanner image {
    width: 100%;
    height: 360rpx;
    display: block;
}

.xiansty image {
    width: 100%;
    height: 190rpx;
    display: block;
}

.jrbot {
    width: 100%;
    margin-top: 20rpx;
}

.jrbot image {
    width: 100%;
    height: 1688rpx;
    display: block;
}

.joincont {
    padding: 0 50rpx;
    background: #fff;
}

.joincont .group_from {
    display: flex;
    -webkit-box-align: center;
    font-size: 30rpx;
    margin-top: 25rpx;
    border-bottom: 1rpx solid #eee;
}

.grouplable {
    width: 150rpx;
    line-height: 70rpx;
    float: left;
}

.groupinput {
    width: calc(100% - 150rpx);
    float: left;
}

.groupinput input,.groupinput text {
    font-size: 30rpx;
    border-radius: 5rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 8rpx;
    list-style: none;
    outline: none;
    border: none;
    color: #292b33;
}

.groupinput text {
    line-height: 60rpx;
}

.lookmore {
    background: url(data:image/png;base64,) no-repeat center right;
    background-size: 14rpx 22rpx;
}

.mflex {
    display: flex;
}

.marbottom {
    margin-bottom: 110rpx;
}

.group_from .kuangsty {
    width: 100%;
}

.sellFoot {
    width: calc(100%);
    height: 100rpx;
    position: fixed;
    background: #fff;
    z-index: 3;
    left: 0;
    bottom: 0;
    box-shadow: 0 1rpx 20rpx 0 rgba(0,0,0,0.2);
    text-align: center;
}

.delivery {
    width: 100%;
    display: block;
}

.delivery button {
    display: block;
    line-height: 100rpx;
    color: #fff;
    width: 100%;
    margin: 0 auto;
    border-radius: 0px;
    background: linear-gradient(to right,#FF9100,#FF7F00);
}

.radio-layer-box {
    width: 100%;
    height: 100%;
    position: fixed;
    left: top;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 10;
}

.radio-layer {
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    padding: 0 42rpx;
    background: #fff;
    font-size: 30rpx;
    text-align: center;
    color: #666;
}

.radio-item {
    line-height: 110rpx;
    border-bottom: 1rpx solid #eee;
}