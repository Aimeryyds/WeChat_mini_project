.certificationBox {
    width: 100%;
    height: 100%;
}

.cerbanner {
    width: 100%;
    margin-bottom: 50rpx;
}

.cerbanner image {
    width: 100%;
    height: 380rpx;
    display: block;
}

.dinzhiCont {
    padding: 0 50rpx;
    background: #fff;
    margin: 0 auto;
}

.dinzhiCont .tit {
    font-size: 40rpx;
    line-height: 100rpx;
    display: block;
}

.dinzhiCont .mart {
    margin-top: 50rpx;
}

.dinzhiCont .group_from {
    display: flex;
    -webkit-box-align: center;
    font-size: 30rpx;
    margin-top: 25rpx;
    border-bottom: 1rpx solid #eee;
}

.grouplable {
    width: 150rpx;
    line-height: 70rpx;
    float: left;
}

.dzgroupbox {
    width: calc(100% - 150rpx);
    float: left;
}

.groupinput {
    width: calc(100% - 150rpx);
    float: left;
}

.groupinput input {
    font-size: 30rpx;
    border-radius: 5rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 8rpx;
    list-style: none;
    outline: none;
    border: none;
    color: #292b33;
}

.dinzhiCont .touxiangsty {
    border: none;
}

.touxiangsty .grouplable {
    width: 180rpx;
    line-height: 180rpx;
    float: left;
}

.lookmore {
    background: url(data:image/png;base64,) no-repeat center right;
    background-size: 14rpx 22rpx;
}

.touxiangsty .groicon {
    margin-top: 0;
    width: 160rpx;
    height: 200rpx;
    text-align: center;
}

.touxiangsty .groicon image {
    width: 100%;
    height: 200rpx;
}

.group_from .kuangsty {
    width: 100%;
}

.queren {
    width: 100%;
}

.queren button {
    display: block;
    line-height: 80rpx;
    color: #fff;
    width: 50%;
    margin-top: 80rpx;
    border-radius: 13rpx;
    font-size: 30rpx;
    background: linear-gradient(to right,#ffc30e,#ffad00);
}

.siqing-box {
    width: 490rpx;
    position: fixed;
    left: 50%;
    top: 30%;
    margin-left: -245rpx;
    margin-top: -210rpx;
    z-index: 5;
}

.siqing-banner {
    display: block;
    width: 490rpx;
    height: 420rpx;
}

.siqing-colse {
    display: block;
    width: 46rpx;
    height: auto;
    margin: 30rpx auto 0 auto;
}

.siqing-bg {
    position: fixed;
    z-index: 4;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    opacity: 0.5;
    background: #000;
}