page {
    background: #f7f7f7;
}

.searchBox {
    font-size: 28rpx;
    width: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 3;
    background: #fff;
    padding: 20rpx 0;
}

.inputBackground {
    display: flex;
    width: calc(100% - 100rpx);
    background: #f9f9f9;
    border-radius: 40rpx;
    padding: 0 30rpx;
    margin: 0 auto;
    position: relative;
}

.search-icon {
    width: 23rpx;
    height: 22rpx;
    display: inline-block;
    margin: 28rpx 20rpx 0 0;
}

.searchInput {
    height: 80rpx;
    font-size: 26rpx;
    background: #f9f9f9;
    flex: none;
    width: 80%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search-del {
    width: 34rpx;
    height: 34rpx;
    position: absolute;
    right: 20rpx;
    top: 11px;
    z-index: 10;
}

.searchListBox {
    width: 100%;
    position: absolute;
    top: 110rpx;
    height: calc(100% - 120rpx);
    font-size: 30rpx;
}

.lenovoBox {
    background: #fff;
    width: calc(100% - 80rpx);
    padding: 20rpx 40rpx 0 40rpx;
}

.seaHistoryBox {
    background: #fff;
    width: calc(100% - 80rpx);
    padding: 20rpx 40rpx 0 40rpx;
}

.historyTit {
    font-size: 32rpx;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.historySty {
    border-bottom: 1rpx solid #eee;
    line-height: 100rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.historySty image {
    width: 28rpx;
    height: 28rpx;
    margin-right: 20rpx;
    vertical-align: -3rpx;
}

.historySty text {
    font-size: 28rpx;
}

.emptyBtn {
    background: #f7f7f7;
    font-size: 28rpx;
    padding: 30rpx 0;
    text-align: center;
    color: #8a8a8a;
}

.noData {
    text-align: center;
    color: #8a8a8a;
}

.noData view {
    font-size: 30rpx;
    margin-top: 10rpx;
}

.noData image {
    width: 163rpx;
    height: 250rpx;
    margin: 0 auto;
    vertical-align: middle;
    padding-top: 200rpx;
}

.no-data {
    font-size: 28rpx;
    color: #777;
    text-align: center;
    margin-top: calc(50% - 40rpx);
}

.no-data image {
    display: inline-block;
    width: 163rpx;
    height: 250rpx;
}

.no-data .no-text {
    display: block;
    margin-top: 40rpx;
}

.no-text text {
    display: block;
}