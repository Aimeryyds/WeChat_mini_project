.newhouse-search {
    padding: 0 40rpx;
}

.input-box {
    overflow: hidden;
}

.input-box .input-wrap {
    width: 566rpx;
    height: 72rpx;
    background: rgba(247,247,247,1);
    border-radius: 36rpx;
    color: #BABABA;
    font-size: 24rpx;
    float: left;
    position: relative;
}

.input-wrap image {
    width: 33rpx;
    height: 33rpx;
    position: absolute;
    right: 20rpx;
    top: 50%;
    margin-top: -16rpx;
}

.input-box .input-wrap input {
    width: 460rpx;
    height: 100%;
    margin-left: 30rpx;
}

.input-box view {
    line-height: 72rpx;
    font-size: 30rpx;
    color: #292B33;
    float: right;
}

.hot-search {
    font-size: 30rpx;
    color: #333;
    margin-top: 71rpx;
    display: flex;
    margin-bottom: 24rpx;
    justify-content: space-between;
}

.hot-search image {
    width: 48rpx;
    height: 48rpx;
}

.hotkeywords {
    overflow: hidden;
}

.hotkeywords view {
    font-size: 24rpx;
    color: #333;
    padding: 20rpx;
    background: #F5F5F5;
    float: left;
    border-radius: 16rpx;
    margin-right: 16rpx;
    margin-bottom: 16rpx;
}

.mind-search {
    position: fixed;
    top: 72rpx;
    width: 100%;
    bottom: 0;
    background: #fff;
    z-index: 20;
}

.mind-search scroll-view {
    padding: 0 40rpx;
    height: 100%;
}

.scroll-wrap {
    width: 100%;
    height: 100%;
}

.search-empty {
    font-size: 28rpx;
    color: #333;
    text-align: center;
    line-height: 500rpx;
}

.mind-search-list {
    width: calc(100% - 80rpx);
    font-size: 30rpx;
    overflow: hidden;
    line-height: 45rpx;
    margin-top: 30rpx;
}

.mind-search-list:first-child {
    margin-top: 60rpx;
}

.mind-search-list:last-child {
    margin-bottom: 60rpx;
}

.quyuStyle {
    background: #FFF9F7;
    font-size: 20rpx;
    border-radius: 2rpx;
    padding: 4rpx 6rpx;
    margin-right: 6rpx;
    margin-left: 6rpx;
}

.mind-search-list .zhuzhai {
    background: #F5F5F5;
    color: #999;
}

.mind-search-list .red {
    color: #FF3333;
}

.mind-search-list .hui {
    color: #999;
}

.mind-search-list .orange {
    color: #FF6633;
}

.mind-search-list .ditie {
    color: #FF9933;
}

.mind-search-list .blue {
    color: #3399FF;
    background: #F7FBFF;
}

.mind-search-list .zhuangtai {
    background: #FFCC33;
}

.mind-search-list .adr {
    width: 100%;
    font-size: 24rpx;
    color: #999;
}

.history view {
    font-size: 28rpx;
    color: #292B33;
    margin-left: 40rpx;
    border-bottom: 1px solid #eaeaea;
    line-height: 70rpx;
}

.textright {
    float: right;
}

.display {
    float: left;
}

.tabloupan {
    font-size: 30rpx;
    margin-top: 36rpx;
    padding-top: 20rpx;
}

.tabloupan text {
    color: #999;
    margin-right: 48rpx;
}

.tabloupan .cur {
    color: #333;
}

.tabloupanCon {
    padding-bottom: 100rpx;
}

.scrollMenu {
    white-space: nowrap;
}

.tabloupan .scrollMenu {
    margin-bottom: 32rpx;
}

.themImage {
    border-radius: 8rpx;
    width: 214rpx;
    height: 148rpx;
    margin-bottom: 16rpx;
    overflow: hidden;
    position: relative;
}

.themImage image {
    width: 214rpx;
    height: 148rpx;
}

.imagetag {
    position: absolute;
    right: 0;
    top: 0;
    color: #333;
    font-size: 20rpx;
    background: #FFCC33;
    z-index: 10;
    border-bottom-left-radius: 20rpx;
    padding: 8rpx 12rpx;
}

.themtitle {
    color: #333;
    font-size: 28rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    margin-bottom: 12rpx;
}

.themadrs {
    color: #999;
    font-size: 24rpx;
}

.scrollMenu2 {
    display: flex;
    white-space: nowrap;
}

.tabloupanConItem {
    margin-right: 22rpx;
    display: inline-block;
}

.tabbangdan {
    color: #999;
    font-size: 30rpx;
    margin-top: 50rpx;
}

.tabbangdanCon {
    background: #F5F5F5;
    padding: 20rpx 20rpx 40rpx 20rpx;
    border-radius: 16rpx;
    margin-top: -2rpx;
}

.bangdanItem {
    display: flex;
    margin-top: 32rpx;
}

.bangdanleft {
    display: flex;
    flex: 1;
}

.tabbangdan view {
    margin-right: 48rpx;
    display: inline-block;
}

.sanjiao {
    display: block;
    width: 0rpx;
    height: 0rpx;
    border-width: 20rpx;
    border-style: solid;
    border-color: transparent transparent #F5F5F5 transparent;
    margin: 0 auto;
    display: none;
}

.tabbangdan .cur {
    color: #333;
}

.tabbangdan .cur .sanjiao {
    display: block;
}

.bangdanright {
}

.bangdanleft .num {
    margin-right: 16rpx;
    margin-top: 5rpx;
}

.bangdanleft .num,.bangdanleft .num image {
    width: 48rpx;
    height: 48rpx;
}

.bandan_info {
    color: #666;
    font-size: 24rpx;
}

.bandan_info .title {
    font-size: 30rpx;
    color: #333;
    margin-bottom: 14rpx;
}

.bangdanright text {
    color: #FF3333;
    font-size: 28rpx;
    font-weight: bold;
}

.bandan_info .leixing {
    margin-right: 10rpx;
}