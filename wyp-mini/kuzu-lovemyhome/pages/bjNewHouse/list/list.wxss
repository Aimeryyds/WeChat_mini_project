page {
    background: #fff;
    box-sizing: border-box;
}

.paddboth {
    padding: 0 32rpx;
}

.bgStyle {
    background: #fff;
    border-radius: 16rpx;
    padding: 20rpx 0;
    margin-bottom: 16rpx;
}

.listtopmenu {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    background: #fff;
    z-index: 80;
}

.topInputStyle {
    width: calc(100% - 64rpx);
    background: #F4F4F4;
    margin: 24rpx 32rpx 0 32rpx;
    border-radius: 40rpx;
    font-size: 28rpx;
    color: #20222A;
}

.topInput {
    display: flex;
    height: 80rpx;
    align-items: center;
    justify-content: space-between;
    padding: 0 30rpx;
}

.topInput .search_icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 20rpx;
}

.topInput .name {
    width: calc(100% - 52rpx);
    color: #BEBEBE;
}

.topInput .name input {
    width: 100%;
    color: #333;
}

.kslist {
    padding-left: 32rpx;
}

.leftMenu {
    display: flex;
    white-space: nowrap;
    margin-bottom: 16rpx;
}

.itemMenu {
    width: 150rpx;
    height: 64rpx;
    text-align: center;
    line-height: 64rpx;
    display: inline-block;
    margin-right: 20rpx;
    background: #F4F4F4;
    color: #333;
    font-size: 24rpx;
    border-radius: 8rpx;
}

.itemMenu.cur {
    background: #FFCC33;
}

.houseItem {
    display: flex;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #F3F4F6;
}

.houseItem:last-child {
    border-bottom: none;
}

.houseItem_img {
    width: 210rpx;
    height: 160rpx;
    position: relative;
    border-radius: 6rpx;
    overflow: hidden;
}

.houseItem_img image {
    width: 210rpx;
    height: 160rpx;
}

.houseItem_con {
    width: calc(100% - 220rpx);
    font-size: 24rpx;
    padding-left: 20rpx;
    margin-top: -5rpx;
    text-align: justify;
}

.houseItem_1 text {
    font-size: 32rpx;
    font-weight: bold;
    color: #333;
}

.houseItem_1 .tag {
    margin-left: 10rpx;
    color: #2184FD;
    font-size: 22rpx;
    background: #F1F7FE;
    border-radius: 4rpx;
    display: inline-block;
    padding: 2rpx 8rpx;
}

.houseItem_2 {
    color: #20222A;
    font-size: 24rpx;
    margin-top: 10rpx;
}

.houseItem_3 {
    margin-top: 10rpx;
}

.houseItem_3 text {
    padding: 3rpx 8rpx;
    color: #83878F;
    font-size: 20rpx;
    background: #F3F4F6;
    display: inline-block;
    margin-right: 10rpx;
}

.houseItem_4 {
    font-size: 22rpx;
    margin-top: 5rpx;
}

.houseItem_4 .red,.houseItem_4 .are {
    display: inline-block;
}

.houseItem_4 .red {
    color: #FF6352;
}

.houseItem_4 .red .big {
    font-size: 30rpx;
    font-weight: bold;
}

.houseItem_4 .are {
    font-size: 24rpx;
    color: #999;
    margin-left: 10rpx;
}

.bjicon3d {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    left: 10rpx;
    bottom: 20rpx;
    font-size: 24rpx;
    animation: icon-circle 3s linear infinite;
}

.bjicon3d image {
    width: 100%;
    height: 100%;
}

.list-tab {
    height: 100rpx;
    display: flex;
    font-size: 26rpx;
    color: #666;
    justify-content: space-around;
    align-items: center;
    background: #fff;
}

.list-item {
    display: flex;
    align-items: center;
    justify-content: center;
    height: 100%;
    position: relative;
    font-size: 26rpx;
    width: 20%;
}

.list-item text {
    display: inline-block;
}

.list-item .name {
    max-width: 80%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.list-tab .arrow {
    width: 16rpx;
    height: 16rpx;
    margin-left: 10rpx;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 16rpx 16rpx;
    vertical-align: 8rpx;
}

.filter-cur {
    color: #333;
}

.filter-cur .arrow {
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 16rpx 16rpx;
}

.list-ctn {
    padding: 27rpx 40rpx;
    overflow: hidden;
}

.list-ctn:nth-child(1) {
    padding-top: 54rpx;
}

.image-wrap {
    width: 210rpx;
    height: 166rpx;
    float: left;
}

.image-wrap image {
    width: 100%;
    height: 100%;
    border-radius: 16rpx;
}

.content {
    width: calc(100% - 230rpx);
    float: right;
}

.item-1 {
    font-size: 32rpx;
    color: #292b33;
}

.item-1 text {
    display: inline-block;
    margin-right: 16rpx;
}

.tag {
    display: inline-block;
    font-size: 12px;
    color: #1c60ea;
    border-radius: 17rpx;
    background: skyblue;
    padding: 2rpx 8rpx;
}

.item-2 {
    font-size: 22rpx;
    color: #7d7e82;
    padding: 8rpx 0 0rpx;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.item-tag {
    display: inline-block;
    padding: 3px;
    background: #ebf5f7;
    color: #608997;
    font-size: 22rpx;
    border-radius: 17rpx;
    margin-right: 8rpx;
}

.item-4 {
    margin-top: -5rpx;
}

.item-4 text:nth-child(1) {
    font-size: 32rpx;
    color: #ff522f;
}

.item-4 text:nth-child(2) {
    font-size: 22rpx;
    color: #7d7e82;
    margin-left: 16rpx;
}

.list-data scroll-view {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.filter-box {
    position: fixed;
    width: 100%;
    top: 200rpx;
    bottom: 0;
    background: rgba(0,0,0,0.5);
    z-index: 100;
}

.filter {
    position: fixed;
    width: 100%;
    top: 200rpx;
    bottom: 0;
    z-index: 100;
}

.area {
    width: 100%;
    height: 740rpx;
    background: #F4F4F4;
    position: relative;
    z-index: 101;
    font-size: 28rpx;
}

.housetype,.moretag,.sorttab {
    position: relative;
    z-index: 101;
}

.area-left {
    width: 33.33%;
    float: left;
}

.area-left view {
    height: 92rpx;
    line-height: 92rpx;
    text-indent: 42rpx;
    color: #292b33;
}

.area-left .duigou,.area-right .duigou,.area_last .duigou {
    display: none;
}

.area-left .filter-cur {
    background: #FAFAFA;
    color: #333;
    font-weight: bold;
}

.area-left .filter-cur .duigou,.area-right .filter-cur .duigou,.area_last .filter-cur .duigou {
    display: inline-block;
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
    vertical-align: middle;
}

.area-right .filter-cur {
    background: #fff;
    color: #333;
    font-weight: bold;
}

.area_last .filter-cur {
    background: #fff;
    color: #333;
    font-weight: bold;
}

.moretag .filter-cur {
    color: #333;
    background: #FFCC33;
}

.area-left .biao,.price-top .biao,.sorttab .biao,.moretag .biao {
    display: none;
}

.area-left .filter-cur .biao,.price-top .filter-cur .biao,.sorttab .filter-cur .biao,.moretag .filter-cur .biao {
    display: block;
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    bottom: 0;
    right: 0;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 24rpx 24rpx;
}

.area-right {
    width: calc(100% - 33.33%);
    float: right;
    height: 100%;
    background: #FAFAFA;
}

.area-right.columns3 {
    width: 33.33%;
    float: left;
}

.area_last {
    width: 33.33%;
    float: right;
    height: 100%;
    background: #fff;
}

.area-right scroll-view,.area_last scroll-view {
    width: 100%;
    height: 100%;
}

.area-right view,.area_last view {
    width: calc(100% - 32rpx);
    padding-left: 32rpx;
    height: 92rpx;
    line-height: 92rpx;
}

.price-bottom {
    width: 100%;
    background: #fff;
}

.price {
    width: 100%;
    background: #fff;
    position: relative;
    z-index: 101;
}

.price-top {
    height: 80rpx;
    line-height: 80rpx;
    padding: 10rpx 32rpx;
}

.price-top view {
    display: inline-block;
    font-size: 30rpx;
    color: #999;
    margin-right: 48rpx;
}

.price-top .filter-cur {
    color: #333;
}

.price-bottom scroll-view {
    padding: 0 0 32rpx 30rpx;
    width: calc(100% - 30rpx);
}

.price-bottom view {
    font-size: 24rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display: inline-block;
    text-align: center;
    margin: 9rpx;
    color: #999;
    width: 158rpx;
    height: 72rpx;
    line-height: 72rpx;
    border-radius: 16rpx;
    background: #F4F4F4;
}

.price-bottom .filter-cur {
    color: #333;
    background: #FFCC33;
}

.priceZi {
    font-size: 30rpx;
    color: #333;
    padding: 0 32rpx;
}

.custom {
    padding: 0 32rpx;
    width: calc(100% - 64rpx);
    height: 110rpx;
    background: #fff;
    float: left;
    overflow: hidden;
    line-height: 110rpx;
}

.custom view {
    float: left;
}

.custom view:nth-child(1) {
    margin-left: 42rpx;
}

.custom .hx {
    width: calc(100% - 600rpx);
    text-align: center;
    color: #F4F4F4;
}

.custom .minprice,.custom .topprice {
    display: block;
    width: 300rpx;
    text-align: center;
    height: 80rpx;
    background: #F4F4F4;
    border-radius: 16rpx;
    float: left;
    margin-top: 28rpx;
    font-size: 24rpx;
}

.priceButtons {
    padding: 32rpx;
    display: flex;
}

.priceButtons .confirm-btn,.priceButtons .empty-btn {
    height: 96rpx;
    line-height: 96rpx;
    font-size: 30rpx;
    text-align: center;
    border-radius: 16rpx;
}

.priceButtons .confirm-btn {
    flex: 1;
    width: 452rpx;
    background: linear-gradient(to right,#FFC30E,#FFAD00);
    text-align: center;
    color: #fff;
    margin-left: 24rpx;
}

.priceButtons .empty-btn {
    width: 210rpx;
    background: #F4F4F4;
}

.housetype {
    width: 100%;
    background: #fff;
    font-size: 28rpx;
    color: #292b33;
}

.housetype scroll-view {
    width: calc(100% - 30rpx);
    padding: 32rpx 0 32rpx 30rpx;
}

.housetype scroll-view view {
    font-size: 24rpx;
    display: inline-block;
    background: #F4F4F4;
    width: 158rpx;
    height: 72rpx;
    margin: 9rpx;
    border-radius: 16rpx;
    text-align: center;
    line-height: 72rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    color: #999;
}

.housetype .filter-cur {
    background: #FFCC33;
    color: #333;
    position: relative;
}

.biao {
    display: none;
}

.shaiTitle {
    color: #333;
    font-size: 30rpx;
    font-weight: bold;
    padding: 32rpx 38rpx 0 38rpx;
}

.housetype .filter-cur .biao {
    display: block;
    width: 24rpx;
    height: 24rpx;
    position: absolute;
    bottom: 0;
    right: 0;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 24rpx 24rpx;
}

.housetypebutton {
    padding: 0 32rpx 32rpx 32rpx;
    display: flex;
    text-align: center;
    line-height: 96rpx;
    height: 96rpx;
    font-size: 30rpx;
}

.housetype_empty,.housetype_yes {
    border-radius: 16rpx;
}

.housetype_empty {
    width: 210rpx;
    background: #F4F4F4;
}

.housetype_yes {
    flex: 1;
    width: 452rpx;
    color: #333;
    background: #FFCC33;
    margin-left: 24rpx;
}

.moretag {
    height: 100%;
    background: #fff;
    overflow: hidden;
}

.moretag-title {
    font-size: 30rpx;
    color: #333;
    font-weight: bold;
    margin-top: 34rpx;
    margin-bottom: 30rpx;
}

.tag-ctn {
    width: 100%;
    padding-left: 40rpx;
    position: absolute;
    top: 0;
    bottom: 110rpx;
}

.tag-ctn scroll-view {
    width: 100%;
    height: 100%;
}

.more-tag-item {
    position: relative;
    width: 158rpx;
    height: 72rpx;
    text-align: center;
    line-height: 72rpx;
    background: #f7f7f7;
    color: #999;
    font-size: 24rpx;
    border-radius: 16rpx;
    float: left;
    margin-right: 18rpx;
    margin-bottom: 20rpx;
}

.tab-btn {
    width: 100%;
    height: 110rpx;
    position: absolute;
    bottom: 0;
    left: 0;
    border-top: 1px solid #eaeaea;
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.tab-btn view {
    width: 312rpx;
    height: 74rpx;
    text-align: center;
    line-height: 74rpx;
    font-size: 30rpx;
    color: #292b33;
    border-radius: 16rpx;
}

.tab-btn view:nth-child(1) {
    background: #f7f7f7;
}

.tab-btn view:nth-child(2) {
    color: #fff;
    background: linear-gradient(to right,#FFC30E,#FFAD00);
}

.sorttab {
    width: 100%;
    height: auto;
    background: #fff;
}

.sorttab view {
    width: 100%;
    height: 92rpx;
    line-height: 92rpx;
    font-size: 28rpx;
    color: #292b33;
    text-indent: 32rpx;
}

.sorttab .duigou {
    display: none;
}

.sorttab .filter-cur {
    background: #F4F4F4;
    color: #333;
    font-weight: bold;
}

.sorttab .filter-cur .duigou {
    display: inline-block;
    width: 32rpx;
    height: 32rpx;
    margin-right: 16rpx;
    vertical-align: middle;
}

.nodata {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    text-align: center;
    color: #eaeaea;
}

.nodata image {
    width: 160rpx;
    height: 250rpx;
    display: block;
    margin: 0 auto;
    margin-top: 160rpx;
    margin-bottom: 40rpx;
}

@-webkit-keyframes icon-circle {
    0% {
        transform: rotate(0deg);
    }

    50% {
        transform: rotate(180deg);
    }

    100% {
        transform: rotate(360deg);
    }
}

@keyframes icon-circle {
    0% {
        transform: rotate(0deg);
    }

    50% {
        transform: rotate(180deg);
    }

    100% {
        transform: rotate(360deg);
    }
}