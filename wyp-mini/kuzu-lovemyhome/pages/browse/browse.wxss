    animation: swing 3s linear infinite;
}

.fixed-point image {
    width: 102rpx;
    height: auto;
}

@-webkit-keyframes swing {
    0% {
        transform: rotate(0deg);
    }

    20% {
        transform: rotate(15deg);
    }

    30% {
        transform: rotate(-15deg);
    }

    40% {
        transform: rotate(15deg);
    }

    50% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(0deg);
    }
}

@keyframes swing {
    0% {
        transform: rotate(0deg);
    }

    20% {
        transform: rotate(15deg);
    }

    30% {
        transform: rotate(-15deg);
    }

    40% {
        transform: rotate(15deg);
    }

    50% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(0deg);
    }
}