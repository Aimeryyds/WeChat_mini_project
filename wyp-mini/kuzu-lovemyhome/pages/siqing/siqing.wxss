page {
    background: url('https://res.5i5j.com/wap/event/year/images/banner.png') no-repeat center top #f7c500;
    background-size: 100% auto;
    position: relative;
}

.sqPage-con {
    padding-top: 55rpx;
    width: 603rpx;
    margin: 0 auto;
    border-radius: 40rpx;
    border: 5rpx solid #fef603;
    background: rgba(255,255,255,0.6);
    position: relative;
    z-index: 3;
}

.sqPage-bottom {
    width: 100%;
    position: relative;
    left: 0;
    bottom: 0;
    z-index: 1;
}

.sqPage-bottom image {
    width: 100%;
    height: auto;
}

.sqList {
    width: 491rpx;
    height: 161rpx;
    background: url('https://res.5i5j.com/wap/event/year/images/quan-bg.png') no-repeat center center;
    margin: 0 auto 36rpx auto;
    background-size: 100% auto;
}

.sqList:last-child {
    margin-bottom: 55rpx;
}

.kj-left,.kj-right {
    float: left;
}

.kj-left image,.kj-right image {
    width: 100%;
    height: auto;
}

.kj-left {
    width: 180rpx;
    height: 160rpx;
}

.kj-right {
    width: 310rpx;
    height: 160rpx;
}

.ylqSty {
    position: relative;
}

.kj-right .ylq-tag {
    width: 118rpx;
    height: 90rpx;
    position: absolute;
    right: -1rpx;
    top: 0rpx;
}

.bottom-bg {
    padding-top: 650rpx;
    background: url('https://res.5i5j.com/wap/event/year/images/bottom.png') no-repeat center bottom;
    background-size: 100% auto;
}

.pdbottom {
    padding-bottom: 300rpx;
}

.popupBox {
    width: 600rpx;
    margin: 0 auto;
    overflow: hidden;
    padding: 50rpx 0;
    position: absolute;
    left: 50%;
    margin-left: -300rpx;
    bottom: 0;
}

.mztk,.hdsm {
    float: left;
    width: 50%;
    color: #666666;
    font-size: 28rpx;
}

.mztk image,.hdsm image {
    width: 24rpx;
    height: 24rpx;
    margin-right: 5rpx;
    vertical-align: -2rpx;
}

.hdsm {
    text-align: right;
}

.lingqu,.lingqu image {
    width: 280rpx;
    height: 80rpx;
}

.lingqu {
    position: absolute;
    bottom: -50rpx;
    left: 50%;
    margin-left: -140rpx;
}

.anniu2 {
    position: absolute;
    bottom: -50rpx;
    left: 50%;
    width: 238rpx;
    height: 80rpx;
    margin-left: -119rpx;
}

.anniu2 image {
    width: 238rpx;
    height: 80rpx;
}

.sqList-btnBox {
    position: absolute;
    bottom: -50rpx;
    left: 60rpx;
}

.tanMask,.receiveMask {
    position: fixed;
    z-index: 4;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    opacity: 0.5;
    background: #000;
}

.mzTan {
    position: fixed;
    z-index: 5;
    left: 50%;
    top: 50%;
    margin-left: -345rpx;
    margin-top: -390rpx;
    background: #fff;
    width: 689rpx;
    height: 780rpx;
    border-radius: 40rpx;
}

.mzTit {
    color: #333;
    font-size: 32rpx;
    text-align: center;
    margin-top: 50rpx;
    margin-bottom: 30rpx;
    font-weight: bold;
}

.mztit2 {
    font-weight: bold;
    font-size: 28rpx;
    padding: 0 50rpx;
    margin-bottom: 30rpx;
}

.mzCon {
    padding: 0 50rpx;
    font-size: 26rpx;
    color: #454545;
    line-height: 40rpx;
    height: 600rpx;
    overflow-y: auto;
}

.maskSty-close {
    width: 40rpx;
    height: 40rpx;
    position: fixed;
    z-index: 5;
    left: 50%;
    bottom: 110rpx;
    margin-left: -20rpx;
}

.maskSty-close image {
    width: 40rpx;
    height: 40rpx;
}

.receive {
    position: fixed;
    z-index: 5;
    padding: 40rpx 0 0 0;
    width: 540rpx;
    border-radius: 40rpx;
    left: 50%;
    top: 50%;
    margin-left: -270rpx;
    margin-top: -25%;
    background: #fff;
    box-sizing: border-box;
}

.receivetit {
    color: #333;
    font-size: 32rpx;
    text-align: center;
    margin-bottom: 20rpx;
    font-weight: bold;
}

.receivecon {
    font-size: 27rpx;
    color: #454545;
    padding: 0 50rpx;
}

.one {
    margin-bottom: 10rpx;
}

.receivecon text {
    color: #FE6C4F;
}

.knowBtn {
    text-align: center;
    margin: 20rpx auto;
}

.knowBtn image {
    width: 237rpx;
    height: 80rpx;
    margin: 0 auto;
}

.loading {
    z-index: 50;
}

.tishi {
    position: fixed;
    top: 50%;
    left: 50%;
    width: 80%;
    height: 80rpx;
    line-height: 80rpx;
    margin-left: -40%;
    margin-top: -45rpx;
    background: rgba(0,0,0,0.6);
    z-index: 20;
    text-align: center;
    color: #fff;
    font-size: 26rpx;
    border-radius: 10rpx;
}