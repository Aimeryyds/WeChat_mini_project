.comment .item .esf-borker {
    display: flex;
    flex-flow: column wrap;
    justify-content: space-between;
    height: 120rpx;
    padding: 30rpx;
    font-size: 24rpx;
    color: #a0a0a0;
}

.comment .item .esf-borker image:first-child {
    display: block;
    width: 100rpx;
    height: 100rpx;
    margin: 10rpx;
    border-radius: 50%;
}

.comment .item .esf-borker text {
    width: 450rpx;
}

.comment .item .esf-borker text:nth-child(2) {
    font-size: 28rpx;
    color: #000;
}

.comment .item .esf-borker image:last-child {
    display: block;
    width: 50rpx;
    height: 50rpx;
    margin: 35rpx;
}

.comment .item .esf-borker-note {
    font-size: 28rpx;
    padding: 0 30rpx 30rpx;
    line-height: 1.5;
}