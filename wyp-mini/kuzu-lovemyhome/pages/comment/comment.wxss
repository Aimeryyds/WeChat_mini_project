#searchbar {
    margin: 10rpx;
    padding: 15rpx 10rpx;
    position: relative;
    width: calc(100% - 40rpx);
}

#searchbar #city {
    position: relative;
    color: #666;
    padding: 10rpx;
    width: 100rpx;
}

#searchbar #city::after {
    content: ' ';
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    border: 4rpx solid #666;
    border-top: transparent;
    border-left: 4rpx solid transparent;
    transform: scale(0.5) rotate(45deg);
}

#searchinput {
    background-color: #efeff4;
    border-radius: 10rpx;
    position: relative;
    padding: 5rpx 0;
}

#searchinput image {
    width: 23rpx;
    height: 23rpx;
    margin: 15rpx;
}

#searchinput picker {
    position: relative;
    width: 130rpx;
    height: 30rpx;
    line-height: 34rpx;
    font-size: 28rpx;
    margin: 10rpx;
    border-right: 2rpx solid #e4e7ec;
}

#searchinput picker::after {
    content: '';
    position: absolute;
    top: 10rpx;
    right: 10rpx;
    width: 0;
    height: 0;
    line-height: 0;
    border: 10rpx solid transparent;
    border-top: 14rpx solid #a0a0a0;
}

#searchinput button {
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 50%;
    font-size: 24rpx;
    color: rgba(0,0,0,0.2);
    margin: 5rpx;
    padding: 0;
}

#searchinput button::after {
    border-radius: 50%;
}

#searchbar #cancel_search {
    width: 80rpx;
    color: #666;
    text-align: center;
    padding: 10rpx;
    line-height: 1.5;
}

#searchbar scroll-view {
    position: fixed;
    z-index: 999;
    top: 90rpx;
    background-color: #fff;
    text-indent: 20rpx;
    width: 100%;
    left: 0;
    height: calc(100% - 90rpx);
}

#searchbar scroll-view text {
    display: block;
    border-top: 2rpx solid #e4e7ec;
    line-height: 78rpx;
}

#filterbar {
    background-color: #fff;
    border-top: 2rpx solid #e4e7ec;
    border-bottom: 2rpx solid #e4e7ec;
    width: 100%;
}

#filterbar.hover {
    z-index: 999;
}

#filterbar view {
    position: relative;
    text-align: center;
    color: #64676c;
    margin: 30rpx 0;
    padding: 0 30rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    display: inline-block;
}

#filterbar view:not(:last-child) {
    border-right: 2rpx solid #e4e7ec;
}

#filterbar view text::after {
    content: "";
    position: absolute;
    right: 15rpx;
    border: 10rpx solid transparent;
    border-top: 10rpx solid #64676c;
    transition: all .4s;
    transform-origin: center 25%;
    top: calc(50% - 6rpx);
}

#filterbar view text.hover {
    color: #fdc915;
}

#filterbar view text.hover::after {
    border-top-color: #fdc915;
    transform: rotate(180deg);
}

