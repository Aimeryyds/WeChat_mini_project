page,.map-wrapper,#map {
    width: 100%;
    height: 100%;
}

.loc-tos {
    height: 64rpx;
    background-color: #fff;
    font-size: 26rpx;
    color: #394344;
    line-height: 64rpx;
    padding: 5rpx 10rpx;
    border-radius: 32rpx;
    transform: translateX(-50%);
}

.location {
    position: absolute;
    top: calc(50% - 92rpx);
    left: calc(50% - 26rpx);
}

.location-img {
    width: 52rpx;
    height: 92rpx;
}

.movetoloaction {
    display: inline-block;
    font-size: 0;
    position: absolute;
    bottom: 52rpx;
    right: 32rpx;
}

.movetoloaction-img {
    width: 96rpx;
    height: 98rpx;
}

.map-search {
    background-color: #fff;
    height: 88rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
}

.city-wrapper,.search-wrapper,.toindex-img-wrapper {
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
}

.city-wrapper {
    font-size: 26rpx;
    color: #7c8085;
    padding-right: 33rpx;
    padding-left: 32rpx;
}

.toindex-img-wrapper {
    padding-right: 32rpx;
}

.city-img {
    width: 18rpx;
    height: 20rpx;
    margin-right: 12rpx;
}

.search-wrapper {
    font-size: 24rpx;
    color: #7c8085;
    -webkit-box-flex: 1;
    flex: 1;
    padding-left: 37rpx;
}

.border {
    width: 1px;
    height: 28rpx;
    background-color: #eaeaea;
}

.search-img {
    width: 24rpx;
    height: 24rpx;
    margin-right: 12rpx;
}

.toindex-img {
    width: 26rpx;
    height: 24rpx;
    padding-left: 33rpx;
}

.room-list-wrapper {
    width: 100%;
    height: 850rpx;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    border-radius: 10rpx 10rpx 0 0;
    padding: 0 32rpx;
    box-sizing: border-box;
}

.room-name {
    font-size: 36rpx;
    color: #333;
    margin: 46rpx 0;
}

.room-list {
    height: 700rpx;
    overflow-y: scroll;
}

.room-item {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}

.item-img-wrapper {
    width: 205rpx;
}

.item-img {
    width: 205rpx;
    height: 153rpx;
}

.mark {
    height: 28rpx;
    width: 60rpx;
    background-color: #fb7c6a;
    font-size: 20rpx;
    color: #fff;
    text-align: center;
    line-height: 28rpx;
    transform: translateY(-153rpx);
    border-radius: 0 0 14rpx 0;
}

.item-detail-wrapper {
    -webkit-box-flex: 1;
    flex: 1;
}

.item-detail {
    padding-left: 28rpx;
}

.detail-name {
    font-size: 32rpx;
    color: #333;
    text-overflow: ellipsis;
}

.pattern-wrapper {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    margin-top: 8rpx;
}

.pattern {
    font-size: 24rpx;
    color: #999;
}

.type {
    font-size: 22rpx;
    color: #666;
    font-weight: 700;
}

.price-wrapper {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: end;
    align-items: flex-end;
    margin-top: 8rpx;
}

.price {
    font-size: 32rpx;
    color: #ff4c59;
    margin-right: 26rpx;
}

.original-price {
    font-size: 24rpx;
    color: #999;
}

.del {
    height: 1px;
    width: 100%;
    background-color: #999;
    transform: translateY(-12rpx);
}

.station {
    font-size: 24rpx;
    color: #999;
    margin-top: 12rpx;
}

.description {
    margin-top: 12rpx;
}

.description-item {
    height: 28rpx;
    display: inline-block;
    border: 1px solid #cbebec;
    font-size: 20rpx;
    color: #53bebf;
    border-radius: 5rpx;
    padding: 0 5rpx;
    margin-right: 10rpx;
    text-align: center;
    line-height: 28rpx;
}

.list-border {
    height: 1px;
    background-color: #f0f0f0;
    margin: 30rpx 0;
}

.nomore-wrapper {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    -webkit-box-align: center;
    align-items: center;
    margin: 30rpx 0;
}

.nomore {
    font-size: 24rpx;
    color: #d3d3d3;
    margin: 0 16rpx;
}

.nomore-border {
    height: 2px;
    width: 82rpx;
    background-color: #f3f2f3;
}

.frist-month {
    height: 32rpx;
    line-height: 32rpx;
    font-size: 20rpx;
    color: #999;
    background-color: #f5f6f7;
    padding: 0 10rpx;
}