.page {
    height: 100%;
    background: #fff;
}

.page {
    height: 100%;
    overflow-y: auto;
}

.swiper-tab {
    width: 100%;
    text-align: center;
    line-height: 80rpx;
    background-color: #fff;
    position: fixed;
    top: 0;
    z-index: 10;
}

.swiper-tab-list {
    font-size: 28rpx;
    display: inline-block;
    width: 33%;
    color: #333;
    border-bottom: 0rpx;
    position: relative;
    z-index: 10;
}

.on {
    color: #3dbcc6;
}

.swiper-box {
    display: block;
    height: 100%;
    width: 100%;
    overflow: hidden;
}

.swiper-box view {
    text-align: center;
}

.borderLine {
    width: 20%;
    margin: 0 auto;
}

.active {
    border-bottom: 4rpx solid #3dbcc6;
    color: #3dbcc6;
}

.aq,.ax,.pp {
    position: absolute;
    top: 86rpx;
    padding-bottom: 200rpx;
}

.hz_idx1 {
    width: 750rpx;
    height: 422rpx;
}

.hz_idx2 {
    width: 538rpx;
    height: 517rpx;
    margin-top: 60rpx;
    margin-left: 70rpx;
}

.hz_idx3 {
    width: 750rpx;
    height: 422rpx;
    margin-top: 60rpx;
}

.hz_idx4 {
    width: 690rpx;
    height: 1270rpx;
    margin-left: 30rpx;
}

.hz_part {
    text-align: center;
    font-size: 28rpx;
    line-height: 50rpx;
}

.hz_part .title {
    font-size: 38rpx;
    font-weight: 700;
    margin: 40rpx 0;
}

.hz_part .line5 {
    font-size: 38rpx;
    font-weight: 700;
    margin-top: 40rpx;
}

.hz_part .line6 {
    font-size: 38rpx;
    font-weight: 700;
    margin-top: 10rpx;
    margin-bottom: 40rpx;
}

.js_bottom {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    height: 130rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    box-shadow: 0 -5px 10px 0 rgba(51,51,51,.2);
    background: #fff;
    z-index: 10;
}

.wxzx,.zxwt {
    width: 332rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    margin-top: 20rpx;
    border-radius: 10rpx;
    font-size: 28rpx;
    margin-left: 30rpx;
}

.wxzx {
    background: #ebfffc;
    color: #3dbcc6;
}

.zxwt {
    background: #31cad6;
    color: #fff;
}

.slide {
    position: relative;
    width: 750rpx;
    height: 422rpx;
    margin: 50rpx 0;
    z-index: 6;
    overflow: hidden;
}

.before_box {
    width: 375rpx;
    height: 422rpx;
    position: absolute;
    z-index: 2;
    overflow: hidden;
}

.after_box {
    width: 750rpx;
    height: 422rpx;
    overflow: hidden;
    position: absolute;
    z-index: 1;
}

.before {
    width: 750rpx;
    height: 422rpx;
}

.after {
    width: 750rpx;
    height: 422rpx;
}

.middle {
    width: 281rpx;
    height: 422rpx;
    position: absolute;
    left: 315rpx;
    z-index: 4;
}

.mask {
    width: 750rpx;
    height: 422rpx;
    background: #2b415d;
    opacity: .5;
    position: absolute;
    top: 0;
    z-index: 3;
}

.text_mask_before {
    width: 90rpx;
    height: 41rpx;
    position: absolute;
    bottom: 14rpx;
    opacity: .6;
    z-index: 10;
    background: #000;
    left: 22rpx;
}

.text_line_before {
    position: absolute;
    bottom: 17rpx;
    color: #fff;
    z-index: 10;
    font-size: 24rpx;
    left: 30rpx;
}

.text_mask_after {
    width: 90rpx;
    height: 41rpx;
    position: absolute;
    bottom: 14rpx;
    opacity: .6;
    z-index: 10;
    background: #000;
    right: 22rpx;
}

.text_line_after {
    position: absolute;
    bottom: 17rpx;
    color: #fff;
    z-index: 10;
    font-size: 24rpx;
    right: 30rpx;
}

.aq_idx {
    width: 750rpx;
    height: 422rpx;
}