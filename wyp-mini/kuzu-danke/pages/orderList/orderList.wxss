.orderList-top-nav {
    position: fixed;
    top: 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    width: 750rpx;
    height: 88rpx;
    background: #fff;
}

.orderList-top-nav view {
    width: 50%;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #333;
    text-align: center;
    line-height: 88rpx;
}

.orderList-top-nav .on {
    color: #3dbcc6;
    background: url(http://public.danke.com.cn/public-20190103-FpgfZT7EfV4_PDYs7rUJpjV3loo-) no-repeat;
    background-size: 108rpx 2rpx;
    background-position: bottom;
}

.orderList-top-notice {
    height: 80rpx;
    line-height: 90rpx;
    margin: 88rpx 0 0 0;
    font-family: PingFangSC-Regular;
    font-size: 22rpx;
    color: #999;
    text-align: center;
}

.orderList-list {
    width: 100%;
    margin: 0 0 100rpx 0;
}

.orderList-item {
    margin: 10rpx 0;
    padding: 40rpx 32rpx 0 32rpx;
    background: #fff;
}

.item-title {
    margin: 0 0 30rpx 0;
    font-family: PingFangSC-Regular;
    font-size: 32rpx;
    color: #333;
    line-height: 32rpx;
    text-align: left;
}

.item-content {
    margin: 20rpx 0 0 0;
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #666;
    line-height: 24rpx;
    text-align: left;
}

.orderDetail-btn {
    width: 116rpx;
    height: 38rpx;
    line-height: 38rpx;
    float: right;
    margin: -23rpx 0 0 0;
    border: 2rpx solid #3dbcc6;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #3dbcc6;
    text-align: center;
}

.item-choose {
    width: 686rpx;
    height: 103rpx;
    line-height: 103rpx;
    margin: 40rpx auto 0 auto;
    border-top: 1rpx solid #e2e2e2;
}

.item-havetopay {
    display: inline-block;
}

.order-havetopay {
    display: inline-block;
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    color: red;
    text-align: right;
    line-height: 24rpx;
}

.item-choose .item-checkbox {
    float: right;
    margin: 23rpx 0 0 0;
    border: 1px solid #979797;
    width: 44rpx;
    height: 44rpx;
    border-radius: 100%;
}

.item-choose .on {
    width: 46rpx;
    height: 46rpx;
    float: right;
    margin: 23rpx 0 0 0;
    background: url(http://public.danke.com.cn/public-20190103-FuUuyni62V_WPeNHMKhBg1azy8zw) no-repeat;
    background-size: 100% 100%;
}

.orderList-list-pay {
    margin: 88rpx 0 100rpx 0;
}

.pay-timelist .orderList-item {
    padding-bottom: 50rpx;
}

.item-content-paylist {
    margin-top: 41rpx;
}

.pay-time-title {
    height: 94rpx;
    line-height: 130rpx;
    padding: 0 0 0 32rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #999;
    text-align: left;
}

.pay-timelist .orderDetail-btn {
    margin: -35rpx 0 0 0;
}

.orderList-foot {
    width: 750rpx;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    flex-direction: row;
    background: #fff;
}

.foot-check-money {
    width: 418rpx;
    line-height: 100rpx;
    padding: 0 0 0 32rpx;
}

.foot-checkall {
    display: inline-block;
}

.foot-checkall .item-checkbox,.foot-checkall .on {
    float: left;
    margin: 27rpx 24rpx 0 0;
    width: 44rpx;
    height: 44rpx;
    box-sizing: border-box;
}

.foot-checkall .item-checkbox {
    border: 1px solid #979797;
    border-radius: 100%;
}

.foot-checkall .on {
    width: 44rpx;
    height: 44rpx;
    background: url(http://public.danke.com.cn/public-20190103-FuUuyni62V_WPeNHMKhBg1azy8zw) no-repeat;
    background-size: 44rpx 44rpx;
}

.foot-checkall .foot-checkall-view {
    display: inline-block;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #666;
    text-align: left;
}

.foot-money-num {
    display: inline;
    color: red;
}

.foot-money {
    display: inline-block;
    margin: 0 0 0 116rpx;
}

.foot-pay-btn {
    width: 333rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-family: PingFangSC-Regular;
    font-size: 36rpx;
    color: #fff;
    text-align: center;
    background: #dbdbdb;
}

.orderList-foot .payOn {
    background: #35c6d3;
}

.orderlist-default {
    width: 100%;
    position: absolute;
    top: 200rpx;
    overflow: hidden;
}

.orderlist-default-pic {
    width: 272rpx;
    height: 272rpx;
    margin: 0 auto;
}

.orderlist-default-pic image {
    width: 272rpx;
    height: 272rpx;
    margin: 0 auto;
}

.orderlist-default-text {
    margin: 32rpx auto;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #999;
    letter-spacing: 0;
    text-align: center;
    line-height: 50rpx;
}