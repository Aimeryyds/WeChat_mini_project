.current-city {
    background-color: #f6f7f8;
    font-size: 24rpx;
    color: #999;
    height: 94rpx;
    width: 100%;
    line-height: 94rpx;
    padding-left: 28rpx;
}

.img {
    width: 24rpx;
    height: 34rpx;
}

.city {
    font-size: 36rpx;
    color: #333;
    margin-left: 10rpx;
}

.position-city {
    padding-left: 28rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    background-color: #fff;
    height: 100rpx;
}

.city-list {
    padding-left: 28rpx;
    background-color: #fff;
}

.item {
    height: 112rpx;
    width: 100%;
    font-size: 32rpx;
    color: #666;
    line-height: 112rpx;
    border-bottom: 1px solid #f7f8f9;
}

.isposition {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-flex: 1;
    flex: 1;
}

.position-ing {
    color: #333;
    font-size: 24rpx;
}

.position-fail {
    width: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    padding-right: 28rpx;
}

.position-fail-text {
    color: #ff6159;
    font-size: 24rpx;
}

.position-btn {
    font-size: 24rpx;
    color: #333;
    border: 1px solid #e4e8eb;
    border-radius: 10rpx;
    padding: 5rpx;
}