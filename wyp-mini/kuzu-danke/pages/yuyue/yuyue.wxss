.range-slider {
    position: relative;
}

.range-bar {
    position: absolute;
}

.range-bar {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    border-radius: 10000rpx;
}

.range-bar-bg {
    position: absolute;
    width: 100%;
    height: 100%;
    border-radius: 10000rpx;
}

.range-bar-progress {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: #8a2be2;
}

.block {
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    background: #fff;
    border-radius: 50%;
    box-shadow: 0rpx 0rpx 6rpx #ccc;
}

.page_all {
    width: 750rpx;
    height: 1334rpx;
    background: #fff;
    position: absolute;
    top: 0rpx;
    left: 0rpx;
}

.page {
    background: #fff;
}

.wt-list {
    font-size: 36rpx;
    background: #fff;
}

.room-detail {
    padding: 30rpx 48rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: relative;
    overflow: hidden;
    border-bottom: 20rpx solid #f7f8f9;
}

.room-detail view {
    box-sizing: border-box;
}

.room-img {
    position: relative;
    border-radius: 6rpx;
}

.room-img image {
    width: 260rpx;
    height: 180rpx;
    border-radius: 6rpx;
}

.room-img view {
    position: absolute;
    top: 0rpx;
    height: 28rpx;
    padding: 0 10rpx;
    text-align: center;
    background: #fb8506;
    font-size: 20rpx;
    color: #fff;
    border-radius: 6rpx 0 0 0;
}

.room-img .triangle {
    content: "";
    position: absolute;
    top: 0;
    right: -5rpx;
    border-top: 15rpx solid #fb8506;
    border-bottom: 14rpx solid #fb8506;
    border-right: 7rpx dashed transparent;
}

.roomOther-info {
    position: relative;
    padding-right: 20rpx;
    margin-left: 30rpx;
    font-size: 26rpx;
}

.other-title {
    width: 384rpx;
    margin-right: 30rpx;
    font-size: 32rpx;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    color: #000;
}

.detail-info {
    height: 40rpx;
    line-height: 40rpx;
    padding-right: 24rpx;
    color: #666;
}

.detail-info .rent-type {
    font-weight: 700;
}

.distence-info {
    display: block;
    margin-top: 5rpx;
    width: 420rpx;
    height: 40rpx;
    line-height: 40rpx;
    color: #666;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.distence-info image {
    width: 24rpx;
    height: 25rpx;
}

.otherroom-tag {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    font-size: 22rpx;
    margin-top: 10rpx;
}

.otherroom-tag view {
    border: 2rpx solid rgba(83,190,191,.5);
    color: #53bebf;
    padding: 5rpx 10rpx;
    margin-right: 15rpx;
    border-radius: 6rpx;
}

.room-price {
