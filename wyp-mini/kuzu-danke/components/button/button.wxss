.bottom-btn {
    background: #fff;
    box-shadow: 0 -6rpx 10rpx 0 rgba(33,33,33,.08);
    border-radius: 6rpx;
    font-size: 32rpx;
    color: #fff;
    text-align: center;
    line-height: 48rpx;
    position: fixed;
    width: 750rpx;
    bottom: 0;
    left: 0;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    z-index: 5;
}

.bottom-btn button {
    border-radius: 6rpx;
    height: 92rpx;
    line-height: 92rpx;
    border: none;
}

.bottom-btn .main {
    width: 686rpx;
    font-size: 32rpx;
    background: #3dbcc6;
    margin: 18rpx 32rpx;
}

.bottom-btn .double-button {
    width: 330rpx;
    margin: 18rpx 0;
    font-size: 32rpx;
    background: #3dbcc6;
}

.bottom-btn .double-button:nth-of-type(1) {
    margin-left: 32rpx;
    margin-right: 26rpx;
}

.bottom-btn button.disabled {
    background: #e4e4e4;
}