.wraper {
    position: fixed;
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 100%;
}

.header {
    flex: 0 0 100rpx;
}

.main {
    flex: 1;
    position: relative;
}

.Search-section {
    background-color: #fff;
    padding: 5px 10px;
    border: 1px #eee solid;
    margin-top: 0px;
}

.Search-pancel {
    display: flex;
    flex-direction: row;
}

.Search-input {
    flex: 8;
    background-color: #eee;
    padding: 5px 10px;
    font-size: 16px;
    text-align: center;
}

.Search-button {
    flex: 1;
    border: none!important;
    color: cornflowerblue!important;
}

.find {
    font-size: 16px;
}

.table {
    border: 1px solid #0FAEFF;
