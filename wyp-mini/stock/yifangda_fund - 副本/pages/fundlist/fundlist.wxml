<view bindtap="back" class="goback">
    <view>
        <image src="../image/back.jpg" style="width:25px;height:20px;"></image>
    </view>
    <view class="bk">返回</view>
</view>
<view class="list1">
    <view wx:if="{{fundstyle==='stockfund'}}">
        <view wx:for="{{stockfundstyle}}" wx:for-item="it">
            <view class="head">
                <view>{{it.style}}</view>
            </view>
            <view class="list2">
                <view wx:if="{{item.subtype===it.style}}" wx:for="{{stockfund}}" wx:for-index="idx">
                    <view class="item">
                        <view bindtap="getcode" class="name" id="{{item.code}}">
                            <view bindtap="gogogo">
        {{item.name}}
      </view>
                        </view>
                    </view>
                    <view class="hr2"></view>
                </view>
            </view>
