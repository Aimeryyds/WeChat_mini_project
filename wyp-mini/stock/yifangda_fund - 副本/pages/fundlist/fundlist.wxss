.goback {
    position: fixed;
    display: flex;
    flex-direction: row;
    margin-top: 0px;
    top: 0;
    width: 100%;
    background-color: #ffffff;
}

.bk {
    color: #0FAEFF;
}

.head {
    display: flex;
    flex-direction: row;
    margin: 10px;
    font-size: 18px;
    color: #999999;
}

.list1 {
    display: flex;
    flex-direction: column;
    flex-wrap: wrap;
    justify-content: space-around;
    margin-left: 5px;
    margin-right: 5px;
    margin-top: 30px;
}

.list2 {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
}

.item {
    display: flex;
    flex-direction: row;
    margin-left: 10px;
    margin-right: 10px;
    margin-bottom: 30px;
}

.desc {
    float: left;
    margin-left: 10px;
    line-height: 30px;
}

.name {
    font-weight: bold;
    font-size: 17px;
}

.hr2 {
    height: 8px;
    background-color: #ffffff;
}

.count {
    display: flex;
    flex-direction: row;
    font-size: 12px;
    color: #999999;
    margin-left: 0px;
}