<view class="i-class i-tab-bar {{fixed?'i-tab-bar-fixed':''}}">
    <slot></slot>
    <view class="i-tab-bar-list">
        <view bindtap="handleClickItem" class="i-tab-bar-layer" data-key="{{item.key}}" style="width: {{100/list.length}}%" wx:for="{{list}}" wx:key="{{ item.key }}"></view>
    </view>
</view>
