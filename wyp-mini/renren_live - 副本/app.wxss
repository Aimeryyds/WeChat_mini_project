.page_container {
    position: absolute;
    z-index: 0;
    width: 100%;
    background: #fff;
}

@-webkit-keyframes tobig {
    0% {
        transform: scale(0.9);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(0.9);
    }
}

@keyframes tobig {
    0% {
        transform: scale(0.9);
    }

    50% {
        transform: scale(1.1);
    }

    100% {
        transform: scale(0.9);
    }
}