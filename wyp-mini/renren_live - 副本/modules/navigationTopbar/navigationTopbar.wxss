.navTop_con {
    position: fixed;
    top: 0rpx;
    left: 0;
    right: 0rpx;
    z-index: 100;
}

.nav_box {
    position: relative;
    width: 100%;
    height: 46px;
}

.nav_back {
    position: absolute;
    width: 46px;
    height: 46px;
    margin-left: 0px;
    margin-top: 0px;
}

.nav_back .back_img {
    margin-top: 11px;
    margin-left: 11px;
    width: 24px;
    height: 24px;
}

.nav_text {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 46px;
    line-height: 46px;
    font-size: 14pt;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: black;
}

.nav_text .txt_img {
    margin-top: 13px;
    width: 214rpx;
    height: 50rpx;
}