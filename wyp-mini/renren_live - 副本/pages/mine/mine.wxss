}

.dengji_pro {
    position: relative;
    left: 20rpx;
    float: left;
    width: 270rpx;
    height: 32rpx;
    border-radius: 16rpx;
    background: #E5E5EA;
    overflow: hidden;
}

.dengji_pro text {
    position: absolute;
    height: 32rpx;
}

.dengji_pro .pro_line {
    width: 0%;
    background: #FFD700;
}

.dengji_pro .pro_num {
    line-height: 32rpx;
    width: 100%;
    color: #fff;
    font-size: 18rpx;
    text-align: center;
}

.wallet {
    padding: 0rpx 32rpx 0rpx 47rpx;
    height: 120rpx;
    vertical-align: middle;
    border-bottom: 1px solid #EBECF1;
}

.wallet_icon {
    float: left;
    margin-top: 36rpx;
    margin-right: 45rpx;
    width: 48rpx;
    height: 48rpx;
}

.wallet_txt {
    float: left;
    height: 120rpx;
    line-height: 120rpx;
    font-size: 32rpx;
    color: #333;
}

.item_icon {
    float: left;
    margin-top: 35rpx;
    margin-bottom: 35rpx;
    margin-right: 45rpx;
    width: 48rpx;
    height: 48rpx;
}

.item_txt {
    float: left;
    height: 110rpx;
    line-height: 110rpx;
    font-size: 32rpx;
    color: #333;
}

.wallet .charge_btn {
    float: right;
    margin-top: 28rpx;
    width: 144rpx;
    height: 64rpx;
}

.item_btn {
    float: right;
    margin-top: 31rpx;
    width: 48rpx;
    height: 48rpx;
}

.list_item {
    padding: 0rpx 15rpx 0rpx 40rpx;
    height: 110rpx;
    overflow: hidden;
    vertical-align: middle;
    border-bottom: 1px solid #EBECF1;
}

.list_item .item_num {
    float: right;
    margin-right: 20rpx;
    height: 110rpx;
    line-height: 110rpx;
    color: #1994fb;
    font-size: 26rpx;
}

.list_item .item_dingyue {
    margin: 19rpx 5rpx 0rpx 20rpx;
    float: right;
    width: 140rpx;
    height: 72rpx;
    line-height: 72rpx;
    color: #4e4e4e;
    font-size: 28rpx;
    text-align: center;
    border-radius: 36rpx;
    border: 1px solid #999;
}

.fruit {
    padding: 38rpx 0rpx;
    text-align: center;
}

.fruit .fruit_num {
    font-size: 48rpx;
    color: #ffa500;
}

.fruit .fruit_txt {
    font-size: 26rpx;
    color: #999;
}

.list_item:last-child {
    border-bottom: none;
}

.list_item {
    position: relative;
}

.btn_servers {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    padding: 0rpx 15rpx 0rpx 40rpx!important;
    margin: 0!important;
    border: 0!important;
    border: none!important;
    background: #fff!important;
    outline: none!important;
}

.btn_servers::after {
    border: none;
}

.guessYouLike {
    position: fixed;
    width: 100%;
    left: 0;
    right: 0;
    bottom: -220rpx;
    background: #fff;
    box-shadow: -2px 0 3px -1px #f2f2f2,0 -2px 3px -1px #f2f2f2,0 2px 3px -1px #f2f2f2,2px 0 3px -1px #f2f2f2;
}

.toTop {
    animation: toTopcss ease-out 0.5s;
    animation-fill-mode: forwards;
}

@keyframes toTopcss {
    from {
        bottom: -220rpx;
    }

    to {
        bottom: 0;
    }
}

@-webkit-keyframes toTopcss {
    from {
        bottom: -220rpx;
    }

    to {
        bottom: 0;
    }
}

.guessYouLike .title {
    position: absolute;
    left: 50%;
    top: -39rpx;
    transform: translateX(-50%);
    width: 180rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 20rpx;
    color: #565658;
    background: #fff;
    border-top-left-radius: 16rpx;
    border-top-right-radius: 16rpx;
    box-shadow: -2px 0 3px -1px #f2f2f2,0 -2px 3px -1px #f2f2f2,0 2px 3px -1px #fff,2px 0 3px -1px #f2f2f2;
}

.guessYouLike .title .line {
    display: inline-block;
    width: 16rpx;
    height: 1px;
    background: #dddde1;
}

.guessYouLike .title .line.left {
    margin-right: 15rpx;
}

.guessYouLike .title .line.right {
    margin-left: 15rpx;
}

.left_btn {
    position: absolute;
    left: -30rpx;
    z-index: 0;
    width: 60rpx;
    height: 200rpx;
    background: #fff;
    transform: skew(13deg);
    box-shadow: -2px 0 3px -1px #f2f2f2,0 -2px 3px -1px #f2f2f2,0 2px 3px -1px #f2f2f2,2px 0 3px -1px #f2f2f2;
}

.list_con {
    display: flex;
    justify-content: space-around;
    flex-direction: row;
    position: relative;
    z-index: 1;
    padding: 12rpx 0rpx;
    overflow: hidden;
    background: #fff;
}

.list_con .item {
    position: relative;
    float: left;
    width: 176rpx;
    height: 176rpx;
}

.list_con .item .img {
    float: left;
    width: 176rpx;
    height: 176rpx;
}

.videoicon {
    position: absolute;
    top: 0;
    left: 0;
    margin: 6rpx 6rpx;
    z-index: 1;
    width: 76rpx;
    height: 32rpx;
    line-height: 32rpx;
    border-radius: 6rpx;
    background: rgba(0,0,0,0.3);
}

.videoicon .icon_txt {
    float: left;
    color: #fff;
    font-size: 16rpx;
    height: 32rpx;
    line-height: 32rpx;
}

.videoicon .video_icon {
    float: left;
    margin: 11rpx 8rpx;
    width: 8rpx;
    height: 8rpx;
    animation: tobig 2s linear infinite;
}