}

.mine_list {
    margin-bottom: 240rpx;
}

.user_con {
    width: 100%;
    height: 244rpx;
    overflow: hidden;
}

.signMine {
    float: right;
    margin-top: 80rpx;
    margin-right: 44rpx;
    width: 86rpx;
    height: 90rpx;
}

.user_header {
    float: left;
    width: 197rpx;
    height: 243rpx;
}

.user_header .header_url {
    float: right;
    margin-top: 50rpx;
    width: 143rpx;
