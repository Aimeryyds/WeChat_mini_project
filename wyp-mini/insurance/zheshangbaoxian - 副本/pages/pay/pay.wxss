.warp {
    min-height: calc(100vh);
    background: #F7F7F7 100%;
}

.realcheck {
    padding-top: 30px;
}

.pay .result {
    background: #fff;
    padding: 15px 0;
    text-align: center;
}

.pay .content {
    background: #fff;
}

.pay .content .line {
    display: flex;
    justify-content: space-between;
}

.line {
    font-size: 14px;
    padding: 10px;
}

.line .line-title {
    color: #737373;
}

.pay .btns {
    color: #E60012;
    border-radius: 20px;
    margin: 20rpx 20px;
    width: auto;
    border: 1px solid #E60012;
}

.pay .btns.backhome {
    background: none;
    background: #E60012;
    color: #fff;
}