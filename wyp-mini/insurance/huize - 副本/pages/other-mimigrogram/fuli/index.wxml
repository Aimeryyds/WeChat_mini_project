<view bindtap="go" class="containner">
    <view class="pg-bg" style="height:{{height}}px">
        <image src="https://img.huizecdn.com/hz/touch/mini-program/fuli/poster_bg.png" style="height:{{height}}px"></image>
    </view>
    <view class="man-bg" style="height:{{height}}px">
        <image src="https://img.huizecdn.com/hz/touch/mini-program/fuli/poster_person.png"></image>
    </view>
    <view class="pot-bg" style="height:{{potHeight}}px">
        <image src="https://img.huizecdn.com/hz/touch/mini-program/fuli/poster_bg_pot.png"></image>
    </view>
    <view class="content">
        <view class="logo">
            <image src="https://img.huizecdn.com/hz/touch/mini-program/fuli/poster_logo.png"></image>
        </view>
        <view class="title">
            <image src="https://img.huizecdn.com/hz/touch/mini-program/fuli/poster_title.png" style="height"></image>
        </view>
    </view>
</view>
