page {
    background: #fff;
    box-sizing: border-box;
    color: #333;
    font-size: 14px;
}

.dialog-wrap {
    padding: 40rpx;
}

.container {
    padding: 0 70rpx 70rpx 70rpx;
}

.container image {
    width: 338rpx;
    height: 344rpx;
    margin: 57rpx 0 33rpx;
}

.center {
    text-align: center;
}

.step {
    font-size: 40rpx;
    font-weight: bold;
    margin: 30rpx 0;
}

.btn {
    height: 90rpx;
    width: 100%;
    color: #fff;
    border-radius: 45rpx;
    font-size: 32rpx;
    margin-top: 54rpx;
    background: linear-gradient(to right,#F04D16,#FE9219);
}