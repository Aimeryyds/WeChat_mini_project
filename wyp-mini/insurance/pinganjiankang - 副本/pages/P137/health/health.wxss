page {
    padding: 20rpx;
    line-height: 1.8;
    box-sizing: border-box;
}

.health1 {
    height: 50px;
    line-height: 50px;
    text-align: center;
    font-weight: bold;
}

.health2 {
    padding-bottom: 50px;
    margin-top: 20rpx;
}

.health3 {
    width: 50%;
    float: left;
    height: 45px;
    text-align: center;
    line-height: 45px;
    border-top: 1px solid #ddd;
    background-color: #fff;
    color: #ddd;
    border-bottom: 1px solid #ddd;
}

.health4 {
    width: 50%;
    float: left;
    height: 47px;
    text-align: center;
    line-height: 45px;
    background-color: #f0a403;
    color: #fff;
    bottom: 0;
}

.health21 {
    font-weight: bold;
}