page {
    font-size: 26rpx;
    color: #898989;
    background: #75c497;
}

image {
    width: 100%;
    display: block;
}

.tiaokuan {
    width: 710rpx;
    background: #fff;
    margin: 0 auto;
    overflow: hidden;
}

.tiaokuan view view {
    width: 186rpx;
    height: 60rpx;
    float: left;
    line-height: 60rpx;
    text-align: center;
    background: #49b069;
    color: #fff;
    font-size: 30rpx;
}

.tiaokuan view view:nth-child(1) {
    margin-left: 30rpx;
}

.middle {
    margin: 0 40rpx;
}

.tiaokuan text {
    color: #49b069;
}