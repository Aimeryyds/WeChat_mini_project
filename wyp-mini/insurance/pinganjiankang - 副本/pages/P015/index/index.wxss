.pa-nav-flex {
    background-color: #fff;
    position: sticky;
    top: 0;
    z-index: 10;
}

.pa-nav-li text {
    display: flex;
    justify-content: center;
    align-items: center;
    border-bottom: 4px solid #fff;
}

.pa-nav-li {
    display: flex;
    justify-content: center;
    flex: 0 0 auto;
    width: 33.333vw;
    height: 50px;
    font-size: 16px;
}

.pa-nav-li.pa-active text {
    color: #fc621d;
    border-bottom: 4px solid #fc621d;
}

page {
    font-size: 28rpx;
    color: #333;
