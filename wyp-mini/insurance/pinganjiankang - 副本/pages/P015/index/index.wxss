.triangle-down {
    width: 0;
    height: 0;
    border-radius: 0;
    border-left: 5px solid transparent;
    border-right: 5px solid transparent;
    border-top: 8px solid #fc621d;
    position: absolute;
    bottom: -8px;
    left: 50%;
    transform: translateX(-50%);
}

#green,#red,#yellow {
