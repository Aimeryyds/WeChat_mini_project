.radio-group {
    margin-left: 50rpx;
}

.radio-group label {
    display: inline-block;
    border: 1px solid #e0e0e0;
    width: 138rpx;
    height: 70rpx;
    margin-right: 30rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 24rpx;
    color: #333;
    position: relative;
    border-radius: 12rpx;
    overflow: hidden;
    box-sizing: border-box;
}

.radio-group label.active {
    border: 1px solid #fc611d;
}

.radio-group label radio {
    display: none;
}

.radio-group label .checked {
    display: inline-block;
    position: absolute;
    right: 0;
    bottom: 0;
    width: 34rpx;
    height: 36rpx;
    margin: 0;
    padding: 0;
}

.txxx .bbr {
    margin-top: 32rpx;
}

.txxx .bbr .flex-row {
    margin-bottom: 24rpx;
}

.txxx .bbr .flex-row view {
    border-bottom: 0;
}

.txxx .bbr .flex-row view input {
    width: 514rpx;
    height: 72rpx;
    border: 1px solid #e0e0e0;
    border-radius: 5px;
    padding-left: 20rpx;
}

.plan .radio-group label {
    width: 295rpx;
