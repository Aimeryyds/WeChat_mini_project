}

.section {
    width: 100%;
}

image {
    width: 100%;
    display: block;
}

.qst {
    width: 15px;
    height: 15px;
    line-height: 15px;
    color: #fff;
    text-align: center;
    border-radius: 50%;
    background: #a6a6a6;
    display: inline-block;
    margin-left: 16rpx;
}

.plantab {
    display: flex;
    width: 605rpx;
    padding: 20rpx 0 48rpx 0;
    margin: auto;
}

.plantab>view {
    flex: 1;
    box-sizing: border-box;
    border: 1px solid #fc621d;
    height: 65rpx;
    line-height: 65rpx;
    text-align: center;
    font-size: 32rpx;
    color: #fc621d;
    position: relative;
}

.plantab>view.active {
    background: #fc621d;
    color: #ffffff;
}

.plantab>view:first-child {
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}

.plantab>view:nth-child(2) {
    border-left: 0px;
    border-right: 0px;
}

.plantab>view:nth-child(3) {
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}

.flex-row {
    display: flex;
    direction: row;
}

.flex-row.header {
    padding: 36rpx 34rpx 32rpx 30rpx;
    border-bottom: 1px solid #e0e0e0;
}

.flex-row.content {
    padding: 17rpx 32rpx;
}

.flex-row.content.has-pdtop {
    padding-top: 34rpx;
}

.flex-row.content.has-pdbottom {
    padding-bottom: 34rpx;
}

.flex-row .title {
    font-size: 34rpx;
    color: #333;
    font-weight: bold;
}

.flex-row .orange {
    color: #ff5105;
}

.flex-row view:nth-child(2) {
    flex: 2;
}

.flex-row.fixNav view {
    flex: 1;
}

.flex-row.baofei {
    padding: 30rpx 32rpx 34rpx 0;
    margin-left: 32rpx;
}

.pull-right {
    text-align: right;
}

.blue {
