.success_icon {
    display: flex;
    flex-direction: column;
    align-items: center;
    margin: 20px 0 0 0;
}

.success_icon image {
    width: 170rpx;
    height: 170rpx;
}

.success_text {
    color: #333;
    margin: 0 auto;
    font-weight: bold;
    font-size: 34rpx;
}

.success_detail {
    font-size: 24rpx;
    color: #999;
    margin: 36rpx;
}

button.confirm {
    background-color: #fff;
    color: #42C196;
    font-size: 30rpx;
    padding: 0 20px 0 20px;
    width: 270rpx;
    border: 1px solid #42C196;
}

button.confirm:active {
    background: #ccc;
}

.auto {
    margin: 72rpx auto 0;
    padding: 38rpx;
    text-align: left;
    background: #FAFAFA;
    width: 690rpx;
    box-sizing: border-box;
    display: flex;
}

.auto .img {
    width: 90rpx;
    height: 90rpx;
}

.auto .arr_r {
    width: 7px;
    height: 12px;
    vertical-align: middle;
    margin-left: 40rpx;
}

.auto .big {
    font-size: 34rpx;
    color: #333;
    font-weight: bold;
    margin-left: 26rpx;
}

.auto .small {
    font-size: 26rpx;
    color: #999;
    margin: 14rpx 20rpx 0 26rpx;
}

.gzh {
    display: flex;
}

.gzh image {
    width: 200px;
    height: 200px;
    margin: auto;
}

.text {
    color: #999;
    text-align: center;
    font-size: 26rpx;
}

.text image {
    width: 300rpx;
    height: 300rpx;
    margin-top: 140rpx;
}

.dialog_title {
    color: #666;
    font-size: 24rpx;
    margin: 38rpx 34rpx 0;
    display: flex;
    line-height: 1.8;
    padding-bottom: 34rpx;
}

.dialog_title .warn {
    width: 26rpx;
    height: 26rpx;
    margin-right: 8rpx;
}

.dialog_body {
    margin: 0 34rpx 0;
    font-size: 14px;
    color: #333;
    border-top: 1px solid #eee;
    padding-top: 34rpx;
}

.dialog_body view {
    display: flex;
    margin-bottom: 20rpx;
}

.dialog_body view view {
    flex: 1;
}

.dialog_body view view.right {
    color: #999;
}

.agreement {
    margin: 0 30rpx 34rpx;
    color: #999;
    font-size: 24rpx;
    display: flex;
    align-items: center;
}

.blue {
    color: #508CEE;
}

.checkbox {
    transform: scale(0.6);
}

.yzm {
    height: 24px;
    line-height: 22px;
    font-size: 12px;
    width: 100px;
}

.vant-popup {
    width: 90%;
}

.button-btm {
    width: 300rpx;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    color: #FF6600;
    border: 1px solid #FF6600;
    background: #fff;
    margin-bottom: 30rpx;
    border-radius: 70rpx;
}