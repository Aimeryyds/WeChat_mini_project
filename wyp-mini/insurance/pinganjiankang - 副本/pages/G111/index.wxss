page {
    font-size: 26rpx;
    color: #333;
}

.section {
    width: 100%;
}

.qst {
    width: 15px;
    height: 15px;
    line-height: 15px;
    color: #fff;
    text-align: center;
    border-radius: 50%;
    background: #a6a6a6;
    display: inline-block;
    margin-left: 16rpx;
}

.flex-row {
    display: flex;
    direction: row;
}

.flex-row.header {
    padding: 36rpx 34rpx 32rpx 30rpx;
    border-bottom: 1px solid #e0e0e0;
}

.flex-row.content {
    padding: 17rpx 32rpx;
}

.flex-row.content.has-pdtop {
    padding-top: 34rpx;
}

.flex-row.content.has-pdbottom {
    padding-bottom: 34rpx;
}

.flex-row .title {
    font-size: 34rpx;
    color: #333;
    font-weight: bold;
}

.flex-row .orange {
    color: #ff5105;
}

.flex-row view:nth-child(2) {
    flex: 2;
}

.flex-row.fixNav view {
    flex: 1;
}

.flex-row.baofei {
    padding: 34rpx 32rpx 34rpx 0;
    margin-left: 32rpx;
    border-top: 1px solid #e0e0e0;
}

.pull-right {
    text-align: right;
}

.blue {
    color: #427eb2;
    font-size: 26rpx;
}

.stand {
    height: 20rpx;
    background: #f0f0f0;
}

.fixNav {
    z-index: 1;
    background: #fff;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 26rpx;
}

.fixNav view {
    flex: 1;
    text-align: center;
}

.fixNav view.active {
    color: #fe5a12;
    border-bottom: 1px solid #fe5a12;
}

#cpxq {
}

.img-wrap img {
    width: 100%;
    display: block;
}

.fixed {
    position: fixed;
    top: 0;
}

.txxx .title {
    height: 100rpx;
    line-height: 100rpx;
    font-size: 34rpx;
    font-weight: bold;
    border-bottom: 1px solid #e0e0e0;
    padding-left: 34rpx;
}

.txxx .qst {
    background: #fe5a12;
    margin: 48rpx 14rpx 30rpx 32rpx;
}

.txxx .flex-row {
    height: 68rpx;
    line-height: 68rpx;
    padding-left: 34rpx;
}

.txxx .flex-row view {
    border-bottom: 1rpx solid #e0e0e0;
}

.txxx .flex-row view:nth-child(1) {
    flex: 1;
}

.txxx .flex-row view:nth-child(2) {
    flex: 2;
}

.txxx .flex-row input {
    height: 68rpx;
    line-height: 68rpx;
}

.radio-group {
    margin-left: 50rpx;
}

.radio-group label {
    display: inline-block;
    border: 1px solid #e0e0e0;
    width: 138rpx;
    height: 70rpx;
    margin-right: 30rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 24rpx;
    color: #333;
    position: relative;
    border-radius: 12rpx;
    overflow: hidden;
}

.radio-group label.active {
    border: 1px solid #fc611d;
}

.radio-group label radio {
    display: none;
}

.radio-group label .checked {
    display: inline-block;
    position: absolute;
    right: 0;
    bottom: 0;
    width: 40rpx;
    height: 40rpx;
}

.txxx .bbr {
    margin-top: 32rpx;
}

.txxx .bbr .flex-row {
    margin-bottom: 24rpx;
}

.txxx .bbr .flex-row view {
    border-bottom: 0;
}

.txxx .bbr .flex-row view input {
    width: 360rpx;
    height: 72rpx;
    border: 1px solid #e0e0e0;
    padding-left: 10rpx;
}

.plan .radio-group label {
    width: 295rpx;
    margin: 50rpx 30rpx 80rpx 0;
}

.plan .radio-group label:nth-of-type(1) {
    margin-left: 28rpx;
}

.bottom {
    background: #f0f0f0;
    padding-top: 30rpx;
}

.bottom .provisions {
    background: #ffffff;
    height: 70rpx;
    line-height: 70rpx;
    padding-left: 30rpx;
}

.bottom .btm-nav {
    background: #ffffff;
    height: 110rpx;
    line-height: 110rpx;
    text-align: center;
    display: flex;
}

.bottom .btm-nav .icon {
    width: 105rpx;
    height: 72rpx;
}

.bottom .btm-nav .icon:nth-child(1) {
    border-right: 1px solid #d3d3d3;
}

.bottom .price {
    font-size: 34rpx;
    font-weight: bold;
    color: #ff5105;
    width: 275rpx;
}

.bottom .buy {
    font-size: 34rpx;
    font-weight: bold;
    background: #fc611d;
    color: #ffffff;
    flex: 1;
}