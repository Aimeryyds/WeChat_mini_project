<form>
    <view class="cu-form-group">
        <view class="title">产品名称</view>{{insrcardOrder.productName}}
	</view>
    <view class="cu-form-group">
        <view class="title">购买方案</view>{{insrcardOrder.caseName}}
	</view>
    <view class="cu-form-group">
        <view class="title">金额</view>{{insrcardOrder.amounts}}元
	</view>
    <view class="cu-form-group">
        <view class="title">购买时间</view>{{insrcardOrder.showCreateTime}}
	</view>
    <view class="cu-form-group">
        <view class="title">联系人</view>{{insrcardOrder.orderContacts}}
	</view>
    <view class="cu-form-group">
        <view class="title">联系方式</view>{{insrcardOrder.orderTelephone}}
	</view>
    <view class="cu-form-group">
        <view class="title">地址</view>{{insrcardOrder.orderAddress}}
	</view>
    <view class="cu-form-group">
        <view class="title">快递公司</view>{{insrcardOrder.courierName}}
	</view>
    <view class="cu-form-group">
        <view class="title">快递单号</view>{{insrcardOrder.courierNumber}}
	</view>
    <view class="cu-form-group">
        <view class="title">订单状态</view>{{insrcardOrder.stateName}}
	</view>
    <view class="cu-load load-modal" wx:if="{{loadModal}}">
        <view class="gray-text">加载中...</view>
    </view>
</form>
