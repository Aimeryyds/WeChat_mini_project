<view class="login-container">
    <view class="login">
        <view class="login-banner">
            <image class="banner-image" src="/images/ava.png"></image>
        </view>
        <view class="login-title">电动车保险</view>
        <view class="cu-form-group phoneGroup">
            <view class="title text-right margin-left">姓名</view>
            <input class="title" placeholder="请输入姓名"></input>
        </view>
        <view class="cu-form-group" style="border: 0px;border-bottom:1px solid #cecece;margin-top: 40rpx;">
            <view class="title ">身份证</view>
            <input class="title" placeholder="请输入身份证号"></input>
        </view>
        <button bindtap="goHome" class="cu-btn block bg-blue margin-tb-sm lg btn">提交认证</button>
    </view>
</view>
