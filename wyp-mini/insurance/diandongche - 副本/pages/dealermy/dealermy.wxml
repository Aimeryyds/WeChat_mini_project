<view class="cu-bar bg-white margin-top solid-bottom">
    <view class="action">
        <text class="cuIcon-title text-blue"></text> 经销商设置
  </view>
</view>
<view class="cu-list menu solid-top">
    <view class="cu-item">
        <view bindtap="goRegister" class="content" data-registerType="renew">
            <text class="cuIcon-edit text-blue"></text>
            <text class="text-black">修改登录密码</text>
        </view>
    </view>
    <view class="cu-item">
        <view bindtap="goDealerAddrList" class="content">
            <text class="cuIcon-deliver text-blue"></text>
            <text class="text-black">管理收货地址</text>
        </view>
    </view>
</view>
<view class="padding flex flex-direction margin-bottom-sm shadow bottom_footer">
    <button bindtap="goLogout" class="cu-btn line-blue lg">退出登录</button>
</view>
