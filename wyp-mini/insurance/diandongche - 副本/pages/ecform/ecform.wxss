.cu-form-group .title {
    min-width: calc(4em + 30rpx);
}

.top-container {
    width: 92%;
    border-radius: 6rpx;
    margin: 0 auto;
    text-align: center;
}

.bottom_footer {
    position: absolute;
    bottom: 0;
    left: 50%;
    width: 100%;
    transform: translate(-50%,-1%);
}

.bottom_button {
    width: 98%;
}

.scrollPage {
    height: auto;
}