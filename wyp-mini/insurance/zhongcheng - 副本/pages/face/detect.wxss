page {
    height: 100%;
}

.contain-view {
    height: 100%;
    width: 100%;
}

.contain {
    height: 100%;
    width: 100%;
}

.face-contain {
    height: 100%;
    width: 100%;
}

.tip-text {
    width: 100%;
    display: flex;
    justify-content: center;
    color: red;
}

.camera-view {
    height: 100%;
    width: 100%;
}

.camera-img {
    position: absolute;
    top: 90%;
    left: 48%;
    width: 80rpx;
    height: 80rpx;
}

.sm1 {
    position: absolute;
    width: 45px;
    height: 45px;
    top: 6px;
    left: 6px;
}

.sm2 {
    position: absolute;
    top: 6px;
    right: 6px;
    width: 45px;
    height: 45px;
}

.sm3 {
    position: absolute;
    bottom: 6px;
    left: 6px;
    width: 45px;
    height: 45px;
}

.sm4 {
    position: absolute;
    right: 6px;
    bottom: 6px;
    width: 45px;
    height: 45px;
}

.animation-element {
    height: 220rpx;
    position: absolute;
    left: 12px;
    top: 12px;
}

.sm-top {
    position: absolute;
    width: 100%;
    height: 12px;
    top: 0rpx;
    background-color: rgba(0,0,0,1);
}

.sm-left {
    position: absolute;
    width: 12px;
    height: 100%;
    top: 0rpx;
    left: 0rpx;
    background-color: rgba(0,0,0,1);
}

.sm-right {
    position: absolute;
    width: 12px;
    height: 100%;
    right: 0rpx;
    top: 0rpx;
    background-color: rgba(0,0,0,1);
}

.sm-bottom {
    position: absolute;
    width: 100%;
    height: 12px;
    bottom: 0rpx;
    background-color: rgba(0,0,0,1);
}