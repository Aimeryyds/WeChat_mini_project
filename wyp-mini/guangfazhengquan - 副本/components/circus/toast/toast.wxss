.v-toast .wrapper {
    position: fixed;
    width: 80%;
    height: 20px;
    top: 42%;
    z-index: 9999;
    text-align: center;
    left: 10%;
}

.v-toast .text {
    display: inline-block;
    max-width: 80%;
    min-width: 50%;
    padding: 35rpx 50rpx;
    background: rgba(40,40,40,0.85);
    border-radius: 5px;
    color: #fff;
    font-size: 32rpx;
}