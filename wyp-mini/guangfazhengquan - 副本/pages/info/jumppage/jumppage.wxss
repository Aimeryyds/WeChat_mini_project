page {
    height: 100%;
    background-color: #fff;
    position: relative;
}

.protocol {
    text-align: justify;
    font-size: 32rpx;
    padding: 5%;
}

.protocol .protocol-title {
    font-size: 50rpx;
    text-align: center;
}

.protocol .protocol-box {
    text-indent: 2em;
    font-weight: bold;
    margin-top: 60rpx;
}

.protocol .protocol-content {
    text-indent: 2em;
    margin-top: 20rpx;
    line-height: 2em;
}

.protocol .protocol-bottom {
    margin-top: 40rpx;
    text-align: right;
}

.protocol .protocol-time {
    margin-top: 20rpx;
    text-align: right;
    margin-bottom: 50rpx;
}