.v-quote-stocks .head {
    box-sizing: border-box;
    display: table;
    width: 100%;
    height: 30px;
    vertical-align: middle;
    padding: 5px 20px 5px 0;
}

.v-quote-stocks .head .cell {
    display: table-cell;
    vertical-align: middle;
    color: #999;
    font-size: 14px;
}

.v-quote-stocks .head .stock-name {
    width: 44%;
}

.v-quote-stocks .head .stock-now {
    width: 28%;
    text-align: center;
}

.v-quote-stocks .head .stock-rise {
    width: 28%;
    text-align: right;
}

.v-quote-stocks .body.c-line-bottom:before {
    position: absolute;
    bottom: -10px;
    border: 1px solid #f00;
}

.v-quote-stocks .body .row {
    position: relative;
    box-sizing: border-box;
    display: table;
    width: 100%;
    vertical-align: middle;
    padding: 9px 20px 9px 0;
    z-index: 1;
}

.v-quote-stocks .body .row.c-line-top:before {
    top: -2px;
}

.v-quote-stocks .body .cell {
    display: table-cell;
    vertical-align: middle;
}

.v-quote-stocks .body .stock {
    width: 100%;
    height: 100%;
}

.v-quote-stocks .body .stock-name-code {
    margin-top: 2px;
    width: 45%;
}

.v-quote-stocks .body .stock-name {
    margin-top: 1px;
    font-size: 18px;
    line-height: 18px;
    height: 18px;
    color: #333;
}

.v-quote-stocks .body .stock-code {
    margin-top: 8px;
    font-size: 14px;
    line-height: 14px;
    color: #999;
}

.v-quote-stocks .body .stock-now {
    position: relative;
    font-size: 18px;
    line-height: 18px;
    font-weight: bold;
    width: 28%;
    text-align: center;
    color: #333;
}

.v-quote-stocks .body .stock-rise,.v-quote-stocks .body .stock-handover {
    position: relative;
    top: 2px;
    font-size: 18px;
    line-height: 18px;
    font-weight: bold;
    width: 28%;
    text-align: right;
    color: #999;
}

.v-quote-stocks .body .stock-handover {
    color: #666;
}

.v-paging {
    position: relative;
    width: 100%;
    height: 60px;
    font-size: 13px;
    color: #999;
}

.v-paging .loading .icon-loading {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -40px;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background-image: url("data:image/png;base64,");
    background-size: 20px 20px;
    background-repeat: no-repeat;
    -webkit-animation: kf-v-loading-rotate 2s steps(1,end) infinite;
}

@-webkit-keyframes kf-v-loading-rotate {
    from {
        -webkit-transform: rotateZ(0);
    }

    8.33333% {
        -webkit-transform: rotateZ(30deg);
    }

    16.66666% {
        -webkit-transform: rotateZ(60deg);
    }

    25% {
        -webkit-transform: rotateZ(90deg);
    }

    33.33333% {
        -webkit-transform: rotateZ(120deg);
    }

    41.66666% {
        -webkit-transform: rotateZ(150deg);
    }

    50% {
        -webkit-transform: rotateZ(180deg);
    }

    58.33333% {
        -webkit-transform: rotateZ(210deg);
    }

    66.66666% {
        -webkit-transform: rotateZ(240deg);
    }

    75% {
        -webkit-transform: rotateZ(270deg);
    }

    83.33333% {
        -webkit-transform: rotateZ(300deg);
    }

    91.66666% {
        -webkit-transform: rotateZ(330deg);
    }

    to {
        -webkit-transform: rotateZ(360deg);
    }
}

.v-paging .loading .text {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -10px;
    margin-top: -10px;
    width: 60px;
    height: 20px;
    line-height: 20px;
}

.v-paging .done,.v-paging .hide,.v-paging .more,.v-paging .fail {
    width: 100%;
    height: 60px;
    line-height: 60px;
    text-align: center;
}

.v-network {
    position: fixed;
    top: -36px;
    z-index: 9999;
    width: 100%;
    height: 36px;
}

.v-network>.notice {
    position: absolute;
    top: 0;
    width: 100%;
    height: 36px;
}

.v-network>.notice .icon {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background-size: cover;
}

.v-network>.notice .msg {
    position: absolute;
    left: 50%;
    line-height: 36px;
    font-size: 14px;
    color: #fff;
}

.v-network .error {
    background-color: #f70e22;
}

.v-network .error .icon {
    margin-left: -90px;
    background-image: url("data:image/png;base64,");
}

.v-network .error .msg {
    margin-left: -60px;
}

.v-network .recover {
    background-color: #51c332;
}

.v-network .recover .icon {
    margin-left: -60px;
    background-image: url("data:image/png;base64,");
}

.v-network .recover .msg {
    margin-left: -30px;
}

.v-quote-explore-board {
    padding-left: 15px;
    margin-bottom: 10px;
}

.v-quote-explore-board .head,.v-quote-explore-board .body {
    padding-right: 5px;
}

.v-quote-explore-board .body .row {
    padding-right: 0;
}

.v-quote-explore-board .board-name,.v-quote-explore-board .board-rise {
    font-size: 18px;
    line-height: 18px;
    height: 18px;
}

.v-quote-explore-board .board-name {
    width: 35%;
}

.v-quote-explore-board .board-rise {
    width: 30%;
    text-align: center;
    font-weight: bold;
}

.v-quote-explore-board .stock-name-code {
    width: 35%;
    text-align: center;
}

.v-quote-explore-board .stock-name-code .stock-name {
    font-size: 16px;
    line-height: 16px;
}