.v-paging {
    position: relative;
    width: 100%;
    height: 60px;
    font-size: 13px;
    color: #999;
}

.v-paging .loading .icon-loading {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -40px;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background-image: url("data:image/png;base64,");
    background-size: 20px 20px;
    background-repeat: no-repeat;
    -webkit-animation: kf-v-loading-rotate 2s steps(1,end) infinite;
}

@-webkit-keyframes kf-v-loading-rotate {
    from {
        -webkit-transform: rotateZ(0);
    }

    8.33333% {
        -webkit-transform: rotateZ(30deg);
    }

    16.66666% {
        -webkit-transform: rotateZ(60deg);
    }

    25% {
        -webkit-transform: rotateZ(90deg);
    }

    33.33333% {
        -webkit-transform: rotateZ(120deg);
    }

    41.66666% {
        -webkit-transform: rotateZ(150deg);
    }

    50% {
        -webkit-transform: rotateZ(180deg);
    }

    58.33333% {
        -webkit-transform: rotateZ(210deg);
    }

    66.66666% {
        -webkit-transform: rotateZ(240deg);
    }

    75% {
        -webkit-transform: rotateZ(270deg);
    }

    83.33333% {
        -webkit-transform: rotateZ(300deg);
    }

    91.66666% {
        -webkit-transform: rotateZ(330deg);
    }

    to {
        -webkit-transform: rotateZ(360deg);
    }
}

.v-paging .loading .text {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -10px;
    margin-top: -10px;
    width: 60px;
    height: 20px;
    line-height: 20px;
}

.v-paging .done,.v-paging .hide,.v-paging .more,.v-paging .fail {
    width: 100%;
    height: 60px;
    line-height: 60px;
    text-align: center;
}

.v-network {
    position: fixed;
    top: -36px;
    z-index: 9999;
    width: 100%;
    height: 36px;
}

.v-network>.notice {
    position: absolute;
    top: 0;
    width: 100%;
    height: 36px;
}

.v-network>.notice .icon {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background-size: cover;
}

.v-network>.notice .msg {
    position: absolute;
    left: 50%;
    line-height: 36px;
    font-size: 14px;
    color: #fff;
}

.v-network .error {
