.v-batchedit {
    padding: 88rpx 0 88rpx;
}

.v-batchedit .header {
    font-size: 32rpx;
    height: 88rpx;
    top: 0;
    position: fixed;
    width: 100%;
    z-index: 99999;
}

.v-batchedit .header .selected-group {
    border-bottom: 2rpx solid #eee;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    overflow: hidden;
    width: 100%;
    background-color: #fff;
}

.v-batchedit .header .selected-group .select-text {
    display: inline-block;
    color: #333;
}

.v-batchedit .header .selected-group .select-icon {
    background: url("data:image/png;base64,");
    width: 26rpx;
    height: 16rpx;
    background-size: 26rpx 16rpx;
    display: inline-block;
    margin-bottom: 4rpx;
}

.v-batchedit .header .selected-group .unSelect-icon {
    transform: rotate(-180deg);
}

.v-batchedit .options {
    background-color: #fff;
}

.v-batchedit .option {
    color: #333;
    height: 88rpx;
    line-height: 88rpx;
    border-bottom: 2rpx solid #eee;
    position: relative;
    z-index: 33333;
    font-size: 32rpx;
    margin-left: 30rpx;
}

.v-batchedit .option .chose-icon {
    width: 26rpx;
    height: 20rpx;
    background: url("data:image/png;base64,");
    background-size: 26rpx 20rpx;
    position: absolute;
    top: 32rpx;
    right: 60rpx;
}

.v-batchedit .chose-color {
    color: #3f7de5;
}

.v-batchedit .body {
    overflow: auto;
}

.v-batchedit .model {
    height: 100%;
    width: 100%;
    position: fixed;
    overflow: hidden;
    background-color: #fff;
    z-index: 9999;
}

.v-batchedit .no-stocks {
    color: #3f7de5;
    margin: 200rpx auto;
    text-align: center;
}

.v-batchedit .groups {
    height: 70rpx;
    background-color: #f5f5f5;
    line-height: 70rpx;
    padding-left: 30rpx;
    color: #999;
    font-size: 28rpx;
}

.v-batchedit .content {
    position: relative;
    text-align: center;
    padding-left: 72rpx;
}

.v-batchedit .true {
    background: url("data:image/png;base64,");
    background-size: 45rpx 45rpx;
}

.v-batchedit .icon {
    height: 43rpx;
    width: 43rpx;
    border: 1px solid #eee;
    display: inline-block;
    border-radius: 23rpx;
    left: 44rpx;
    position: absolute;
}

.v-batchedit .footer {
    position: fixed;
    width: 100%;
    bottom: 0;
    height: 88rpx;
    border-top: 2rpx solid #eee;
    line-height: 88rpx;
    color: #333;
    font-size: 32rpx;
    background: #fff;
    z-index: 100;
}

.v-batchedit .footer view {
    display: inline-block;
}

.v-batchedit .footer .icon-all {
    width: 42rpx;
    height: 42rpx;
    border: 2rpx solid #eee;
    border-radius: 22rpx;
    position: absolute;
    top: 20rpx;
    left: 30rpx;
}

.v-batchedit .footer .bg-icon {
    background: url("data:image/png;base64,");
    background-size: 46rpx 46rpx;
}

.v-batchedit .footer .sel-all {
    padding-left: 100rpx;
    padding-right: 60rpx;
}

.v-batchedit .footer .remove {
    padding: 0 60rpx 0 170rpx;
}

.v-batchedit .footer .default-color {
    color: #999;
}

.v-batchedit .footer .delete {
    color: #999;
    padding-right: 72rpx;
    padding-left: 30rpx;
    position: absolute;
    right: 0;
}

.v-batchedit .footer .delete-color {
    color: #f91a1a;
}

.v-batchedit .fix-iphonex-button {
    bottom: 76rpx;
}

.v-batchedit .fix-iphonex-button:before {
    content: '';
    position: absolute;
    left: 0;
    top: 43px;
    width: 100%;
    height: 90rpx;
    background-color: #fff;
}

.v-batchedit .fix-iphonex-body {
    margin-bottom: 76rpx;
}