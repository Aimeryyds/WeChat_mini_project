.v-kline .content-kline {
    position: relative;
    margin: 0 12rpx;
}

.v-kline .content-kline .chart {
    position: absolute;
    left: 0;
    width: 100%;
    height: 100%;
}

.v-kline .content-kline .loading {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 50px;
    height: 20px;
    margin-left: -25px;
    background-color: #fff;
    text-align: center;
}

.v-kline .rline-cvs,.v-kline .kline-data-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.v-kline .hint-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.v-kline .touch-cvs,.v-kline .zoom-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.v-kline .oper-cvs {
    position: absolute;
    left: 60px;
    width: 204px;
    height: 36px;
}

.v-kline .hidden {
    display: none;
}

.v-kline .content-kline .hint {
    position: absolute;
    width: 100%;
    height: 100%;
    line-height: 20px;
    font-size: 11px;
}

.v-kline .content-kline .c-line-top:before {
    right: 10px;
}

.v-kline .content-kline .c-line-bottom:after {
    right: 10px;
}

.v-kline .R-line-explain .header {
    height: 44px;
    position: relative;
}

.v-kline .R-line-explain .header .title {
    width: 90px;
    margin-left: 15px;
    display: inline-block;
}

.v-kline .R-line-explain .header .title .text {
    float: left;
    line-height: 44px;
    display: inline-block;
    font-size: 16px;
}

.v-kline .R-line-explain .header .title .icon-explain {
    display: inline-block;
    margin-left: 5px;
    margin-top: 14px;
    float: left;
    width: 16px;
    height: 16px;
    background: url("data:image/png;base64,");
    background-size: 16px 16px;
}

.v-kline .R-line-explain .header .select {
    float: right;
    display: inline-block;
    margin-right: 37.5px;
}

.v-kline .R-line-explain .header .select .text {
    line-height: 44px;
    display: inline-block;
    font-size: 14px;
    color: #999;
}

.v-kline .R-line-explain .header .select .icon-select {
    position: absolute;
    width: 19px;
    height: 16px;
    top: 50%;
    margin-top: -8px;
    right: 15px;
    background: url("data:image/png;base64,");
    background-size: 19px 16px;
}

.v-kline .R-line-explain .content-kline-list {
    margin-left: 15px;
}

.v-kline .R-line-explain .content-kline-list .info .title {
    position: absolute;
    left: 22px;
    top: 12px;
    font-size: 18px;
    line-height: 18px;
}

.v-kline .R-line-explain .content-kline-list .info .subtitle {
    position: absolute;
    left: 22px;
    bottom: 11px;
    font-size: 14px;
    line-height: 14px;
    color: #999;
}

.v-kline .R-line-explain .content-kline-list .bar {
    position: relative;
    float: right;
    top: 13px;
    right: 0;
    width: 4%;
    height: 15px;
    border-radius: 7.5px 0 0 7.5px;
    background-color: #ccc;
}

.v-kline .R-line-explain .content-kline-list .number {
    display: inline-block;
    position: relative;
    float: right;
    top: 13px;
    font-size: 13px;
    margin-right: 5px;
}

.v-kline .R-line-explain .content-kline-list .up-beyond {
    position: relative;
    height: 60px;
}

.v-kline .R-line-explain .content-kline-list .up-beyond .icon {
    position: absolute;
    top: 50%;
    margin-top: -10px;
    left: 0;
    width: 11px;
    height: 20px;
    background: url("data:image/png;base64,");
