.v-fiveday .content .content-t {
    display: flex;
    align-items: center;
    height: 40rpx;
    font-size: 20rpx;
    color: #ff7f00;
    margin: 0 12rpx;
}

.v-fiveday .content .content-t text+text {
    margin-left: 10rpx;
}

.v-fiveday .content .content-b {
    position: relative;
    display: flex;
    height: 325px;
    margin: 0 12rpx;
}

.v-fiveday .content .content-b .chart {
    position: relative;
    flex: 1;
    height: 100%;
    overflow: hidden;
}

.v-fiveday .content .content-b .loading {
    position: absolute;
    left: 50%;
    top: 50%;
    width: 50px;
    height: 20px;
    margin-left: -25px;
    background-color: #fff;
    text-align: center;
}

.v-fiveday .content .content-b .hint {
    position: absolute;
    width: 100%;
    height: 100%;
    line-height: 20px;
    font-size: 11px;
}

.v-fiveday .content .content-b .hint:before {
    left: -1px;
}

.v-fiveday .content .content-b .hint:after {
    right: -1px;
}

.v-fiveday .content .content-b .five-tick {
    flex: 210rpx 0 0;
    height: 100%;
    overflow: hidden;
    font-size: 20rpx;
    box-sizing: border-box;
}

.v-fiveday .content-b .data-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.v-fiveday .content-b .hint-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.v-fiveday .content-b .touch-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.v-fiveday .hidden {
    display: none;
}

.v-quote-timeseries .date-list {
    position: relative;
    height: 32rpx;
    margin: 0 12rpx;
}

.v-quote-timeseries .chart .volume {
    position: absolute;
    left: 0;
    height: 40rpx;
    display: flex;
    align-items: center;
    color: #666;
    font-size: 20rpx;
}

.v-quote-timeseries .chart .volume .volume-text {
    height: 28rpx;
    line-height: 28rpx;
    padding: 0 6rpx;
    margin-right: 12rpx;
    background-color: #e8f1ff;
}