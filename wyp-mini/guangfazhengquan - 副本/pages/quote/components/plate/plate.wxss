.v-quote-plate .flex {
    display: flex;
    flex-direction: column;
    text-align: center;
    justify-content: center;
}

.v-quote-plate .basic {
    padding: 0 24rpx 14rpx;
}

.v-quote-plate .basic .info {
    display: flex;
    justify-content: space-between;
}

.v-quote-plate .basic .right {
    display: flex;
    flex: calc(100% - 228rpx) 0 0;
}

.v-quote-plate .basic .right .msg {
    flex: 1;
}

.v-quote-plate .basic .right .msg .row {
    display: flex;
    width: 100%;
    line-height: 36rpx;
}

.v-quote-plate .basic .right .msg .row .cell {
    display: flex;
    flex: 1;
    justify-items: center;
}

.v-quote-plate .basic .right .msg .row .cell:nth-child(2) {
    padding-left: 30rpx;
}

.v-quote-plate .basic .right .msg .row .cell-0 {
    color: #808080;
    font-size: 26rpx;
}

.v-quote-plate .basic .right .msg .row .cell-1 {
    flex: 1;
    text-align: right;
    color: #000;
    font-size: 28rpx;
}

.v-quote-plate .basic .right .show-more {
    display: flex;
    flex: 48rpx 0 0;
    align-items: center;
    padding-left: 34rpx;
    box-sizing: border-box;
}

.v-quote-plate .basic .right .show-more .icon-more {
    position: relative;
    width: 6rpx;
    height: 34rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 6rpx 34rpx;
}

.v-quote-plate .now {
    font-size: 52rpx;
    line-height: 72rpx;
}

.v-quote-plate .change-rise {
    display: flex;
    justify-content: space-between;
    box-sizing: border-box;
}

.v-quote-plate .change,.v-quote-plate .rise {
    font-size: 24rpx;
    line-height: 32rpx;
}

.v-quote-plate .rise {
    margin-left: 10rpx;
}

.v-quote-plate .mask {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.3);
    z-index: 9;
}

.v-quote-plate .more .wrapper {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 300px;
    height: 335px;
    border-radius: 4px;
    background-color: #fff;
}

.v-quote-plate .more-cvs,.v-quote-plate .line-cvs {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    border-radius: 4px;
}

.v-quote-plate .more .content {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    border-radius: 4px;
    z-index: 999;
    background-color: #fff;
}

.v-quote-plate .more .title {
    margin-top: 16px;
    text-align: center;
    font-weight: bold;
}

.v-quote-plate .more .sep-line {
    margin: 12px 25px 17px;
}

.v-quote-plate .status {
    min-height: 28rpx;
    padding: 20rpx 24rpx;
}

.v-quote-plate .status,.v-quote-plate .status .left,.v-quote-plate .status .right {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.v-quote-plate .status .left {
    color: #808080;
    font-size: 24rpx;
}

.v-quote-plate .status .right .icon {
    display: flex;
    justify-content: center;
    width: 32rpx;
    height: 26rpx;
    line-height: 26rpx;
    margin-left: 8rpx;
    color: #fff;
    border-radius: 2rpx;
    font-size: 20rpx;
}

.v-quote-plate .status .right .icon-hk {
    background-color: #a875f8;
}

.v-quote-plate .status .right .icon-sz {
    background-color: #ff6c8e;
}

.v-quote-plate .status .right .icon-sh {
    background-color: #ffa45a;
}

.v-quote-plate .status .right .icon-us {
    background-color: #27a6ff;
}

.v-quote-plate .status .right .icon-stb {
    background-color: #e55ecd;
}

.v-quote-plate .status .right .icon-rong {
    background-color: #fc5126;
}

.v-quote-plate .status .right .icon-gt {
    background-color: #4dc9e0;
}

.v-quote-plate .status .right .icon-kcb {
    background-color: #617bff;
}

.v-quote-plate .status .right .icon-inno {
    background-color: #3eabff;
}

.v-quote-plate .status .right .icon-base {
    background-color: #7693fb;
}

.v-quote-plate .status .right .icon-auction {
    background-color: #ff6c8e;
}

.v-quote-plate .status .right .icon-market_make {
    background-color: #e55ecd;
}

.v-quote-plate .status .right .icon-kcb,.v-quote-plate .status .right .icon-inno,.v-quote-plate .status .right .icon-base,.v-quote-plate .status .right .icon-gt {
    width: 66rpx;
}

.v-quote-plate .status .right .icon-auction,.v-quote-plate .status .right .icon-market_make {
    width: 44rpx;
}

.v-quote-plate .status .right .tip {
    margin-left: 8rpx;
    color: #808080;
    font-size: 24rpx;
}