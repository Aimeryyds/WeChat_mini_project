.v-favourite {
    margin-bottom: 0;
}

.v-favourite .c-line-bottom:after {
    border-bottom: 1px solid #e5e5e5;
}

.v-favourite .guide-bar {
    position: absolute;
    width: 390rpx;
    right: 46rpx;
    top: 24rpx;
    padding: 24rpx 56rpx 24rpx 24rpx;
    font-size: 28rpx;
    color: #000;
    background-color: #fff;
    z-index: 99;
    border-radius: 8rpx;
    box-shadow: 0 4rpx 14rpx rgba(172,181,196,0.7);
}

.v-favourite .guide-bar::before {
    position: absolute;
    content: "";
    right: 76rpx;
    top: -42rpx;
    border-color: transparent transparent rgba(172,181,196,0.15);
    border-width: 22rpx 19rpx;
    border-style: solid;
}

.v-favourite .guide-bar::after {
    position: absolute;
    content: "";
    right: 76rpx;
    top: -40rpx;
    border-color: transparent transparent #fff;
    border-width: 22rpx 19rpx;
    border-style: solid;
}

.v-favourite .guide-bar .icon-addMini {
    display: inline-block;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: contain;
}

.v-favourite .guide-bar .icon-cancel {
    position: absolute;
    width: 36rpx;
    height: 36rpx;
    top: 34rpx;
    right: 22rpx;
    background-size: cover;
    background-image: url("data:image/png;base64,");
}

.v-favourite .top-bar {
    height: 90rpx;
    padding: 0 30rpx;
}

.v-favourite .top-bar .tab-bar {
    position: relative;
    width: 530rpx;
    float: left;
    padding-right: 50rpx;
    box-sizing: border-box;
}

.v-favourite .top-bar .tab-bar .icon-operate {
    position: absolute;
    right: 0;
    top: 0;
    width: 36rpx;
    height: 100%;
    background: url("data:image/png;base64,") no-repeat right center;
    background-size: contain;
}

.v-favourite .top-bar .search-bar {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    top: 17rpx;
    height: 56rpx;
    line-height: 56rpx;
    width: 140rpx;
    margin-left: 550rpx;
    border-radius: 28rpx;
    background-color: #eee;
    z-index: 5;
}

.v-favourite .search-bar .text {
    font-size: 26rpx;
    line-height: 26rpx;
    width: 60rpx;
    height: 26rpx;
    color: #999;
}

.v-favourite .search-bar .icon-search {
    margin-right: 12rpx;
    width: 26rpx;
    height: 26rpx;
    background: url("data:image/png;base64,");
    background-size: 26rpx 26rpx;
    z-index: 10;
}

.v-favourite .sort-bar .cell-box {
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.v-favourite .sort-bar .cell-box .icon-up-down {
    margin-left: 10rpx;
}

.v-favourite .add-favourite,.v-favourite .sync-favourite {
    display: table;
    width: 100%;
    height: 70rpx;
    text-align: center;
    font-size: 28rpx;
    color: #3f7de5;
}

.v-favourite .sync-favourite {
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 99;
    background-color: #fff;
}

.v-favourite .add-favourite .add-row,.v-favourite .sync-favourite .add-row {
    display: flex;
    height: 100%;
    justify-content: center;
    align-items: center;
}

.v-favourite .sync-favourite .add-row {
    background-color: rgba(63,125,229,0.06);
}

.v-favourite .add-favourite .add-row .add-inbox {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 254rpx;
    height: 74rpx;
    background-color: rgba(63,125,229,0.06);
    font-size: 32rpx;
    border-radius: 8rpx;
}

.v-favourite .add-favourite .add-row .add-inbox .icon-add {
    width: 32rpx;
    height: 32rpx;
    background-image: url("data:image/png;base64,");
    margin-right: 4rpx;
}

.v-favourite .add-favourite .add-row .icon-add,.v-favourite .sync-favourite .add-row .icon-sync {
    width: 24rpx;
    height: 24rpx;
    margin-right: 14rpx;
    background-image: url("data:image/png;base64,");
    background-size: contain;
}

.v-favourite .sync-favourite .add-row .icon-sync {
    background-image: url("data:image/png;base64,");
}

.v-favourite .no-record {
    padding-top: 140rpx;
}

.v-favourite .no-record image {
    display: block;
    width: 272rpx;
    height: 245rpx;
    margin: 0 auto;
}

.v-favourite .no-text {
    margin: 40rpx 0 100rpx;
    font-size: 32rpx;
    color: #999;
    text-align: center;
}

.v-favourite .sort-bar {
    display: flex;
    width: 100%;
    height: 70rpx;
    align-items: center;
    padding: 0 30rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #999;
}

.v-favourite .cell {
    flex: 1;
}

.v-favourite .sort-bar .stock-now,.v-favourite .sort-bar .stock-rise {
    flex: 28% 0 0;
    text-align: right;
}

.v-favourite .sort-bar .stock-rise {
    flex: 204rpx 0 0;
}

.v-favourite .sep-bar {
    background-color: #eee;
    height: 8px;
}

.v-favourite .stock-list {
    height: 100%;
    background-color: #fff;
    box-sizing: border-box;
}

.v-favourite .stock-list.padding-bottom {
    padding-bottom: 70rpx;
}

.v-favourite .stock-list .body {
    position: relative;
}

.v-favourite .stock-list .body .row {
    position: relative;
    display: flex;
    width: 100%;
    align-items: center;
    padding: 14rpx 30rpx;
    background-color: #fff;
    box-sizing: border-box;
}

.v-favourite .stock-list .body .row:first-child {
    padding-top: 22rpx;
}

.v-favourite .stock-name-code {
    display: flex;
    height: 100%;
    align-items: center;
    overflow: hidden;
}

.v-favourite .stock-name-code .stock-logo {
    height: 72rpx;
    flex: 72rpx 0 0;
    padding: 8rpx;
    box-sizing: border-box;
}

.v-favourite .stock-name-code .stock-logo .logo {
    width: 100%;
    height: 100%;
}

.v-favourite .stock-name-code .stock-info {
    overflow: hidden;
    margin-left: 22rpx;
}

.v-favourite .stock-list .body .stock-name {
    font-size: 34rpx;
    line-height: 34rpx;
    color: #333;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.v-favourite .stock-list .body .stock-code {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    margin-top: 10rpx;
    color: #999;
}

.v-favourite .stock-list .body .stock-code .icon {
    display: flex;
    justify-content: center;
    width: 28rpx;
    height: 20rpx;
    line-height: 20rpx;
    margin-right: 10rpx;
    color: #fff;
    font-size: 18rpx;
    border-radius: 2rpx;
}

.v-favourite .stock-list .body .stock-code .icon-hk {
    background-color: #b671dc;
}

.v-favourite .stock-list .body .stock-code .icon-us {
    background-color: #27a6ff;
}

.v-favourite .stock-list .body .stock-code .icon-sz {
    background-color: #ff6c8e;
}

.v-favourite .stock-list .body .stock-code .icon-sh {
    background-color: #ffa45a;
}

.v-favourite .stock-list .body .stock-code .icon-neeq {
    width: 40rpx;
    background-color: #ff7f3c;
}

.v-favourite .stock-list .body .stock-now,.v-favourite .stock-list .body .stock-rise {
    text-align: right;
    font-weight: 300;
}

.v-favourite .stock-list .body .stock-now {
    flex: 28% 0 0;
    font-size: 40rpx;
    font-weight: bold;
    color: #333;
    word-break: break-all;
}

.v-favourite .stock-list .body .stock-rise {
    top: 0;
    flex: 204rpx 0 0;
}

.v-favourite .stock-list .body .stock-rise-box {
    flex: 160rpx 0 0;
    height: 64rpx;
    line-height: 64rpx;
    background-color: #f5f5f5;
    color: #fff;
    font-size: 36rpx;
    border-radius: 2rpx;
    text-align: center;
    margin-left: 44rpx;
}

.v-favourite .stock-list .oper-menu {
    position: absolute;
    top: 0;
    right: 0;
    width: 140px;
    display: none;
}

.v-favourite .stock-list .oper-menu.c-line-top:before {
    top: -2px;
    z-index: 2;
}

.v-favourite .stock-list .oper-menu .cell {
    display: inline-block;
    width: 70px;
    height: 56px;
    text-align: center;
    line-height: 56px;
    font-size: 15px;
    color: #fff;
}

.v-favourite .stock-list .oper-menu .top {
    background-color: #c8c7cd;
}

.v-favourite .stock-list .oper-menu .delete {
    background-color: #fe3529;
}

.v-favourite .bottom-line {
    margin-top: 1px;
}

.v-favourite .sync {
    position: relative;
    width: 100%;
    height: 60px;
}

.v-favourite .sync .text {
    position: absolute;
    left: 50%;
    margin-left: -70px;
    width: 120px;
    height: 60px;
    font-size: 14px;
    line-height: 60px;
    color: #00a5f6;
    text-align: center;
}

.v-favourite .sync .icon-sync {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -9px;
    margin-left: 50px;
    width: 18px;
    height: 18px;
    background-image: url("data:image/png;base64,");
    background-size: 18px 18px;
}

.sync-box {
    height: 80rpx;
}

.modal_content .true {
    background: url("data:image/png;base64,");
    background-size: 45rpx 45rpx;
}

.modal_content .icon {
    height: 43rpx;
    width: 43rpx;
    border: 1px solid #eee;
    display: inline-block;
    border-radius: 23rpx;
    left: 44rpx;
    position: absolute;
}