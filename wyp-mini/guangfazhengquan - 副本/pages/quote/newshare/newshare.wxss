page {
    background-color: #edf1f4;
    font-family: DNI Medium;
}

.pagebody {
    padding-right: 10px;
}

.clearfix::after {
    content: " ";
    display: block;
    overflow: hidden;
    clear: both;
}

.clearfix {
    zoom: 1;
}

.yearlist .yeartitle {
    height: 20px;
    line-height: 20px;
    padding-left: 10px;
    border-left: 4px solid #73c5f7;
    margin: 12px 0;
    font-size: 16px;
    font-weight: bold;
}

.yearlist .yearbody .month {
    position: relative;
    z-index: 3;
    float: left;
    width: 15%;
    font-size: 14px;
    text-align: center;
}

.yearlist .yearbody .month:before {
    display: block;
    content: "";
    width: 4px;
    height: 4px;
    position: absolute;
    top: -4px;
    left: 50%;
    transform: translateX(-2px);
    background-color: #edf1f4;
}

.yearlist .yearbody .month:after {
    display: block;
    content: "";
    width: 4px;
    height: 4px;
    position: absolute;
    bottom: -4px;
    left: 49%;
    transform: translateX(-2px);
    background-color: #edf1f4;
}

.yearlist .yearbody .month view {
    display: inline-block;
    box-sizing: border-box;
}

.yearlist .yearbody .month .outer {
    width: 40px;
    height: 40px;
    background-color: #bcdff5;
    border-radius: 50%;
    padding: 4px;
}

.yearlist .yearbody .month .outer view {
    width: 100%;
    height: 100%;
    color: #fff;
    line-height: 32px;
    background-color: #73c5f7;
    border-radius: 50%;
}

.yearlist .yearbody .lists-body {
    float: right;
    width: 85%;
    margin-bottom: 10px;
}

.yearlist .yearbody .lists-body .lists {
    background-color: #fff;
    padding: 0 10px;
    border-radius: 5px;
}

.yearlist .yearbody .lists .list-header view {
    display: inline-block;
}

.yearlist .yearbody .lists .list-header .image-down {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
    position: absolute;
    right: 2%;
    top: 18px;
    width: 12px;
    height: 8px;
}

.yearlist .yearbody .lists .list-header .image-up {
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100%;
    position: absolute;
    right: 2%;
    top: 18px;
    width: 12px;
    height: 8px;
}

.yearlist .yearbody .lists-body .lists .list-header {
    position: relative;
    height: 44px;
    line-height: 44px;
}

.yearlist .yearbody .lists-body .lists .list-header .name {
    color: #333;
    font-size: 16px;
    width: 28%;
}

.yearlist .yearbody .lists-body .lists .list-header .code {
    color: #999;
    font-size: 14px;
    width: 22%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-header .type {
    color: #eeaa3b;
    font-size: 14px;
    width: 25%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-header .date {
    color: #73c5f7;
    font-size: 14px;
    width: 25%;
}

.yearlist .yearbody .lists-body .lists .list-content .title1 view {
    display: inline-block;
    width: 33.33%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-content .title1 {
    padding-top: 6px;
    border-top: 1px solid #eee;
    height: 25px;
    line-height: 25px;
}

.yearlist .yearbody .lists-body .lists .list-content .content1 {
    color: #666;
    height: 25px;
    line-height: 25px;
    font-size: 14px;
    padding-bottom: 7px;
}

.yearlist .yearbody .lists-body .lists .list-content .title2 {
    padding-top: 6px;
    border-top: 1px solid #eee;
    line-height: 25px;
}

.yearlist .yearbody .lists-body .lists .list-content .content2 {
    color: #666;
    line-height: 25px;
    font-size: 14px;
    padding-bottom: 7px;
}

.yearlist .yearbody .lists-body .lists .list-content .content3 {
    border-top: 1px solid #eee;
    min-height: 40px;
    line-height: 1;
}

.yearlist .yearbody .lists-body .lists .list-content .content1 view {
    display: inline-block;
    width: 33.33%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-content .title2 view {
    display: inline-block;
    width: 33.33%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-content .content2 view {
    display: inline-block;
    width: 33.33%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-content .content3 .industry {
    float: right;
    font-size: 14px;
    color: #666;
    width: 66.66%;
    text-align: left;
    box-sizing: border-box;
    line-height: 20px;
    padding: 10px 10px 10px 0;
}

.yearlist .yearbody .lists-body .lists .list-content .content3 .tite {
    float: left;
    line-height: 40px;
    width: 33.33%;
    text-align: center;
}

.yearlist .yearbody .lists-body .lists .list-content .tite {
    color: #999;
    font-size: 12px;
}

.yearlist .yearbody .lists-body .lists .list-content {
    display: block;
}

.show {
    display: block!important;
}

.hide {
    display: none!important;
}

.ipocolor {
    color: #576b95!important;
}

.todaycolor {
    color: #e94242!important;
}

.yearbody {
    position: relative;
}

.line {
    position: absolute;
    width: 2px;
    height: 100%;
    background-color: #73c5f7;
    left: 7.25%;
    z-index: 2;
}

.yearlist .yearbody .lists.show-bg .list-header .image-down {
    background-image: url("data:image/png;base64,");
}

.yearlist .yearbody .lists-body .lists.show-bg {
