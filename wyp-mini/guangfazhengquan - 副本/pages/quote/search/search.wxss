.v-quote-search {
    padding-bottom: 30px;
}

.v-quote-search .search-wrapper {
    position: relative;
    background-color: #f5f5f5;
    height: 28px;
    line-height: 28px;
    margin: 8px 55px 8px 10px;
    border-radius: 14px;
}

.v-quote-search .search-input {
    font-size: 13px;
    line-height: 28px;
    height: 28px;
    padding-left: 30px;
    color: #333;
    border: 0;
}

.v-quote-search .search-input::-webkit-input-placeholder {
    font-size: 13px;
    line-height: 32px;
    color: #999;
}

.v-quote-search .search-icon {
    position: absolute;
    left: 10px;
    top: 50%;
    margin-top: -6px;
    width: 13px;
    height: 13px;
    background: url("data:image/png;base64,");
    background-size: 13px 13px;
    z-index: 2;
}

.icon-import {
    width: 32px;
    height: 28px;
    position: absolute;
    top: 50%;
    margin-top: -14px;
    right: 0;
    z-index: 100;
}

.icon-import image {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -7px;
    margin-left: -8px;
    width: 16px;
    height: 14px;
    z-index: 101;
}

.v-quote-search .seach-cancel {
    color: #3f7de5;
    position: absolute;
    top: 0;
    right: -40px;
    font-size: 14px;
}

.v-quote-search .cancel-icon {
    width: 15px;
    height: 15px;
    background: url("data:image/png;base64,");
    background-size: 15px 15px;
    position: absolute;
    top: 6px;
    right: 11px;
    z-index: 5;
}

.v-quote-search .stock {
    position: relative;
    padding: 24rpx 160rpx 24rpx 34rpx;
}

.v-quote-search .stock .stock-name {
    font-size: 16px;
    line-height: 16px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.v-quote-search .stock .stock-name.stock-name-hz {
    display: flex;
}

.v-quote-search .stock .stock-name .key {
    color: #f91a1a;
}

.v-quote-search .stock .stock-code {
    margin-top: 5px;
    font-size: 12px;
    color: #999;
}

.v-quote-search .stock .stock-code .key {
    color: #f91a1a;
}

.v-quote-search .stock .stock-operation {
    position: absolute;
    right: 0;
    top: 0;
    width: 160rpx;
    height: 100%;
}

.v-quote-search .stock .stock-operation-rect {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -20px;
    margin-top: -13px;
    width: 40px;
    height: 25px;
}

.v-quote-search .stock .icon1 {
    position: relative;
    top: 50%;
    left: 50%;
    margin-left: -10px;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background-size: 20px 20px;
    text-align: center;
    z-index: 2;
}

.v-quote-search .stock .icon-add {
    background-image: url("data:image/png;base64,");
}

.v-quote-search .clear-wrapper {
    position: relative;
    width: 100%;
    height: 68rpx;
    line-height: 68rpx;
}

.v-quote-search .clear {
    position: absolute;
    margin-top: 16rpx;
    text-align: center;
    width: 150px;
    left: 50%;
    margin-left: -75px;
    color: #00a5f6;
    font-size: 13px;
}

.v-quote-search .no-record {
    position: absolute;
    margin-top: 10px;
    text-align: center;
    line-height: 40px;
    width: 100px;
    left: 50%;
    margin-left: -50px;
    color: #999;
    font-size: 13px;
}

.v-quote-search .fuzzy-wrapper .show-more {
    color: #999;
    position: relative;
    font-size: 14px;
    line-height: 34px;
    height: 34px;
    left: 50%;
    margin-left: -50px;
    width: 45%;
    overflow: hidden;
}

.v-quote-search .fuzzy-wrapper .show-more .more-icon {
    width: 13px;
    height: 8px;
    background: url("data:image/png;base64,");
    background-size: 13px 8px;
    position: absolute;
    top: 14px;
    left: 90px;
}

.dialog__input {
    text-align: left;
}

.stock-groups {
    margin-top: 10rpx;
    font-size: 24rpx;
    line-height: 26rpx;
    color: #999;
    width: 80%;
}

.stock-groups .stock-group {
    display: inline-block;
    letter-spacing: 2px;
}

.v-quote-search .search-results .stock-code {
    display: flex;
    align-items: center;
    font-size: 24rpx;
    margin-top: 10rpx;
    color: #999;
}

.v-quote-search .search-results .stock-code .icon {
    display: flex;
    justify-content: center;
    width: 28rpx;
    height: 20rpx;
    line-height: 20rpx;
    margin-right: 10rpx;
    color: #fff;
    font-size: 18rpx;
    border-radius: 2rpx;
    text-align: center;
}

.v-quote-search .search-results .stock-code .icon-hk {
    background-color: #b671dc;
}

.v-quote-search .search-results .stock-code .icon-us {
    background-color: #27a6ff;
}

.v-quote-search .search-results .stock-code .icon-sz {
    background-color: #ff6c8e;
}

.v-quote-search .search-results .stock-code .icon-sh {
    background-color: #ffa45a;
}

.v-quote-search .search-results .stock-code .icon-tong {
    width: 20rpx;
    background-color: #53b3f2;
}

.v-quote-search .search-results .stock-code .icon-ke {
    width: 20rpx;
    background-color: #617bff;
}

.v-quote-search .search-results .stock-code .icon-neeq {
    width: 40rpx;
    background-color: #ff7f3c;
}