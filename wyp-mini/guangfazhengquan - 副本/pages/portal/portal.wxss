.v-confirm {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.3);
    z-index: 9999;
    color: #333;
}

.v-confirm .wrapper {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -140px;
    margin-top: -90px;
    width: 280px;
    height: 150px;
    background-color: #fff;
    border-radius: 5px;
    overflow: hidden;
}

.v-confirm .wrapper .msg {
    margin-top: 40px;
    width: 100%;
    text-align: center;
    line-height: 24px;
    font-size: 16px;
}

.v-confirm .wrapper .buttons {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 50px;
}

.v-confirm .wrapper .button {
    display: inline-block;
    width: 50%;
    height: 100%;
    font-size: 16px;
    text-align: center;
    line-height: 50px;
}

.v-confirm .wrapper .button.confirm {
    background-color: #00a5f6;
    color: #fff;
}

.v-toast .wrapper {
    position: fixed;
    width: 80%;
    height: 20px;
    top: 42%;
    z-index: 9999;
    text-align: center;
    left: 10%;
}

.v-toast .text {
    display: inline-block;
    max-width: 80%;
    min-width: 50%;
    padding: 35rpx 50rpx;
    background: rgba(40,40,40,0.85);
    border-radius: 5px;
    color: #fff;
    font-size: 32rpx;
}

.v-network {
    position: fixed;
    top: -36px;
    z-index: 9999;
    width: 100%;
    height: 36px;
}

.v-network>.notice {
    position: absolute;
    top: 0;
    width: 100%;
    height: 36px;
}

.v-network>.notice .icon {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -10px;
    width: 20px;
    height: 20px;
    background-size: cover;
}

.v-network>.notice .msg {
    position: absolute;
    left: 50%;
    line-height: 36px;
    font-size: 14px;
    color: #fff;
}

.v-network .error {
    background-color: #f70e22;
}

.v-network .error .icon {
    margin-left: -90px;
    background-image: url("data:image/png;base64,");
}

.v-network .error .msg {
    margin-left: -60px;
}

.v-network .recover {
    background-color: #51c332;
}

.v-network .recover .icon {
    margin-left: -60px;
    background-image: url("data:image/png;base64,");
}

.v-network .recover .msg {
    margin-left: -30px;
}

