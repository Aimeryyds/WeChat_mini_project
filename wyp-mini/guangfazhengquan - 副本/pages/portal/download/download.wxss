page {
    height: 100%;
}

.v-download .download-bg {
    width: 100%;
    height: 100%;
    position: absolute;
    background-size: 100% 100%;
    z-index: -2;
}

.v-download .download-btn {
    position: absolute;
    width: 290px;
    height: 62px;
    bottom: 21%;
    background-size: 100%;
    box-shadow: 0 8px 8px rgba(190,176,96,0.5);
    border-radius: 40px;
    left: 50%;
    transform: translate(-50%);
}

.v-download .download-btn .contact-list {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99999;
    overflow: hidden;
    opacity: 0;
}

.v-download .go_back {
    position: absolute;
    font-size: 18px;
    bottom: 20px;
    color: #fff;
    left: 50%;
    transform: translate(-50%);
}

.v-download .FF6957 {
    color: #ff6957;
}