.c-line-top,.c-line-right,.c-line-bottom,.c-line-left {
    box-sizing: border-box;
    position: relative;
}

.c-line-left:before,.c-line-top:before,.c-line-right:after,.c-line-bottom:after {
    content: "";
    position: absolute;
    box-sizing: border-box;
}

.c-line-left:before {
    left: 0;
    top: 0;
    width: 1px;
    height: 100%;
    border-left: 1px solid #eee;
    transform-origin: 0 0;
    transform: scaleX(.5);
}

.c-line-top:before {
    left: 0;
    top: 0;
    width: 100%;
    height: 1px;
    border-bottom: 1px solid #eee;
    transform-origin: 0 0;
    transform: scaleY(.5);
}

.c-line-right:after {
    top: 0;
    right: 0;
    width: 1px;
    height: 100%;
    border-left: 1px solid #eee;
    transform-origin: 100% 0;
    transform: scaleX(.5);
}

.c-line-bottom:after {
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    border-bottom: 1px solid #eee;
    transform-origin: 0 100%;
    transform: scaleY(.5);
}

.c-round-rect:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    transform: scale(.5);
    transform-origin: 0 0;
    border-radius: 10px;
    border: 1px solid #d9d9d9;
    box-sizing: border-box;
    background-color: #fff;
}

.c-right-angle-rect:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 200%;
    height: 200%;
    transform: scale(.5);
    transform-origin: 0 0;
    border: 1px solid #d9d9d9;
    box-sizing: border-box;
}

.c-clear:after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0;
}

.c-btn {
    position: relative;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 14px;
    padding-right: 14px;
    box-sizing: border-box;
    font-size: 16px;
    text-align: center;
    text-decoration: none;
    color: #fff;
    height: 42px;
    line-height: 42px;
    border-radius: 5px;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    overflow: hidden;
    background-color: #dd5e33;
    border: 1px solid #dd5e33;
}

.c-btn:after {
    content: " ";
    width: 200%;
    height: 200%;
    position: absolute;
    top: 0;
    left: 0;
    border: 1px solid rgba(0,0,0,0.2);
    transform: scale(.5);
    transform-origin: 0 0;
    box-sizing: border-box;
    border-radius: 10px;
}

