.v-wait {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0);
    z-index: 9999;
}

.v-wait .wrapper {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -40rpx;
    margin-top: -40rpx;
    width: 80rpx;
    height: 80rpx;
    background: url("data:image/gif;base64,") no-repeat;
    background-size: 100% 100%;
    text-align: center;
    color: #fff;
}

.v-wait .leafs {
    position: absolute;
    width: 0;
    z-index: 2000000000;
    left: 50%;
    top: 38%;
}

.v-wait .leaf {
    animation: opacity-60-25-0-12 1.25s linear infinite;
    position: absolute;
    top: -1px;
    opacity: .25;
}

.v-wait .leaf:before {
    content: " ";
    position: absolute;
    width: 8.14px;
    height: 3.08px;
    background: #d1d1d5;
    box-shadow: rgba(0,0,0,0.098) 0 0 1px;
    border-radius: 1px;
    transform-origin: left 50% 0;
}

.v-wait .leaf_0 {
    animation: opacity-60-25-0-12 1.25s linear infinite;
}

.v-wait .leaf_0:before {
    transform: rotate(0) translate(7.92px,0);
