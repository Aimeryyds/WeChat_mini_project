.iconfont.on {
    color: #3F7DE5;
}

.icon {
    width: 34rpx;
    height: 34rpx;
}

@font-face {
    font-family:"iconfont";src:url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 32rpx;
    font-style: normal;
    color: #bbb;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.micon-down:before {
    content: "\e629";
}

.micon-delete:before {
    content: "\e623";
}

.micon-edit:before {
    content: "\e6b0";
}

.micon-unchecked:before {
    content: "\e71b";
}

.micon-up:before {
    content: "\e612";
}

.micon-checked:before {
    content: "\e69f";
}