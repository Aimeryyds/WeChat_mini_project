label {
    position: relative;
}

checkbox {
    position: absolute;
    left: 0;
    visibility: hidden;
    width: 20px;
    height: 20px;
}

icon,.un-checked {
    vertical-align: middle;
    margin-right: 12rpx;
}

.un-checked {
    display: inline-block;
    width: 18px;
    height: 18px;
    border-radius: 50%;
    border: 1px solid #ddd;
}