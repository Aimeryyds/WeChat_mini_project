@import "../../__wuBaseWxss__/1.wxss";

@import "../../__wuBaseWxss__/2.wxss";

.wp_content {
    position: relative;
}

.warn {
    position: absolute;
    z-index: 10;
}

.required {
    color: #f00;
    padding-left: 0rpx;
}

.title {
    margin-bottom: 20rpx;
    white-space: nowrap;
    margin-right: 40rpx;
    position: relative;
}

.title .sup {
    position: absolute;
    color: #f00;
}

.clearFix:after {
    content: '';
    clear: both;
    width: 0;
    height: 0;
    display: block;
}

.form_options_content checkbox {
    zoom: 60%;
}

.option-label {
    margin-right: 16rpx;
    display: inline-block;
}