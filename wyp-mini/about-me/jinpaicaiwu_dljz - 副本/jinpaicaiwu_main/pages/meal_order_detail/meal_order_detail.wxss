.top {
    width: 100%;
    height: 200rpx;
    background: #ff6619;
}

.image {
    width: 100%;
    height: 154rpx;
    text-align: center;
    padding-top: 15rpx;
    position: relative;
}

.image>image {
    height: 154rpx;
    width: 700rpx;
}

.pos {
    position: absolute;
    display: flex;
    color: #ff6619;
    width: 80%;
    top: 70rpx;
    left: 10%;
}

.pos>text {
    height: 50rpx;
    line-height: 50rpx;
}

.ri {
    text-align: right;
    flex: 1;
}

.le {
    text-align: left;
    flex: 1.5;
    padding-left: 10rpx;
    font-size: 28rpx;
}

.eatch_item {
    background: #f5f5f5;
    border-bottom: 1px solid #ffffff;
    padding: 20rpx;
    display: flex;
}

.eatch_item_l {
    flex: 1;
    border: 1px solid #ffffff;
    height: 140rpx;
}

.eatch_item_l>image {
    width: 130rpx;
    height: 130rpx;
    margin-top: 5rpx;
}

.eatch_item_r1,.eatch_item_r2 {
    height: 50rpx;
    line-height: 50rpx;
}

.eatch_item_r {
    flex: 8;
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    padding-left: 20rpx;
}

.eatch_item_r2 {
    display: flex;
    justify-content: space-between;
}

.red {
    color: #ff6619;
}

.hj {
    height: 73rpx;
    line-height: 73rpx;
    font-size: 30rpx;
    background: #f5f5f5;
    text-align: right;
    padding-right: 20rpx;
}

.input_item {
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
    border-bottom: 1px solid #f5f5f5;
}

.input_item>text {
    font-size: 24rpx;
}

.f1 {
    flex: 1;
}

.f2 {
    flex: 4;
    text-align: right;
}

.title {
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
}

.title_t {
    height: 50rpx;
    display: flex;
}

.title {
    padding-left: 20rpx;
    display: flex;
    height: 80rpx;
    line-height: 80rpx;
}

.title_t>view>image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    margin-top: 15rpx;
}

.title_t>view>text,.title_b>view>text {
    font-size: 28rpx;
    padding-left: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.title_b>text {
    font-size: 28rpx;
    height: 50rpx;
    line-height: 28rpx;
    padding: 10rpx;
}

.title_b1 {
    background: #ff6619;
    color: #ffffff;
    border: 1px solid #ff6619;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
}

.title_b2 {
    color: #ff6619;
    border: 1px solid #ff6619;
    border-top-right-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}

.newstyle2 {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    word-wrap: break-word;
    word-break: break-all;
}

@font-face {
    font-family:'iconfont';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 28px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-dengdaiwenjian:before {
    content: "\e600";
}

.icon-dingdan:before {
    content: "\e601";
}

.icon-tuikuan:before {
    content: "\e636";
}

.icon-yijiedan:before {
    content: "\e60d";
}

.icon-weijiedan:before {
    content: "\e60e";
}

.icon-jiaoyiguanbi:before {
    content: "\e62c";
}

.icon-tuikuanguanhuai:before {
