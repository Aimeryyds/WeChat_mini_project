@import "../../__wuBaseWxss__/0.wxss";

.top {
    width: 100%;
    position: relative;
    border-bottom: 20px solid #f5f5f5;
}

.bg {
    background: #ff6619;
    height: 100rpx;
    padding: 20rpx;
}

.bg>text {
    height: 100rpx;
    line-height: 100rpx;
    color: #ffffff;
    font-size: 35rpx;
}

.til {
    padding: 20rpx;
    font-size: 28rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #f5f5f5;
}

.title_t {
    height: 50rpx;
    display: flex;
}

.title_t>text {
    font-size: 28rpx;
    padding-left: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.title {
    justify-content: space-between;
    padding: 20rpx;
    padding-left: 20rpx;
    display: flex;
}

.eatch_item {
    background: #f5f5f5;
    border-top: 1px solid #ffffff;
    padding: 20rpx;
    display: flex;
}

.eatch_item_l {
    flex: 1;
    border: 1px solid #ffffff;
    height: 140rpx;
}

.eatch_item_l>image {
    width: 130rpx;
    height: 130rpx;
    margin-top: 5rpx;
}

.eatch_item_r1,.eatch_item_r2 {
    height: 50rpx;
    line-height: 50rpx;
}

.eatch_item_r {
    flex: 8;
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    padding-left: 20rpx;
}

.eatch_item_r2 {
    display: flex;
    justify-content: space-between;
}

.text_list {
    display: flex;
    height: 20rpx;
    justify-content: space-between;
    line-height: 20rpx;
    padding: 30rpx;
    padding-top: 10rpx;
}

.radio {
    width: 20rpx;
    height: 20rpx;
    border-radius: 50%;
    background: #999999;
    line-height: 20rpx;
    flex: 1;
}

.red {
    color: #ff6619;
}

.hj {
    font-size: 30rpx;
    background: #ffffff;
    text-align: right;
    padding: 20rpx;
    border-bottom: 20rpx solid #f5f5f5;
    border-top: 1px dashed #999999;
    margin-top: 20rpx;
}

.input_item {
    display: flex;
    justify-content: space-between;
    padding: 20rpx;
}

.input_item>text {
    font-size: 24rpx;
}

.f {
    color: #999999;
}

.f1 {
    flex: 1;
}

.f2 {
    flex: 4;
    text-align: right;
}

.flex {
    display: flex;
    justify-content: space-between;
}

.flex>text {
    height: 80rpx;
    line-height: 80rpx;
}

.list {
    border-bottom: 20rpx solid #f5f5f5;
}

.eatch_item_r1 {
    display: flex;
    color: #999999;
    justify-content: flex-start;
    flex-wrap: wrap;
}

.eatch_item_r1>text {
    padding-right: 10px;
}

.count_money {
    padding: 20rpx;
    font-size: 28rpx;
    border-top: 1px solid #f5f5f5;
    display: flex;
    justify-content: space-between;
}

.eatch_list {
    background: #f5f5f5;
}

.btn1,.btn2 {
    border-radius: 10rpx;
}

.btn1 {
    border: 1px solid #999999;
    color: #ffffff;
    margin-right: 20rpx;
    background: #999999;
    font-size: 28rpx;
}

.btn2 {
    border: 1px solid #f56321;
}

.col1 {
    color: #ffffff;
    background: #f56321;
    font-size: 28rpx;
}

.diandan_item_b {
    padding: 20rpx;
    display: flex;
    justify-content: flex-end;
}

.diandan_item_b>text {
    height: 50rpx;
    line-height: 50rpx;
    width: 130rpx;
    text-align: center;
}

.dress_t {
    display: flex;
    height: 140rpx;
}

.bg_light {
    line-height: 140rpx;
}

.flx_t {
    display: flex;
    flex-direction: column;
    font-size: 28rpx;
    color: #1a1a1a;
    flex: 12;
    padding-top: 24rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
}

.flx_dre {
    padding-top: 5rpx;
}

.flx_c {
    display: flex;
    justify-content: space-between;
    padding-bottom: 5rpx;
}

