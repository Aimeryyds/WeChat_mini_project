.header_pos_T {
    color: #000000;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
    padding: 20rpx;
    border-bottom: 1px solid #f5f5f5;
}

.item_l>text,.item_r>text {
    height: 100rpx;
    line-height: 100rpx;
}

.item_l {
    flex: 1;
    text-align: center;
}

.item_r {
    flex: 10;
    display: flex;
    justify-content: space-between;
}

.fx {
    display: flex;
    justify-content: space-between;
}

.icon-jiantou {
    font-size: 60rpx;
}

page,.bg_color {
    background: #f1f1f1;
    height: 100%;
    overflow: hidden;
}

.header {
    height: 510rpx;
    background: #f1f1f1;
}

.icon_bg {
    color: #999999;
    font-size: 40rpx;
}

.item_list {
    width: 96%;
    margin: 0 auto;
}

.header_pos {
    background: #fefefe;
    width: 94%;
    position: absolute;
    border-radius: 10rpx;
    margin: 0 auto;
    left: 3%;
    box-shadow: -4px 0 4px #f5f5f5,4px 0 4px #f5f5f5,0 -4px 4px #f5f5f5,0 4px 4px #f5f5f5;
    top: 344rpx;
}

.header_top {
    display: flex;
    height: 422rpx;
    background: #ffffff;
    flex-direction: column;
    align-items: center;
    position: relative;
}

.header_top_L {
    height: 150rpx;
    width: 150rpx;
    flex: 1;
}

.header_top_L image {
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    margin-top: 47rpx;
}

.header_top_R {
    display: flex;
    justify-content: space-between;
    width: 100%;
    flex: 1;
}

.b_r>text,.b_l>text {
    height: 37rpx;
    line-height: 37rpx;
    padding: 10rpx;
}

.b_l,.b_r {
    display: flex;
    flex-direction: column;
    align-items: center;
    flex: 8;
    text-align: center;
}

.m {
    font-size: 28rpx;
    z-index: 22;
}

.t {
    font-size: 25rpx;
    z-index: 22;
}

.line {
    flex: 1;
    text-align: center;
    line-height: 100rpx;
}

.flx1 {
    display: flex;
    flex: 6;
    flex-direction: column;
}

.flx1>text {
    height: 50rpx;
    line-height: 50rpx;
}

.flx2>button {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #ffffff;
    background: #ff6619;
    border-radius: 20rpx;
    margin-top: 25rpx;
}

.bg1,.bg2,.bg3 {
    position: absolute;
    background: #fafafa;
    height: 250rpx;
    width: 250rpx;
    border-radius: 50%;
}

.bg1 {
    left: -100rpx;
    top: -100rpx;
}

.bg2 {
    right: -70rpx;
    top: -70rpx;
}

.bg3 {
    bottom: -49rpx;
    right: -32rpx;
}

.item {
    display: flex;
    font-size: 28rpx;
    color: #1a1a1a;
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1px solid #f5f5f5;
    padding: 15rpx;
    background: #ffffff;
    width: 95%;
    margin: 0 auto;
}

.t_radio {
    background: radial-gradient(circle at top left,transparent 5px,#ffffff 0) top left,radial-gradient(circle at top right,transparent 5px,#ffffff 0) top right,radial-gradient(circle at bottom right,transparent 5px,#ffffff 0) bottom right,radial-gradient(circle at bottom left,transparent 5px,#ffffff 0) bottom left;
    background-size: 70% 70%;
    background-repeat: no-repeat;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
}

.b_radio {
    background: radial-gradient(circle at top left,transparent 5px,#ffffff 0) top left,radial-gradient(circle at top right,transparent 5px,#ffffff 0) top right,radial-gradient(circle at bottom right,transparent 5px,#ffffff 0) bottom right,radial-gradient(circle at bottom left,transparent 5px,#ffffff 0) bottom left;
    background-size: 70% 70%;
    background-repeat: no-repeat;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
}

.top {
    margin-top: 20rpx;
    border-radius: 20rpx;
}

@font-face {
    font-family:'iconfont';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
