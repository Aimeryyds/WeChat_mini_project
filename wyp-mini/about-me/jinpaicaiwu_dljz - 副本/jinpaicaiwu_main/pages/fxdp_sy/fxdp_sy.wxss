.fixed {
    width: 100%;
    position: fixed;
    right: 0;
    top: 0px;
    z-index: 105;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 2rem;
    background: #ccc;
}

page {
    background: #f5f5f5;
}

.header {
    height: 200rpx;
    width: 100%;
    display: flex;
    position: relative;
}

.header image {
    height: 200rpx;
    width: 100%;
    position: absolute;
}

.head_img {
    width: 126rpx;
    height: 126rpx;
    flex: 1;
    margin-top: 32rpx;
    padding-left: 30rpx;
    z-index: 99;
}

.header_top_L {
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
    overflow: hidden;
}

.head_img image {
    width: 126rpx;
    height: 126rpx;
    border-radius: 50%;
}

.cd_img image {
    width: 25px;
    height: 25px;
    margin-top: 75rpx;
}

.cd_img {
    width: 50px;
    height: 90px;
    z-index: 100;
    flex: 1;
}

.head_txt {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    color: #ffffff;
    flex: 5;
    z-index: 11;
    padding-left: 65rpx;
    margin-top: 53rpx;
}

.head_txt .orver {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    font-size: 28rpx;
    padding-top: 5px;
}

.fxcontainer {
    display: flex;
    font-family: "微软雅黑";
    flex-direction: column;
}

.view-list {
    box-sizing: border-box;
    background: #ffffff;
    border-bottom: 1px solid #f5f5f5;
}

.product-item {
    padding: 20rpx;
    display: flex;
}

.img_padd image {
    width: 100%;
    height: 190rpx;
    display: block;
}

.scroll-view1 {
    width: 26%;
    background: #f5f5f5;
}

.scroll-view2 {
    width: 74%;
    background: #ffffff;
}

.flex {
    display: flex;
}

.aside {
    width: 4rem;
    border-right: 1px solid #ddd;
    font-size: .85rem;
}

.type-nav {
    text-align: center;
    float: left;
    width: 100%;
    color: #666666;
    font-size: 28rpx;
    padding-top: 32rpx;
    padding-bottom: 32rpx;
}

.selected {
    margin-right: -1px;
    padding-left: -1px;
    color: #ff6619;
    background-color: #fff;
}

.b-flex {
    display: flex;
    flex-direction: column;
}

.b-flex>text {
    height: 40rpx;
    line-height: 40rpx;
}

.m1 {
    color: #ff6619;
    font-size: 28rpx;
}

.m2 {
    color: #999999;
    text-decoration: line-through;
    font-size: 22rpx;
}

.img_padd {
    border: 1px solid #e5e5e5;
    height: 190rpx;
    flex: 3;
    display: flex;
    justify-content: center;
    align-items: center;
}

.flex_r {
    flex: 5;
    padding-left: 27rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.bg {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
    overflow: hidden;
    color: #fff;
    z-index: 35;
}

.more_btn {
    width: 98%;
    height: 70rpx;
    font-size: 24rpx;
    line-height: 70rpx;
    text-align: center;
    border: 1rpx solid #dddee1;
    border-radius: 8rpx;
    margin: 0rpx auto;
    color: #666;
    margin-top: 3px;
}

.dx {
    font-size: 34rpx;
    color: #ffffff;
}

.fixed text {
    margin-top: 1px;
    height: 40px;
    line-height: 40px;
    width: 100%;
    text-align: center;
    font-size: 28rpx;
    color: #ffffff;
    background: #333333;
}

.name_txt {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    font-size: 28rpx;
    color: #333333;
}

.active {
    color: #ffffff;
    background: #ff6619;
}

.default {
    color: #333333;
    background: #f5f5f5;
}

.fx_footer {
    width: 100%;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
    border-top: 1px solid #ffffff;
    background: #FFF;
    z-index: 20;
    display: flex;
    height: 50px;
    line-height: 50px;
}

.fx_footer .tab1 {
    display: flex;
    border-right: 1px solid #ffffff;
    flex: 1;
    justify-content: center;
    font-size: 28rpx;
}

.fx_footer .tab-title {
    font-size: 28rpx;
    padding-left: 5rpx;
    color: #333;
}

.fx_footer .active .tab-title {
    color: #fff;
}

@font-face {
    font-family:'iconfont';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

