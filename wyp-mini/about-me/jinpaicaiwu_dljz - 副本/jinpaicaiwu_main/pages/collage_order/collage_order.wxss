.phone {
    display: flex;
    justify-content: space-between;
    border-bottom: 20rpx solid #f8f8f8;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 20rpx;
}

.p {
    display: flex;
    justify-content: space-between;
    flex: 12;
}

.icon {
    flex: 1;
}

.p .p2 {
    width: 90%;
    line-height: 40rpx;
}

.p1 {
    text-align: left;
}

.phone1 {
    display: flex;
    justify-content: space-between;
    border-bottom: 1rpx solid #f8f8f8;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 20rpx;
}

.count {
    color: #999999;
}

.item {
    padding: 20rpx;
    margin-top: 20rpx;
    border-bottom: 20rpx solid #f8f8f8;
}

.detail_c {
    display: flex;
    align-items: center;
    width: 96%;
    margin: 0 auto;
    border-bottom-right-radius: 30rpx;
    padding-bottom: 10rpx;
}

.deta_img image {
    width: 246rpx;
    height: 246rpx;
}

.deta_img {
    width: 246rpx;
    height: 246rpx;
    flex: 1;
}

.cc_add {
    display: flex;
    flex-direction: column;
    height: 220rpx;
    padding-left: 10rpx;
    flex: 5;
}

.cc_addt {
    display: flex;
    justify-content: space-between;
}

.cc_addc {
    width: 100%;
    flex: 1;
    padding-top: 74rpx;
    display: flex;
    justify-content: space-between;
    color: #333333;
    -webkit-flex: 5;
    flex: 5;
}

.flx_txt {
    display: flex;
    flex-direction: column;
    margin-left: 16rpx;
    margin-top: 3rpx;
    flex: 1;
}

.flx_txt>text {
    text-align: right;
}

.te1 {
    color: #ff0000;
    font-size: 30rpx;
}

.cc_text1 {
    font-size: 30rpx;
}

.item_list {
    background: #ffffff;
    padding: 20rpx;
}

.cc_text {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color: #1a1a1a;
    font-size: 35rpx;
    flex: 11;
}

.title {
    color: #333333;
    font-size: 30rpx;
    height: 66rpx;
    line-height: 66rpx;
    padding: 20rpx;
    border-bottom: 1px solid #f8f8f8;
}

.shop {
    flex: 4;
}

.textarea {
    flex: 11;
    margin-top: 16rpx;
}

.button {
    display: flex;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    justify-content: space-between;
    padding: 20rpx;
}

.button>view {
    flex: 1;
}

.button>text,.button>form {
    flex: 1;
    font-size: 30rpx;
    width: 50%;
}

.button .total_price {
    text-align: left;
    margin-left: 20rpx;
}

.bt {
    padding-left: 10rpx;
    padding-right: 10rpx;
    background: #ff3366;
    border-radius: 10rpx;
    color: #ffffff;
    font-size: 30rpx;
    height: 80rpx;
    line-height: 80rpx;
}

.mony {
    color: #333333;
}

.picker {
    width: 444rpx;
}

@font-face {
    font-family:'iconfont2';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
}

.iconfont2 {
    font-family: "iconfont2"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-jiantou:before {
    content: "\e619";
}

.icon-edit:before {
    content: "\e6ec";
}

.icon-jia:before {
    content: "\e612";
}

.icon-jiantou-xia:before {
    content: "\e62d";
}