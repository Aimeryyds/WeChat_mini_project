.bg_color {
    background: #f5f5f5;
    height: 100%;
    width: 100%;
    position: absolute;
}

.item {
    display: flex;
    flex-direction: column;
    padding: 20rpx;
    background: #ffffff;
    border-bottom: 1px solid #f5f5f5;
}

.item_c {
    display: flex;
    justify-content: space-between;
}

.item_t>text,.item_c>text,.item_b>text {
    height: 50rpx;
    line-height: 50rpx;
    font-size: 28rpx;
}

.item_t {
    font-weight: bold;
}

.col {
    color: #999999;
}

.col1 {
    color: #fe661a;
}

.list>.pagebox {
    border-bottom: 2rpx solid #d8d8d8;
    color: #c5c5c5;
    height: 2rpx;
    margin: 80rpx 10% 0;
    text-align: center;
    width: 80%;
}

.list>.pagebox>text {
    background-color: #ededed;
    display: inline-block;
    height: 48rpx;
    line-height: 48rpx;
    padding: 0 30rpx;
    margin-top: -26rpx;
    overflow: hidden;
}