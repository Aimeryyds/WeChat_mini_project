    width: 100rpx;
    height: 100rpx;
    text-align: center;
}

.eatch_item_r2 {
    display: flex;
    justify-content: space-between;
}

.red {
    color: #ff6619;
}

.hj {
    font-size: 30rpx;
    background: #ffffff;
