    height: 80rpx;
    line-height: 80rpx;
    width: 90%;
    margin: 0 auto;
    background: #ffffff;
}

.title_t>view>image {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    margin-top: 15rpx;
}

.title_t>view>text {
    font-size: 28rpx;
    padding-left: 10rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.list {
    border-bottom: 20rpx solid #f5f5f5;
}

