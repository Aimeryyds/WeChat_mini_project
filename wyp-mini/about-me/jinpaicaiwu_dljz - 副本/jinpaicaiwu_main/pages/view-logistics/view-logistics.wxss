@import "../../__wuBaseWxss__/0.wxss";

.main {
    background-color: #ededed;
    overflow-x: hidden;
}

.confirm-box {
    background-color: #fff;
    padding: 20rpx 30rpx;
}

.logistics-top {
    display: flex;
    align-items: center;
}

.logistics-top image {
    width: 180rpx;
    height: 180rpx;
    margin-right: 20rpx;
}

.logistics-top .normal_font {
    font-weight: bold;
}

.logistics-bottom {
    padding: 20rpx 30rpx;
    background-color: #fff;
}

.logistics-item {
    line-height: 50rpx;
    border-bottom: 1rpx solid #e9e9e9;
    margin-bottom: 20rpx;
}