.bu {
    font-size: 32rpx;
    color: #ffffff;
    background: #FF5801;
    width: 95%;
    margin: 0 auto;
    margin-top: 62rpx;
    height: 100rpx;
    line-height: 100rpx;
}

.aclist {
    font-size: 32rpx;
    color: #000;
    width: 95%;
    margin: 0 auto;
    border-bottom: 1px solid #ccc;
}

.aclist .ac {
    float: left;
    width: 65%;
    height: 100rpx;
    line-height: 100rpx;
}

.aclist .cl {
    clear: both;
}

.aclist .edit {
    float: right;
    display: flex;
    flex-direction: row;
    color: #FF0000;
    width: 35%;
    height: 100rpx;
    line-height: 100rpx;
}

.aclist .edit view:first-child {
    margin-right: 20rpx;
}

.aclist .edit .icon-shanchu {
    font-size: 36rpx;
}