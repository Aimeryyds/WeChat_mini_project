.body {
    width: 100%;
    height: 100%;
    color: #7d7d7d;
    position: absolute;
}

.header_top_L {
    width: 147rpx;
    height: 147rpx;
    border-radius: 50%;
    overflow: hidden;
}

.error_tips {
    position: absolute;
    width: 300rpx;
    margin-left: 30rpx;
    margin-top: 130rpx;
    font-size: 14px;
    color: red;
    z-index: 2;
}

.content {
    height: 772rpx;
    width: 80%;
    border-radius: 20rpx;
    margin: 0 auto;
    box-shadow: -10px 0 10px #f5f5f5,10px 0 10px #f5f5f5,0 -10px 10px #f5f5f5,0 10px 10px #f5f5f5;
    margin-top: 84rpx;
}

.content_t {
    height: 520rpx;
    width: 100%;
    background: #f7883a;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    position: relative;
}

.content_b {
    height: 252rpx;
    width: 100%;
    background: #ffffff;
    border-bottom-left-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
}

.code {
    width: 349rpx;
    height: 349rpx;
    background: linear-gradient(to left,#fbc49d,#fbc49d) left top no-repeat,linear-gradient(to bottom,#fbc49d,#fbc49d) left top no-repeat,linear-gradient(to left,#fbc49d,#fbc49d) right top no-repeat,linear-gradient(to bottom,#fbc49d,#fbc49d) right top no-repeat,linear-gradient(to left,#fbc49d,#fbc49d) left bottom no-repeat,linear-gradient(to bottom,#fbc49d,#fbc49d) left bottom no-repeat,linear-gradient(to left,#fbc49d,#fbc49d) right bottom no-repeat,linear-gradient(to left,#fbc49d,#fbc49d) right bottom no-repeat;
    background-size: 4px 30px,30px 4px,4px 30px,30px 4px;
    border-radius: 20rpx;
    position: absolute;
    top: 64rpx;
    left: 130rpx;
}

.code>image {
    width: 310rpx;
    height: 310rpx;
    position: absolute;
    border-radius: 20rpx;
    top: 18rpx;
    left: 18rpx;
}

.people {
    position: absolute;
    height: 147rpx;
    border-radius: 50%;
    overflow: hidden;
    bottom: -70rpx;
    margin: 0 auto;
    text-align: center;
    left: 38%;
}

.people>image {
    width: 147rpx;
    height: 147rpx;
    border-radius: 50%;
    border: 2rpx solid #ffffff;
}

.content_b {
    width: 100%;
    text-align: center;
    display: flex;
    flex-direction: column;
}

.d1 {
    font-size: 28rpx;
    color: #1a1a1a;
    height: 53rpx;
    line-height: 53rpx;
    margin-top: 100rpx;
}

.d2 {
    font-size: 30rpx;
    color: #f7883a;
    height: 53rpx;
    line-height: 53rpx;
}

.active {
    color: #ffffff;
    background: #ff6619;
}

.default {
    color: #333333;
    background: #f5f5f5;
}

.fx_footer {
    width: 100%;
    margin: 0 auto;
    position: fixed;
    bottom: 0;
    border-top: 1px solid #e1e1e1;
    background: #FFF;
    z-index: 20;
    display: flex;
    height: 50px;
    line-height: 50px;
}

.fx_footer .tab1 {
    display: flex;
    border-right: 1px solid #e1e1e1;
    flex: 1;
    justify-content: center;
    font-size: 28rpx;
}

.fx_footer .tab-title {
    font-size: 28rpx;
    padding-left: 5rpx;
    color: #333;
}

.fx_footer .active .tab-title {
    color: #fff;
}

@font-face {
    font-family:'iconfont';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal;
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

