@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-dianpu:before {
    content: "\e625";
}

.icon-yue:before {
    content: "\e637";
}

.icon-renqun:before {
    content: "\e622";
}

.icon-erweima:before {
    content: "\e7dd";
}

.icon-jiantou:before {
    content: "\e65e";
}

.icon-fenxiao:before {
    content: "\e61e";
}

.icon-zhanghuxinxi:before {
    content: "\e64f";
}

.icon-tianjia:before {
    content: "\e620";
}

.icon-zhanghaoguanli:before {
    content: "\e651";
}

.bg_color {
    background: #f5f5f5;
    height: 100%;
    width: 100%;
    position: absolute;
}

.card {
    width: 90%;
    border-radius: 20rpx;
    margin: 0 auto;
    box-shadow: -10px 0 10px #f5f5f5,10px 0 10px #f5f5f5,0 -10px 10px #f5f5f5,0 10px 10px #f5f5f5;
    margin-top: 20rpx;
    background: #ffffff;
    display: flex;
    justify-content: space-between;
    color: #1a1a1a;
    font-size: 28rpx;
    padding-left: 20rpx;
    padding-right: 20rxp;
    height: 120rpx;
}

.flex_row {
    display: flex;
    justify-content: space-between;
    flex: 11;
    height: 120rpx;
    line-height: 120rpx;
}

.card1 {
    flex: 1;
}

.card1>text,.flex_row>text {
    height: 120rpx;
    line-height: 120rpx;
}

.list {
    font-size: 28rpx;
    color: #1a1a1a;
    margin-top: 25rpx;
    background: #ffffff;
}

.list_item {
    display: flex;
    background: #ffffff;
    border-bottom: 1px solid #f5f5f5;
    justify-content: space-between;
}

.t>text,.b>input,.b>text {
    height: 77rpx;
    line-height: 77rpx;
    padding: 10rpx;
}

.t {
    flex: 3;
    display: flex;
}

.b {
    flex: 8;
    display: flex;
    justify-content: space-between;
}

.place {
    color: #e5e5e5;
}

.bu {
    font-size: 32rpx;
    color: #ffffff;
    background: #cccccc;
    width: 95%;
    margin: 0 auto;
    margin-top: 62rpx;
}

.add {
    color: #ff6619;
    font-size: 28rpx;
    display: flex;
    justify-content: flex-end;
    padding: 20rpx;
    margin-top: 25rpx;
}

.icon-tianjia {
    padding-right: 10rpx;
}

.active {
    background: #ff6619;
}

.selheight {
    line-height: 120rpx;
    height: 120rpx;
}