@import "../../__wuBaseWxss__/0.wxss";

.search_box {
    width: 100%;
    font-size: 24rpx;
    padding-top: 10rpx;
}

.search_box .search_box_content {
    box-sizing: border-box;
    position: relative;
    width: 720rpx;
    height: 76rpx;
    border: 1rpx solid #e9e9e9;
    background-color: #fff;
    padding-right: 92rpx;
    padding-left: 10rpx;
    border-radius: 10rpx;
    margin: 0 auto;
    margin-bottom: 10rpx;
}

.search_box .search_box_content image {
    position: absolute;
    right: 0;
    top: 12rpx;
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    margin-right: 10rpx;
}

.search_box .search_box_content input {
    width: 100%;
    height: 72rpx;
    padding: 0 10rpx;
    border: none;
    outline: none;
    background-color: #fff;
}

.search_box_content .search_history_lists {
    position: absolute;
    left: 0;
    top: 72rpx;
    border: 1rpx solid #e9e9e9;
    background-color: #fff;
    width: 100%;
    max-height: 600rpx;
    overflow-y: auto;
    z-index: 999;
}

.search_box_content .search_history_lists view {
    border-bottom: 1rpx solid #e9e9e9;
    line-height: 50rpx;
    padding: 0 20rpx;
}

.search_box_content .search_history_lists view:last-child {
    border-bottom: none;
}

