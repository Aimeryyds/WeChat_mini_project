.dig {
    width: 100%;
    background-color: #fff;
    position: fixed;
    left: 0;
    z-index: 14;
    bottom: 0;
    animation: action 1s;
    animation-fill-mode: forwards;
}

.dig_special {
    animation: action2 1s;
    animation-fill-mode: both;
}

@-webkit-keyframes action {
    0% {
        bottom: -100%;
    }

    100% {
        bottom: 0rpx;
    }
}

@keyframes action {
    0% {
        bottom: -100%;
    }

    100% {
        bottom: 0rpx;
    }
}

@-webkit-keyframes action2 {
    0% {
        bottom: -100%;
    }

    100% {
        bottom: 100rpx;
    }
}

@keyframes action2 {
    0% {
        bottom: -100%;
    }

    100% {
        bottom: 100rpx;
    }
}

