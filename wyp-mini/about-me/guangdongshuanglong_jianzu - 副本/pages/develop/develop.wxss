.container {
    flex-flow: column nowrap;
    justify-content: flex-start;
    align-items: flex-start;
}

.develop-cell {
    width: 100%;
    display: flex;
    justify-content: flex-start;
    flex-direction: row;
}

.develop-catalogue {
    display: flex;
    align-items: center;
    flex-direction: column;
    margin: 10rpx 40rpx -10rpx 40rpx;
    background-color: #ccc;
    width: 1rpx;
}

.develop-point {
    color: #ccc;
    height: 20rpx;
    font-size: 16px;
    margin-top: -18rpx;
    padding: 0;
}

.develop-content {
    margin-bottom: 20px;
    margin-right: 15px;
    display: flex;
    flex-direction: column;
    font-size: 16px;
    color: #333;
}

.develop-content .time {
    padding-top: 5px;
    font-size: 14px;
    color: #999;
}

.title-name {
    margin-top: 35px;
    font-size: 30px;
    margin-left: 15px;
    color: #333333;
}

.title-line {
    margin-top: 20px;
    margin-bottom: 20px;
    margin-left: 15px;
    background-color: #ccc;
    width: 52px;
    height: 2px;
}