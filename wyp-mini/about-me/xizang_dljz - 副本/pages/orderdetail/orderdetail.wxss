.order-status {
    height: 124rpx;
    display: flex;
    align-items: center;
    padding: 0 26rpx;
    background: #bb3333;
    width: 100%;
    color: #fff;
    font-size: 28rpx;
}

.order-status image {
    width: 70rpx;
    height: 70rpx;
    margin-right: 26rpx;
}

.order-address {
    display: flex;
    padding: 26rpx;
    background: #fff;
    margin-top: 26rpx;
    width: 100%;
}

.order-status>view {
    display: flex;
    flex-direction: column;
    line-height: 1.5;
}

.order-status>view .time-row {
    font-size: 26rpx;
    margin-top: 8rpx;
}

.order-address image {
    width: 32rpx;
    height: 32rpx;
    flex: 0 0 32rpx;
    margin-right: 26rpx;
}

.order-address .user-info {
    font-size: 28rpx;
    color: #373745;
    font-weight: bold;
    line-height: 1;
    display: flex;
    align-items: center;
}

.order-address .user-info>text {
    display: flex;
    align-items: center;
}

.order-address .user-info .phone {
    margin-left: 64rpx;
}

.order-address .address-info {
    font-size: 24rpx;
    color: #7e7e85;
    margin-top: 16rpx;
    line-height: 1.3;
}

.order-info {
    width: 100%;
    background: #fff;
    padding: 26rpx;
    margin-top: 26rpx;
    margin-bottom: 130rpx;
}

.order-info .info-row {
    display: flex;
    justify-content: space-between;
    height: 42rpx;
    align-items: center;
}

.order-info .info-row .left {
    color: #373745;
    font-size: 28rpx;
}

.order-info .info-row .left .label {
    color: #7e7e85;
    margin-right: 16rpx;
}

.group-label {
    background: #bb3333;
    color: #fff;
    font-size: 24rpx;
    padding: 2rpx 6rpx;
    margin-left: 4rpx;
}

.order-info .info-row .copy-btn {
    width: 88rpx;
    height: 42rpx;
    font-size: 22rpx;
    color: #97979e;
    border: 1px solid #97979e;
    border-radius: 42rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.order-footer {
    height: 104rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    width: 100%;
    background: #fff;
    position: fixed;
    left: 0;
    bottom: 0;
}

.order-footer .contact-btn {
    width: 144rpx;
    height: 52rpx;
    color: #bb3333;
    font-size: 24rpx;
    border: 1px solid #bb3333;
    border-radius: 52rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.order-cell .cell__footer {
    padding: 24rpx;
    text-align: right;
}

.order-cell .cell__footer .footer__row {
    font-size: 22rpx;
    color: #7e7e85;
}

.order-cell .cell__footer .footer__row:first-child {
    margin-top: 0;
}

.order-cell .cell__footer .footer__row {
    margin-top: 16rpx;
}

.order-cell .cell__footer .footer__row:last-child,.order-cell .cell__footer .footer__row:last-child .price-wrap {
    color: #bb3333;
}

.order-cell .cell__footer .footer__row:last-child {
    margin-top: 16rpx;
}

.order-cell .cell__footer .price-wrap {
    color: #373745;
}

.order-cell .cell__footer .price-wrap .price {
    font-size: 30rpx;
}

.buttons__row {
    display: flex;
    padding: 26rpx;
    justify-content: flex-end;
    border-top: 1px solid #efeff0;
}

.buttons__row .row__btn {
    width: 144rpx;
    height: 52rpx;
    border-radius: 52rpx;
    border: 1px solid #373745;
    font-size: 24rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #373745;
    margin-left: 26rpx;
}

.buttons__row .row__btn:last-child {
    border-color: #bb3333;
    color: #bb3333;
}

.item-image-wrap {
    position: relative;
}

.item-image-wrap .tag-label {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 56rpx;
    background: rgba(55,55,69,0.6);
    color: #fff;
    font-size: 26rpx;
    text-align: center;
    line-height: 56rpx;
}

.logistics-info {
    width: 100%;
    background: #fff;
}

.logistics-info .info-wrap {
    font-size: 26rpx;
    color: #c3c3c3;
    padding: 26rpx;
    padding-left: 84rpx;
}

.logistics-info .info-wrap view {
    line-height: 1.6;
}

.logistics-info .info-nav {
    margin-top: 0;
    border-top: 1px solid #efeff0;
    box-sizing: border-box;
}

.logistics-info .order-address.info-nav .user-info {
    color: #bb3333;
}

.logistics-info .info-nav {
    position: relative;
}

.logistics-info .info-nav .arrow {
    width: 15rpx;
    height: 26rpx;
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
}

.logistics-info .info-nav .no-title {
    font-size: 28rpx;
    color: #373745;
}