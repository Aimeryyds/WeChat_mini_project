.container {
    background-image: radial-gradient(circle at 50% 95%,#cc478a,#4f256b);
}

.empty-wrap {
    position: relative;
    left: auto;
    top: auto;
    transform: none;
    margin-top: 40rpx;
    background: rgba(62,29,85,0.4);
    width: 670rpx;
    height: 330rpx;
    border-radius: 20rpx;
    justify-content: center;
}

.empty-wrap>image {
    width: 120rpx;
    height: 120rpx;
}

.empty-wrap text {
    font-size: 24rpx;
    opacity: 0.6;
    letter-spacing: 2rpx;
    text-align: center;
    color: rgba(255,255,255,0.8);
}

.header {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.header .title {
    height: 280rpx;
    width: 750rpx;
}

.header .header__info {
    position: relative;
    display: flex;
    align-items: center;
    font-size: 30rpx;
    letter-spacing: 1.2rpx;
    color: rgba(255,255,255,0.8);
}

.header .header__info text {
    font-weight: 600;
    letter-spacing: 1.1rpx;
    color: #f97c26;
}

.header .header__shop {
    width: 660rpx;
    background: #fff9fd;
    display: flex;
    margin-top: 30rpx;
    border-radius: 40rpx 40rpx 0 0;
    align-items: center;
    padding: 40rpx;
    position: relative;
    z-index: 1;
}

.header .header__shop .left_icon {
    position: absolute;
    width: 56rpx;
    height: 53rpx;
    top: -16rpx;
    left: -24rpx;
    z-index: 0;
    transform: scale(0.8);
}

.header .header__shop .right_icon {
    position: absolute;
    width: 80rpx;
    height: 81rpx;
    right: -60rpx;
    bottom: -160rpx;
    transform: scale(0.8);
}

.header .header__shop::after {
    content: '';
    position: absolute;
    left: 40rpx;
    right: 40rpx;
    bottom: 0;
    height: 1px;
    border-top: 1px dashed #d5d5db;
}

.header .header__shop .shop__thumb {
    width: 170rpx;
    height: 170rpx;
    border-radius: 6rpx;
    flex: 0 0 auto;
    background: rebeccapurple;
    margin-right: 16rpx;
}

.header .header__shop .shop__info {
    display: flex;
    flex-direction: column;
    align-self: stretch;
}

.header .header__shop .info__title {
    color: #373745;
    letter-spacing: 2.6rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    font-size: 28rpx;
    line-height: 1.3;
    height: 1.3em;
}

.header .header__shop .info__desc {
    font-size: 24rpx;
    letter-spacing: 1.7rpx;
    opacity: 0.6;
    margin-top: 16rpx;
    flex: 1;
    line-height: 1.3;
    color: #5f5d69;
}

.header .header__shop .info__price-wrap {
    display: flex;
    align-items: flex-end;
}

.header .header__shop .info__price-wrap .price-tag {
    font-size: 20rpx;
    height: 36rpx;
    padding-left: 30rpx;
    padding-right: 8rpx;
    line-height: 36rpx;
    text-align: center;
    margin-left: 12rpx;
    letter-spacing: 0.7rpx;
    color: #fb8c44;
    background: linear-gradient(115deg,transparent 20rpx,rgba(251,137,68,0.2) 0) top left,linear-gradient(65deg,transparent 20rpx,rgba(251,137,68,0.2) 0) bottom left,linear-gradient(0,transparent,rgba(251,137,68,0.2) 0) top right,linear-gradient(0,transparent,rgba(251,137,68,0.2) 0) bottom right;
    background-size: 50% 50%;
    background-repeat: no-repeat;
}

.header .header__shop .info__price {
    font-size: 24rpx;
    color: #fb8c44;
}

.header .header__shop .info__price text {
    font-size: 40rpx;
}

.header .header__bargain {
    width: 660rpx;
    border-radius: 0 0 40rpx 40rpx;
    background: #fff9fd;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 40rpx;
    position: relative;
}

.header .header__bargain .bargain__title {
    font-size: 28rpx;
    color: #373745;
    letter-spacing: 1.7rpx;
    display: flex;
    align-items: center;
}

.header .header__bargain .bargain__title image {
    width: 36rpx;
    height: 36rpx;
    margin-right: 18rpx;
}

.header .header__bargain .bargain__title text {
    font-size: 36rpx;
    color: #fb8c44;
    font-weight: 500;
    margin: 0 10rpx;
}

.header .header__bargain .bargain__progress {
    align-self: stretch;
    height: 12rpx;
    border-radius: 8rpx;
    background-color: rgba(87,39,109,0.2);
    margin: 16rpx 0 24rpx;
    display: flex;
    justify-content: flex-end;
}

.header .header__bargain .bargain__progress .sub_progress {
    height: 16rpx;
    margin-top: -2rpx;
    border-radius: 8rpx;
    width: 90%;
    background-image: linear-gradient(268deg,#fb8c44,#fa2b48);
    box-shadow: 0 2px 10px 0 rgba(250,81,70,0.4);
}

.header .header__bargain .bargain__price {
    display: flex;
    justify-content: space-between;
    align-self: stretch;
    font-size: 24rpx;
    font-weight: 500;
    line-height: 1;
    letter-spacing: 1rpx;
    align-items: flex-end;
    color: #ffd2b5;
}

.header .header__bargain .bargain__price view {
    padding: 8rpx;
    color: #fb8c44;
}

.header .header__num {
    width: 690rpx;
    height: 72rpx;
    line-height: 72rpx;
    background: #480d5a;
    color: #fff;
    font-size: 28rpx;
    margin-top: 30rpx;
    text-align: center;
    letter-spacing: 2.1rpx;
    border-radius: 22rpx;
}

.header .header__num text {
    font-weight: 600;
    margin: 0 8rpx;
}

.header .header__btn {
    color: #fff;
    margin-top: 56rpx;
    width: 660rpx;
    height: 110rpx;
    line-height: 110rpx;
    border-radius: 55rpx;
    background-image: linear-gradient(260deg,#fb8c44,#fa2b48);
}

.header .header__time {
    font-size: 28rpx;
    color: #fff;
    line-height: 1.07;
    letter-spacing: 1.7rpx;
    margin-top: 20rpx;
}

.header .header__time text {
    font-weight: 500;
}

.common {
    margin-top: 56rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.ctit {
    font-size: 30rpx;
    font-weight: 500;
    letter-spacing: 1.6rpx;
    color: #fff;
    position: relative;
}

.ctit::before {
    content: '';
    position: absolute;
    width: 80rpx;
    height: 6rpx;
    border-radius: 4rpx;
    background-image: linear-gradient(to right,rgba(255,255,255,0.0),rgba(255,255,255,0.8));
    display: block;
    left: -110rpx;
    top: 50%;
    transform: translateY(-50%);
}

.ctit::after {
    content: '';
    position: absolute;
    width: 80rpx;
    height: 6rpx;
    border-radius: 4rpx;
    background-image: linear-gradient(to right,rgba(255,255,255,0.0),rgba(255,255,255,0.8));
    display: block;
    right: -110rpx;
    top: 50%;
    transform: translateY(-50%) rotate(-180deg);
}

.help-wrap {
    max-height: 330rpx;
    overflow: scroll;
    background: rgba(62,29,85,0.4);
    border-radius: 20rpx;
    margin-top: 30rpx;
    width: 670rpx;
}

.help-item {
    height: 88rpx;
    display: flex;
    align-items: center;
    padding: 16rpx;
}

.help-item>image {
    width: 56rpx;
    height: 56rpx;
    border-radius: 50%;
    flex: 0 0 auto;
    margin-right: 30rpx;
}

.help-item .h_info {
    font-size: 18rpx;
    letter-spacing: 1rpx;
    color: #fff;
    flex: 1;
}

.help-item .h_info text {
    opacity: 0.6;
}

.help-item .h_info view {
    font-size: 24rpx;
    font-weight: 500;
    margin-bottom: 8rpx;
    line-height: 1.17;
}

.help-item .h_pri {
    font-size: 24rpx;
    letter-spacing: 1.3rpx;
    color: #fff;
    align-items: center;
    display: flex;
}

.help-item .h_pri image {
    width: 16rpx;
    height: 32rpx;
    margin-right: 16rpx;
}

.recommon-grid {
    width: 690rpx;
    margin-top: 30rpx;
}

.recommon-grid .recommon-item {
    border-radius: 22rpx;
    position: relative;
    background: #fff;
}

.recommon-grid .recommon-item>image {
    border-radius: 22rpx 22rpx 0 0;
}

.recommon-grid .recommon-item .item-footer {
    flex-direction: column;
    padding: 16rpx;
}

.recommon-grid .recommon-item .item-footer .item-left .item-title {
    font-size: 26rpx;
    color: #373745;
    letter-spacing: 1.5rpx;
    margin: 0;
}

.recommon-grid .recommon-item .item-footer .item-price {
    display: flex;
    align-self: stretch;
    justify-content: space-between;
    align-items: center;
    font-size: 32rpx;
    font-weight: 500;
    letter-spacing: 2rpx;
    color: #fb8944;
    margin-top: 20rpx;
}

.recommon-grid .recommon-item .tag-label {
    position: absolute;
    left: -2px;
    top: -3px;
    width: 72rpx;
    height: 72rpx;
}

.recommon-grid .recommon-item .group-tag {
    font-size: 18rpx;
    letter-spacing: 2rpx;
    height: 30rpx;
    padding: 0 8rpx;
    line-height: 30rpx;
    border-radius: 8rpx;
    color: #fff;
    background-color: #bb3333;
}

.footer {
    position: fixed;
    left: 0;
    bottom: 0;
    right: 0;
    height: 96rpx;
    display: flex;
}

.footer .btn,.footer .p-btn {
    font-size: 32rpx;
    color: #fff;
    letter-spacing: 1rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex: 2;
    background-image: linear-gradient(256deg,#fdbb7b,#fb8645);
}

.footer .p-btn {
    background-image: linear-gradient(256deg,#ff7766,#f23f39);
}

.tbox {
    width: 560rpx;
    position: fixed;
    left: 50%;
    top: 50%;
    transform: translateX(-50%) translateY(-50%);
    z-index: 999;
}

.tbox .t {
    border-radius: 16rpx 16rpx 0 0;
    height: 120rpx;
    background: #fff;
    position: relative;
}

.tbox .t image {
    position: absolute;
    width: 240rpx;
    height: 176rpx;
    left: 50%;
    transform: translateX(-50%);
    top: -66rpx;
}

.tbox .c {
    padding-bottom: 40rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #fff;
    width: 100%;
    color: #373745;
    text-align: center;
}

.tbox .c view {
    font-size: 36rpx;
    font-weight: 500;
    letter-spacing: 1.8rpx;
}

.tbox .c text {
    margin-top: 16rpx;
    font-size: 30rpx;
    line-height: 1.3;
}

.tbox .c text text {
    font-weight: 500;
    color: #ff661f;
}

.tbox .f {
    border-radius: 0 0 16rpx 16rpx;
    height: 96rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background-image: linear-gradient(260deg,#fb8c44,#fa2b48);
    color: #fff;
    letter-spacing: 1.8rpx;
    font-weight: 500;
    font-size: 30rpx;
    border-top: 1px solid #ededed;
}

.tbox .close-btn {
    width: 56rpx;
    height: 56rpx;
    display: block;
    margin: 72rpx auto 0;
}

@import "../../../wuBaseWxss/12.wxss";

.purchase-window .purchase-form .info .content .price-wrap {
    color: #fb8c44;
    font-size: 24rpx;
}

.purchase-window .purchase-form .info .content .price-wrap .price {
    font-size: 40rpx;
}

.purchase-window .button-wrap .purchase-button {
    background-image: linear-gradient(263deg,#ff7766,#f23f39);
    font-size: 30rpx;
    font-weight: 500;
    letter-spacing: 2rpx;
}

.purchase-window .standard>view text {
    background: #f4f4f4;
    letter-spacing: 2rpx;
    color: #b2b2b6;
}

.purchase-window .standard>view text.selected {
    background-image: linear-gradient(251deg,#ff7766,#f23f39);
    color: #fff;
}