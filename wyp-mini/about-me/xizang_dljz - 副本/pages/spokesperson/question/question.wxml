<view class="container">
    <view class="question-list">
        <view wx:for="{{list}}" wx:key="*this">
            <view>Q : {{item.title}}</view>
            <text>A : {{item.answer}}</text>
        </view>
        <view style="color: #7e7e85;font-size: 28rpx;">如有其他不清楚的问题请咨询商家客服。</view>
    </view>
</view>
