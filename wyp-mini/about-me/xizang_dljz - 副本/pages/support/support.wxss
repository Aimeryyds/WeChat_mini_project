.model-title {
    font-size: 34rpx;
    margin-top: 42rpx;
    font-weight: bold;
    position: relative;
    padding-bottom: 18rpx;
}

.white-wrap {
    background: #fff;
}

.model-title::after {
    content: '';
    position: absolute;
    display: block;
    width: 36rpx;
    height: 4rpx;
    background: #000;
    left: 50%;
    transform: translateX(-50%);
    bottom: 0;
}

.support-header {
    background: linear-gradient(60deg,#d80645,#e83a2f);
    width: 100%;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.support-header .support-title {
    color: #fff;
    font-size: 40rpx;
    margin: 72rpx 0 24rpx;
    font-weight: bold;
}

.support-header .support-desc {
    font-size: 28rpx;
    color: #e0ffff;
    margin-bottom: 56rpx;
}

.support-header .phone-button {
    width: 192rpx;
    height: 72rpx;
    border-radius: 3px;
    font-size: 30rpx;
    color: #ff1a2a;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #fff;
}

.support-header .support-bg {
    width: 667rpx;
    margin-top: 72rpx;
    display: block;
}

.support-pro {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.support-pro .support-child {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    margin-top: 108rpx;
}

.support-pro .support-child:last-child {
    margin-bottom: 108rpx;
}

.support-pro .support-child .child-title {
    font-size: 28rpx;
    color: #363636;
    display: flex;
    margin-bottom: 56rpx;
    align-items: center;
}

.support-pro .support-child .child-title .text {
    margin-left: 16rpx;
}

.support-pro .support-child .child-title .first {
    width: 48rpx;
    height: 53rpx;
}

.support-pro .support-child .child-title .second {
    width: 108rpx;
    height: 56rpx;
}

.support-pro .support-child .child-title .third {
    width: 132rpx;
    height: 56rpx;
}

.support-pro .support-child .pro-model {
    display: flex;
    justify-content: space-between;
    width: 100%;
    padding: 0 54rpx;
}

.support-pro .support-child .pro-model .pro-item {
    width: 120rpx;
    font-size: 24rpx;
    color: #363636;
    text-align: center;
}

.support-pro .support-child .pro-model .pro-item .pro-image {
    width: 120rpx;
    height: 120rpx;
    margin-bottom: 24rpx;
    box-shadow: 0 12px 24px rgba(118,118,118,0.12);
}

.support-pro .support-child .industry-model {
    width: 100%;
    padding: 0 27rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    margin-bottom: -32rpx;
}

.support-pro .support-child .industry-model .industry-item {
    font-size: 24rpx;
    color: #363636;
    border: 1px solid #f3f3f3;
    padding: 10rpx 20rpx;
    border-radius: 25% / 100%;
    margin: 0 12rpx;
    margin-bottom: 32rpx;
}

.support-pro .support-child .marketing-plug {
    width: 100%;
    margin-bottom: -32rpx;
    display: flex;
    flex-wrap: wrap;
    padding: 0 30rpx;
}

.support-pro .support-child .marketing-plug .plug-item {
    width: 20%;
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 24rpx;
    color: #363636;
    margin-bottom: 32rpx;
}

.support-pro .support-child .marketing-plug .plug-item .plug-image {
    width: 72rpx;
    height: 72rpx;
    margin-bottom: 24rpx;
}

.idea-feature {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #f8f8f8;
    margin-bottom: 108rpx;
}

.idea-feature .feature-item {
    font-size: 24rpx;
    color: #363636;
    height: 192rpx;
    background: #fff;
    position: relative;
    margin: 36rpx 0;
    display: flex;
    align-items: center;
}

.idea-feature .feature-item .feature-img {
    position: absolute;
    left: 0;
    top: -24rpx;
    width: 133rpx;
    height: 284rpx;
}

.idea-feature .feature-item .feature-text {
    line-height: 1.5;
    margin-left: 140rpx;
    margin-right: 30rpx;
}

.idea-feature .feature-item:last-child {
    margin-bottom: 0;
}

.idea-feature .feature-item:nth-child(even) .feature-img {
    right: 0;
    left: auto;
}

.idea-feature .feature-item:nth-child(even) .feature-text {
    margin-left: 30rpx;
    margin-right: 140rpx;
}

.support-footer {
    width: 100%;
    background: #fff;
}

.support-footer .footer-info {
    display: flex;
    padding: 54rpx;
    justify-content: space-between;
    align-items: center;
}

.support-footer .footer-info .info-left .info-title {
    font-size: 48rpx;
    color: #363636;
    font-weight: bold;
    margin-bottom: 16rpx;
}

.support-footer .footer-info .info-left .info-tag {
    font-size: 24rpx;
    background: #363636;
    height: 40rpx;
    color: #fff;
    line-height: 40rpx;
    padding: 0 8rpx;
}

.support-footer .footer-info .info-left .info-websize {
    font-size: 30rpx;
    margin-top: 34rpx;
    color: #363636;
}

.support-footer .footer-info .logo-icon {
    width: 160rpx;
    height: 160rpx;
}

.support-footer .contact-info {
    height: 88rpx;
    text-align: center;
    line-height: 88rpx;
    color: #fff;
    background: linear-gradient(60deg,#d80645,#e83a2f);
    font-size: 30rpx;
}