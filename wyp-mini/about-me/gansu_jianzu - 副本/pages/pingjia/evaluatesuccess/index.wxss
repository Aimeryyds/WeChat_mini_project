.evaluate-success {
    display: flex;
    flex-flow: wrap column;
    justify-content: center;
    font-size: 28rpx;
    align-items: center;
    padding: 160rpx 0 64rpx;
}

.evaluate-success__img {
    width: 200rpx;
    height: 200rpx;
    margin-bottom: 32rpx;
}

.evaluate-success__groupbtn {
    display: flex;
    justify-content: center;
    font-size: 28rpx;
}

.evaluate-success__backbtn {
    width: 216rpx;
    height: 64rpx;
    border: 1px solid #d2d2d2;
    margin-right: 40rpx;
    text-align: center;
    line-height: 64rpx;
    border-radius: 8rpx;
}

.evaluate-success__viewbtn {
    width: 216rpx;
    height: 64rpx;
    background: #f22222;
    border: 1px solid #f22222;
    color: #fff;
    text-align: center;
    line-height: 64rpx;
    border-radius: 8rpx;
}