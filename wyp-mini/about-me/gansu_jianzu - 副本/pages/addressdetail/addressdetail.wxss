.location-wrap {
    top: 0;
    left: 0;
    right: 0;
    display: flex;
    height: 82rpx;
    background: #fff;
    align-items: center;
    justify-content: center;
    color: #e31719;
    font-size: 30rpx;
}

.search-wrap {
    padding: 15rpx 20rpx;
    display: flex;
    background-color: #fff;
}

.search-box {
    min-width: 368rpx;
    background-color: #f4f4f4;
    flex: 1;
    border-radius: 30rpx;
    height: 56rpx;
    display: flex;
    align-items: center;
    padding: 0 14rpx;
}

.search-box text {
    margin-left: 14rpx;
    font-size: 24rpx;
    color: #aaa;
}

.myAddressd__title {
    display: block;
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 40rpx;
    font-size: 28rpx;
}

.location-wrap image {
    width: 33rpx;
    height: 33rpx;
    margin-right: 22rpx;
}

.myAddressd-list {
    display: flex;
    background: #fff;
    align-items: center;
    border-bottom: 1px solid #e5e5e5;
    padding-right: 30rpx;
}

.myAddressd-list-wrap {
    margin-bottom: 200rpx;
}

.myAddressd-list__info {
    flex: 1;
    padding: 30rpx;
}

.myAddressd-list__info.active {
}

.myAddressd-list__info-user text {
    margin-right: 126rpx;
    color: #999;
    font-size: 28rpx;
}

.myAddressd-list__detail {
    margin-top: 15rpx;
    font-size: 26rpx;
    color: #666;
}

.myAddressd-list__edit image {
    width: 34rpx;
    height: 34rpx;
}

.new-myAddressd-btn {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 92rpx;
    line-height: 92rpx;
    background: #c41412;
    border-radius: 0;
    font-size: 32rpx;
}