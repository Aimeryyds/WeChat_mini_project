.search-wrap {
    display: flex;
    align-items: center;
    background-color: #fff;
}

.search-back__img {
    width: 22rpx;
    height: 39rpx;
    margin-left: 30rpx;
}

.search-cont {
    flex: 1;
    padding: 0rpx 15rpx;
    display: flex;
    border: 1px solid #ededed;
    align-items: center;
    background: #fff;
    margin: 12rpx 45rpx 12rpx 25rpx;
    border-radius: 3px;
}

.search-input {
    width: 90%;
    margin-left: 8rpx;
    font-size: 24rpx;
    height: 46rpx;
}

.pro-card {
    display: flex;
    align-items: center;
    padding: 20rpx 30rpx 15rpx;
    border-bottom: #ededed 1px solid;
    background: #fff;
}

.pro-card__img {
    width: 128rpx;
    height: 128rpx;
    margin-right: 20rpx;
}

.pro-card__detail {
    flex: 1;
}

.pro-card__style {
    font-size: 30rpx;
}

.pro-card__num {
    font-size: 24rpx;
    line-height: 24rpx;
    color: #999999;
    margin: 10rpx 0 30rpx;
}

.pro-card__price {
    color: #c41412;
    font-size: 30rpx;
}

.search-end {
    font-size: 22rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 20rpx 0;
}

.search-end text {
    color: #C9C9C9;
    padding: 0 15rpx;
}

.search-end .search-end-border {
    width: 40rpx;
    border-top: 1rpx #C9C9C9 solid;
}