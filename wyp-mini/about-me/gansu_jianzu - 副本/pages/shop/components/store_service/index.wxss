.tuan-tag {
    display: flex;
    height: 28rpx;
    line-height: 28rpx;
    padding: 0 10rpx;
    background: linear-gradient(to right,#cb58e9,#de56c3,#fa548d);
    font-size: 16rpx;
    color: #fff;
    border-radius: 4rpx;
}

.seckill-tag {
    width: 68rpx;
    height: 28rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #f22222;
    color: #fff;
    font-size: 20rpx;
    font-style: italic;
    border-radius: 4rpx;
}

.seckill-tag image {
    width: 14rpx;
    height: 22rpx;
    margin-right: 4rpx;
}

.manjian-tag {
    width: 48rpx;
    height: 28rpx;
    line-height: 28rpx;
    background: #9a6bff;
    font-size: 20rpx;
    color: #fff;
    text-align: center;
    border-radius: 4rpx;
}

.zan-panel__no-magrin-top {
    margin-top: 0;
}

.zan-panel {
    background: #fff;
    border-top: 0px;
    border-bottom: 1rpx solid #eee;
    margin-top: 12rpx;
    overflow: hidden;
}

.shop__header {
    padding: 24rpx;
}

.shop__header-header {
    display: flex;
}

.shop__header-logo {
    width: 180rpx;
    height: 180rpx;
    border: 1px solid #e5e5e5;
}

.shop__header-name {
    font-weight: bold;
    margin: 20rpx 0 0 30rpx;
    font-size: 28rpx;
}

.shop__header-album {
    margin-top: 40rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.shop__header-album__image1 {
    width: 702rpx;
    height: 222rpx;
}

.shop__header-album__image2 {
    width: 342rpx;
    height: 222rpx;
}

.shop__header-album__image3 {
    width: 222rpx;
    height: 222rpx;
}

.shop__info {
}

.shop__info-item {
    font-size: 24rpx;
    display: flex;
    padding: 24rpx;
    align-items: center;
    border-bottom: 1px solid #e5e5e5;
}

.shop__info-item-icon {
    width: 30rpx;
    height: 42rpx;
}

.shop__info-item-text {
    margin-left: 24rpx;
    flex: 1;
}

.shop__info-item-lt {
    font-size: 24rpx;
    color: #aaa;
}

.shop__info-item__icon-order {
    width: 24rpx;
    height: 24rpx;
    color: #fff;
    font-size: 20rpx;
    background-color: orange;
    padding: 4rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.workers-list {
}

.workers-list__title {
    display: flex;
    padding: 24rpx;
    border-bottom: 1rpx solid #e5e5e5;
}

.workers-list__title-name {
    flex: 1;
    font-size: 28rpx;
}

.workers-list__list {
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding: 24rpx;
}

.workers-list__item {
    margin-right: 20rpx;
    width: 160rpx;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    display: flex;
    background-color: #fff;
}

.workers-list__item__avatar {
    border-radius: 50%;
    width: 158rpx;
    height: 158rpx;
}

.workers-list__item__name {
    margin-top: 20rpx;
    font-size: 28rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    width: 6em;
    text-align: center;
}

.workers-list__item__title {
    margin-top: 12rpx;
    font-size: 24rpx;
    color: #aaa;
}

.workers-list__item__desc {
    margin-top: 12rpx;
    font-size: 24rpx;
    color: #aaa;
}

.services-list {
    font-size: 28rpx;
}

.services-list__item {
    padding: 24rpx;
    align-items: center;
    justify-content: flex-start;
    display: flex;
    background-color: #fff;
    border-bottom: 1rpx solid #e5e5e5;
}

.services-list__item__avatar {
    width: 160rpx;
    height: 160rpx;
}

.services-list__item__detail {
    margin-left: 24rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    flex: 1;
}

.services-list__item__name {
    position: relative;
    color: #000;
    margin-bottom: 12rpx;
    display: flex;
    flex-direction: row;
}

.services-list__item__desc {
    font-size: 24rpx;
    color: #aaa;
    height: 38rpx;
    line-height: 38rpx;
}

.services-list__price-wrap {
    display: flex;
    align-items: flex-end;
}

.services-list__item__detail__price {
    margin-top: 20rpx;
    flex: 1;
}

.services-list__item__detail__price-cur {
    font-size: 36rpx;
    color: #f22626;
    margin-right: 12rpx;
}

.services-list__item__detail__price-old {
    margin-left: 12rpx;
    font-size: 28rpx;
    color: #aaa;
}

.goods-list__btn {
    align-self: flex-end;
    width: 116rpx;
    height: 48rpx;
    font-size: 24rpx;
    background: #f22222;
    color: #fff;
    text-align: center;
    line-height: 48rpx;
    border-radius: 8rpx;
}

.zan-ellipsis--l2 {
    max-height: 40px;
    line-height: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.shop__info-services {
    display: flex;
    align-items: center;
    border-bottom: 2rpx solid #e5e5e5;
}

.shop__info-services-label {
    font-size: 28rpx;
    padding: 36rpx 24rpx;
    width: 112rpx;
}

.shop__info-services-fac {
    display: flex;
    flex: 1;
}

.shop__info-services-item {
    font-size: 24rpx;
    text-align: center;
    margin: 0 20rpx;
    color: #9d9d9d;
}

.navigator-arrow {
    padding-right: 50rpx;
    position: relative;
}

.navigator-arrow:after {
    content: " ";
    display: inline-block;
    height: 16rpx;
    width: 16rpx;
    border-width: 2rpx 2rpx 0 0;
    border-color: #888888;
    border-style: solid;
    transform: matrix(0.71,0.71,-0.71,0.71,0,0);
    position: absolute;
    top: 50%;
    margin-top: -8rpx;
    right: 28rpx;
}