.cart-jiesuan-button {
    margin-right: 0;
    border-radius: 0;
    line-height: 3.2;
}

.cart-jiesuan-button::after {
    border: 0;
}

.cart-jiesuan-button-able {
    background: #f22222;
    font-size: 32rpx;
    color: #fff;
}

.cart-jiesuan-button-disable {
    background: #d2d2d2;
    font-size: 32rpx;
    color: #fff;
}