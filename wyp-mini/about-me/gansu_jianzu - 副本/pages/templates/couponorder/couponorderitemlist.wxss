.coupon_order_goods-detail-box {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 20rpx 15rpx 20rpx 15rpx;
    font-size: 28rpx;
    position: relative;
    background: #fff;
}

.new_order_hor {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.price {
    font-size: 30rpx;
    color: #333333;
}

.new_order_coupon-price {
    font-size: 26rpx;
    color: #333333;
    margin-left: 10rpx;
}

.new_order_coupon-num {
    font-size: 20rpx;
    color: #333333;
    margin-top: 10rpx;
    margin-left: 10rpx;
    display: flex;
    justify-content: flex-end;
}

.new_order_coupon {
    position: relative;
    width: 162rpx;
    height: 90rpx;
    border-radius: 8rpx;
    overflow: visible;
}

.new_order_coupon::before {
    position: absolute;
    top: 35rpx;
    left: -1px;
    width: 12rpx;
    height: 20rpx;
    background: #fff;
    content: '';
    border-radius: 0 10rpx 10rpx 0;
}

.new_order_coupon::after {
    position: absolute;
    top: 35rpx;
    right: -1px;
    width: 12rpx;
    height: 20rpx;
    background: #fff;
    content: '';
    border-radius: 10rpx 0 0 10rpx;
}

.new_order_coupon image {
    width: 162rpx;
    height: 90rpx;
    border-radius: 8rpx;
}

.new_order_giftcard {
    border: 1px solid #f4f4f4;
    border-radius: 8rpx;
}

.new_order_giftcard image {
    width: 140rpx;
    height: 90rpx;
    vertical-align: top;
}