.supplyinfo-detail {
    display: flex;
    padding: 30rpx 0;
    border-bottom: 2rpx #f4f4f4 solid;
}

.supplyinfo-detail-img {
    margin: 0 24rpx;
    width: 132rpx;
    height: 132rpx;
}

.supplyinfo-detail-right {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.supplyinfo-detail-name {
    padding-right: 28rpx;
    font-size: 28rpx;
    position: relative;
}

.supplyinfo-detail-name view {
    text-indent: 42rpx;
    max-height: 88rpx;
    line-height: 44rpx;
}

.supplyinfo-detail-name image {
    position: absolute;
    top: 8rpx;
    left: 0;
    width: 32rpx;
    height: 35rpx;
}

.supplyinfo-detail-footer {
    display: flex;
    justify-content: space-between;
    padding-right: 64rpx;
}

.supplyinfo-detail-phone image {
    width: 43rpx;
    height: 43rpx;
}

.supplyinfo-detail-time {
    margin-top: 14rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}