<template name="news">
    <view bindtap="handlenews" class="newsinfo-detail" data-id="{{id}}">
        <image class="newsinfo-detail-img" src="{{imgsrc}}"></image>
        <view class="newsinfo-detail-right">
            <view class="newsinfo-detail-name zan-ellipsis--l2">{{title}}</view>
            <view class="newsinfo-detail-time">{{time}}</view>
        </view>
    </view>
</template>
