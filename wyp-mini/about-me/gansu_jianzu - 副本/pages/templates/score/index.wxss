.score-panel {
    margin-bottom: 20rpx;
    background: #fff;
    font-size: 28rpx;
    color: #333;
}

.score-item {
    display: flex;
    padding: 20rpx;
    border-top: 1px solid #f4f4f4;
}

.score-item text {
    width: 130rpx;
}

.score-item__cont {
    flex: 1;
    color: rgba(0,0,0,0.43);
}