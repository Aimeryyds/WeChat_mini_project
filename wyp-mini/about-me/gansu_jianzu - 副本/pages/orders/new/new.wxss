@import "../../templates/orders/orderitemlistdetail.wxss";

@import "../../templates/orders/orderaddress.wxss";

.order-no_text {
    font-size: 24rpx;
    margin-top: 10rpx;
    color: #535353;
}

.order-no_state {
    position: relative;
    align-items: center;
    line-height: 1.4;
    display: flex;
    flex-direction: row;
}

.sec-wrap {
    background-color: #fff;
    margin-top: 20rpx;
}

.bottom-fiexd {
    position: fixed;
    bottom: 0;
    left: 0;
}

.sec-wrap .order-status {
    width: 720rpx;
    margin-left: 30rpx;
    border-bottom: 1rpx solid #eee;
    height: 140rpx;
    display: flex;
    align-items: center;
}

.order-status .icon-box {
    width: 80rpx;
    height: 80rpx;
    overflow: hidden;
    margin-right: 30rpx;
}

.order-status .icon-box .icon {
    width: 80rpx;
    height: 80rpx;
}

.order-status .right-text {
    width: 580rpx;
    overflow: hidden;
}

.order-status .right-text .status {
    font-size: 28rpx;
    color: #000;
    margin-bottom: 10rpx;
}

.order-status .right-text .red {
    color: #e64340;
}

.order-status .right-text .des {
    font-size: 24rpx;
    color: #999;
}

.address-sec {
    width: 720rpx;
    margin-left: 30rpx;
    display: flex;
    align-items: center;
    padding: 30rpx 0;
}

.address-sec .icon-box {
    width: 30rpx;
    align-self: flex-start;
    overflow: hidden;
    margin-right: 35rpx;
}

.address-sec .icon-box .icon {
    width: 30rpx;
    height: 30rpx;
}

.address-sec .right-box {
    width: 620rpx;
}

.address-sec .right-box .name-tel {
    font-size: 28rpx;
    color: #000;
    margin-bottom: 20rpx;
}

.address-sec .right-box .text {
    font-size: 24rpx;
    color: #888;
    line-height: 36rpx;
    height: 72rpx;
    overflow: hidden;
}

.wuliu-box {
    width: 720rpx;
    margin-left: 30rpx;
    border-bottom: 1rpx solid #eee;
    display: flex;
    align-items: center;
    padding: 30rpx 0;
}

.wuliu-box .icon-box {
    width: 40rpx;
    height: 40rpx;
    overflow: hidden;
    margin-right: 31rpx;
    align-self: flex-start;
}

.wuliu-box .icon-box .icon {
    width: 40rpx;
    height: 40rpx;
}

.wuliu-box .arrow-right {
    width: 15rpx;
    height: 24rpx;
}

.wuliu-box .arrow-right .arrow {
    width: 15rpx;
    height: 24rpx;
}

.wuliu-box .right-text {
    width: 575rpx;
    margin-right: 30rpx;
}

.wuliu-box .right-text .order-number {
    font-size: 28rpx;
    color: #000;
    margin-bottom: 14rpx;
}

.wuliu-box .right-text .wuliu-text,.wuliu-box .right-text .wuliu-date {
    font-size: 24rpx;
    color: #888;
    line-height: 36rpx;
}

.goods-list {
    width: 100%;
    background-color: #fff;
    margin-bottom: 20rpx;
    margin-top: 20rpx;
}

.goods-list .list-title {
    font-size: 28rpx;
    color: #000;
    padding: 30rpx 0 25rpx 30rpx;
}

.goods-list .a-goods {
    width: 720rpx;
    margin-left: 30rpx;
    display: flex;
    border-top: 1px solid #eee;
    padding: 30rpx 30rpx 30rpx 0;
}

.goods-list .a-goods .img-box {
    width: 160rpx;
    height: 160rpx;
    overflow: hidden;
    margin-right: 20rpx;
    background-color: #d8d8d8;
}

.goods-list .img-box .img {
    width: 160rpx;
    height: 160rpx;
}

.goods-list .a-goods .text-box {
    width: 510rpx;
    box-sizing: border-box;
    padding-top: 10rpx;
}

.goods-list .btn-row {
    width: 720rpx;
    margin-left: 30rpx;
    border-top: 1rpx solid #eee;
}

.confirm-btn {
    background: #fff;
    border: 1rpx solid #e64340;
    border-radius: 6rpx;
    width: 164rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    margin: 20rpx 30rpx 20rpx auto;
    font-size: 26rpx;
    color: #e64340;
}

.a-goods .text-box .arow {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.a-goods .text-box .arow .goods-name {
    width: 360rpx;
    font-size: 26rpx;
    height: 74rpx;
    color: #000;
    line-height: 1.6;
    overflow: hidden;
}

.a-goods .text-box .arow01 {
    margin-bottom: 30rpx;
}

.a-goods .text-box .arow .goods-price {
    font-size: 26rpx;
    color: #000;
    align-self: flex-start;
}

.a-goods .text-box .arow .goods-label {
    font-size: 26rpx;
    color: #999;
}

.a-goods .text-box .arow .goods-num {
    font-size: 26rpx;
    color: #999;
}

.new-order-between-box {
    justify-content: space-between;
}

.peisong-box {
    color: #000;
    font-size: 30rpx;
    display: flex;
    padding: 20rpx 24rpx;
    flex-direction: row;
    align-items: center;
}

.peisong-box .peisong-type-container {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.peisong-box .peisong-type_default {
    font-size: 28rpx;
    opacity: 0.65;
    color: #000;
    padding: 10rpx;
    background: #e8e8e8;
    border-radius: 2px;
    margin-left: 16rpx;
}

.peisong-box .peisong-type_select {
    font-size: 28rpx;
    color: #fff;
    padding: 10rpx;
    background: #f22222;
    border-radius: 2px;
    margin-left: 16rpx;
}

.peisong-box type {
    color: #333;
    font-size: 30rpx;
    background: #ccc;
    margin-left: 20rpx;
}

.peisong-box .label {
    color: #000;
    font-size: 28rpx;
    width: 160rpx;
}

.mark-label {
    color: #333;
    font-size: 30rpx;
}

.peisong-box .remark {
    color: #333;
    font-size: 28rpx;
    flex: 1;
    margin-left: 10rpx;
}

.bottom-line {
    border-bottom: 1px solid #eee;
}

.top-line {
    border-top: 1px solid #eee;
}

.goods-info {
    width: 100%;
    background-color: #fff;
}

.goods-info .row-box {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 20rpx 24rpx;
    font-size: 28rpx;
    color: #000;
}

.goods-info .row-box .right-text {
    text-align: right;
    color: #f22222;
}

.jiesuan-box-wrap {
    position: fixed;
    bottom: 0;
    left: 0;
}

.address-outRange {
    background-color: #FFF7CD;
    font-size: 24rpx;
    color: #FF9440;
    height: 60rpx;
    display: flex;
    align-items: center;
    padding-left: 24rpx;
}

.jiesuan-box {
    display: flex;
    justify-content: space-between;
    width: 100%;
    height: 100rpx;
    border-top: 1px solid #eee;
    background-color: #fff;
    z-index: 99;
}

.jiesuan-box .to-pay-btn {
    width: 250rpx;
    text-align: center;
    height: 100%;
    line-height: 100rpx;
    border-radius: 0;
}

.jiesuan-box .left-price {
    display: flex;
    width: 500rpx;
    line-height: 100rpx;
    padding: 0 15rpx;
    font-size: 30rpx;
    box-sizing: border-box;
    justify-content: flex-end;
    background-color: #fff;
}

.jiesuan-box .jiesuan-box-total {
    color: #e64340;
    text-align: right;
    background-color: #fff;
}

.new-order-direction-row {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.new-order-direction-right {
    flex-direction: row-reverse;
}

.new-order-label {
    font-size: 24rpx;
    color: #000;
}

.new-order-value {
    font-size: 26rpx;
    color: #333;
}

.new-order-direction-column {
    display: flex;
    flex-direction: row;
    padding: 24rpx 17rpx 24rpx 17rpx;
    justify-content: space-between;
    border-bottom: 1rpx solid #f1f1f1;
    background: #fff;
}

.picker-text {
    font-size: 28rpx;
}

.radio-group {
    margin-top: 10rpx;
}

.radio-label {
    font-size: 30rpx;
    color: #333;
}

.new_order_arrow-right {
    width: 16rpx;
    height: 24rpx;
    margin-left: 18rpx;
}

.new_order_shop-logo {
    width: 52rpx;
    height: 52rpx;
    margin-right: 15rpx;
}

.vipcard-image {
    width: 48rpx;
    height: 38rpx;
    margin-right: 10rpx;
}

.coupon-image {
    width: 48rpx;
    height: 48rpx;
    margin-right: 10rpx;
}

.shenfen-label {
    color: #333;
    font-size: 30rpx;
}

.shenfen-input {
    color: #333;
    font-size: 26rpx;
}

.shenfen-tip {
    color: #999;
    font-size: 26rpx;
}

.new-order-direction-row .zan-switch {
    width: 44px;
    height: 24px;
    background: #f22222;
    border: 1px solid #e8e8e8;
}

.new-order-direction-row .zan-switch__bg {
    width: 44px;
    height: 24px;
}

.new-order-direction-row .zan-switch__circle {
    width: 22px;
    height: 22px;
}

.reject-reason {
    display: flex;
    flex-direction: row;
    margin: 10rpx;
}

.reject-reason image {
    width: 120rpx;
    height: 120rpx;
    margin-right: 16rpx;
}

.reject-reason-content {
    flex: 1;
}

.reject-reason-price {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.order-msg {
    background-color: #fff;
}

.order-msg__item {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 28rpx;
    padding: 20rpx 24rpx;
}

.order-msg__item-label {
    width: 200rpx;
}

.order-msg__item-input {
    flex: 1;
    color: #333;
    font-size: 28rpx;
    margin-left: 10rpx;
    text-align: right;
}

.order-msg__item-area {
    height: 106rpx;
}

.order-msg__item-require::before {
    content: '*';
    color: red;
}

.order-msg__item-norequire::before {
    content: ' ';
}

.to-pay-btn-dis {
    background: #d2d2d2;
    font-size: 32rpx;
    color: #fff;
}

.to-pay-btn-able {
    background-color: #e64340;
    font-size: 32rpx;
    color: #fff;
}