.worker__desc {
    padding: 0 24rpx;
    font-size: 24rpx;
}

.worker__desc-title {
    margin-top: 12rpx;
    line-height: 40rpx;
}

.worker__desc-content-default,.worker__desc-content-collspand {
    color: #aaa;
    line-height: 40rpx;
}

.worker__desc-content-collspand {
    height: 120rpx;
}

.worker__desc-expand {
    font-size: 24rpx;
    text-align: center;
    color: #f22222;
    padding: 12rpx 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.worker__desc-expand__arrow {
    margin-left: 10rpx;
    width: 16rpx;
    height: 16rpx;
}

.services-list {
    font-size: 28rpx;
}

.services-list__title {
    font-weight: bold;
    padding: 24rpx;
    border-bottom: 1rpx solid #e5e5e5;
}

.services-list__item {
    padding: 24rpx;
    align-items: center;
    justify-content: flex-start;
    display: flex;
    background-color: #fff;
    border-bottom: 1rpx solid #e5e5e5;
}

.services-list__item__avatar {
    border-radius: 50%;
    width: 120rpx;
    height: 120rpx;
}

.services-list__item__detail {
    height: 120rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content: space-between;
    flex: 1;
}

.services-list__item__name {
    margin-top: 12rpx;
    margin-left: 24rpx;
    flex: 1;
    height: 48rpx;
    line-height: 48rpx;
    font-weight: bold;
    color: #000;
}

.services-list__item__desc {
    font-size: 24rpx;
    color: #aaa;
    margin-bottom: 12rpx;
    margin-left: 24rpx;
}

.services-list__item__price {
    font-size: 36rpx;
    color: #f22626;
}

.store {
    padding: 24rpx;
    display: flex;
    align-items: center;
    justify-content: flex-start;
}

.store__detail {
    font-size: 28rpx;
    flex: 1;
    display: flex;
    flex-direction: column;
}

.store__detail-name {
    font-weight: bold;
    font-size: 28rpx;
    color: #000;
    letter-spacing: 0;
    line-height: 28rpx;
    margin-bottom: 20rpx;
}

.store__detail-address {
    margin-top: 12rpx;
    font-size: 24rpx;
    color: #aaa;
}

.order-btn-container {
    bottom: 0;
    position: fixed;
    width: 100%;
    height: 98rpx;
    display: flex;
    flex-direction: row-reverse;
    background: white;
    align-items: center;
}

.btn-share {
    margin-right: 0;
    padding-left: 28rpx;
    padding-right: 0;
    border-radius: 0;
    -webkit-tap-highlight-color: transparent;
    overflow: hidden;
    color: #000;
    background-color: #fff;
    line-height: 1;
    width: 90rpx;
}

.btn-share::after {
    content: " ";
    border: 0px;
}

.btn-share .share {
    font-size: 24rpx;
    color: #666;
}

.store__detail-priceContainer {
    display: flex;
    align-items: center;
}

.store__detail-price {
    font-size: 36rpx;
    color: #f22222;
    letter-spacing: 0;
    line-height: 40rpx;
    display: inline-block;
    margin-right: 30rpx;
}

.store__detail-preprice-tag {
    font-size: 18rpx;
    color: #f87e27;
    letter-spacing: 0;
    line-height: 20rpx;
    border: 1px solid #f87e27;
    border-radius: 2px;
    padding: 1px;
}

.store__detail-preprice {
    opacity: 0.43;
    margin-left: 10rpx;
    font-size: 24rpx;
    color: #000;
}

.store-item-icon {
    width: 42rpx;
    height: 42rpx;
    margin-right: 10rpx;
}

.rightarrow {
    width: 12rpx;
    height: 23rpx;
    margin-left: 20rpx;
    order: 10;
}

.store-item-name {
    font-size: 24rpx;
    color: #000;
    letter-spacing: 0;
}

.store-item-right-tip {
    opacity: 0.43;
    font-size: 24rpx;
    color: #000;
    letter-spacing: 0;
}

.storeContainer:not(:last-child):after {
    content: '';
    height: 1rpx;
    background: #f4f4f4;
    display: block;
    position: relative;
}

.product-show-spec-container {
    display: flex;
    align-items: center;
    font-size: 28rpx;
    padding: 16rpx 30rpx;
}

.product-show-spec-left {
    flex-grow: 1;
    display: flex;
}

.product-show-spec-text {
    margin-right: 20rpx;
}

.yuyuetip {
    opacity: 0.43;
    font-size: 14px;
    color: #000;
    letter-spacing: 0;
    margin-left: 20rpx;
}