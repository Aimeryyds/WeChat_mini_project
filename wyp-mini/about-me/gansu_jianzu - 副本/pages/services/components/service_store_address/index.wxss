.service_store {
    display: flex;
    flex-direction: row;
    padding: 26rpx;
    background: #fff;
    justify-content: space-between;
    align-items: center;
}

.service_store_left {
    flex: 1;
}

.service_store image {
    width: 32rpx;
    height: 32rpx;
    border-radius: 4px;
}

.name {
    font-size: 28rpx;
    color: #000;
}

.detail {
    opacity: 0.43;
    font-size: 24rpx;
    color: #000;
}