.services-list {
    display: flex;
    align-items: center;
    padding: 16rpx 30rpx;
    border-bottom: 1px solid #e5e5e5;
    font-size: 28rpx;
    background: #fff;
}

.services-list__img {
    width: 130rpx;
    height: 130rpx;
    margin-right: 20rpx;
}

.services-list__cont {
    display: flex;
    flex: 1;
    flex-flow: wrap column;
    height: 152rpx;
}

.font-color {
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
    margin-top: 4rpx;
}

.services-list__name {
    font-size: 30rpx;
    color: #000;
    line-height: 30rpx;
}

.services-list__storename {
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
    white-space: nowrap;
}

.services-list__money {
    color: #c41412;
}

.services-list__money text {
    font-size: 18rpx;
}