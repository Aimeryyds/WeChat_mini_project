.brands-list {
    display: flex;
    align-items: center;
    padding: 0 30rpx;
    height: 148rpx;
    border-bottom: 1px solid #e5e5e5;
    font-size: 28rpx;
    background: #fff;
}

.brands-list__img {
    width: 130rpx;
    height: 130rpx;
    margin-right: 20rpx;
    vertical-align: top;
}

.brands-list__cont {
    flex: 1;
}

.font-color {
    height: 80rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
    margin-top: 10rpx;
}