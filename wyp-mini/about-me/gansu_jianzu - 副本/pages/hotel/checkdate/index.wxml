<view class="container">
    <date-picker bindonsubmit="handleSubmitDate" endDate="{{endDate}}" startDate="{{startDate}}">
        <calendar isEndMonth="{{item.isEndMonth}}" limitDays="{{limitDays}}" month="{{item.month}}" year="{{item.year}}" wx:for="{{calendars}}"></calendar>
    </date-picker>
</view>
