.addressList {
    width: 100%;
    background: #fff;
}

.addressList--item {
    width: 100%;
    box-sizing: border-box;
    padding: 24rpx 30rpx;
    display: flex;
    justify-content: space-between;
    border-bottom: 1rpx solid #e5e5e5;
}

.addressList--item__title {
}

.about-conter {
    font-size: 26rpx;
    width: 100%;
    padding-bottom: 100rpx;
}

.address-list-title {
    padding: 22rpx 30rpx;
    color: #b2b2b2;
    font-size: 24rpx;
}

.list-addBtn-wrap {
    position: fixed;
    left: 0;
    bottom: 0;
    height: 100rpx;
    width: 100%;
    opacity: 1;
}

.list-addBtn {
    font-size: 32rpx;
    line-height: 100rpx;
    background: #ca2626;
    color: #fff;
    height: 100%;
    border-radius: 0px;
}