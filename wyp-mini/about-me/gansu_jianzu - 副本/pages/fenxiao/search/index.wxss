.search-wrap {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    background: #fff;
    z-index: 9999;
    background: #fff;
    padding: 26rpx 28rpx;
}

.products-search {
    display: flex;
    align-items: center;
    height: 60rpx;
    border-radius: 30rpx;
    background: #f4f4f4;
    font-size: 24rpx;
    padding-left: 30rpx;
}

.products-search__img {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}

.products-search__text {
    width: 85%;
}

.products-wrap {
    margin-top: 122rpx;
}

.products-list {
    display: flex;
    align-items: center;
    padding: 0 24rpx;
    background: #fff;
    font-size: 28rpx;
}

.products-list__img {
    width: 160rpx;
    height: 160rpx;
    margin-right: 26rpx;
}

.products-list__info {
    position: relative;
    flex: 1;
    border-bottom: 1px solid #f4f4f4;
    padding: 24rpx 24rpx 24rpx 0;
}

.products-list__cont {
    margin: 16rpx 0;
}

.products-list__extendPrice {
    margin-right: 40rpx;
    color: #F32828;
}

.products-list__costPrice {
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}

.products-list__collection {
    position: absolute;
    bottom: 24rpx;
    right: 24rpx;
}

.products-list__joinImg {
    width: 42rpx;
    height: 42rpx;
}

.noData {
    display: flex;
    flex-flow: column wrap;
    align-items: center;
    justify-content: center;
    height: 1018rpx;
    font-size: 30rpx;
}