.order-list {
    display: flex;
    min-height: rpx;
    padding: 26rpx 24rpx 8rpx;
    border-bottom: 1px solid #f4f4f4;
    background: #fff;
    font-size: 28rpx;
}

.order-list__info {
    flex: 1;
}

.order-list--modif {
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
    line-height: 34rpx;
    margin-top: 8rpx;
}

.order-list__money {
    margin-bottom: 24rpx;
    text-align: right;
}

.order-list__money text {
    font-size: 20rpx;
    margin-right: 6rpx;
    margin-left: 12rpx;
}

.order-list__commission text {
    font-size: 44rpx;
    margin-left: 6rpx;
}

.noData {
    display: flex;
    flex-flow: column wrap;
    align-items: center;
    justify-content: center;
    height: 1204rpx;
    font-size: 30rpx;
}