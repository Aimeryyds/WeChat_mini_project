@import "../../pages/templates/supply/supply.wxss";

@import "tab.wxss";

page {
    overflow: hidden;
}

.supply-container {
    overflow: hidden;
}

.a-supply {
    width: 100%;
    background-color: #fff;
}

.search-wrap {
    background: #fff;
    padding: 26rpx 28rpx;
}

.supply-search {
    display: flex;
    align-items: center;
    height: 60rpx;
    border-radius: 30rpx;
    background: #f4f4f4;
    font-size: 24rpx;
    justify-content: center;
    padding: 0 20rpx;
}

.supply-search input {
    flex: 1;
}

.supply-search__img {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}

.supply-search__input {
    color: rgba(0,0,0,0.43);
}

.no-supply {
    width: 100%;
    position: absolute;
    bottom: 0;
    top: 240rpx;
    left: 0;
    right: 0;
    text-align: center;
    padding-top: 203rpx;
    background-color: #F2f2f2;
}

.no-supply .text {
    font-size: 28rpx;
    color: #999999;
    text-align: center;
}