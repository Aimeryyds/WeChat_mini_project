@import "../miaosha/index.wxss";

.vipcard-wrap {
    width: 618rpx;
    margin: 24rpx;
    height: 80rpx;
    background: #FFFFFF;
    border-radius: 78rpx;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    justify-content: space-between;
    padding: 0 46rpx 0 38rpx;
}

.vipcard-name {
    display: flex;
    align-items: center;
}

.vipcard-name image {
    width: 48rpx;
    height: 38rpx;
    margin-right: 30rpx;
}

.vipcard-btn {
    color: #EC4040;
}