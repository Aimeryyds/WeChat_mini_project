<view class="zan-panel zan-panel__no-margin-top zan-panel__no-border">
    <view class="title-mod mod-spcaing">
        <view class="title-cont">
            <image src="{{module.options.leftIcon}}" wx:if="{{module.options.leftIcon}}"></image>
            <view class="title-text" style="color: {{module.options.color||'#000'}}">{{module.options.title}}</view>
        </view>
        <view bindtap="makecustome" class="title-more" data-item="{{module.options}}">{{module.options.rightText}}<image src="http://imgcdnali.wfenxiao.com.cn/xcx/c/icons/more-icon.png"></image>
        </view>
    </view>
</view>
