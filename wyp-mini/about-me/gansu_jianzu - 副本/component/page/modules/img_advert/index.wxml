<view class="zan-panel zan-panel__no-magrin-top imgaDvert-list zan-panel__no-border" wx:if="{{module.options.adsStyle=='type1'||module.options.adsStyle=='type2'||module.options.adsStyle=='type3'||module.options.adsStyle=='type4'}}">
