@import "../miaosha/index.wxss";

.continer {
    font-size: 28rpx;
    position: relative;
}

.single-item {
    background-color: #fff;
    display: flex;
    align-items: center;
    height: 88rpx;
    padding: 0 24rpx;
}

.single-list {
    display: flex;
    flex-wrap: wrap;
}

.twoline-item {
    background-color: #fff;
    display: flex;
    align-items: center;
    height: 88rpx;
    padding: 0 24rpx;
}

.twoline-list {
    display: flex;
    flex-wrap: wrap;
    background-color: #fff;
}

.more-single-item {
    width: 250rpx;
    justify-content: center;
    display: flex;
    align-items: center;
    height: 88rpx;
}

.fixedBox {
    position: absolute;
    width: 88rpx;
    height: 88rpx;
    background-color: #fff;
    top: 0;
    right: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 100;
}

.single-more-wrap {
    position: fixed;
    top: 0;
    left: 0;
    background-color: rgba(255,255,255,0);
    z-index: 100;
    width: 100%;
    height: 100%;
}

.single-more {
    display: flex;
    position: absolute;
    left: 0;
    width: 100%;
    background-color: #f1f1f1;
    z-index: 1000;
    flex-wrap: wrap;
    opacity: 1;
}

.products-wrap {
    display: flex;
    flex-wrap: wrap;
    flex-direction: row;
    justify-content: space-between;
    background: #f1f1f1;
}

.products {
    padding: 16rpx 16rpx 0;
    height: auto;
    background: #fff;
    display: flex;
    flex-flow: wrap column;
    justify-content: space-between;
    box-sizing: border-box;
    margin-bottom: 6rpx;
    width: 372rpx;
    box-sizing: border-box;
}

.product-name {
    height: 80rpx;
    font-size: 28rpx;
}

.product-storename {
    display: flex;
    align-items: center;
    width: 340rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}

.products-cont {
    display: flex;
    flex-direction: row;
    align-items: flex-end;
}

.products-price {
    font-size: 40rpx;
    line-height: 32rpx;
    color: #F32828;
    margin-right: 18rpx;
}

.products-stock {
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}

.newsinfo-detail {
    display: flex;
    padding: 30rpx 0;
    border-bottom: 2rpx #f4f4f4 solid;
}

.newsinfo-detail-img {
    margin: 0 24rpx;
    width: 132rpx;
    height: 132rpx;
}

.newsinfo-detail-right {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.newsinfo-detail-name {
    padding-right: 28rpx;
    font-size: 28rpx;
}

.newsinfo-detail-time {
    margin-top: 14rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}

.services__projectItem {
    display: flex;
    padding: 10px;
    border-bottom: solid 1px #f4f4f4;
    background: white;
}

.services__projectItem:last-child {
    border-bottom: none;
}

.services__img {
    width: 180rpx;
    height: 180rpx;
    margin: 0 20rpx 20rpx 0;
}

.services__right {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: flex-start;
}

.services__name {
    font-size: 30rpx;
    color: #000;
    line-height: 30rpx;
}

.services__desc {
    opacity: 0.43;
    font-size: 26rpx;
    color: #000;
    letter-spacing: -0.27px;
}

.services__priceContainer {
    display: flex;
    align-items: center;
}

.services__price1 {
    display: inline-block;
    color: #f22222;
    margin-right: 10px;
}

.services__price1_t {
    font-size: 30rpx;
}

.services__price1_p {
    font-size: 38rpx;
}

.supplyinfo-detail {
    display: flex;
    padding: 30rpx 0;
    border-bottom: 2rpx #f4f4f4 solid;
}

.supplyinfo-detail-img {
    margin: 0 24rpx;
    width: 132rpx;
    height: 132rpx;
}

.supplyinfo-detail-right {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.supplyinfo-detail-name {
    padding-right: 28rpx;
    font-size: 28rpx;
    position: relative;
}

.supplyinfo-detail-name view {
    text-indent: 42rpx;
    max-height: 88rpx;
    line-height: 44rpx;
}

.supplyinfo-detail-name image {
    position: absolute;
    top: 8rpx;
    left: 0;
    width: 32rpx;
    height: 35rpx;
}

.supplyinfo-detail-footer {
    display: flex;
    justify-content: space-between;
    padding-right: 64rpx;
}

.supplyinfo-detail-phone image {
    width: 43rpx;
    height: 43rpx;
}

.supplyinfo-detail-time {
    margin-top: 14rpx;
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}