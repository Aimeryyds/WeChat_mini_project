.tuan-dailog {
    background: #fff;
}

.tuan-dialog__title {
    height: 108rpx;
    padding-top: 24rpx;
    border-bottom: 1px solid #f4f4f4;
    font-size: 28rpx;
    line-height: 40rpx;
    text-align: center;
    box-sizing: border-box;
}

.tuanDialog-list {
    border-bottom: 1px solid #f4f4f4;
}

.tuanDialog-list .tuanDtail-list__cont {
    border-bottom: 0;
}

.tuan-dialog__footer {
    height: 76rpx;
    line-height: 76rpx;
    color: rgba(0,0,0,0.43);
    font-size: 24rpx;
    text-align: center;
}

.tuan-detail {
    background: #fff;
}

.tuan__title {
    height: 88rpx;
    display: flex;
    align-items: center;
    font-size: 28rpx;
    color: #000;
    padding: 0 24rpx;
    border-bottom: 1px solid #f4f4f4;
}

.tuan__title-link {
    font-size: 24rpx;
    color: rgba(0,0,0,0.43);
}

.tuanDtail-list {
    display: flex;
    align-items: center;
}

.tuanDtail-list__img {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    margin-left: 24rpx;
}

.tuanDtail-list__cont {
    display: flex;
    flex: 1;
    align-items: flex-end;
    padding: 20rpx;
    border-bottom: 1px solid #f4f4f4;
}

.tuanDtail-list__info {
    flex: 1;
}

.tuanDtail-list__title {
    font-size: 28rpx;
    line-height: 28rpx;
    color: #000;
}

.tuanDtail-list__time {
    margin-top: 10rpx;
    font-size: 24rpx;
    line-height: 24rpx;
    color: rgba(0,0,0,0.43);
}

.tuanDtail-list__btn {
    padding: 0 16rpx;
    height: 48rpx;
    line-height: 48rpx;
    background: #f32828;
    color: #fff;
    font-size: 28rpx;
    border-radius: 8rpx;
}