.wxSearch-section {
    height: 96rpx;
    background-color: #fff;
    padding: 0rpx 24rpx;
    border-bottom: 1px #eee solid;
    border-top: 1px #eee solid;
    border-sizing: border-box;
    display: flex;
    align-items: center;
}

.wxSearch-input--wrapper {
    padding: 0 12rpx;
    flex: 8;
    position: relative;
    line-height: 56rpx;
    border-radius: 8rpx;
    border: 1rpx solid #ddd;
    height: 56rpx;
    display: flex;
    align-items: center;
}

.wxSearch-input {
    font-size: 24rpx;
    flex: 1;
    margin: 0 12rpx;
}

.wxSearch-button {
    margin-left: 12rpx;
    flex: 1;
    border: 1rpx solid #ddd!important;
    height: 56rpx;
    line-height: 56rpx;
}

.wxSearch-button--disable {
    background-color: #eee!important;
}

.wxSearch {
    position: fixed;
    top: 43px;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 10000;
    border-top: 1px #eee solid;
}

.wxSearchInner {
    background-color: #fff;
}

.wxSearchTitle {
    padding: 12rpx 24rpx;
    background-color: #f5f5f5;
    display: block;
    font-size: 28rpx;
}

.wxSearchMindKeyItem {
    padding: 10px 5px;
    border-bottom: 1px solid #eee;
    display: flex;
    font-size: 14px;
    color: cornflowerblue;
}

.wxSearchKeyList {
    padding: 0 24rpx;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.wxSearchKeyItem {
    flex: 0 0 20%;
    font-size: 14px;
    text-align: center;
    border: 1px solid #eee;
    margin: 12rpx 12rpx 12rpx 0px;
    padding: 8rpx 8rpx;
    border-radius: 5px;
}

.wxSearchHistoryList {
}

.wxSearchHistoryItem {
    padding: 24rpx;
    border-bottom: 1px solid #eee;
    display: flex;
}

.wxSearchHistoryItemText {
    flex: 8;
    font-size: 28rpx;
}

.wxSearchDeleteAllBtn {
    height: 50px;
    line-height: 50px;
    text-align: center;
    font-size: 12px;
}