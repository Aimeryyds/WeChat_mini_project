        <view bindtap="to_url" class="ff03" data-appid="{{item.list[2].appid}}" data-key="{{item.list[2].key?item.list[2].key:1}}" data-lat="{{item.list[2].lat}}" data-lng="{{item.list[2].lng}}" data-path="{{item.list[2].path}}" data-phone="{{item.list[2].phone}}" data-url="{{item.list[2].url}}">
            <image mode="aspectFill" src="{{item.list[2].imgurl}}"></image>
        </view>
        <view bindtap="to_url" class="ff03" data-appid="{{item.list[3].appid}}" data-key="{{item.list[3].key?item.list[3].key:1}}" data-lat="{{item.list[3].lat}}" data-lng="{{item.list[3].lng}}" data-path="{{item.list[3].path}}" data-phone="{{item.list[3].phone}}" data-url="{{item.list[3].url}}">
            <image mode="aspectFill" src="{{item.list[3].imgurl}}"></image>
        </view>
    </view>
    <view class="clear"></view>
</view>
