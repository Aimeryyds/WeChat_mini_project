<view style="background:{{item.bg_color}}">
    <view bindtap="to_url" class="tf1" data-appid="{{item.list[0].appid}}" data-key="{{item.list[0].key?item.list[0].key:1}}" data-lat="{{item.list[0].lat}}" data-lng="{{item.list[0].lng}}" data-path="{{item.list[0].path}}" data-phone="{{item.list[0].phone}}" data-url="{{item.list[0].url}}">
        <image mode="aspectFill" src="{{item.list[0].imgurl}}"></image>
    </view>
    <view class="tf2">
        <view bindtap="to_url" class="tf21" data-appid="{{item.list[1].appid}}" data-key="{{item.list[1].key?item.list[1].key:1}}" data-lat="{{item.list[1].lat}}" data-lng="{{item.list[1].lng}}" data-path="{{item.list[1].path}}" data-phone="{{item.list[1].phone}}" data-url="{{item.list[1].url}}">
            <image mode="aspectFill" src="{{item.list[1].imgurl}}"></image>
        </view>
        <view bindtap="to_url" class="tf22" data-appid="{{item.list[2].appid}}" data-key="{{item.list[2].key?item.list[2].key:1}}" data-lat="{{item.list[2].lat}}" data-lng="{{item.list[2].lng}}" data-path="{{item.list[2].path}}" data-phone="{{item.list[2].phone}}" data-url="{{item.list[2].url}}">
            <image mode="aspectFill" src="{{item.list[2].imgurl}}"></image>
        </view>
    </view>
    <view class="clear"></view>
</view>
