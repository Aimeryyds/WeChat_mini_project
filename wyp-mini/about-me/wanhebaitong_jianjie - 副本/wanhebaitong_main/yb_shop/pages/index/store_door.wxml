<view class="shop_box" style="background:{{item.bg_color}}">
    <view class="shop_logo">
        <image src="{{item.imgurl}}"></image>
        <view class="shop_info">
            <text style="color:{{item.text_color}};font-size:32rpx;">{{item.name}}</text>
            <text style="color:{{item.text_color}};font-size:26rpx;">营业时间：{{item.time}}</text>
        </view>
    </view>
    <view bindtap="phone" class="shop_phone" data-phone="{{item.phone}}" wx:if="{{item.phone}}">
        <view style="width:92px;height:46px;position:relative;">
            <view style="width:46px;height:100%;position: absolute;left: 0;top: 0;z-index:100;background:{{item.bg_color}}"></view>
            <view style="background: url(data:image/png;base64,) no-repeat;background-size: 46px 46px;right: 0px;width:92px;height:46px;filter: drop-shadow({{item.text_color}} 46px 0px);z-index:999;"></view>
        </view>
    </view>
</view>
