page {
    background: #f2f2f2;
}

.coupon_box {
    height: 220rpx;
    background: url(http://ddfwz.sssvip.net/asmce/diancan/yhq_bg.png) no-repeat center center;
    background-size: cover;
    margin-bottom: 0.2rem;
    position: relative;
    padding-right: 200rpx;
    margin-top: 0.8rem;
    padding-top: 50rpx;
}

.coupon_box1 {
    height: 220rpx;
    background: url(http://ddfwz.sssvip.net/asmce/diancan/yhq_bg1.png) no-repeat center center;
    background-size: cover;
    margin-bottom: 0.2rem;
    position: relative;
    padding-right: 200rpx;
    margin-top: 0.8rem;
    padding-top: 50rpx;
}

.coupon_no {
    position: absolute;
    right: 220rpx;
    top: 20rpx;
}

.coupon_no text {
    color: #8c8c8c;
    font-size: 28rpx;
}

.coupon_time {
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    width: 60%;
    position: absolute;
    bottom: 60rpx;
    right: 220rpx;
    overflow: hidden;
}

.coupon_time text {
    color: #8c8c8c;
    font-size: 28rpx;
}

.coupon_receive {
    position: absolute;
    right: 60rpx;
    top: 66rpx;
    width: 90rpx;
}

.coupon_receive text {
    color: #fff;
    font-size: 42rpx;
}

.coupon_price text {
    color: #f45068;
}

.coupon_price {
    text-align: center;
}

.coupon_price .price02 {
    font-size: 100rpx;
    font-weight: bold;
}

.receive_btn {
    width: 150rpx;
    position: absolute;
    top: 0;
    right: 36rpx;
    height: 240rpx;
    z-index: 99999999;
}