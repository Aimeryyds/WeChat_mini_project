.sur_no {
    font-size: 0.8rem;
    color: #909090;
    text-align: right;
    height: 1rem;
    margin-top: 0.7rem;
}

.price_left .price01,.price_left .price02 {
    float: right;
    height: 2rem;
    display: table-cell;
    vertical-align: bottom;
}

.price_left .price01 {
    margin-left: 0.6rem;
    height: 1rem;
    margin-top: 0.7rem;
}

.price_left,.price_right {
    height: 2rem;
}

.price_right {
    width: 5rem;
}

.index_price {
    padding-top: 0rem;
    height: 2.4rem;
}

.count_info_box {
    border-top: 6rpx solid #ed4f4e;
}

.count_info_li {
    box-sizing: border-box;
    float: left;
    border-left: 1px dashed #eeeeee;
    height: 4rem;
    text-align: center;
    padding-top: 0.6rem;
    padding-bottom: 0.8rem;
    width: 33.333%;
}

.count_info_li text {
    text-align: center;
    display: block;
    font-size: 0.9rem;
    height: 1.3rem;
    line-height: 1.3rem;
}

.count_info_li:first-child {
    border: 0;
}

.bottom_info_box {
    height: 3.2rem;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    border-top: 0px solid #eeeeee;
    z-index: 99999999;
    background: #fff;
}

.bottom_info_text {
    float: left;
    height: 2.3rem;
    font-size: 0.8rem;
    padding-left: 0.5rem;
    padding-top: 0.5rem;
    line-height: 1.1rem;
    width: 8rem;
    overflow: hidden;
}

.bottom_info_text text {
    display: block;
    color: #d0b670;
}

.bottom_info_btn {
    height: 3.2rem;
    width: 6.5rem;
    color: #fff;
    text-align: center;
    line-height: 3.2rem;
    font-size: 1rem;
    position: absolute;
}

.bottom_info_btn.btn01 {
    background: #fe8534;
    right: 6.5rem;
    top: 0;
}

.bottom_info_btn.btn02 {
    background: #ed4f4e;
    right: 0;
    top: 0;
    border-radius: 0;
    border: 0;
}

.bottom_info_btn.btn02:after {
    border-radius: 0;
    border: 0;
}

.bottom_info_btn.btn03 {
    background: #7a7a7b;
}

.shop_info {
    border-bottom: 16rpx solid #f2f2f2;
}

.shop_info_li {
    padding-top: 0.7rem;
    padding-bottom: 0.7rem;
    border-top: 1px dashed #eeeeee;
    padding-left: 3rem;
    position: relative;
}

.shop_info_li02 {
    padding-top: 0.5rem;
    padding-bottom: 0.5rem;
    border-top: 1px dashed #eeeeee;
    padding-left: 3rem;
    position: relative;
}

.info_icon,.shop_name,.arrow_icon,.shop_time {
    float: left;
}

.info_icon {
    height: 2rem;
    line-height: 2rem;
    width: 3rem;
    text-align: center;
    margin-left: -3rem;
}

.info_icon image,.arrow_icon image {
    height: 1.5rem;
    width: 1.5rem;
    vertical-align: middle;
    margin-bottom: 0.25rem;
}

.shop_name {
    margin-right: 3rem;
}

.shop_name text {
    display: block;
}

.shop_name .name {
    font-size: 0.9rem;
    height: 1.2rem;
    overflow: hidden;
    line-height: 1.2rem;
}

.shop_name .address {
    font-size: 0.7rem;
    color: #808080;
    height: 1rem;
    line-height: 1rem;
    overflow: hidden;
}

.arrow_icon {
    width: 2rem;
    height: 2rem;
    position: absolute;
    top: 0.8rem;
    right: 0.8rem;
    line-height: 2rem;
}

.shop_time {
    height: 2rem;
    line-height: 2rem;
    font-size: 0.9rem;
}

.info_content_box,.other_info_box {
    padding: 0.7rem 1rem 0 1rem;
    border-bottom: 16rpx solid #f2f2f2;
}

.info_con_tit {
    font-size: 1rem;
    height: 1.5rem;
    line-height: 1.5rem;
}

.info_con_info {
    font-size: 0.8rem;
    line-height: 1.5rem;
    padding-top: 0.5rem;
}

.other_tit {
    font-size: 1rem;
    border-left: 8rpx solid #ed4f4e;
    padding-left: 0.3rem;
}

.other_info_li {
    height: 5.5rem;
    padding-left: 4.8rem;
    border-bottom: 1px dashed #eeeeee;
    padding-top: 0.7rem;
    position: relative;
}

.other_info_li:first-child {
    padding-top: 1rem;
}

.other_info_li image {
    height: 4.8rem;
    width: 4.8rem;
    margin-left: -4.8rem;
    float: left;
}

.info_box {
    margin-left: 0.6rem;
}

.info_box .title {
    font-size: 0.9rem;
    max-height: 2.4rem;
    line-height: 1.2rem;
    overflow: hidden;
    display: block;
}

.other_count {
    width: 4.8rem;
    height: 1.2rem;
    line-height: 1.2rem;
    text-align: center;
    position: absolute;
    top: 4.3rem;
    background: rgba(0,0,0,0.5);
    left: 0rem;
    overflow: hidden;
}

.other_count text {
    color: #fff;
    font-size: 0.7rem;
}

.other_price .price01 .price_no {
    font-size: 0.9rem;
}

.other_price .price01 {
    font-size: 0.8rem;
    float: left;
    margin-right: 0.5rem;
}

.other_price .price02 {
    float: left;
    padding-top: 0rem;
    font-size: 0.9rem;
    color: #909090;
    text-decoration: line-through;
}

.other_price {
    padding-top: 0.8rem;
}

.other_info_btn {
    position: absolute;
    background: #ed4f4e;
    height: 2rem;
    width: 5rem;
    top: 3.3rem;
    right: 0.2rem;
    text-align: center;
    line-height: 2rem;
    border-radius: 1rem;
    box-shadow: 2px 6px 10px #f5c8cb;
}

.other_info_btn text {
    font-size: 0.8rem;
    color: #fff;
}

.wxParse-img {
    width: 100%;
    height: auto;
}