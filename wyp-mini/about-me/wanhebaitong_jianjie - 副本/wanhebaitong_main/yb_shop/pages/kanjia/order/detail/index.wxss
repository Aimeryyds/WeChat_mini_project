.operate {
    display: flex;
    background: #fff;
    position: relative;
    margin-top: 30rpx;
}

.operate:after {
    content: "";
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 0;
    border-bottom: 2rpx solid #d9d9d9;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.operate:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 0;
    border-top: 2rpx solid #d9d9d9;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.operate navigator {
    flex: 1;
}

