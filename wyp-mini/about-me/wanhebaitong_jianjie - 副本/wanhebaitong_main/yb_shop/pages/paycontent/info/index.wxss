.page_content {
    margin: 0 0.5rem;
    overflow: hidden;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}

.case_tit {
    font-size: 1.2rem;
    line-height: 1.8rem;
    margin-top: 0.5rem;
    margin-bottom: 0.1rem;
}

.case_content {
    font-size: 0.9rem;
    padding-top: 0.5rem;
    position: relative;
    overflow: hidden;
    width: 100%;
}

.case_time {
    color: #9e9e9e;
    font-size: 0.75rem;
    margin-bottom: 0.6rem;
}

.case_content:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 0;
}

.goods_box {
    width: 60%;
    height: 200rpx;
    position: relative;
    margin: 50rpx auto;
    background: #fcfcfc;
    padding-left: 200rpx;
}

.goods_box:after {
    content: "  ";
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    right: 0;
    z-index: 1;
    width: 200%;
    height: 200%;
    border: 1px solid #d9d9d9;
    transform-origin: 0 0;
    transform: scale(.5,.5);
}

.goods_pic {
    margin-left: -200rpx;
    width: 200rpx;
    height: 200rpx;
    float: left;
}

.goods_box text {
    display: block;
}

.goods_box .goods_name {
    font-size: 32rpx;
    line-height: 45rpx;
    height: 45rpx;
    color: #262626;
    overflow: hidden;
    margin: 0 14rpx;
    padding-top: 16rpx;
}

.goods_box .goods_intro {
    font-size: 26rpx;
    height: 42rpx;
    line-height: 42rpx;
    margin: 0 14rpx;
    color: #999;
    overflow: hidden;
}

.goods_box .goods_price {
    margin: 0 14rpx;
    font-size: 38rpx;
    color: #e02e24;
    position: absolute;
    bottom: 20rpx;
    left: 200rpx;
}

.goods_buy {
    position: absolute;
    bottom: 20rpx;
    right: 26rpx;
    width: 42rpx;
    height: 42rpx;
}

.case_content.active {
    display: block;
}

.wxParse-img {
    width: 100%;
}

.nobuy {
    display: flex;
    flex-direction: column;
    width: 100%;
    padding: 24rpx 0.5rem;
    box-sizing: border-box;
    font-size: 13px;
}

.nobuy .top,.tool {
    display: flex;
    justify-content: space-between;
    margin-bottom: 16rpx;
}

.tool {
    font-size: 13px;
    margin-bottom: 0;
    margin-top: 6rpx;
}

.tool .shares {
    display: flex;
    justify-content: space-between;
}

.tool .shares image {
    width: 26px;
    height: 26px;
    margin-right: 8px;
}

.tool .shares .like {
    margin-right: 12px;
}

.tool .shares .share {
    width: 26px;
    height: 26px;
    background: url(data:image/png;base64,) 26px 26px;
    background-size: 26px 26px;
    border: none;
    padding: 0;
    padding-left: 0;
    padding-right: 0;
}

.tool .shares .share::after {
    border: none;
}

.nobuy .top .cover {
    width: 46vw;
    height: 46vw;
    border-radius: 10rpx;
}

.nobuy .top .txtinfo {
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    width: 48vw;
    padding: 20rpx;
    height: 46vw;
    box-sizing: border-box;
}

.nobuy .top .txtinfo .title {
    font-size: 15px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.nobuy .top .txtinfo .buycount {
    color: #ff5e10;
}

.nobuy .top button {
    width: 25vw;
    height: 64rpx;
    background: #fea500;
    color: #ffffff;
    border: none;
    padding: 0;
    margin: 0;
    font-size: 12px;
    border-radius: 40rpx;
    line-height: 64rpx;
}

.nobuy .top .views {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-right: 10vw;
    color: #818181;
}

.nobuy .top .views .num,.tool .num {
    display: flex;
    align-items: center;
}

.tool .num image {
    width: 36rpx;
    height: 36rpx;
    margin-right: 8rpx;
}

.nobuy .top .views image {
    width: 20rpx;
    height: 20rpx;
    margin-right: 8rpx;
}

.nobuy .info {
    color: #818181;
    font-size: 12px;
    padding: 0 12rpx;
}

.line {
    background: #efefef;
    height: 20rpx;
}

.groups {
    font-size: 13px;
    padding: 24rpx;
    color: #333333;
    display: flex;
    justify-content: space-between;
}

.same {
    display: flex;
    align-items: center;
    padding: 20rpx 12rpx;
    box-sizing: border-box;
    border-top: 1rpx solid #eeeeee;
}

.same .cover {
    width: 40vw;
    height: 23vw;
    border-radius: 22rpx;
    overflow: hidden;
    position: relative;
}

.same .cover .img {
    width: 100%;
    height: 100%;
    border-radius: 22rpx;
}

.same .info {
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin-left: 22rpx;
}

.same .info .title {
    color: #333333;
    font-size: 13px;
    margin-bottom: 22rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.same .info .num {
    display: flex;
    align-items: center;
    color: #818181;
    font-size: 12px;
}

.same .info image {
    width: 20rpx;
    height: 20rpx;
    margin-right: 8rpx;
}

.dobuy {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 999999999;
    background: rgba(33,33,33,0.6);
}

.skus {
    position: absolute;
    bottom: 50px;
    left: 0;
    width: 100%;
    background: #ffffff;
    display: flex;
    flex-direction: column;
    padding: 18px 0px 12px 12px;
    box-sizing: border-box;
}

.skus .price_type {
    display: flex;
    font-size: 13px;
}

.price_type .title {
    flex-shrink: 0;
    padding: 10rpx 20rpx 10rpx 0;
}

.price_type .sku {
    display: flex;
    flex-wrap: wrap;
}

.price_type .sku text,.price_type .sku input {
    padding: 8rpx 34rpx;
    font-size: 12px;
    color: #c2893e;
    border: 1px solid #c2893e;
    border-radius: 40px;
    background: #ffffff;
    box-sizing: border-box;
    margin-bottom: 10px;
    margin-right: 10px;
}

.price_type .sku .num {
    border-radius: 6px;
    margin-bottom: 0;
    width: 140rpx;
    text-align: center;
    box-sizing: border-box;
    padding: 0;
    height: 30px;
}

.price_type .sku .price {
    color: #e93323;
    font-weight: 600;
    margin-left: 12px;
}

.buy_num {
    align-items: center;
}

.mark {
    flex-shrink: 0;
    padding-right: 20rpx;
}

.price_type .sku .active {
    color: #ffffff;
    border: 1px solid #f4bc40;
    background: #f4bc40;
}

.mark_line {
    font-size: 11px;
    border-top: 1rpx solid #eeeeee;
    padding-top: 22rpx;
    padding-bottom: 12rpx;
}

.pay_line {
    display: flex;
    justify-content: space-between;
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 50px;
    background: #ffffff;
    align-items: center;
    border-top: 1rpx solid #eeeeee;
}

.pay_btn {
    width: 35vw;
    line-height: 50px;
    height: 50px;
    text-align: center;
    background: #f4bc40;
    color: #ffffff;
}

.tprice {
    line-height: 50px;
    font-size: 13px;
    margin-left: 12px;
}

.tprice text {
    font-weight: 600;
    color: #e93323;
    font-size: 15px;
}

.lock {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    box-sizing: border-box;
    border-radius: 22rpx;
    background: #424242;
    opacity: 0.6;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 999;
}

.lock image {
    width: 50rpx;
    height: 50rpx;
}

.mp3 {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
}

.mp3 .poster {
    width: 60vw;
    height: 60vw;
    border-radius: 30vw;
    margin-bottom: 20px;
    -webkit-animation-iteration-count: infinite;
}

.mp3 .timeline {
    width: 100%;
    display: flex;
    justify-content: space-between;
    font-size: 14px;
    align-items: center;
    margin-bottom: 12px;
}

.mp3 .timeline slider {
    width: 57vw;
    margin: 0;
    padding: 0;
}

.mp3 .timeline slider::after {
    margin: 0 20px;
    padding: 0 20px;
}

.mp3 .control {
    width: 7vw;
    height: 7vw;
}

.music-on {
    animation: music-rotate 10s linear infinite;
}

@-webkit-keyframes music-rotate {
    0% {
        transform: rotateZ(0deg);
    }

    100% {
        transform: rotateZ(360deg);
    }
}

@keyframes music-rotate {
    0% {
        transform: rotateZ(0deg);
    }

    100% {
        transform: rotateZ(360deg);
    }
}