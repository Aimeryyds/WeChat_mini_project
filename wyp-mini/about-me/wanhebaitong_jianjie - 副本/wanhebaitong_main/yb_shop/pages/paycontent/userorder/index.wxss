page,view,text,image,button {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
    font-size: 16px;
}

.nobuy {
    width: 100%;
    text-align: center;
    margin-top: 20px;
}

.buycell {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 12px;
    border-top: 1rpx solid #eeeeee;
    width: 100%;
}

.line {
    width: 100%;
    background: #eeeeee;
    height: 1rpx;
}

.time,.price {
    font-size: 12px;
    width: 20%;
}

.time {
    color: #999999;
}

.price {
    color: #e93323;
    text-align: right;
}