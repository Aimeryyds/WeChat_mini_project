.header-wrap {
    height: 130rpx;
    border-bottom: 1rpx solid #eee;
    color: #999;
}

.fs22 {
    font-size: 22rpx;
}

.fs-26 {
    font-size: 26rpx;
}

.header-wrap .week-day {
    margin-bottom: 6rpx;
    color: #333;
}

.day-choosed {
    position: relative;
}

.day-choosed .active-bar {
    content: "";
    display: inline-block;
    width: 100%;
    height: 4rpx;
    background-color: #333;
    position: absolute;
    left: 0;
    bottom: -32rpx;
}

.header-wrap .icon-turn {
    text-align: center;
    font-size: 24rpx;
    line-height: 130rpx;
}

.header-wrap .prev-btn {
    transform: rotate(-180deg);
}

.time-wrap {
    padding: 20rpx 0;
}

.timepicker-box .grid {
    width: 182rpx;
}

.timepicker-box {
    background-color: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
}

.time {
    width: 138rpx;
    height: 66rpx;
    line-height: 66rpx;
    background-color: #F4F4F4;
    border-radius: 50rpx;
    color: #333;
}

.time-choosed-color {
    color: #fff;
}

.day-disable {
    color: #ccc!important;
}

.time-forbidden-bg {
    background-color: #ccc!important;
    color: #fff;
}