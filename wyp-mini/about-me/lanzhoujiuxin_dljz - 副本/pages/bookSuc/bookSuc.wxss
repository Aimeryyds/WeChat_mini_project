.bookSuc-container {
    font-size: 28rpx;
}

.complete-wrapper {
    height: 100%;
}

.complete-wrapper .complete {
    margin-top: 100rpx;
    width: 520rpx;
    margin: 100rpx auto;
    overflow: hidden;
}

.complete-wrapper .complete .img {
    width: 480rpx;
    height: 320rpx;
    margin-top: -320rpx;
    margin-bottom: 25rpx;
}

.complete-wrapper .complete:before {
    content: '';
    width: 100%;
    height: 328rpx;
    display: block;
}

.complete-wrapper .view-order-btn {
    width: 230rpx;
    height: 80rpx;
    line-height: 80rpx;
    background-color: #72B54E;
    border-radius: 8rpx;
    color: #fff;
    float: left;
    text-align: center;
    cursor: pointer;
}

.complete-wrapper .share-btn {
    width: 226rpx;
    height: 76rpx;
    line-height: 80rpx;
    border: 1rpx solid #72B54E;
    border-radius: 8rpx;
    background-color: #fff;
    color: #72B54E;
    text-align: center;
    float: right;
    cursor: pointer;
}