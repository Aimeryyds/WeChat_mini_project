@import "../../libs/wxParse/wxParse.wxss";

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.bookDetail-container {
    position: relative;
    height: 100%;
}

.bookDetail-wrapper {
    height: 100%;
    min-height: 1105rpx;
    background-color: #F4F4F4;
}

.bookDetail-wrapper .footerPosition {
    height: 96rpx;
}

.bookDetail-wrapper .swiperWrap {
    height: 380rpx;
    padding: 30rpx 30rpx 10rpx;
    background-color: #fff;
}

.bookDetail-wrapper .swiperWrap .swiper {
    height: 100%;
}

.bookDetail-wrapper .bookDetail-intro {
    padding: 25rpx 30rpx;
    margin-bottom: 18rpx;
    background-color: #fff;
    position: relative;
}

.bookDetail-wrapper .detail-title {
    font-size: 30rpx;
    color: #353535;
    margin-right: 50rpx;
    word-wrap: break-word;
    line-height: 30rpx;
    max-height: 60rpx;
    overflow: hidden;
}

.bookDetail-wrapper .detail-sub-title {
    margin-top: 18rpx;
    color: #888;
    font-size: 24rpx;
}

.bookDetail-wrapper .detail-price-wrap {
    height: 56rpx;
    line-height: 56rpx;
    margin-top: 4rpx;
    margin-bottom: 8rpx;
    overflow: hidden;
}

.bookDetail-wrapper .selling-price {
    font-size: 32rpx;
    color: #D02F3E;
    position: relative;
    top: 6rpx;
}

.bookDetail-wrapper .original-price {
    font-size: 24rpx;
    color: #999;
    text-decoration: line-through;
    position: relative;
    top: 6rpx;
}

.bookDetail-wrapper .promotion {
    font-size: 22rpx;
    padding: 2rpx 6rpx;
    margin-left: 10rpx;
    border-radius: 4rpx;
    background-color: #D03038;
    color: #fff;
}

.bookDetail-wrapper .icon-share {
    color: #999;
    width: 40rpx;
    height: 32rpx;
    float: right;
    text-align: center;
    line-height: 32rpx;
    font-size: 28rpx;
    margin-top: -2rpx;
}

.bookDetail-wrapper .shareBtn {
    position: absolute;
    right: 20rpx;
    top: 10rpx;
    opacity: 0;
    width: 40rpx;
    height: 50rpx;
}

.bookDetail-wrapper .service-detail {
    background-color: #fff;
}

.bookDetail-wrapper .detail-content {
    padding: 10rpx 30rpx 10rpx 30rpx;
    color: #878787;
    text-align: center;
    font-size: 28rpx;
}

.bookDetail-wrapper .detail-header {
    height: 100rpx;
    line-height: 100rpx;
    border-bottom: 1rpx solid #eee;
    padding-left: 30rpx;
    overflow: hidden;
    font-size: 28rpx;
    color: #353535;
}

.bookDetail-wrapper .detail-header::before {
    content: '';
    border-left: 4rpx solid #353535;
    height: 24rpx;
    margin-top: -2px;
    margin-right: 20rpx;
    display: inline-block;
    vertical-align: middle;
}

.bookDetail-wrapper .grey-color {
    color: #000;
    font-size: 28rpx;
}

.detail-nav {
    height: 100%;
    display: flex;
}

.detail-nav .nav-wx-item {
    width: 100rpx;
    margin-left: 50rpx;
    margin-right: 30rpx;
    font-size: 24rpx;
    color: #999;
    cursor: pointer;
    height: 100rpx;
    position: relative;
}

.detail-nav .nav-wx-item view {
    position: absolute;
    height: 30rpx;
    line-height: 30rpx;
    top: 60rpx;
}

.detail-nav .wxContact {
    position: absolute;
    opacity: 0;
    width: 110rpx;
    height: 100rpx;
    left: 45rpx;
}

.detail-nav .nav-wx-item .icon-weixin {
    text-align: center;
    display: block;
    width: 100rpx;
    height: 50rpx;
    line-height: 50rpx;
    position: absolute;
    top: 10rpx;
}

.detail-nav .nav-wx-item .icon-weixin:before {
    content: "\e902";
    color: #999;
    font-size: 42rpx;
}

.detail-nav .nav-phone-item {
    margin-left: 40rpx;
    margin-right: 40rpx;
    font-size: 24rpx;
    color: #999;
    cursor: pointer;
    text-align: center;
}

.detail-nav .nav-phone-item .icon-phone {
    text-align: center;
    display: block;
    width: 48rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-top: 8rpx;
}

.detail-nav .nav-phone-item .icon-phone:before {
    content: "\e909";
    color: #999;
    font-size: 42rpx;
}

.detail-nav .nav-booking-item {
    line-height: 96rpx;
    flex-grow: 1;
    background-color: #D12F3A;
    text-align: center;
    color: #fff;
    cursor: pointer;
    font-size: 28rpx;
}

.detail-nav .nav-booking-item.has-out {
    background-color: #d4d4d4;
}

.bookDetail-footer {
    width: 100%;
    height: 96rpx;
    position: fixed;
    bottom: 0;
    background-color: #fff;
    border-top: 1rpx solid #eee;
}

.detail-content-empty {
    height: 340rpx;
    background: #fff;
}

.detail-content-empty .imageWrap {
    width: 140rpx;
    height: 122rpx;
    margin: 0 auto;
    margin-top: 74rpx;
    overflow: hidden;
}

.detail-content-empty .des {
    text-align: center;
    font-size: 26rpx;
    color: #9f9f9f;
    margin-top: 42rpx;
}

.mySharePanel {
    position: absolute;
    top: 0;
    width: 100%;
    z-index: 101;
}

.commodity_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
    overflow: hidden;
    z-index: 100;
    color: #fff;
}

.commodity_attr_box {
    width: 100%;
    height: 304rpx;
    overflow: hidden;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 2000;
    background: #fff;
}

.commodity_attr_box .shareChoose {
    height: 204rpx;
    font-size: 24rpx;
    color: #353535;
}

.commodity_attr_box .shareBtn {
    position: absolute;
    left: 70px;
    top: 30rpx;
    opacity: 0;
    width: 100rpx;
    height: 100rpx;
    z-index: 100;
}

.commodity_attr_box .iconBg {
    width: 80rpx;
    height: 80rpx;
    margin: 0 auto;
    border-radius: 40rpx;
    background-color: #72b44d;
    margin-top: 40rpx;
}

.commodity_attr_box .iconfont {
    position: absolute;
    left: 164rpx;
    top: 57rpx;
    font-size: 50rpx;
}

.commodity_attr_box .des {
    font-size: 24rpx;
    color: #353535;
    margin-top: 22rpx;
}

.commodity_attr_box .shareChoose .shareFriend {
    height: 204rpx;
    float: left;
    width: 50%;
    text-align: center;
    position: relative;
}

.commodity_attr_box .shareChoose .shareFriendWord {
    height: 204rpx;
    float: left;
    width: 50%;
    text-align: center;
    position: relative;
}

.commodity_attr_box .cancelBtn {
    height: 100rpx;
    text-align: center;
    font-size: 30rpx;
    color: #353535;
    background-color: #f9f9f9;
    line-height: 100rpx;
}

.saveSharePic {
    background-color: #fff;
    font-size: 28rpx;
    color: #353535;
    height: 96rpx;
    width: 640rpx;
    margin: 20rpx auto;
    text-align: center;
    line-height: 96rpx;
    border-radius: 4rpx;
}