page {
    background: #FAFAFA;
}

.interval {
    width: 100%;
    height: 40rpx;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.money {
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    color: #666;
    text-indent: 50rpx;
}

.interval text {
    color: #EF4F4C;
    margin-left: 20rpx;
}

.listMode {
    width: 95%;
    height: 100rpx;
    line-height: 100rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.listMode .title {
    float: left;
    width: 170rpx;
    height: 100%;
    line-height: 100rpx;
    text-align: center;
    font-size: 30rpx;
}

.listMode .formValue {
    float: left;
    width: 70%;
    height: 100%;
    line-height: 100rpx;
    font-size: 30rpx;
}

.listMode .formValue input {
    width: 100%;
    height: 100%;
    line-height: 100rpx;
    font-size: 30rpx;
}

.listMode .formValue textarea {
    width: auto;
    height: 100%;
    line-height: 100rpx;
    font-size: 30rpx;
}

.listMode .mapicon {
    float: left;
    line-height: 100rpx;
    width: 40rpx;
    height: 100rpx;
    text-align: center;
}

.listMode .mapicon image {
    display: inline-block;
    vertical-align: middle;
    width: 30rpx;
    height: 30rpx;
}

.listMode .item {
    width: 80rpx;
    height: 80rpx;
    float: left;
    position: relative;
    margin: 10rpx 0 10rpx 10rpx;
}

.delete {
    position: absolute;
    height: 20px;
    bottom: 0;
    width: 85rpx;
    left: -1rpx;
    background: #ccc;
    opacity: .8;
    text-align: center;
}

.delete image {
    vertical-align: top;
    width: 20px;
    height: 20px;
}

.bottom {
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 120rpx;
    box-shadow: 0rpx -4rpx 5rpx rgba(0,0,0,.1);
    background: #fff;
}

.bottom .buttomTow {
    width: 50%;
    height: 100%;
    line-height: 120rpx;
    float: right;
    margin-top: 15rpx;
}

.buttomTow button.apply {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0 30rpx;
    color: #fff;
    text-align: center;
    font-size: 30rpx;
    border: none;
    background-color: #64C164;
}

.buttomTow button.cancel {
    margin-left: 30rpx;
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    width: 150rpx;
    color: #fff;
    text-align: center;
    font-size: 30rpx;
    background: rgba(0,0,0,.3);
}

.bgtypeList {
    position: fixed;
    z-index: 3;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.cardAgainType {
    padding: 10rpx 20rpx 15rpx 20rpx;
    border-radius: 15rpx;
    width: 455rpx;
    height: auto;
    line-height: 103rpx;
    background-color: #fff;
    border-top: 1rpx solid #d2d3d5;
    margin: 0 auto;
    margin-top: 250rpx;
    color: #666;
    font-size: 30rpx;
}

.radio-group {
    width: 100%;
    height: auto;
}

.radio-group .radio {
    display: block;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    color: #000;
}

.determineAgin {
    margin: 0 auto;
    width: 200rpx;
    line-height: 62rpx;
    height: 70rpx;
    border-top: 1px solid #ccc;
    background: #D95940;
    border-radius: 10rpx;
    color: #fff;
    font-size: 30rpx;
}

.pickerTreatmentmodeitems {
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    padding-top: 100rpx;
    width: 100%;
    height: 400rpx;
    line-height: 400rpx;
}

.pickerTreatmentmodeitems image {
    display: inline-block;
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
    vertical-align: middle;
}

.pickerTreatmentmodeitems text {
    vertical-align: middle;
}