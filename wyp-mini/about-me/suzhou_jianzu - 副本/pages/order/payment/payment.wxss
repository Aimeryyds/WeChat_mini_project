.view-title {
    height: 100rpx;
    display: flex;
    flex-direction: row;
    padding-left: 40rpx;
    align-items: center;
    justify-content: space-between;
}

.view-title image {
    width: 56rpx;
    height: 56rpx;
}

.view-title text {
    font-size: 34rpx;
    margin-left: 30%;
    color: #9ebf6b;
}

.class-line1 {
    width: 750rpx;
    height: 2rpx;
    background: #F4F4F4;
}

.view-orderinfo {
    display: flex;
    flex-direction: column;
    padding: 30rpx;
    align-items: flex-start;
}

.view-orderinfo text {
    font-size: 30rpx;
    margin-top: 36rpx;
    color: #696969;
}

.view-orderinfo button {
    margin-top: 40rpx;
    width: 700rpx;
}