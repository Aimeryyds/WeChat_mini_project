}

.submitMy button::after {
    content: " ";
    width: 16%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    border: none;
    transform: scale(0.5);
    transform-origin: 0 0;
    box-sizing: border-box;
    border-radius: 10px;
    background: #fff;
}

