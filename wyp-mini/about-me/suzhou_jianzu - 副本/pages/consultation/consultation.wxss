.couponNum {
    width: 100%;
    height: 80rpx;
    font-size: 28rpx;
    border-bottom: 1px solid #ccc;
    background-color: #277DE2;
    overflow: scroll-x;
    white-space: nowrap;
}

.newTitle {
    padding: 0 25rpx;
    height: 76rpx;
    line-height: 78rpx;
    display: inline-block;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
}

.active {
    border-bottom: 2px solid #fff;
    color: #fff;
}

.newCont {
    margin: 0 auto;
    width: 95%;
    height: 180rpx;
    padding: 30rpx 0 30rpx 0;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.newCont .contLeft {
    width: 65%;
    height: 180rpx;
    float: left;
}

.newCont .contLeft .contLeftTitle {
    font-family: "微软雅黑";
    font-size: 32rpx;
    width: 90%;
    height: 120rpx;
    line-height: 60rpx;
    display: block;
    padding: 13rpx 0;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.newCont .contLeft .special {
    font-size: 28rpx;
    margin-right: 20rpx;
    color: #277DE2;
}

.newCont .contLeft .specialTitle {
    font-size: 28rpx;
    margin-right: 20rpx;
    color: #ccc;
}

.newCont .contLeft .thumbs-up {
    font-size: 28rpx;
    margin-right: 10rpx;
    color: #ccc;
}

.newCont .contLeft image {
    margin-top: 10rpx;
    margin-left: 10rpx;
    width: 34rpx;
    height: 34rpx;
    vertical-align: -4rpx;
}

.newCont .contRight {
    width: 35%;
    height: 180rpx;
    float: left;
}

.newCont .contRight image {
    width: 100%;
    height: 100%;
}