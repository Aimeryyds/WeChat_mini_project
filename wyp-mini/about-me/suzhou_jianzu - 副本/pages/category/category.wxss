}

.gridview4 {
    background-color: #EFF3F2;
    height: auto;
}

.gridview4 .gridview4Cont {
    width: 100%;
    height: 280rpx;
    padding-bottom: 10rpx;
    background: #EFF3F2;
}

.gridview4 .gridview4Cont .thumbLeft {
    display: block;
    width: 38%;
    height: 280rpx;
    line-height: 280rpx;
    float: left;
    background: #fff;
    text-align: center;
}

.gridview4 .gridview4Cont .thumbLeft image {
    width: 80%;
    height: 230rpx;
    vertical-align: middle;
}

.gridview4 .gridview4Cont .thumbRight {
    display: block;
    height: 280rpx;
    width: 62%;
    float: left;
    background: #fff;
}

.gridview4 .gridview4Cont .thumbRight .thumbTitle1 {
    width: 90%;
    font-size: 32rpx;
    font-weight: bolder;
    height: 55rpx;
    margin-top: 20rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.gridview4 .gridview4Cont .thumbRight .thumbTitle2 {
    width: 90%;
    font-size: 30rpx;
    height: 55rpx;
    line-height: 40rpx;
    color: #D96A70;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.gridview4 .gridview4Cont .thumbRight .thumbTitle3 {
    width: 90%;
    font-size: 30rpx;
    height: 110rpx;
    line-height: 50rpx;
    color: #88BAA4;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

