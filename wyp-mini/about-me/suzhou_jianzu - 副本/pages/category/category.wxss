.clear {
    clear: both;
}

.footerTip {
    width: 100%;
    margin: 0 auto 80rpx;
    background-color: #EFF3F2;
    padding-top: 70rpx;
}

.footerInner {
    width: 90%;
    margin: 0 auto;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.footerInner .commonLine {
    width: 30%;
    height: 2rpx;
    background: #ccc;
}

.footerInner .midText {
    font-size: 28rpx;
    color: #999;
    display: inline-block;
    margin-top: -20rpx;
}

.noMoreData {
    text-align: center;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 25rpx;
    color: #666;
}

.diy_wrap {
    position: fixed;
    bottom: 0;
    height: 100rpx;
    width: 100%;
    border-top: 1rpx solid #ccc;
    display: flex;
    flex-direction: row;
    margin-top: 100rpx;
    background-color: #fff;
    z-index: 999;
}

.diy_item {
    height: 100rpx;
    width: 25%;
    display: flex;
    flex-direction: column;
    text-align: center;
    font-size: 24rpx;
    line-height: 40rpx;
    position: relative;
}

.diy_img {
    width: 60rpx;
    height: 60rpx;
    margin: 0 auto;
}

.diy_contact {
    width: 100%;
    opacity: 0;
    position: absolute;
    z-index: 9;
    height: 100%;
}