page {
    background-color: #efefef;
}

.selectbox {
    width: 100%;
}

.listbox {
    width: 100%;
    overflow-y: scroll;
}

.listbox .addresslist {
    background-color: #fff;
    position: relative;
    width: 100%;
    padding: 0;
    overflow: hidden;
    border-bottom: 2rpx solid #efefef;
    padding-bottom: 20rpx;
}

.addresslist .select {
    height: 35rpx;
    width: 35rpx;
    position: absolute;
    top: 50%;
    left: 20rpx;
    margin-top: -25rpx;
}

.addresslist .name {
    position: absolute;
    top: 20rpx;
    left: 100rpx;
    font-size: 30rpx;
    font-weight: 700;
}

.addresslist .address {
    font-size: 30rpx;
    margin: 0;
    margin-top: 60rpx;
    margin-left: 100rpx;
    width: 540rpx;
    white-space: pre-wrap;
    color: #666;
}

.addresslist .btn-edit {
    width: 100rpx;
    height: 100%;
    position: absolute;
    font-size: 30rpx;
    top: 0;
    right: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    line-height: 50rpx;
    color: #e90c48;
}

.btn-add {
    position: fixed;
    bottom: 0rpx;
    left: 0rpx;
    line-height: 100rpx;
    height: 100rpx;
    width: 100%;
    background-color: #fff;
    font-size: 35rpx;
    color: #ff5858;
    text-align: center;
}

.box {
    width: 90%;
    border-radius: 10rpx;
    padding: 20rpx;
    overflow: hidden;
    background: #fff;
    margin: 0 auto;
    margin-top: 10rpx;
}

.title {
    font-size: 30rpx;
    font-weight: 700;
    display: block;
    color: #333;
    margin-top: 15rpx;
}

.list {
    width: 100%;
    height: 70rpx;
    border-bottom: 1rpx solid #e5e5e5;
    display: flex;
    align-items: center;
    font-size: 30rpx;
    color: #333;
    position: relative;
    padding-left: 180rpx;
}

.list>text:first-of-type {
    font-size: 30rpx;
    width: 130rpx;
    position: absolute;
    left: 20rpx;
    border-right: 3rpx solid #888;
    display: block;
    height: 30rpx;
    line-height: 30rpx;
    top: 50%;
    margin-top: -15rpx;
}

.address .list {
    padding-left: 280rpx;
}

.address .list:last-child {
    border: none;
}

.address .list>text:first-of-type {
    font-size: 27rpx;
    width: 230rpx;
}

.list input,.picker {
    font-weight: 500;
    font-size: 30rpx;
    display: block;
    height: 30rpx;
    line-height: 30rpx;
    color: #888;
}

.boy,.girl {
    display: flex;
    align-items: center;
    margin-right: 40rpx;
}

.boy text,.girl text {
    font-size: 30rpx;
    color: #333;
    margin-left: 20rpx;
}

view.select {
    width: 30rpx;
    height: 30rpx;
    border-radius: 50%;
    border: 1rpx solid #e5e5e5;
}

view.select.active {
    background-color: #ff6a6a;
}

.button {
    width: 80%;
    background-color: #ff6a6a;
    color: #fff;
    margin: 0 auto;
    margin-top: 50rpx;
    height: 70rpx;
    border-radius: 10rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 38rpx;
}

.button:active {
    background-color: #ff5858;
    color: #fff;
}

.search {
    height: 80rpx;
    width: 100%;
    background-color: #fff;
    position: fixed;
    top: 0;
    left: 0;
    display: flex;
    align-items: center;
    border-bottom: 2rpx solid #e5e5e5;
}

.search>input {
    width: 600rpx;
    height: 50rpx;
    padding-left: 20rpx;
    font-size: 20rpx;
    border-radius: 10rpx;
    background-color: #efefef;
    margin-left: 20rpx;
}

.search>image {
    width: 25rpx;
    height: 25rpx;
    position: absolute;
    top: 50%;
    margin-top: -12.5rpx;
    right: 130rpx;
    z-index: 99;
}

.search .search-btn {
    width: 80rpx;
    height: 50rpx;
    background-color: #c3c3c3;
    color: #fff;
    font-size: 20rpx;
    line-height: 50rpx;
    text-align: center;
    margin-left: 20rpx;
    border-radius: 15rpx;
}

.search .search-btn:active {
    background-color: #e5e5e5;
}

.search .search-btn-red {
    width: 80rpx;
    height: 50rpx;
    background-color: #e90c48;
    color: #fff;
    font-size: 20rpx;
    line-height: 50rpx;
    text-align: center;
    margin-left: 20rpx;
    border-radius: 15rpx;
}

.search .search-btn-red:active {
    background-color: #c60c3e;
}

.address-list {
    width: 100%;
    height: 130rpx;
    background-color: #fff;
    border-bottom: 2rpx solid #e5e5e5;
    overflow: hidden;
    padding-left: 20rpx;
    box-sizing: border-box;
    position: relative;
}

.address-list .title {
    font-size: 30rpx;
    font-weight: 500;
    line-height: 70rpx;
}

.address-list .text {
    font-size: 20rpx;
    font-weight: 500;
}

.setdefault>view {
    width: 600rpx;
    height: 50rpx;
    padding-left: 20rpx;
    font-size: 20rpx;
    margin-left: 20rpx;
    display: flex;
    align-items: center;
}

.default {
    width: 60rpx;
    height: 30rpx;
    color: red;
    font-size: 20rpx;
    position: absolute;
    top: 50%;
    margin-top: -15rpx;
    left: 20rpx;
    text-align: center;
    line-height: 30rpx;
    border: 1rpx solid red;
}