.userinfo {
    width: 90%;
    margin: 34rpx auto 0;
}

.input_search {
    width: 98%;
    border-radius: 8rpx;
    height: 60rpx;
    font-size: 30rpx;
    padding-left: 10rpx;
    margin-right: 35rpx;
    margin-top: 20rpx;
    background: #eee;
}

.userinfo button {
    background: #ff6a6a;
    color: #fff;
    width: 100%;
    margin-right: 58rpx;
    margin-top: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 36rpx;
}

.page {
    min-height: 100%;
    flex: 1;
    font-size: 32rpx;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
    overflow: hidden;
}

.picker {
    display: inline-block;
    background-color: #eee;
    width: 100%;
}

.section {
    width: 98%;
    border-radius: 8rpx;
    height: 60rpx;
    font-size: 30rpx;
    padding-left: 10rpx;
    margin-right: 35rpx;
    margin-top: 20rpx;
    color: #626462;
    margin-bottom: 30rpx;
    background: #eee;
}

.section__title {
    padding-right: 30rpx;
    display: inline-block;
    line-height: 60rpx;
}

.pickBox {
    display: inline-block;
    width: 74%;
}

.express-title {
    width: 90%;
    margin: 0 auto;
}

.list {
    font-size: 28rpx;
    padding-right: 20rpx;
    margin-bottom: 65rpx;
    margin-top: 30rpx;
}

.item {
    padding: 30rpx 0 0 20rpx;
    border-left: 1rpx solid #ccc;
    margin-left: 20rpx;
    position: relative;
}

.item_wrap {
    border-bottom: 1rpx solid #ccc;
    margin-left: 20rpx;
    padding-bottom: 30rpx;
    display: flex;
    flex-direction: column;
}

.first {
    position: absolute;
    left: -9rpx;
    top: 44%;
    z-index: 1;
    width: 18rpx;
    height: 18rpx;
    background-color: #999;
    border-radius: 50%;
}

.last {
    position: absolute;
    left: -10rpx;
    top: 44%;
    z-index: 1;
    width: 22rpx;
    height: 22rpx;
    background-color: #ff6a6a;
    border-radius: 50%;
    box-shadow: 0 0 10rpx #ff6a6a;
    -webkit-box-shaow: 0 0 10rpx #ff6a6a;
}

.highLight text {
    color: #ff6a6a;
}

.icon {
    position: absolute;
    left: -9rpx;
    top: 44%;
    z-index: 1;
    width: 18rpx;
    height: 18rpx;
    background-color: gray;
    border-radius: 50%;
}

text {
    font-family: arial;
}

.noexpress {
    width: 90%;
    justify-content: center;
    margin: 200rpx auto 0;
    text-align: center;
}

.noexpress text {
    text-align: center;
    width: 100%;
    margin: 0 auto;
    font-size: 34rpx;
    color: #626462;
}