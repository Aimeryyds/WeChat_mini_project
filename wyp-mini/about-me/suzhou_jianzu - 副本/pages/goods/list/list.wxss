page {
    background: #fff;
}

.search {
    width: 100%;
    height: 100rpx;
    border-bottom: 3rpx solid #e3e3e3;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background: #f8f8f8;
}

.search-content {
    margin-left: 20rpx;
    width: 620rpx;
    height: 70rpx;
    background: #e3e3e3;
    border-radius: 10rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.search-content image {
    width: 44rpx;
    height: 44rpx;
    margin-left: 20rpx;
}

.input_search {
    flex-grow: 2;
    margin-left: 15rpx;
    color: #999;
    font-size: 28rpx;
    line-height: 28rpx;
}

.search text {
    font-size: 28rpx;
    color: #5a5a5a;
    width: 80rpx;
    margin-right: 10rpx;
    text-align: start;
}

.menu {
    display: block;
    height: 87rpx;
}

.menu image {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}

.menu dt {
    font-size: 15px;
    float: left;
    width: 20%;
    height: 80rpx;
    text-align: center;
    background-color: #f4f4f4;
    color: #5a5a5a;
    line-height: 80rpx;
}

.menu dt.highlight {
    color: red;
}

.menu dt.highlight2 {
    color: red;
}

.menu dt.highlight1 {
    color: #5a5a5a;
}

.menu dd {
    position: absolute;
    width: 100%;
    top: 180rpx;
    left: 0;
    z-index: 999;
    border-top: 1rpx solid rgba(0,0,0,.1);
    height: 100%;
    background: rgba(0,0,0,0.4);
    min-height: 400rpx;
}

.menu ul {
    height: auto;
    width: 100%;
    background-color: #f8f8f8;
}

.menu li {
    font-size: 14px;
    line-height: 34px;
    color: #666;
    height: 34px;
    display: block;
    padding-left: 8px;
    background-color: #f8f8f8;
    border-bottom: 1px solid #dbdbdb;
}

.menu li.highlight {
    background-color: #f4f4f4;
    color: red;
}

.show {
    display: block;
}

.hidden {
    display: none;
}

.goods-score {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
}

.goods-score text {
    display: block;
    width: 100px;
    font-size: 12px;
    color: #999;
    margin: 8px 4px;
}

.goodsList {
    margin-top: 14rpx;
    z-index: 1;
}

.carts-list {
    display: flex;
    flex-direction: column;
    padding: 0 30rpx;
}

.carts-item {
    display: flex;
    flex-direction: row;
    height: 170rpx;
    width: 100%;
    border-bottom: 3rpx solid #e3e3e3;
    padding: 30rpx 0;
    align-items: center;
    justify-content: space-between;
}

.carts-image {
    width: 170rpx;
    height: 170rpx;
}

.carts-text {
    margin-left: 20rpx;
    width: 550rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.carts-title {
    font-size: 30rpx;
    color: #444;
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}

.carts-subtitle {
    font-size: 25rpx;
    color: #a9a9a9;
    padding: 0 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.carts-price {
    color: #FF6A6A;
    font-size: 29rpx;
    line-height: 22px;
    margin-top: 10rpx;
    display: inline-block;
    width: 83%;
}

.carts-img {
    width: 42rpx;
    height: 40rpx;
    vertical-align: middle;
}

.view-empty {
    width: 100%;
    height: 400rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.view-empty text {
    font-size: 32rpx;
    color: #444;
}

.goodsList1 {
    margin-top: 14rpx;
    margin-bottom: 14rpx;
    z-index: 1;
    overflow: hidden;
}

.carts-list1 {
    padding: 0 30rpx;
    margin-bottom: 80rpx;
    overflow: hidden;
}

.carts-item1 {
    display: inline-block;
    height: 450rpx;
    width: 46%;
    border-bottom: 3rpx solid #e3e3e3;
    overflow: hidden;
    border: 1rpx solid rgba(0,0,0,.1);
    margin-left: 15rpx;
}

.imgView {
    width: 100%;
    height: 270rpx;
    text-align: center;
    line-height: 270rpx;
}

.carts-image1 {
    width: 260rpx;
    height: 260rpx;
    vertical-align: middle;
}

.carts-text1 {
    margin-left: 20rpx;
}

.carts-title1 {
    display: block;
    font-size: 30rpx;
    color: #444;
    height: 60rpx;
    line-height: 60rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.carts-subtitle1 {
    font-size: 25rpx;
    color: #a9a9a9;
    padding: 0 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.carts-price1 {
    color: #FF6A6A;
    font-size: 29rpx;
    line-height: 22px;
    margin-top: 10rpx;
    width: 80%;
    display: inline-block;
}

.carts-img1 {
    width: 40rpx;
    height: 38rpx;
    vertical-align: middle;
}

.goods-score1 {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    flex-wrap: nowrap;
}

.goods-score1 text {
    float: left;
    height: 30rpx;
    display: block;
    width: 200rpx;
    font-size: 24rpx;
    color: #999;
    margin: 10rpx 0rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    flex-wrap: nowrap;
}

.goods-score1 text:nth-child(2) {
    text-indent: 10rpx;
}

.buyText {
    display: inline-block;
    width: 64rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    vertical-align: middle;
    margin-right: 10rpx;
    border-radius: 10rpx;
    color: #fff;
    font-size: 24rpx;
    background: #fe5455;
}

.noMoreData {
    text-align: center;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 25rpx;
    color: #666;
}

.light {
    color: red!important;
}

.scrollTop {
    width: 100rpx;
    height: 100rpx;
    position: fixed;
    right: 0rpx;
    bottom: 50rpx;
    z-index: 999;
}

.scrollTop image {
    width: 60rpx;
    height: 60rpx;
}

.tabTitle {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.titleRight .arrowTop {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-bottom: 12rpx solid #b8bcbb;
    margin: 5rpx 0 5rpx 10rpx;
}

.titleRight .arrowBottom {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 12rpx solid #b8bcbb;
    margin: 0 0 0 10rpx;
}

.arrowTopLight {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-bottom: 12rpx solid red;
    margin: 6rpx 0 5rpx 10rpx;
}

.arrowBottomLight {
    width: 0;
    height: 0;
    border-left: 10rpx solid transparent;
    border-right: 10rpx solid transparent;
    border-top: 12rpx solid red;
    margin: 6rpx 0 0 10rpx;
}