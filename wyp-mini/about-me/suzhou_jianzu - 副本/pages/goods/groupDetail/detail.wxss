page {
    background: #fff;
}

.parent {
    display: flex;
    flex-direction: column;
}

.class-tab {
    display: flex;
    flex-direction: row;
    margin-top: 20rpx;
    justify-content: center;
    background: #fff;
    width: 100%;
}

.text-select {
    width: 180rpx;
    font-size: 33rpx;
    color: red;
    height: 60rpx;
    border-bottom-width: 6rpx;
    border-color: red;
    border-style: solid;
    text-align: center;
    border-top-width: 0;
    border-left-width: 0;
    border-right-width: 0;
}

.text-normal {
    width: 180rpx;
    font-size: 33rpx;
    color: #4D4D4D;
    height: 60rpx;
    border-bottom-width: 0;
    border-color: red;
    border-style: solid;
    text-align: center;
    border-top-width: 0;
    border-left-width: 0;
    border-right-width: 0;
}

.images {
    position: relative;
    background: #fff;
}

.swiper-image {
    width: 100%;
    height: 100%;
    background: #fff;
}

.view-class-title {
    display: flex;
    height: 120rpx;
    width: 100%;
    background: #fcfcfc;
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
}

.summary {
    width: 550rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin: 20rpx 0;
}

.class-line {
    width: 3rpx;
    height: 80rpx;
    background: #d3d3d3;
}

.class-collect {
    width: 170rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

.class-collect image {
    width: 100%;
    height: 100%;
}

.title {
    font-size: 28rpx;
    margin: 10rpx;
    line-height: 46rpx;
}

.view-price {
    width: 100%;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
}

.view-other-info {
    width: 80%;
    height: 75rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 15rpx 20rpx 15rpx 20rpx;
}

.view-other-info text {
    font-size: 27rpx;
    width: 150rpx;
    color: #999;
    text-align: center;
}

.text-price {
    margin-left: 30rpx;
    font-size: 31rpx;
    color: #999;
    background-image: url(http://tws.cnweisou.com/images/delete.png);
    background-size: 180% 100%;
    text-decoration-line: line-through;
}

.text-red {
    margin-left: 30rpx;
    font-size: 43rpx;
    color: #ff4500;
}

.price {
    color: #f60;
    margin: 10rpx;
}

.detail {
    margin-bottom: 50px;
}

.detail image {
    width: 100%;
}

.footer {
    background: #f9f9f9;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    position: fixed;
    bottom: 0;
}

.add-cart {
    float: right;
    border: 0;
    line-height: 100rpx;
    text-align: center;
    width: 375rpx;
    height: 100rpx;
    background-color: #f60;
    color: #fff;
    font-size: 36rpx;
    border-radius: 0;
}

.btn-bug {
    float: left;
    border: 0;
    line-height: 100rpx;
    text-align: center;
    width: 375rpx;
    height: 100rpx;
    background-color: red;
    color: #fff;
    font-size: 36rpx;
    border-radius: 0;
}

.favor {
    width: 100%;
}

.favor-image {
    width: 40rpx;
    height: 40rpx;
}

.favor-text {
    font-size: 30rpx;
}

.text-prop {
    font-size: 30rpx;
    margin-left: 19rpx;
    margin-top: 22rpx;
    color: #5e5e5e;
}

.rowline {
    width: 700rpx;
    height: 2rpx;
    align-self: center;
    background: #F4F4F4;
    margin-bottom: 20rpx;
    margin-top: 20rpx;
}

.view-sub-prop {
    padding: 10rpx 10rpx 10rpx 20rpx;
    display: flex;
    flex-direction: column;
}

.view-btns {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-top: 25rpx;
}

.view-btns-text-normal {
    padding: 10rpx 20rpx 10rpx 20rpx;
    margin-left: 10rpx;
    font-size: 27rpx;
    color: #4D4D4D;
    border-width: 2rpx;
    border-style: solid;
    border-color: #999;
    border-radius: 8rpx;
}

.view-btns-text-select {
    padding: 10rpx 20rpx 10rpx 20rpx;
    margin-left: 10rpx;
    font-size: 27rpx;
    color: #fff;
    border-radius: 8rpx;
    background: red;
}

.stepper {
    margin-top: 20rpx;
    border: 1px solid #ccc;
    border-radius: 3px;
    width: 80px;
    height: 26px;
    margin-left: 30rpx;
}

.stepper text {
    width: 19px;
    line-height: 26px;
    text-align: center;
    float: left;
}

.stepper input {
    color: #000;
    float: left;
    margin: 0 auto;
    width: 40px;
    height: 26px;
    text-align: center;
    font-size: 12px;
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
}

.stepper .normal {
    color: #000;
}

.stepper .disabled {
    color: #ccc;
}

.carts-list icon {
    margin-top: 60rpx;
    margin-right: 20rpx;
}

.xq-image {
    width: 100%;
}

.text-desc-title {
    margin-top: 20rpx;
    margin-left: 20rpx;
    font-size: 33rpx;
    color: #5e5e5e;
    border-width: 0 0 0 6rpx;
    border-color: red;
    border-style: solid;
    padding-left: 10rpx;
}

.text-propdesc {
    font-size: 28rpx;
    margin-left: 25rpx;
    margin-top: 12rpx;
    color: #5e5e5e;
}

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

.carts-list {
    display: flex;
    flex-direction: column;
    padding: 0 40rpx;
    margin-bottom: 80rpx;
}

.carts-item {
    display: flex;
    flex-direction: row;
    height: 180rpx;
    width: 100%;
    border-bottom: 1px solid #eee;
    padding: 30rpx 0;
    justify-content: space-between;
}

.carts-userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.carts-image {
    width: 150rpx;
    height: 150rpx;
    border-width: 3rpx;
    border-style: solid;
    border-color: #ccc;
}

.text-username {
    font-size: 28rpx;
    margin-left: 20rpx;
    margin-top: 12rpx;
    color: #5e5e5e;
}

.carts-text {
    margin-left: 10rpx;
    width: 500rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.carts-right {
    height: 100%;
    width: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-end;
}

.carts_bottom {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.carts_xx {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
}

.stars {
    margin-left: 20rpx;
    width: 150rpx;
    height: 25rpx;
}

.btn-red {
    color: #fff;
    background: red;
    font-size: 28rpx;
    margin-left: 15rpx;
    padding: 3rpx 5rpx;
}

.text-endtime {
    color: red;
    font-size: 28rpx;
    margin-left: 15rpx;
    padding: 3rpx 5rpx;
}