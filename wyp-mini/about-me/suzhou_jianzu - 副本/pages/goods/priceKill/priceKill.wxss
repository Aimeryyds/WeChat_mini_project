page {
    background: #fafafa;
}

.couponNum {
    width: 100%;
    height: 120rpx;
    font-size: 28rpx;
    border-bottom: 1px solid #ccc;
    overflow: scroll-x;
    white-space: nowrap;
    background: #4B4B52;
}

.newTitle {
    height: 100%;
    display: inline-block;
    text-align: center;
    color: #eee;
    font-size: 30rpx;
    width: 25%;
    position: relative;
}

.newTitle text {
    display: block;
    width: 100%;
    height: 50%;
    text-align: center;
    line-height: 55rpx;
}

.newTitle text.type {
    font-size: 29rpx;
}

.newTitle .triangle {
    position: absolute;
    margin: auto auto;
    left: 0;
    right: 0;
    bottom: 0;
    width: 0;
    height: 0;
    border-left: 40rpx solid transparent;
    border-right: 40rpx solid transparent;
    border-top: 80rpx solid #000;
}

.shopNum {
    display: block;
    width: 100%;
    height: 50%;
    text-align: center;
    line-height: 55rpx;
}

.shopNum image {
    width: 45rpx;
    height: 45rpx;
    vertical-align: middle;
}

.active {
    font-size: 35rpx;
    color: #fff;
    background: #FF5555;
}

.shopList {
    width: 100%;
    height: auto;
    margin: 0rpx auto 0;
}

.shopList .title {
    width: 100%;
    height: 100rpx;
    margin: 0rpx auto 0;
}

.title .titleLine {
    float: left;
    width: 6rpx;
    height: 100%;
    background: #FF5555;
}

.title .titleLeft {
    float: left;
    line-height: 100rpx;
    width: 50%;
    height: 100rpx;
    font-size: 30rpx;
    color: #000;
}

.title .titleLeft text {
    margin: 20rpx 0;
}

.title .titleRight {
    float: left;
    line-height: 100rpx;
    width: 47%;
    height: 100rpx;
    font-size: 30rpx;
    color: #000;
}

.title .titleRight text.isStart {
    width: auto;
    margin-right: 15rpx;
    font-size: 30rpx;
    color: #000;
    background: #fff;
}

.title .titleRight text {
    display: inline-block;
    width: 45rpx;
    height: 40rpx;
    background: #464553;
    border-radius: 8rpx;
    font-size: 28rpx;
    color: #fff;
    text-align: center;
    line-height: 13rpx;
}

.goodsList {
    margin: 0rpx auto 0;
    width: 95%;
    height: 200rpx;
    background: #fff;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
    position: relative;
    overflow: hidden;
}

.percentage {
    position: absolute;
    width: 210rpx;
    height: 40rpx;
    right: 10rpx;
    bottom: 10rpx;
}

.percentage .hasSell {
    position: absolute;
    width: auto;
    height: 100%;
    left: -120rpx;
    top: -20rpx;
}

.percentage .hasSell text {
    margin-right: 10rpx;
}

.goodsList .left {
    float: left;
    width: 180rpx;
    height: 170rpx;
    line-height: 170rpx;
    text-align: center;
}

.left image {
    width: 160rpx;
    height: 160rpx;
    vertical-align: middle;
}

.goodsList .center {
    float: left;
    width: 51%;
    height: 100%;
}

.goodsList .center text.top {
    display: block;
    font-size: 30rpx;
    color: #666;
    width: 365rpx;
    height: 74rpx;
    line-height: 64rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.goodsList .center text.middle {
    display: block;
    font-size: 33rpx;
    color: red;
    width: 365rpx;
    height: 55rpx;
}

.goodsList .center .bottom {
    display: inline-block;
    font-size: 30rpx;
    color: #ccc;
    width: auto;
    position: relative;
    vertical-align: top;
    margin-top: 15rpx;
}

.goodsList .center .bottom text.line {
    display: block;
    font-size: 28rpx;
    background: #ccc;
    position: absolute;
    width: 100%;
    height: 2rpx;
    left: 0;
    top: 18rpx;
}

.percentage text {
    display: inline-block;
    font-size: 22rpx;
    color: #ccc;
}

progress {
    font-size: 22rpx;
    color: #ccc;
}

.goodsList .right {
    float: left;
    width: 23%;
    height: 100%;
    line-height: 170rpx;
    text-align: center;
}

.goodsList .right .btn {
    display: inline-block;
    font-size: 30rpx;
    color: #fff;
    width: auto;
    height: 60rpx;
    line-height: 20rpx;
    padding: 0 20rpx;
    background: #F55;
    text-align: center;
    border-radius: 8rpx;
}

.goodsList .right .btnGreen {
    display: inline-block;
    font-size: 30rpx;
    color: #fff;
    width: auto;
    height: 60rpx;
    line-height: 20rpx;
    padding: 0 20rpx;
    background: #04BE02;
    text-align: center;
    border-radius: 8rpx;
}

.skillPrices {
    width: 100%;
    height: 370rpx;
}

.skillPrices .top {
    width: 100%;
    height: 120rpx;
    background: #fff;
}

.skillPrices .top .left {
    float: left;
    text-align: center;
    width: 145rpx;
    height: 120rpx;
    line-height: 120rpx;
    color: #DA322D;
    font-size: 30rpx;
    font-family: STHeiti;
    font-weight: 700;
}

.skillPrices .top .center {
    float: left;
    width: 510rpx;
    height: 120rpx;
    line-height: 120rpx;
    color: #000;
    font-size: 30rpx;
}

.skillPrices .top .center text.isStart {
    width: auto;
    margin-right: 15rpx;
    font-size: 30rpx;
    color: #000;
    background: #fff;
    border: none;
}

.skillPrices .top .center text {
    display: inline-block;
    width: 45rpx;
    height: 40rpx;
    border-radius: 8rpx;
    font-size: 28rpx;
    color: #000;
    text-align: center;
    line-height: 13rpx;
    border: 1rpx solid #ccc;
}

.skillPrices .top .right {
    float: left;
    width: 90rpx;
    height: 120rpx;
    line-height: 120rpx;
    color: #666;
    font-size: 30rpx;
}

.skillPrices .top .right text {
    display: inline-block;
    width: auto;
    vertical-align: middle;
}

.skillPrices .top .right image.rightArrow {
    display: inline-block;
    vertical-align: middle;
    height: 20rpx;
    width: 20rpx;
}

.skillPrices .bottom {
    width: 100%;
    height: 250rpx;
}

.skillPrices .shopscroll {
    width: 100%;
    height: 250rpx;
    overflow: scroll-x;
    background: red;
}

.shopscroll .shoplist {
    height: 100%;
    display: inline-block;
    text-align: center;
    color: #000;
    font-size: 30rpx;
    width: 25%;
}

.shopscroll .shoplist .topimg {
    width: 100%;
    height: 175rpx;
    text-align: center;
}

.shopscroll .shoplist .topimg image {
    width: 95%;
    height: 175rpx;
}

.bottomprice {
    width: 100%;
    text-align: center;
}

.nowprice {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 32rpx;
    color: #DA322D;
}

.bottomprice .agoprice {
    display: inline-block;
    width: auto;
    position: relative;
    font-size: 26rpx;
    color: #666;
}

.agoprice text {
    font-size: 26rpx;
    color: #666;
    position: absolute;
    top: 15rpx;
    left: 0rpx;
    width: 100%;
    height: 2rpx;
    background: #666;
}

.line {
    width: 100%;
    margin: 0 auto;
    height: 20rpx;
    background: #eee;
}