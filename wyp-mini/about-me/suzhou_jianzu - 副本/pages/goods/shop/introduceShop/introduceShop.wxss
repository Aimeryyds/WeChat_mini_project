page {
    font-family: '微软雅黑';
}

swiper {
    height: 350rpx;
    position: relative;
}

.swiperitem {
    text-align: center;
    line-height: 370rpx;
}

.swiperitem image {
    width: 100%;
    height: 100%;
}

.titleSwiper {
    position: absolute;
    z-index: 10;
    top: 30rpx;
    left: 15rpx;
    width: 90rpx;
    height: 50rpx;
    line-height: 50rpx;
    background: #d95940;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    border-radius: 8rpx;
}

.cover {
    position: absolute;
    z-index: 1;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.4);
    top: 0;
    left: 0;
}

.logoImg {
    position: absolute;
    left: 0;
    top: 240rpx;
    z-index: 20;
    width: 100%;
    height: 233rpx;
    text-align: center;
}

.logoImg image {
    width: 200rpx;
    height: 200rpx;
    border: 1rpx solid rgba(0,0,0,.1);
}

.logo {
    width: 100%;
    height: 290rpx;
    text-align: center;
    position: relative;
}

.logo .logoTitle {
    margin-top: 130rpx;
    color: rgba(0,0,0,.8);
    font-size: 32rpx;
    font-weight: 700;
}

.logo .logoMap {
    width: 100%;
    height: 150rpx;
    margin-top: 20rpx;
    text-align: center;
}

.logoMap .map {
    display: inline-block;
    border: 1px solid #ccc;
    border-radius: 10rpx;
    width: 185rpx;
    height: 70rpx;
    line-height: 65rpx;
}

.logoMap .map image {
    display: inline-block;
    margin-right: 5rpx;
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
}

.logoMap .map text {
    color: #666;
    font-size: 28rpx;
}

.list {
    border-top: 1rpx solid rgba(0,0,0,.1);
    width: 100%;
    min-height: 80rpx;
    line-height: 75rpx;
}

.list .title {
    display: inline-block;
    width: 22%;
    text-align: center;
    height: 100%;
    font-size: 28rpx;
    color: #666;
    vertical-align: top;
}

.list .phoneTitle {
    vertical-align: middle;
}

.list .cont {
    display: inline-block;
    width: 75%;
    height: auto;
    font-size: 28rpx;
    color: #666;
    text-indent: 20rpx;
}

.list .contphone {
    display: inline-block;
    width: 70%;
    height: 100%;
    font-size: 28rpx;
    color: #666;
    text-indent: 20rpx;
}

.list image {
    width: 45rpx;
    height: 40rpx;
    vertical-align: middle;
}

.listBottom {
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.shopphoto .title {
    width: 100%;
    height: 100rpx;
    line-height: 95rpx;
}

.shopphoto .title image {
    display: inline-block;
    margin-left: 30rpx;
    width: 45rpx;
    height: 40rpx;
    vertical-align: middle;
}

.shopphoto .title text {
    display: inline-block;
    font-size: 28rpx;
    text-indent: 20rpx;
}

.shopphoto .photo {
    width: 95%;
    height: auto;
    margin: 0 auto;
    padding-bottom: 30rpx;
}

.shopphoto .photo .photoDetial {
    display: inline-block;
    width: 33%;
    height: 260rpx;
    text-align: center;
}

.photoDetial image {
    display: inline-block;
    width: 95%;
    height: 95%;
    margin: 0 auto;
    border: 1rpx solid #ccc;
}

.interval {
    width: 100%;
    height: 60rpx;
}

.bgtypeList {
    position: fixed;
    z-index: 300;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.6);
}

.slideImgs {
    position: absolute;
    top: 0rpx;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;
    margin: 150rpx auto 0;
    text-align: center;
    vertical-align: middle;
}

.slide-image {
    max-width: 90%;
    max-height: 90%;
    text-align: center;
    vertical-align: middle;
}