.container {
    width: 100%;
    height: 100%;
}

.Commission {
    width: 100%;
    height: 80rpx;
    border-top: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    font-size: 32rpx;
    line-height: 80rpx;
    background-color: #fff;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.left {
    margin-left: 30rpx;
    margin-right: 400rpx;
}

.right {
    margin-right: 30rpx;
}

.tabClass {
    width: 750rpx;
    display: flex;
    font-size: 30rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ccc;
    align-items: center;
}

.normalClass {
    width: 187.5rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
}

.selectClass {
    border-bottom: 1px solid red;
    color: red;
}

.list {
    width: 100%;
    height: 120rpx;
    border-bottom: 1px solid #ccc;
    display: flex;
    font-size: 30rpx;
    align-items: center;
    justify-content: space-between;
}

.listBox {
    width: 98%;
    margin-left: 2%;
    height: 100rpx;
    border-bottom: 1px solid #ccc;
    display: flex;
    font-size: 30rpx;
    align-items: center;
    justify-content: space-between;
}

.time {
    color: gray;
    font-size: 26rpx;
}

.listLeft {
    display: flex;
    flex-direction: column;
    margin-left: 20rpx;
}

.listRight {
    display: flex;
    flex-direction: column;
    margin-right: 20rpx;
    justify-content: center;
    align-items: flex-end;
}