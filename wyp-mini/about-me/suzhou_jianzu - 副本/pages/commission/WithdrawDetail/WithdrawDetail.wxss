.container {
    width: 100%;
    height: 100%;
}

.top {
    width: 98%;
    display: flex;
    flex-direction: column;
    font-size: 28rpx;
    color: gray;
    margin-left: 2%;
    height: 200rpx;
    justify-content: space-around;
    padding-bottom: 10rpx;
}

.box {
    margin-top: 30rpx;
}

.list {
    width: 100%;
    height: 150rpx;
    display: flex;
    font-size: 28rpx;
    align-items: center;
}

.list image {
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
    margin-left: 20rpx;
    border-radius: 10rpx;
}

.title {
    display: flex;
    flex-direction: column;
    width: 440rpx;
    border-right: 1px solid #ccc;
    margin-right: 20rpx;
}

.title1 {
    font-size: 30rpx;
    color: gray;
}

.title2 {
    font-size: 28rpx;
}

.right {
    display: flex;
    flex-direction: column;
    text-align: left;
}

.rank {
    font-size: 30rpx;
}

.rank2 {
    color: red;
}