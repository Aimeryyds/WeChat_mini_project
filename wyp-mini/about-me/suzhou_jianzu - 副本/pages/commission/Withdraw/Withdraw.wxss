.container {
    width: 100%;
    height: 100%;
}

.Commission {
    width: 100%;
    height: 80rpx;
    border-top: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    font-size: 32rpx;
    line-height: 80rpx;
    background-color: #fff;
}

.left {
    margin-left: 30rpx;
    margin-right: 360rpx;
}

.Commission .right {
    width: 200rpx;
    text-align: right;
    display: inline-block;
}

.tabClass {
    width: 750rpx;
    display: flex;
    font-size: 30rpx;
    height: 80rpx;
    border-bottom: 1rpx solid #ccc;
    align-items: center;
}

.normalClass {
    width: 150rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
}

.selectClass {
    border-bottom: 1px solid red;
    color: red;
}

.list {
    width: 100%;
    height: 150rpx;
    border-bottom: 1px solid #ccc;
    display: flex;
    font-size: 24rpx;
    justify-content: space-between;
    align-items: center;
}

.listLeft {
    display: flex;
    flex-direction: column;
    margin-left: 15rpx;
    border-right: 1px solid #ccc;
    width: 83%;
}

.listLeft text {
    margin-bottom: 5rpx;
}

.listRight {
    display: flex;
    flex-direction: column;
    margin-right: 15rpx;
    text-align: right;
    padding-left: 6rpx;
}

.num {
    color: gray;
    font-size: 30rpx;
}