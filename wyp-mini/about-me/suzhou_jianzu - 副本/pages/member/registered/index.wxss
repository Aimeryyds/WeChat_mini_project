.container {
    font-size: 30rpx;
    background-color: #eee;
    padding-bottom: 25rpx;
}

image {
    width: 100%;
    height: 300rpx;
    margin-bottom: 20rpx;
}

.Pending {
    width: 100%;
    height: 130rpx;
    background-color: orange;
    font-size: 30rpx;
    color: #fff;
    margin-bottom: 20rpx;
    text-align: center;
    line-height: 130rpx;
}

.box {
    width: 99%;
    border-top: 1px solid #CCC;
    border-bottom: 1px solid #CCC;
    margin-bottom: 30rpx;
    margin-top: 30rpx;
}

.name {
    border-bottom: 1px solid #CCC;
    width: 95%;
    height: 100rpx;
    line-height: 100rpx;
    margin-left: 20rpx;
    padding-left: 10rpx;
}

.xing {
    color: red;
    margin-left: 10rpx;
    margin-right: 10rpx;
}

text.kong {
    background: red;
    margin-left: 10rpx;
    margin-right: 10rpx;
    width: 20rpx;
    display: inline-block;
}

input {
    display: inline-block;
    height: 100rpx;
}

input.address {
    margin-left: 20rpx;
}

text {
    float: left;
}

.goods1 {
    width: 95%;
    height: 100rpx;
    margin-left: 20rpx;
    padding-left: 10rpx;
    line-height: 100rpx;
}

.contact {
    font-size: 26rpx;
    text-align: center;
    color: #ccc;
    height: 30rpx;
    margin-left: 30rpx;
    margin-bottom: 30rpx;
}

.password {
    border-bottom: none;
}

.btn {
    background-color: #eee;
}

.btn button {
    background-color: orange;
    color: #fff;
    width: 80%;
    margin: 0 auto;
}

.business {
    border: none;
}

.bor {
    border: none;
}

checkbox {
    color: #ccc;
    float: left;
    margin-top: -10rpx;
}

.detector {
    margin-left: 30rpx;
    font-size: 28rpx;
    margin-top: -3rpx;
}

.word {
    font-size: 30rpx;
    font-family: Helvetica;
    display: block;
    width: 160rpx;
}

.word1 {
    line-height: 100rpx;
}

.shenqing {
    color: #191970;
    font-size: 30rpx;
    line-height: 90rpx;
}

view {
    background-color: #fff;
}

::-webkit-scrollbar {
    display: none;
}

.dn {
    display: none;
}

.db {
    display: block;
}

.dr {
    display: flex;
    flex-direction: row;
}

.wrap {
    width: 80%;
    height: 1000rpx;
    position: fixed;
    top: 100rpx;
    left: 10%;
    background-color: #fff;
    z-index: 5;
    border: 1rpx solid #e5e5e5;
}

.title {
    margin-top: 20rpx;
    width: 100%;
    text-align: center;
    line-height: 48rpx;
    font-size: 28rpx;
    font-weight: bolder;
}

.content {
    margin-top: 30rpx;
    width: 94%;
    padding: 0 3%;
    height: 820rpx;
    overflow-y: scroll;
    text-indent: 52rpx;
    font-size: 26rpx;
}

.sub {
    position: absolute;
    bottom: 20rpx;
    width: 50%;
    text-align: center;
    left: 25%;
    border-radius: 8rpx;
    font-size: 28rpx;
    line-height: 48rpx;
    background-color: orange;
    color: #fff;
    z-index: 9;
}