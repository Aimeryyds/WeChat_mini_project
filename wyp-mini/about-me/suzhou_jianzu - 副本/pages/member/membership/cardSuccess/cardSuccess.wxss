page {
    background: #eee;
}

.successCont {
    width: 100%;
    margin: 30rpx 0 40rpx 0;
    height: auto;
    line-height: 40rpx;
    font-size: 30rpx;
    text-indent: 40rpx;
}

.view-title {
    height: 100rpx;
    display: flex;
    flex-direction: row;
    padding-left: 40rpx;
    align-items: center;
    justify-content: space-between;
}

.view-title image {
    width: 56rpx;
    height: 56rpx;
}

.view-title text {
    font-size: 34rpx;
    margin-left: 26rpx;
    color: #9ebf6b;
}

.view-orderinfo {
    display: flex;
    flex-direction: column;
    padding: 30rpx;
    align-items: flex-start;
}

.view-orderinfo text {
    font-size: 30rpx;
    margin-top: 36rpx;
    color: #696969;
}

.view-orderinfo button {
    margin-top: 40rpx;
    width: 700rpx;
}

.weixin_app_payimg {
    width: 500rpx;
    height: 500rpx;
    margin: 60rpx auto;
}