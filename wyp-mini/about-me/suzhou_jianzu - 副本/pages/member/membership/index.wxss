page {
    background: #eee;
}

.card {
    margin: 20rpx 20rpx 20rpx 20rpx;
    height: 280rpx;
    position: relative;
}

.cardLogo {
    position: absolute;
    width: 150rpx;
    left: 30rpx;
    top: 30rpx;
    z-index: 10;
}

.cardLogo image {
    width: 100%;
}

.bg {
    position: absolute;
    width: 100%;
    height: 130rpx;
    top: 80rpx;
}

.vipCont {
    position: absolute;
    width: 100%;
    line-height: 130rpx;
    text-align: center;
    top: 80rpx;
    z-index: 2;
    margin: auto auto;
}

.vipcard {
    position: relative;
    width: 100%;
    height: 130rpx;
    color: #fff;
    font-size: 120rpx;
    font-weight: bolder;
}

.vipCont .vipcard .cardType {
    color: #fff;
    font-size: 28rpx;
}

.vipCont .vipcard .price {
    position: absolute;
    color: #fff;
    font-size: 50rpx;
    right: 6rpx;
    top: 0rpx;
    font-weight: bolder;
}

.activateCard {
    position: absolute;
    bottom: 13rpx;
    right: 37rpx;
    width: 115rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    color: #fff;
    background: #000;
    border-radius: 14rpx;
    font-size: 32rpx;
    border: 2rpx solid #ccc;
}

.cardTip {
    margin: 0 20rpx 40rpx 20rpx;
    width: 710rpx;
    height: auto;
    background: #F5F5F5;
    border-radius: 15rpx;
}

.cardTip text {
    padding: 20rpx 0;
    display: inline-block;
    line-height: 50rpx;
    color: #1e1e1e;
    font-size: 28rpx;
    margin-right: 15rpx;
    text-align: left;
    margin-left: 30rpx;
}

.vipCont .supremeblackColor {
    text-indent: -30rpx;
    color: #fff;
}

.vipCont .price.supremeblackColor {
    text-indent: -30rpx;
    color: #fff;
}

.vipCont .supremeblackColor .cardType {
    color: #fff;
}

.noCardCont {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: #fff;
}

.noImg {
    margin-top: 350rpx;
    width: 100%;
    text-align: center;
    height: auro;
}

.noImg image {
    width: 170px;
    height: 140px;
}

.noCont {
    width: 100%;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
}