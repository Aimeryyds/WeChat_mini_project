.careful {
    width: 100%;
    height: 20rpx;
    background: #EBEBEB;
}

.careful text.icon {
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
    background: red;
    margin: 0 30rpx 0 30rpx;
}

.careful text.cont {
    text-align: center;
    line-height: 82rpx;
    color: #666;
    font-size: 28rpx;
}

form {
    overflow: hidden;
}

.formSetion {
    background-color: #fff;
    padding: 0 30rpx;
}

.formList {
    display: flex;
    height: 90rpx;
    line-height: 90rpx;
}

.formList {
    border-bottom: 1px solid #eaeaea;
}

.formDateValue,.formKey,.formTimeValue,.formValue {
    font-size: 28rpx;
}

.formKey {
    flex: 1;
}

.formKey image {
    display: inline-block;
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}

.formValue {
    flex: 12;
}

.formDateValue,.formTimeValue {
    flex: 12;
}

.formmap {
    display: flex;
    justify-content: space-between;
}

.formmap input {
    flex: 6;
}

.mapicon {
    flex: 1;
    width: 60rpx;
    height: 50rpx;
    margin-top: 20rpx;
}

.mapicon image {
    width: 100%;
    height: 100%;
}

.formValue,.formValue input {
    height: 90rpx;
    line-height: 90rpx;
    color: #2d2d2d;
}

.formtype {
    position: relative;
}

.fomtype input {
    width: 100%;
}

.getVerification {
    color: #0ff;
    font-size: 22rpx;
}

button {
    margin-top: 30rpx;
    width: 95%;
    background: #1AAC19;
    color: #fff;
}