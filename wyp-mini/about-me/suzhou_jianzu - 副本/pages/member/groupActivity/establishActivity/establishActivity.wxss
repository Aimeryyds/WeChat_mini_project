page {
    font-family: '微软雅黑';
    background: #EBEBEB;
}

swiper {
    height: 450rpx;
    position: relative;
}

.swiperitem {
    text-align: center;
}

.titleSwiper {
    position: absolute;
    z-index: 10;
    top: 30rpx;
    left: 15rpx;
    width: 90rpx;
    height: 50rpx;
    line-height: 50rpx;
    background: #D95940;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    border-radius: 8rpx;
}

.readNum {
    position: absolute;
    z-index: 10;
    top: 30rpx;
    right: 15rpx;
    width: 160rpx;
    height: 50rpx;
    line-height: 50rpx;
    background: rgba(0,0,0,.7);
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    border-radius: 8rpx;
}

.readNum text {
    margin-left: 8rpx;
}

.contSwiper {
    position: absolute;
    z-index: 10;
    bottom: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    background: rgba(0,0,0,.3);
    color: #fff;
    font-size: 30rpx;
    border-radius: 8rpx;
    text-indent: 30rpx;
}

.participant {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
}

.participantLeft {
    float: left;
    width: 75%;
    height: 100%;
    font-size: 28rpx;
    text-indent: 30rpx;
}

.participantRight {
    float: left;
    width: 25%;
    height: 100%;
    font-size: 28rpx;
    color: #666;
    line-height: 80rpx;
}

.participantRight image {
    display: inline-block;
    margin-left: 20rpx;
    width: 20rpx;
    height: 30rpx;
    vertical-align: middle;
}

.readImage {
    background: #fff;
    width: 100%;
    height: auto;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.readImageLeft {
    width: 95%;
    height: 185rpx;
    margin: 0 auto;
    overflow: hidden;
}

.joinNum {
    margin-top: 20rpx;
    margin-left: 10rpx;
    display: inline-block;
    width: 110rpx;
    height: 150rpx;
    text-align: center;
}

.joinNumImage {
    width: 100%;
    height: 100rpx;
    text-align: center;
}

.joinNumImage image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    box-shadow: 0rpx 0rpx 10rpx #666;
}

.joinNum text {
    display: block;
    width: 100%;
    height: 60rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 28rpx;
    color: #000;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.readImageLeft .joinNum:nth-child(1) text {
    color: #6662AC;
}

.more {
    margin-top: 40rpx;
    width: 20%;
    height: 90rpx;
    line-height: 90rpx;
    border-left: 2rpx solid #ccc;
    float: left;
}

.more text {
    display: inline-block;
    margin: 0 30rpx 0 30rpx;
    color: #666;
    font-size: 28rpx;
}

.more image {
    width: 20rpx;
    height: 30rpx;
}

.allActivity1 {
    width: 100%;
    height: auto;
    font-size: 30rpx;
    background: #fff;
}

.allActivityImage {
    display: inline-block;
    width: 13%;
    height: 100%;
    line-height: 80rpx;
    text-align: center;
}

.allActivityImage image {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}

.allActivityImage image.typeImg {
    width: 35rpx;
    height: 35rpx;
}

.allActivityTime {
    margin: 0 auto;
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #666;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.allActivityTime text.hourMin {
    margin-left: 20rpx;
}

.allActivityTime .startTime {
    font-size: 26rpx;
    color: #ccc;
    float: right;
    margin-right: 30rpx;
}

.allActivityPosition {
    margin: 0 auto;
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #666;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.allActivityPosition text {
    display: inline-block;
    width: 43%;
}

.allActivityPosition text.locate {
    width: 70%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    display: inline-block;
    height: 100%;
    vertical-align: middle;
}

.allActivityPosition text.userNavigation {
    width: 8%;
    text-align: right;
    vertical-align: middle;
}

.userNavigation {
    font-size: 28rpx;
    line-height: 80rpx;
}

image.allActivityPositionImage {
    display: inline-table;
    margin-left: 20rpx;
    width: 20rpx;
    height: 30rpx;
    vertical-align: middle;
}

.allActivityUser {
    margin: 0 auto;
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #666;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.allActivityType {
    margin: 0 auto;
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #666;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.allActivityType text {
    display: inline-block;
}

.allActivityType text:nth-child(2) {
    color: #FBBE48;
}

.allActivityType text:nth-child(4) {
    color: #CC255A;
}

.allActivityType text:nth-child(6) {
    color: #30BFBC;
}

.allActivityType .pointer {
    font-size: 36rpx;
    width: 40rpx;
    vertical-align: 4rpx;
    text-align: center;
}

.allActivity2 {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    text-align: center;
    background: #fff;
    color: #666;
}

.allActivity2 {
    width: 100%;
    height: auto;
    background: #fff;
}

.release {
    width: 100%;
    height: 150rpx;
}

.releaseTime {
    width: 100%;
    height: 70rpx;
    line-height: 120rpx;
    font-size: 30rpx;
    text-align: center;
    color: #666;
}

.division {
    width: 100%;
    height: 160rpx;
}

.joinChannel {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    color: #fff;
    background: #58C3E5;
}

