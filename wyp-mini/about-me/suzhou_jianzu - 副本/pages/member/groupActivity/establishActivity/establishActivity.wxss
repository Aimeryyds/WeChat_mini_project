    top: 0;
    width: 100%;
    height: 100%;
    background-color: #000;
}

.Imagenum {
    position: absolute;
    top: 35rpx;
    left: 20rpx;
    width: 130rpx;
    height: 55rpx;
    line-height: 55rpx;
    text-align: center;
    background: #ccc;
    color: #000;
