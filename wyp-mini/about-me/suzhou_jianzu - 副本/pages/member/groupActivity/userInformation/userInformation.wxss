page {
    font-family: '微软雅黑';
    background: #EBEBEB;
}

.numAll {
    width: 100%;
    height: 80rpx;
    text-indent: 30rpx;
    color: #666;
    font-size: 30rpx;
    line-height: 80rpx;
    background: #EBEBEB;
}

.userList {
    margin: 0 auto;
    width: 95%;
    height: 100rpx;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
    background: #fff;
}

.userListLeft {
    float: left;
    width: 44%;
    height: 100rpx;
    line-height: 90rpx;
    overflow: hidden;
}

.userList image.avatar {
    width: 60rpx;
    height: 60rpx;
    vertical-align: middle;
    border-radius: 50%;
    margin-left: 10rpx;
    margin-right: 20rpx;
    box-shadow: 0rpx 0rpx 10rpx #666;
}

.userList text {
    display: inline-block;
    max-width: 190rpx;
    width: auto;
    height: 50rpx;
    line-height: 50rpx;
    vertical-align: middle;
    font-size: 30rpx;
    overflow: hidden;
}

.userList:nth-child(2) .userListLeft text {
    color: #6662AC;
}

.userList image.sex {
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
    border-radius: 50%;
}

.userListCenter {
    float: left;
    width: 29%;
    height: 100rpx;
    line-height: 110rpx;
    color: #666;
    font-size: 30rpx;
    text-align: center;
}

.userListRight {
    float: left;
    width: 24%;
    height: 100rpx;
    line-height: 110rpx;
    color: #666;
    font-size: 30rpx;
    text-align: right;
}

.numAllBottom {
    width: 100%;
    height: 90rpx;
    text-align: center;
    color: #666;
    font-size: 30rpx;
    line-height: 90rpx;
    background: #EBEBEB;
}