page {
    background: #f0f2f5;
}

.container {
    width: 100%;
    height: 100%;
}

.header {
    width: 100%;
    height: 80rpx;
    background-color: #f7f7f7;
    border-bottom: 1rpx solid #ccc;
    font-size: 34rpx;
    text-align: center;
    line-height: 80rpx;
}

.tarbar {
    display: flex;
    width: 750rpx;
    height: 80rpx;
    align-items: center;
    border-bottom: 1px solid #ccc;
    line-height: 80rpx;
    background: #fff;
}

.normal {
    width: 250rpx;
    height: 100%;
    text-align: center;
}

.select {
    border-bottom: 1px solid red;
    color: red;
}

.content {
    width: 100%;
    height: 100%;
}

.getCoupon {
    background-color: #fff;
    width: 100%;
    height: 100rpx;
    font-size: 30rpx;
    position: fixed;
    z-index: 100;
    bottom: 0;
    box-shadow: 5rpx 0rpx 15rpx #ccc;
    text-align: center;
    line-height: 100rpx;
}

.getCoupon text {
    text-align: center;
}

.getCoupon .icon {
    width: 40rpx;
    height: 40rpx;
    margin-left: 40rpx;
    margin-right: 20rpx;
}

.none {
    text-align: center;
    height: 1000rpx;
}

.status1 {
    width: 150rpx;
    height: 150rpx;
    margin: 100rpx auto 40rpx;
    display: block;
}

.warn {
    width: 100%;
    color: gray;
}

.warn text {
    text-align: center;
    font-size: 32rpx;
}

.coupon {
    width: 92%;
    margin-left: 4%;
    height: 170rpx;
    margin-top: 30rpx;
    background: url(http://pic1.win4000.com/wallpaper/f/56e7d508c0630.jpg);
    display: flex;
    font-size: 28rpx;
    align-items: center;
}

.myImg {
    width: 110rpx;
    height: 110rpx;
    margin-left: 15rpx;
    margin-right: 15rpx;
}

.left {
    display: flex;
    flex-direction: column;
    width: 360rpx;
    height: 120rpx;
    justify-content: space-around;
}

.leftBottom {
    font-size: 24rpx;
    color: #ee7021;
}

.leftBottom text {
    color: gray;
}

.leftTime {
    color: gray;
    font-size: 26rpx;
}

.right {
    display: flex;
    flex-direction: column;
    width: 150rpx;
    align-items: flex-end;
    color: #fff;
}

.money {
    margin-bottom: 10rpx;
    font-size: 35rpx;
    font-weight: 700;
}

.price {
    font-size: 48rpx;
}

.item_img {
    margin: 0 0 0 50rpx;
    width: 80rpx;
    height: 80rpx;
}

.item_nam {
    margin-left: 24rpx;
    display: flex;
    flex-direction: column;
    font-size: 26rpx;
}

.item_prc {
    width: 216rpx;
    background-color: #d83ef4;
    padding-top: 24rpx;
    display: flex;
    flex-direction: column;
    text-align: right;
    padding-right: 20rpx;
    position: relative;
    color: #fff;
}

.wavy_item {
    position: absolute;
    z-index: 5;
    top: -4rpx;
    left: 0;
    transform-origin: center left;
    transform: rotate(90deg);
    width: 128rpx;
    height: 8rpx;
    background-image: radial-gradient(circle,#fff,#fff 5rpx,#e5e5e5 6rpx,#e5e5e5 7rpx,#fff 8rpx,#fff);
    background-size: 16rpx 16rpx;
}

.wavy_left {
    position: absolute;
    z-index: 5;
    top: -4rpx;
    left: 0;
    transform-origin: center left;
    transform: rotate(90deg);
    width: 128rpx;
    height: 8rpx;
    background-image: radial-gradient(circle,#fff,#fff 5rpx,#d83ef4 6rpx,#d83ef4 8rpx,#d83ef4 8rpx,#d83ef4);
    background-size: 16rpx 16rpx;
}

.wavy_right {
    position: absolute;
    z-index: 5;
    top: -4rpx;
    right: 0;
    transform-origin: center right;
    transform: rotate(-90deg);
    width: 128rpx;
    height: 8rpx;
    background-image: radial-gradient(circle,#fff,#fff 5rpx,#d83ef4 6rpx,#d83ef4 8rpx,#d83ef4 8rpx,#d83ef4);
    background-size: 16rpx 16rpx;
}

.container view:nth-child(2) .couponItem:last-child {
    margin-bottom: 150rpx;
}

.couponItem {
    width: 95%;
    margin: 40rpx 20rpx;
    display: flex;
    flex-direction: row;
    height: 170rpx;
    background: #fff;
}

.couponItem .couponLeft {
    width: 40%;
    height: 100%;
    background: linear-gradient(left top,#5f90cf,#627cd3);
    position: relative;
    text-align: center;
}

.myCouponLeft {
    position: absolute;
    z-index: 5;
    top: -3rpx;
    left: 0;
    transform-origin: center left;
    transform: rotate(90deg);
    width: 168rpx;
    height: 8rpx;
    background-image: radial-gradient(circle,#fff,#fff 5rpx,#5f90cf 6rpx,#5f90cf 8rpx,#5f90cf 8rpx,#5f90cf);
    background-size: 16rpx 16rpx;
    background-repeat: repeat-x;
}

.couponText {
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    padding-top: 40rpx;
}

.couponText text {
    font-size: 40rpx;
}

.couponLeft text:nth-child(3) {
    text-align: center;
    color: #fff;
    font-size: 28rpx;
}

.couponRight {
    width: calc(100% - 40%);
    display: flex;
    flex-direction: row;
    align-items: center;
    position: relative;
}