page {
    background: #eee;
}

.order-item0 {
    margin-bottom: 10px;
    background: #fff;
    background-image: url(http://tws.cnweisou.com/images/coupon1.png);
    background-repeat: round;
}

.order-item1 {
    margin-bottom: 10px;
    background: #fff;
    background-image: url(http://tws.cnweisou.com/images/coupon2.png);
    background-repeat: round;
}

.order-item2 {
    margin-bottom: 10px;
    background: #fff;
    background-image: url(http://tws.cnweisou.com/images/coupon3.png);
    background-repeat: round;
}

.order-item:last-child {
    margin-bottom: 0;
}

.order-footer {
    padding: 3px 0;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    margin-right: 20rpx;
}

.order-down {
    padding: 3px 0;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    margin-left: 30rpx;
    height: 70rpx;
    align-items: center;
    border-bottom: 1px solid #eee;
}

.order-down text {
    font-size: 28rpx;
    margin-top: 12rpx;
    margin-bottom: 10rpx;
}

.order-footer text {
    font-size: 28rpx;
    margin-top: 12rpx;
    margin-bottom: 10rpx;
}

.carts-item {
    display: flex;
    flex-direction: row;
    height: 220rpx;
    width: 720rpx;
    border-bottom: 1px solid #eee;
    padding: 20rpx 0 10rpx 30rpx;
}

.item-left {
    width: 400rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.item-right {
    width: 320rpx;
    margin-left: 26rpx;
    height: 220rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
}

.text-value {
    font-size: 110rpx;
    color: #def0d0;
}

.text-date {
    color: #69b034;
    margin-top: 30rpx;
    font-size: 20rpx;
}

.text-hint {
    color: #def0d0;
    margin-left: 20rpx;
    margin-top: 10rpx;
    font-size: 36rpx;
}

.view-btn0 {
    width: 260rpx;
    height: 80rpx;
    border-radius: 40rpx;
    background: #69b034;
    display: flex;
    align-items: center;
    margin-top: 25rpx;
    justify-content: center;
}

.view-btn1 {
    width: 260rpx;
    height: 80rpx;
    border-radius: 40rpx;
    background: #5ba0f1;
    display: flex;
    align-items: center;
    margin-top: 25rpx;
    justify-content: center;
}

.view-btn2 {
    width: 260rpx;
    height: 80rpx;
    border-radius: 40rpx;
    background: #69b034;
    display: flex;
    align-items: center;
    margin-top: 25rpx;
    justify-content: center;
}

.text-btn {
    color: #def0d0;
    font-size: 28rpx;
}

.carts-image {
    width: 180rpx;
    height: 210rpx;
}

.carts-text {
    margin-left: 10rpx;
    width: 510rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.btn-pay {
    margin-top: 10rpx;
    margin-bottom: 10rpx;
    height: 50rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    font-size: 27rpx;
    line-height: 50rpx;
    color: #fff;
    background: orange;
    align-self: flex-end;
    border-radius: 6rpx;
}

.carts-title {
    font-size: 28rpx;
    color: #444;
    line-height: 38rpx;
}

.carts-subtitle {
    margin-top: 20rpx;
    font-size: 32rpx;
    color: red;
    padding: 0 0rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.carts-price {
    color: #383838;
    font-size: 28rpx;
    line-height: 22px;
}

.carts-num {
    color: #383838;
    font-size: 28rpx;
    line-height: 22px;
}

.carts-right {
    width: 80rpx;
    display: flex;
    height: 120rpx;
    justify-content: space-around;
    flex-direction: column;
    align-items: flex-end;
    margin-right: 20rpx;
}

.view-title {
    height: 68rpx;
    display: flex;
    flex-direction: row;
    align-items: flex-end;
}

.view-title text {
    font-size: 28rpx;
    color: gray;
    margin-left: 30rpx;
}

.class-tab {
    display: flex;
    flex-direction: row;
    margin-top: 0rpx;
    justify-content: center;
    background: #fff;
    width: 750rpx;
    border-bottom: 3px solid #eee;
}

.text-select {
    width: 200rpx;
    font-size: 30rpx;
    color: red;
    line-height: 60rpx;
    height: 60rpx;
    border-bottom-width: 6rpx;
    border-color: red;
    border-style: solid;
    text-align: center;
    border-top-width: 0;
    border-left-width: 0;
    border-right-width: 0;
}

.text-normal {
    width: 200rpx;
    font-size: 30rpx;
    color: #4D4D4D;
    height: 60rpx;
    line-height: 60rpx;
    border-bottom-width: 0;
    border-color: red;
    border-style: solid;
    text-align: center;
    border-top-width: 0;
    border-left-width: 0;
    border-right-width: 0;
}

.stars {
    margin-left: 20rpx;
    width: 135rpx;
    height: 25rpx;
}