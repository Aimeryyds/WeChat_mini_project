page {
    background: #f9f9f9;
}

.couponNum {
    width: 100%;
    height: 78rpx;
    font-size: 28rpx;
    border-bottom: 1px solid #ccc;
    overflow: scroll-x;
    white-space: nowrap;
}

.newTitle {
    height: 76rpx;
    line-height: 78rpx;
    display: inline-block;
    width: 50%;
    text-align: center;
    color: #000;
    font-size: 30rpx;
    background: #fff;
}

.active {
    border-bottom: 2px solid #24B2F2;
    color: #24B2F2;
}

.recharge {
    width: 100%;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    border-bottom: 1px solid rgba(0,0,0,.1);
    font-size: 30rpx;
}

.signCard {
    margin-top: 30rpx;
    width: 100%;
    height: 150rpx;
    background: #fff;
    border-bottom: 1px solid rgba(0,0,0,.1);
}

.icon {
    width: 15%;
    height: 100%;
    float: left;
    line-height: 150rpx;
    text-align: center;
}

.icon image {
    width: 65rpx;
    height: 55rpx;
    vertical-align: middle;
}

.left {
    width: 47%;
    height: 100%;
    float: left;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    line-height: 150rpx;
}

.left .cardRecord {
    width: 100%;
    height: 100%;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    color: #666;
    font-size: 26rpx;
    vertical-align: middle;
}

.left .nowTime {
    display: block;
    width: 100%;
    color: #fff;
    font-size: 28rpx;
}

.left .nowTime text {
    width: 80rpx;
    height: 30rpx;
    background: #24b2f2;
    padding: 5rpx 10rpx;
    margin-left: 10rpx;
    margin-right: 10rpx;
    border-radius: 10rpx;
}

.right {
    width: 38%;
    height: 100%;
    float: left;
    line-height: 150rpx;
    text-align: center;
    color: #24b2f2;
    font-size: 47rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.noConsumption {
    width: 100%;
    height: 600rpx;
    margin-top: 100rpx;
}

.noConsumption image {
    display: block;
    width: 395rpx;
    margin: 0 auto;
}

.noConsumption text {
    display: block;
    width: 100%;
    margin: 0 auto;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
}