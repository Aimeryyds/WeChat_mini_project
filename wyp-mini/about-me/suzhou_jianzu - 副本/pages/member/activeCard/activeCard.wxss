page {
    background: #fff;
}

.card {
    margin: 20rpx 20rpx 20rpx 20rpx;
    height: 280rpx;
    position: relative;
}

.cardLogo {
    position: absolute;
    width: 150rpx;
    left: 30rpx;
    top: 30rpx;
    z-index: 10;
}

.cardLogo image {
    width: 100%;
}

.bg {
    position: absolute;
    width: 100%;
    height: 120rpx;
    top: 80rpx;
}

.vipCont {
    position: absolute;
    width: 100%;
    line-height: 120rpx;
    text-align: center;
    top: 80rpx;
    z-index: 2;
    margin: auto auto;
}

.vipcard {
    position: relative;
    width: 100%;
    height: 120rpx;
    color: #1e1e1e;
    font-size: 120rpx;
    font-weight: bolder;
}

.nocard {
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    color: #fff;
    font-size: 40rpx;
    text-align: center;
}

.vipCont .vipcard .cardType {
    color: #1e1e1e;
    font-size: 32rpx;
}

.numCard {
    position: absolute;
    bottom: 13rpx;
    right: 0rpx;
    width: 100%;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    color: #f6f6f6;
    font-size: 40rpx;
}

.memberInfo {
    margin: 20rpx 20rpx 20rpx 20rpx;
    width: 710rpx;
    height: auto;
}

.memberInfo .infoTitle {
    color: rgba(0,0,0,.6);
    font-size: 34rpx;
    border-bottom: 1px solid rgba(0,0,0,.1);
    height: 70rpx;
    line-height: 70rpx;
    width: 100%;
}

.integralSum {
    color: #1e1e1e;
    font-size: 30rpx;
    border-bottom: 1px solid rgba(0,0,0,.1);
    height: 70rpx;
    line-height: 70rpx;
    width: 100%;
}

.integralSum image {
    vertical-align: -5rpx;
    width: 30rpx;
    height: 40rpx;
    margin-right: 30rpx;
}

.integralSum .image1 {
    width: 35rpx;
    height: 30rpx;
}

.itemTitle {
    display: inline-block;
    width: 45%;
    height: 70rpx;
    line-height: 70rpx;
    color: #1e1e1e;
    font-size: 30rpx;
}

.itemNum {
    display: inline-block;
    width: 45%;
    text-align: right;
    color: #1e1e1e;
    font-size: 30rpx;
}

.integralSum3 {
    color: #1e1e1e;
    font-size: 30rpx;
    height: 200rpx;
    width: 100%;
}

.integralSum3 .minusImg {
    float: left;
    height: 200rpx;
    line-height: 200rpx;
    width: 20%;
    text-align: center;
}

.integralSum3 image {
    vertical-align: middle;
    width: 60rpx;
    height: 70rpx;
    margin-right: 30rpx;
}

.integralSum3 .minus {
    float: left;
    height: 200rpx;
    width: 80%;
}

.integralSum3 .minusTitle {
    color: #1e1e1e;
    font-size: 30rpx;
    height: 60rpx;
    line-height: 90rpx;
    margin: 20rpx 0 30rpx 0;
}

.minusTip {
    color: rgba(0,0,0,.6);
    font-size: 30rpx;
}

.separate {
    width: 100%;
    height: 20rpx;
    background: #F6F6F6;
}

.cardupdate {
    color: #1e1e1e;
    font-size: 30rpx;
    border-bottom: 1px solid rgba(0,0,0,.1);
    height: 90rpx;
    width: 100%;
}

.cardupdate image {
    vertical-align: middle;
    width: 40rpx;
    height: 40rpx;
    margin-right: 30rpx;
}

.cardupdate .itemTitle1 {
    display: inline-block;
    width: 44%;
    height: 70rpx;
    line-height: 70rpx;
    color: #1e1e1e;
    font-size: 30rpx;
}

.cardupdate .arrowright1 {
    display: inline-block;
    width: 44%;
    height: 70rpx;
    line-height: 70rpx;
    text-align: right;
    color: #1e1e1e;
    font-size: 40rpx;
}

.cardupdate .image5 {
    width: 40rpx;
    height: 40rpx;
}

.cardupdate .image6 {
    width: 45rpx;
    height: 45rpx;
}