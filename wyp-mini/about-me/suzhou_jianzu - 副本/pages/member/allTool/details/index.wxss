.text-save {
    width: 680rpx;
    align-self: center;
    font-size: 32rpx;
    margin-top: 30rpx;
    color: #fff;
    text-align: center;
    height: 70rpx;
    border-radius: 9rpx;
    background: orange;
    line-height: 70rpx;
}

.parent {
    display: flex;
    flex-direction: column;
    width: 100%;
}

.Order {
    display: flex;
    flex-direction: row-reverse;
}

.Order view {
    width: 200rpx;
    height: 60rpx;
