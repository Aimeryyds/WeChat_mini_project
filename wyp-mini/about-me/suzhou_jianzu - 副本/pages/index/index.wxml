<template name="new4">
    <view class="container">
        <view class="top" style="height:{{date.topHeight}}px;line-height:{{date.topHeight}}px;">
            <view class="cover {{isClick==true?'showcover':''}}">
                <scroll-view scrollY bindtap="clickContAgain" class="coverscro">
                    <text class="titleCover">
                        {{date.test.shopinfo.name}}
                    </text>
                    <text class="contCover">
                        {{date.test.shopinfo.description}}
                    </text>
                </scroll-view>
            </view>
            <image class="brand" src="{{date.test.shopinfo.logo}}"></image>
            <view bindtap="clickCont" class="info">
                <view class="title1">
                    <text>{{date.test.shopinfo.name}}</text>
                </view>
                <view class="content">
                    <text>{{date.test.shopinfo.description}}</text>
                </view>
            </view>
            <view bindtap="click_phone" class="phoneX">
                <image src="https://tws.cnweisou.com/images/Phone3.png"></image>
            </view>
        </view>
        <view class="body" style="height:{{date.swiperHeight}}px;padding-top:{{date.topHeight}}px;">
            <scroll-view scrollY style="height:{{date.swiperHeight}}px;">
                <view class="titles" style="height:{{date.swiperHeight+300}}px;">
                    <view bindtap="show" class="title {{selectedTitle==index?'active':''}}" id="{{index}}" wx:if="{{date.test.category}}" wx:for="{{date.test.category}}">
                        <view>
                            <image src="{{item.advimg}}" style="width:42rpx;height:42rpx;border-radius:50%;margin:0px 10rpx;"></image>
                        </view>
                        <text>{{item.name}}</text>
                    </view>
                </view>
            </scroll-view>
            <scroll-view scrollY style="height:{{date.swiperHeight}}px;">
                <view style="height:{{date.swiperHeight+300}}px;">
                    <block wx:if="{{date.arr}}">
                        <view class="right" wx:for="{{date.arr[selectedTitle]}}">
                            <image class="repast" src="{{item.thumb}}"></image>
                            <view class="descirbe" style="width:280rpx;height:180rpx;">
                                <view class="describe_one">
                                    <text class="con_title">{{item.title}}</text>
                                </view>
                                <view class="describe_two">
                                    <text class="amount">已售{{item.sales}}</text>
                                </view>
                                <view class="describe_three">
                                    <text class="price">￥{{item.marketprice}}</text>
                                </view>
                            </view>
                            <view class="shopping">
                                <view bindtap="add" class="circle add" data-id="{{item.id}}" data-index="{{index}}">+</view>
                                <view class="selectnum" wx:if="{{item.mytotal&&item.mytotal>0}}">
                                    <text class="selectNumber">{{item.mytotal}}</text>
                                </view>
                                <view bindtap="reduce" class="circle delete circle1" data-index="{{index}}" style="color:gray" wx:if="{{item.mytotal&&item.mytotal>0}}">-</view>
                            </view>
                        </view>
                    </block>
                </view>
            </scroll-view>
        </view>
        <view bindtap="getCarts" class="shopCar">
            <view class="circleNum  shopCartI" wx:if="{{Allnum}}">{{Allnum}}</view>
            <text wx:if="{{Allnum}}">￥{{Allprice}}</text>
            <image src="https://tws.cnweisou.com/images/cart1.png"></image>
        </view>
    </view>
    <view class="orderForm dn {{showView?'db':''}} " style="z-index:100000;bottom:-30rpx;">
        <view catchtap="topShow" class="shadow "></view>
        <view class="formBox">
            <view class="orderTop">
                <image src="https://tws.cnweisou.com/images/delete.png"></image>
                <text bindtap="deleteCart">清空购物车</text>
            </view>
            <scroll-view scrollY style="height:380rpx;">
                <view class="list" wx:for="{{selectGoods}}">
                    <text class="listText">{{item.title}}</text>
                    <text class="listPrice">￥{{item.marketprice}}</text>
                    <view class="shopping listShopping">
                        <view class="box">
                            <view bindtap="allAdd" class="circle add" data-index="{{index}}">+</view>
                        </view>
                        <view class="numBox">
                            <text class="shopNumber">{{item.mytotal}}</text>
                        </view>
                        <view bindtap="allDelete" class="circle delete circle1 " data-index="{{index}}" style="color:gray">-</view>
                    </view>
                </view>
            </scroll-view>
            <view class="listBottom">
                <text class="totalPrice">￥{{Allprice}}</text>
                <view bindtap="bindCheckout" class="btn">去下单</view>
            </view>
        </view>
    </view>
    <view class="bgtypeList" wx:if="{{isSecond}}">
        <scroll-view scrollY class="cardAgainType">
            <view class="title">
                <view class="left">{{goods_name}}</view>
                <image bindtap="closeSecond" src="https://tws.cnweisou.com/images/canyinError.png"></image>
            </view>
            <view class="specifications" data-myindex="{{index}}" wx:for="{{goods_spec_list}}">
                <view class="specificationsTitle">{{item[index].spec_name}}：</view>
                <view bindtap="selectSpecif" class="specificationsCont" data-index="{{index}}" data-spec_name="{{item.spec_name}}" wx:for="{{item}}">
                    <text class="{{item.isClick==1?'selected':''}}">{{item.item}}</text>
                </view>
            </view>
            <view class="determineAgin">
                <text class="money">￥{{nowPrice}}</text>
                <text bindtap="selectOk" class="ok">选好了</text>
            </view>
        </scroll-view>
    </view>
</template>
<template name="map">
    <view class="mapAssembly">
        <map showLocation bindcontroltap="controltap" bindmarkertap="markertap" controls="{{controls}}" id="map" latitude="{{data.style.latitude}}" longitude="{{data.style.longitude}}" markers="{{markers}}" scale="14" style="width: 100%; height: 300px;"></map>
    </view>
</template>
<template name="audioView">
