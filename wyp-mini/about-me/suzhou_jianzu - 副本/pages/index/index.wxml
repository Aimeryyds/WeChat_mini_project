<template name="wxapp_merchgroup">
    <block wx:if="{{data.params.openlocation!='1'&&!store&&!store_num}}">
        <view style="line-height:80rpx;border-bottom: 1px solid {{data.style.titlecolor}};font-size:28rpx;color:{{data.style.titlecolor}};padding-left:20rpx;background:{{data.style.background}};">推荐商家</view>
        <view bindtap="storebtn" class="article_wrap" id="{{item.merchid}}" style="background:{{data.style.background}};" wx:for="{{data.data}}">
            <image class="article_img" lazyLoad="true" src="{{item.thumb}}"></image>
            <view class="article_title">
                <view class="article_subtitle" style="color:{{data.style.titlecolor}}">{{item.name}}</view>
                <text class="article_subdesc" style="color:{{data.style.desccolor}}">{{item.desc}}</text>
            </view>
        </view>
    </block>
    <block wx:else>
        <view style="background:red;" wx:if="{{store}}">
            <view bindtap="to_store" class="store_tp" style="background:{{data.style.background}};color:{{data.style.titlecolor}};">
                    店铺街
                    <image class="store_bd" lazyLoad="true" src="https://tws.cnweisou.com/images/right.png"></image>
            </view>
            <scroll-view class="list_store" scrollX="true" style="background:{{data.style.background}};" wx:if="{{store_num>0}}">
                <block wx:for="{{store.list}}">
                    <view bindtap="storebtn" class="item" id="{{item.id}}" style="margin-right: 0;padding-right: 0;" wx:if="{{store_num==index+1}}">
                        <image lazyLoad="true" src="{{item.logo}}" style="width: 100%;height: 100%;"></image>
                    </view>
                    <view bindtap="storebtn" class="item" id="{{item.id}}" wx:elif="{{store_num!=index+1}}">
                        <image lazyLoad="true" src="{{item.logo}}" style="width: 100%;height: 100%;"></image>
                        <view class="item_line"></view>
                    </view>
                </block>
            </scroll-view>
            <view style="text-align: center;font-size: 28rpx;height: 44rpx;line-height: 28rpx;color: #d5d5d5;background-color: #fff" wx:else>
                        该城市暂无入住的品牌，换个城市试试
                    </view>
        </view>
    </block>
</template>
