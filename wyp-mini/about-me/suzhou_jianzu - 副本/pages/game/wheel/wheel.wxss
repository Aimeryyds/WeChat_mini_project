page {
    background: #fff;
    width: 100%;
    height: 100%;
}

.wheelBg {
    width: 100%;
    height: 100%;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 100% 100%;
    overflow: hidden;
    margin: 0 auto;
}

.wheelTop {
    width: 100%;
    height: 140rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
    margin-bottom: 50rpx;
    color: #fff;
}

.wheelTop image {
    display: inline-block;
    width: 80rpx;
    height: 80rpx;
    border-radius: 18rpx;
    vertical-align: middle;
    margin-right: 40rpx;
}

.wheelTop text {
    padding: 0 10rpx;
    display: inline-block;
    height: 50rpx;
    line-height: 50rpx;
    background: #E44245;
    border-radius: 8rpx;
    vertical-align: middle;
}

.countPrize {
    width: 45%;
    height: 65rpx;
    line-height: 65rpx;
    margin: 25rpx auto 0;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
    background: #B90329;
    border-radius: 10rpx;
}

.countPrize text {
    padding: 0 10rpx;
    color: #FFF9A0;
    font-size: 43rpx;
}

.header {
    text-align: center;
}

.header-title {
    display: block;
    padding: .4rem 0;
    font-size: 1rem;
}

.header-subtitle {
    color: #999;
}

.canvas-container ul,.canvas-container li {
    margin: 0;
    padding: 0;
    list-style: none;
}

.canvas-container {
    margin: 0 auto;
    position: relative;
    width: 310px;
    height: 320px;
    border-radius: 50%;
    border: 2px solid #E44025;
    box-shadow: 0 2px 3px #333,0 0 2px #000;
    background: #F35855;
}

.canvas-content {
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0rpx;
    right: 0rpx;
    margin: auto auto;
    z-index: 1;
    display: block;
    width: 300px;
    height: 300px;
    border-radius: inherit;
    background-clip: padding-box;
    background-color: #8CD29F;
    overflow: hidden;
}

.canvas-contentbg {
    position: absolute;
    width: 100%;
    height: 100%;
    box-sizing: border-box;
}

.arc {
    position: absolute;
    transform-origin: left top;
    left: 300rpx;
    top: 300rpx;
    width: 300rpx;
    height: 300rpx;
    border: 1px solid white;
    box-sizing: border-box;
    z-index: 1;
}

.canvas-container-circle {
    position: absolute;
    display: block;
    border-radius: 50%;
    height: 20rpx;
    width: 20rpx;
}

.canvas-element {
    width: inherit;
    height: inherit;
    border-radius: 50%;
}

.canvas-list {
    position: absolute;
    left: 0;
    top: 0;
    width: inherit;
    height: inherit;
    z-index: 2;
}

.canvas-item {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    color: #e4370e;
    font-size: 28rpx;
    text-shadow: 0 1px 1px rgba(255,255,255,.6);
    overflow: hidden;
}

.canvas-item-text {
    position: relative;
    display: block;
    padding-top: 20px;
    margin: 0 auto;
    text-align: center;
    transform-origin: 50% 150px;
    overflow: hidden;
}

.canvas-item-text text {
    display: block;
}

.canvas-item image {
    width: 100rpx;
    max-height: 70rpx;
    margin-top: 10rpx;
}

.canvas-btn {
    position: absolute;
    top: 0rpx;
    bottom: 0;
    left: 0rpx;
    right: 0;
    margin: auto auto;
    z-index: 3;
    width: 170rpx;
    height: 170rpx;
    border-radius: 50%;
    text-decoration: none;
    background: url(https://tws.cnweisou.com/images/wheelBtn.png) no-repeat center center;
    background-size: contain;
}

.canvas-btn.disabled {
    pointer-events: none;
    background: url(https://tws.cnweisou.com/images/wheelBtnGray.png) no-repeat center center;
    background-size: contain;
}

.canvas-btn.disabled::after {
    border-bottom-color: #B07A7B;
}

.canvas-btn .arrow {
    position: absolute;
    display: block;
    width: 0;
    height: 0;
    border-left: 30rpx solid transparent;
    border-right: 30rpx solid transparent;
    border-bottom: 70rpx solid #E44025;
    top: -54rpx;
    left: 35rpx;
}

.canvas-btn .arrow.disabled {
    pointer-events: none;
    border-bottom: 70rpx solid #B07A7B;
    color: #ccc;
}

.canvas-btn .arrow.disabled::after {
}

.gb-run {
    transition: all 6s ease;
}

.main-container {
    margin: 2rem 2rem;
}

.main-container-rule {
    padding: 1rem 0;
}

.main-rule-title {
    display: block;
    padding: 4px 0;
    font-size: 16px;
    font-weight: bold;
}

.main-rule-item {
    display: block;
    padding: 2px 0;
}

button {
    background: #ff6600;
    color: #fff;
}

.rule {
    margin: 30rpx auto 0;
    width: 85%;
    height: 290rpx;
    border: 2rpx solid #FEED9B;
    border-top: none;
    position: relative;
}

.ruleTop {
    position: absolute;
    top: -10px;
    left: 38%;
    width: 200rpx;
    height: 60rpx;
}

.ruleCont {
    padding-top: 50rpx;
    margin: 0 auto;
    width: 95%;
    height: 240rpx;
}

.wxParse-p {
    color: #fff;
    font-size: 28rpx;
}

.ruleCont text {
    font-size: 28rpx;
    color: #fff;
}

.bgtypeList {
    position: fixed;
    z-index: 1000;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.8);
    background-repeat: no-repeat;
    background-size: contain;
    text-align: center;
}

.bgtypeList .prizeCont {
    position: absolute;
    width: 57%;
    height: 530rpx;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    font-size: 32rpx;
    color: orange;
    background: #C76365;
    border-radius: 10rpx;
    overflow: hidden;
}

.prizeCont text.close {
    display: block;
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    border-radius: 50%;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.prizeCont .pirzeIcon {
    width: 65%;
    overflow: hidden;
    text-align: center;
    background: #fff;
    margin: 90rpx auto 0;
    height: 250rpx;
    line-height: 250rpx;
    border-radius: 10rpx;
}

.prizeCont .pirzeIcon image {
    display: inline-block;
    vertical-align: middle;
    width: 75%;
}

.prizeDetail {
    width: 400rpx;
    height: 70rpx;
    font-size: 32rpx;
    color: #fff;
    padding: 0 10rpx;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin: 20rpx auto 0;
    line-height: 70rpx;
    font-weight: bolder;
}

.bgtypeList .prizeOk {
    display: block;
    margin-top: 20rpx;
    width: 100rpx;
    height: 60rpx;
    line-height: 60rpx;
    margin: 20rpx auto;
    font-size: 28rpx;
    color: #fff;
    border-radius: 8rpx;
    background: #FEBE36;
    padding: 0 10rpx;
    text-align: center;
}