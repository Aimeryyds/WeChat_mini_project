.interval {
    width: 100%;
    height: 40rpx;
    border-top: 1rpx solid rgba(0,0,0,.1);
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.money {
    position: relative;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    color: #666;
}

.money .imageD {
    display: inline-block;
    width: 86rpx;
    height: 100%;
    line-height: 100rpx;
    text-align: center;
}

.money image {
    width: 40rpx;
    height: 40rpx;
    vertical-align: middle;
}

.read {
    display: inline-block;
    width: 40%;
}

.readNum {
    text-indent: 0rpx;
    display: inline-block;
    vertical-align: middle;
    border-radius: 10rpx;
    width: auto;
    height: 60rpx;
    padding: 0 10rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: #9bd3d2;
}

.interval text {
    color: #9BD3D2;
    font-size: 32rpx;
}

.listMode1 {
    background: #fff;
    width: 95%;
    height: auto;
    line-height: 100rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.listMode1 .titleR {
    width: 170rpx;
    height: 70rpx;
    line-height: 70rpx;
    vertical-align: middle;
    text-align: center;
    font-size: 30rpx;
}

.listMode1 .formValueR {
    width: 90%;
    height: auto;
    line-height: 30rpx;
    font-size: 30rpx;
    padding-bottom: 20rpx;
    margin-left: 30rpx;
    text-indent: 30rpx;
}

.clear {
    clear: both;
}

.listMode {
    background: #fff;
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.contDetail {
    border: none;
}

.listMode .title {
    float: left;
    width: 170rpx;
    height: 100%;
    line-height: 80rpx;
    font-size: 30rpx;
}

.listMode .imageD {
    display: inline-block;
    width: 50rpx;
    height: 100%;
    line-height: 80rpx;
    text-align: center;
}

.listMode image {
    width: 45rpx;
    height: 45rpx;
    vertical-align: middle;
}

image.type {
    width: 40rpx;
    height: 40rpx;
}

image.info {
    width: 50rpx;
    height: 50rpx;
}

image.name {
    width: 35rpx;
    height: 35rpx;
}

.listMode .formValue {
    float: left;
    width: 64%;
    height: 100%;
    line-height: 80rpx;
    font-size: 30rpx;
}

.listMode .formValue text {
    margin-left: 30rpx;
}

.listMode .mapicon {
    float: left;
    line-height: 80rpx;
    width: 40rpx;
    height: 80rpx;
    text-align: center;
}

.listMode .mapicon image {
    display: inline-block;
    vertical-align: middle;
    width: 30rpx;
    height: 30rpx;
}

.inputD {
    display: none;
    margin: 0 auto;
    width: 95%;
    height: 70rpx;
    line-height: 70rpx;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.inputD input {
    margin: 0 auto;
    width: 90%;
    height: 70rpx;
    line-height: 70rpx;
    border: none;
    font-size: 30rpx;
}

.areaD {
    display: none;
    margin: 0 auto;
    width: 95%;
    height: 100rpx;
    padding: 20rpx 0 30rpx 0;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
}

.areaD textarea {
    margin: 0 auto;
    width: 95%;
    height: 100rpx;
}

.fname {
    display: block;
}

.contDetail {
    position: relative;
}

.cont {
    margin: 30rpx auto 0;
    width: 95%;
    height: auto;
    text-indent: 30rpx;
    line-height: 45rpx;
    padding-bottom: 20rpx;
    border: 1rpx solid rgba(0,0,0,.1);
    border-top: none;
    font-size: 30rpx;
    border-radius: 10rpx;
}

.clickphone {
    display: inline-block;
    vertical-align: middle;
    border-radius: 10rpx;
    width: auto;
    height: 60rpx;
    padding: 0 10rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: #9bd3d2;
}

.bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 80rpx;
    text-align: center;
}

.clickEnd {
    display: inline-block;
    width: 40%;
    height: 80rpx;
    padding: 0 10rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: #9bd3d2;
    border-radius: 10rpx;
    margin-right: 10rpx;
}

.clickmodify {
    margin-left: 10rpx;
    display: inline-block;
    width: 40%;
    height: 80rpx;
    padding: 0 10rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    background: #9bd3d2;
    border-radius: 10rpx;
}

.bgtypeList {
    position: fixed;
    z-index: 3;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.cardAgainType {
    padding: 10rpx 20rpx 15rpx 20rpx;
    border-radius: 15rpx;
    width: 455rpx;
    height: 192rpx;
    line-height: 103rpx;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #d2d3d5;
    margin: 0 auto;
    margin-top: 250rpx;
    color: #666;
    font-size: 30rpx;
}

.determineAgin {
    margin: 0 auto;
    width: 200rpx;
    line-height: 62rpx;
    height: 70rpx;
    border-top: 1px solid #ccc;
    background: #9BD3D2;
    border-radius: 10rpx;
    color: #fff;
    font-size: 30rpx;
}

.bgtypeList1 {
    position: fixed;
    z-index: 3;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.typebg {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto auto;
    width: 435rpx;
    height: 80%;
}

.typeList {
    width: 100%;
    height: 75rpx;
    line-height: 83rpx;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #9BD3D2;
}

.typeListDetail {
    display: block;
    height: 75rpx;
    line-height: 103rpx;
    padding-left: 140rpx;
}

.yearAndMonth {
    color: #666;
    font-size: 30rpx;
}