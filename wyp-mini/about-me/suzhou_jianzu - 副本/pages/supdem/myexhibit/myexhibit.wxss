.select {
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    margin: 20rpx auto;
    position: relative;
}

.selectLeft {
    float: left;
    width: 19%;
    height: 70rpx;
    line-height: 70rpx;
}

.selectLeft image {
    width: 35rpx;
    height: 35rpx;
    margin-left: 20rpx;
    vertical-align: middle;
}

.selectLeft text {
    display: inline-block;
    width: 175rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
    color: #666;
    margin-left: 20rpx;
    text-align: center;
    background: #eee;
    border-radius: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: bottom;
}

.selectRight {
    float: left;
    width: 71%;
    background: #eee;
    height: 70rpx;
    line-height: 70rpx;
    margin-left: 10rpx;
    position: relative;
}

.selectRight image {
    width: 35rpx;
    height: 35rpx;
    margin-left: 20rpx;
    vertical-align: middle;
}

.selectRight input {
    display: inline-block;
    vertical-align: middle;
    width: 430rpx;
    font-size: 30rpx;
    margin-left: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    color: #666;
    border: none;
    background: #eee;
    border-radius: 10rpx;
}

.serachbg {
    position: absolute;
    top: 70rpx;
    left: 0rpx;
    z-index: 10;
    width: 100%;
    height: 300rpx;
    border-radius: 8rpx;
}

.selectBg {
    position: fixed;
    left: 0rpx;
    top: 0rpx;
    width: 100%;
    height: 900rpx;
    background: rgba(0,0,0,.7);
}

.add {
    position: fixed;
    left: 20rpx;
    bottom: 100rpx;
    width: 100rpx;
    height: 100rpx;
}

.add image {
    width: 100%;
    height: 100%;
}

.typeTitle {
    width: 90%;
    margin: 30rpx auto 20rpx;
    height: 76rpx;
}

.newTitle {
    height: 76rpx;
    line-height: 78rpx;
    display: inline-block;
    width: 49%;
    text-align: center;
    font-size: 30rpx;
    background: #EEECEC;
    color: #908F8F;
    border: 1rpx solid #eee;
}

.newTitle:nth-child(1) {
    border-bottom-left-radius: 15rpx;
    border-top-left-radius: 15rpx;
}

.newTitle:nth-child(2) {
    border-bottom-right-radius: 15rpx;
    border-top-right-radius: 15rpx;
}

.active {
    background: #fff;
    color: #000;
}

.concent {
    width: 95%;
    margin: 0 auto 30rpx;
    border-bottom: 4rpx solid #ccc;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.concentLeft {
    width: 4%;
    text-align: center;
}

.concentLeft .type {
    display: block;
    width: 90%;
    text-align: center;
    font-size: 28rpx;
    color: #000;
}

.concentRight {
    width: 95%;
    margin-left: 30rpx;
}

.concent .title {
    width: 100%;
    height: 85rpx;
    font-size: 28rpx;
}

.concentRight .title image {
    border-radius: 50%;
    width: 70rpx;
    height: 70rpx;
    vertical-align: middle;
    margin-right: 20rpx;
}

.concentRight .title text:nth-child(2) {
    display: inline-block;
    width: 62%;
    vertical-align: middle;
}

.concentRight .title text:nth-child(3) {
    display: inline-block;
    width: 24%;
    vertical-align: middle;
}

.concentRight .showImg {
    width: 100%;
    height: 220rpx;
    white-space: nowrap;
    overflow: scroll;
    margin-bottom: 20rpx;
}

.concentRight .showImg image {
    width: 32%;
    margin-right: 2%;
    vertical-align: middle;
    height: 90%;
}

.concentRight .showDescribe {
    margin-top: 20rpx;
    width: 100%;
    font-size: 28rpx;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #ccc;
}

.concentRight .showMyDetail {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 28rpx;
}

.concentRight .showMyDetail text {
    display: inline-block;
    width: 18%;
    vertical-align: middle;
}

.concentRight .showMyDetail text:nth-child(1) {
    width: 62%;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.concentRight .showMyDetail text:nth-child(2) {
    text-align: right;
    margin-right: 10rpx;
}

.concent .introduce {
    float: left;
    width: 30%;
    height: 36rpx;
    font-size: 30rpx;
    color: #666;
    line-height: 40rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.concent .person {
    width: 100%;
    height: auto;
}

.concent .person .personImages {
    display: inline-block;
    height: 100%;
    width: 70%;
    vertical-align: middle;
    line-height: 100rpx;
    overflow: hidden;
}

.concent .person .personImages image {
    vertical-align: middle;
    border-radius: 50%;
    margin-right: 10rpx;
    width: 70rpx;
    height: 70rpx;
}

.concent .person .personNum {
    color: #d95940;
    display: inline-block;
    width: 30%;
    vertical-align: middle;
    line-height: 60rpx;
    text-align: right;
    font-size: 34rpx;
}

.concent .person .personNum text {
    font-size: 28rpx;
    color: #000;
    margin-right: 10rpx;
}

.interval {
    width: 100%;
    height: 100rpx;
}

.noActivity {
    width: 77%;
    height: auto;
    margin: 200rpx auto 0;
    text-align: center;
    font-size: 32rpx;
    color: #000;
    line-height: 60rpx;
}

.noActivity text {
    color: #666;
}

