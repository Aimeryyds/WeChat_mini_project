page {
    background: #F5F5F5;
}

.contain {
    margin: 0 auto;
    margin-top: 40rpx;
    width: 98%;
    height: auto;
    background: #fff;
}

.typeTitle {
    width: 90%;
    margin: 30rpx auto 0;
    height: 76rpx;
}

.newTitle {
    height: 76rpx;
    line-height: 78rpx;
    display: inline-block;
    width: 50%;
    text-align: center;
    font-size: 30rpx;
    background: #EEECEC;
    color: #908F8F;
}

.newTitle:nth-child(1) {
    border-bottom-left-radius: 15rpx;
    border-top-left-radius: 15rpx;
}

.newTitle:nth-child(2) {
    border-bottom-right-radius: 15rpx;
    border-top-right-radius: 15rpx;
}

.active {
    background: #fff;
    color: #000;
}

.contain .mylist {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
    position: relative;
}

.mylist .imageD {
    display: inline-block;
    width: 60rpx;
    height: 100%;
    line-height: 80rpx;
    text-align: right;
}

.mylist image {
    display: inline-block;
    width: 45rpx;
    height: 41rpx;
    vertical-align: middle;
}

image.type {
    width: 36rpx;
    height: 36rpx;
}

image.info {
    width: 50rpx;
    height: 50rpx;
}

image.position {
    width: 50rpx;
    height: 50rpx;
}

.mylist .title {
    margin-left: 20rpx;
    display: inline-block;
    width: 120rpx;
    height: 100%;
    font-size: 28rpx;
}

.mylist .myinput {
    display: inline-block;
    width: 520rpx;
    height: 80rpx;
    text-align: right;
}

.myinput input {
    display: inline-block;
    margin-left: 20rpx;
    font-size: 28rpx;
    width: 500rpx;
    border: none;
    vertical-align: middle;
    z-index: 0;
}

.mylist .must {
    position: absolute;
    color: red;
    left: 65rpx;
    top: 10rpx;
    font-size: 26rpx;
}

.listMode {
    background: #fff;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    margin: 0 auto;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
    border-top: 1rpx solid rgba(0,0,0,.1);
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-content: center;
    align-items: center;
}

.listMode .imageD {
    display: inline-block;
    width: 60rpx;
    text-align: right;
}

.listMode image {
    display: inline-block;
    width: 45rpx;
    height: 45rpx;
    vertical-align: middle;
}

.listMode .title {
    margin-left: 20rpx;
    display: inline-block;
    width: 90rpx;
    font-size: 28rpx;
}

.listMode .formValue {
    display: block;
    width: 73%;
    line-height: 100rpx;
    font-size: 30rpx;
}

.listMode .mapicon {
    float: left;
    line-height: 100rpx;
    width: 40rpx;
    height: 100rpx;
    text-align: center;
}

.listMode .mapicon image {
    display: inline-block;
    vertical-align: middle;
    width: 30rpx;
    height: 30rpx;
}

.listMode .item {
    width: 80rpx;
    position: relative;
    margin: 10rpx 0 10rpx 10rpx;
    display: inline-block;
    vertical-align: middle;
}

.delete {
    position: absolute;
    height: 20px;
    bottom: 0;
    width: 85rpx;
    left: -1rpx;
    background: #ccc;
    opacity: .8;
    text-align: center;
}

.delete image {
    vertical-align: top;
    width: 20px;
    height: 20px;
}

.radio {
    font-size: 28rpx;
}

.cont {
    width: 100%;
    height: 260rpx;
}

.cont textarea {
    margin: 20rpx auto 0;
    width: 95%;
    height: 95%;
    font-size: 28rpx;
}

.createSubmit {
    margin: 60rpx auto 0;
    width: 90%;
}

.bgtypeList {
    position: fixed;
    z-index: 100;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.cardAgainType {
    padding: 10rpx 20rpx 15rpx 20rpx;
    border-radius: 15rpx;
    width: 455rpx;
    height: 192rpx;
    line-height: 103rpx;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #d2d3d5;
    margin: 0 auto;
    margin-top: 250rpx;
    color: #666;
    font-size: 30rpx;
}

.determineAgin {
    margin: 0 auto;
    width: 200rpx;
    line-height: 62rpx;
    height: 70rpx;
    border-top: 1px solid #ccc;
    background: #666;
    border-radius: 10rpx;
    color: #fff;
    font-size: 30rpx;
}

.bgtypeList1 {
    position: fixed;
    z-index: 100;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.typebg {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto auto;
    width: 435rpx;
    height: 80%;
}

.typeList {
    width: 100%;
    height: 75rpx;
    line-height: 83rpx;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #9BD3D2;
}

.typeListDetail {
    display: block;
    height: 75rpx;
    line-height: 103rpx;
    padding-left: 140rpx;
}

.yearAndMonth {
    color: #666;
    font-size: 30rpx;
}