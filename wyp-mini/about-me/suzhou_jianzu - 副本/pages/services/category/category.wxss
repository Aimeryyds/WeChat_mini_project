page {
    background-color: #EFF3F2;
    width: 100%;
    height: 100%;
}

.contain {
    width: 100%;
    height: 100%;
    overflow: hidden;
}

.search {
    width: 660rpx;
    height: 50rpx;
    border-radius: 40rpx;
    margin: 24rpx 23rpx;
    background-color: #EFF3F2;
}

.searchBox {
    background: #fff;
    padding-top: 5rpx;
    padding-bottom: 5rpx;
    margin-bottom: 12rpx;
}

.searchBox image {
    width: 41rpx;
    height: 40rpx;
    position: absolute;
    left: 4%;
    top: 32rpx;
}

.conbox {
    display: flex;
    flex-direction: row;
    height: 90%;
}

.sidebar {
    width: 240rpx;
    border-top: 1px solid #e5e5e5;
    overflow: hidden;
    background-color: #F8F8F8;
}

.sidebar view {
    display: block;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #232626;
    font-size: 14px;
}

.sidebar4 view {
    display: inline-block;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #232626;
    font-size: 14px;
    width: auto;
    padding: 0 35rpx;
}

.sidebar .highlight {
    background-color: #FF6A6A;
    color: #FF6A6A;
}

.sidebar .highlight text {
    display: inline-block;
    width: 95%;
    background-color: #fff;
    color: #FF6A6A;
    padding: 0 25rpx 0 0;
}

.sidebar text.highlight {
    background-color: #fff;
    color: #FF6A6A;
}

.main {
    width: 100%;
    display: flex;
    flex-direction: column;
}

.main {
    width: 100%;
    display: flex;
    flex-direction: column;
}

.banner {
    width: 100%;
    height: 180rpx;
    margin: 20rpx 0;
}

.gridview {
    background-color: #fff;
    height: auto;
}

.gridview dl {
    width: 240rpx;
    float: left;
    margin: 15rpx;
    text-align: center;
}

.gridview dt image {
    width: 200rpx;
    height: 200rpx;
}

.gridview dd text {
    color: #6c6c6c;
    font-size: 12px;
    width: 100%;
    height: 40rpx;
    text-align: center;
    display: block;
    line-height: 40rpx;
    font-family: 'Microsoft YaHei';
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.gridviewC {
    height: auto;
    background: #fff;
    padding: 20rpx 0 10rpx 0;
}

.gridviewC dl {
    width: 180rpx;
    float: left;
    text-align: center;
}

.gridviewC dt image {
    width: 150rpx;
    height: 150rpx;
}

.gridviewC dd text {
    color: #000;
    font-size: 26rpx;
    width: 100%;
    height: 40rpx;
    text-align: center;
    display: block;
    line-height: 40rpx;
    overflow: hidden;
    font-family: 'Microsoft YaHei';
}

.clear {
    clear: both;
}