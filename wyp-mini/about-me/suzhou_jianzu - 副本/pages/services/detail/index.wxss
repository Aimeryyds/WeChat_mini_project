.parent {
    display: flex;
    flex-direction: column;
    width: 100%;
}

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

.carts-list {
    display: flex;
    flex-direction: column;
    margin-bottom: 120rpx;
}

.carts-item {
    display: flex;
    flex-direction: row;
    height: 180rpx;
    width: 100%;
    border-bottom: 1px solid #eee;
    padding: 30rpx 0;
    justify-content: space-between;
}

.carts-userinfo {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.carts-image {
    width: 150rpx;
    height: 150rpx;
    border-width: 3rpx;
    border-style: solid;
    border-color: #ccc;
}

.text-username {
    font-size: 28rpx;
    margin-left: 20rpx;
    margin-top: 12rpx;
    color: #5e5e5e;
}

.carts-text {
    margin-left: 10rpx;
    width: 500rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.carts-right {
    height: 100%;
    width: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-end;
}

.carts_bottom {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}

.carts_xx {
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
}

.stars {
    width: 150rpx;
    height: 25rpx;
}

.Evaluation_top {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    width: 100%;
    font-size: 28rpx;
}

.Evaluation_top .item {
    display: flex;
    flex-direction: column;
    align-items: cneter;
    justify-content: center;
    width: 20%;
    text-align: center;
    margin: 20rpx 0;
    padding: 20rpx 0;
    line-height: 40rpx;
}

.select {
    color: red;
}

.slideImgs {
    position: absolute;
    top: 0rpx;
    bottom: 0rpx;
    left: 0rpx;
    right: 0rpx;
    margin: auto;
    text-align: center;
    vertical-align: middle;
    z-index: 99;
}

.imagesNum {
    position: absolute;
    left: 20rpx;
    bottom: 20rpx;
    color: #fff;
    font-size: 30rpx;
}

swiper {
    height: 100%;
}

.bgtypeList {
    position: fixed;
    z-index: 300;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: #000;
}

.slide-image {
    width: 75%;
    text-align: center;
    vertical-align: middle;
    padding: 60rpx 20rpx;
}

.wxParse-p {
    padding: 10rpx;
}

.selectDate {
    padding: 35rpx 0rpx 52rpx 0rpx;
    border-bottom: 2rpx solid #e9e9e9;
    width: 100%;
}

.select_title {
    font-size: 36rpx;
    color: #333;
}

.select_title text {
    color: #bcbcbc;
    font-size: 26rpx;
}

.select_Content {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.select_Content .Date_item {
    border: 1rpx solid #e1e1e1;
    border-radius: 8rpx;
    color: #bcbcbc;
    font-size: 24rpx;
    padding: 10rpx 15rpx;
    margin-right: calc(20% - 120rpx);
    margin-top: 20rpx;
    width: 120rpx;
    height: 90rpx;
    line-height: 45rpx;
}

.selectLight {
    color: #ff9a11;
}

.selectMore {
    color: #ff9a11;
    line-height: 90rpx;
}

.select_Content .Date_item:nth-child(4n) {
    margin-right: 0;
}

.select_Content .Date_item view {
    text-align: center;
}

.date_itemLight {
    border: 1rpx solid #ff9a11!important;
    color: #ff9a11!important;
}

.NumBox {
    font-size: 28rpx;
    display: flex;
    flex-direction: row;
    margin-top: 14rpx;
    align-items: center;
}

.titleNum {
    display: flex;
    flex-direction: row;
    line-height: 50rpx;
    border: 1rpx solid #ff9a11;
    width: 210rpx;
    height: 50rpx;
}

.nav_img {
    height: 750rpx;
    background-color: #fff;
    width: 100%;
}

.wx-video-cover-duration {
    display: none;
}

.wx-video-cover-play-button {
    padding: 30rpx;
    border: 2rpx solid #fff;
    border-radius: 50%;
}

.container {
    margin: 0 36rpx 0 36rpx;
    padding: 0;
    align-items: flex-start;
}

.T_title {
    font-size: 36rpx;
}

.T_detail {
    font-size: 32rpx;
    color: #d5d5d5;
    margin-top: 8rpx;
}

.title_wrap {
    padding: 60rpx 0rpx 52rpx 0rpx;
    line-height: 30rpx;
    border-bottom: 2rpx solid #e9e9e9;
    width: 100%;
}

.title_wrap .title {
    font-size: 40rpx;
    color: #515151;
    line-height: 50rpx;
}

.title_wrap .summary,.title_wrap text {
    font-size: 28rpx;
    letter-spacing: 2px;
    color: #d5d5d5;
}

.titleNum .calNum {
    width: 70rpx;
    line-height: 50rpx;
    height: 50rpx;
    box-sizing: border-box;
    color: #ff9a11;
    text-align: center;
    align-items: center;
}

#em {
    font-size: 45rpx;
    color: red;
}

.Evaluation {
    display: flex;
    flex-direction: row;
    align-items: flex-start;
    padding: 36rpx 0 72rpx 0;
    letter-spacing: 2px;
}

.Evaluation image {
    margin: 0 20rpx 0 40rpx;
}

.store {
    border-bottom: 2rpx solid #e9e9e9;
}

.store_wrap {
    display: flex;
    flex-direction: row;
}

.store_img {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
    background-color: #000;
}

.store_contain {
    letter-spacing: 2rpx;
    margin-left: 20rpx;
    margin-bottom: 48rpx;
}

.store_con {
    font-size: 28rpx;
    line-height: 40rpx;
    margin-top: 14rpx;
    letter-spacing: 2px;
    color: #d5d5d5;
}

.addr {
    padding: 40rpx 0 40rpx 0;
    width: 100%;
    border-bottom: 2rpx solid #e9e9e9;
}

.detail {
    padding: 40rpx 0 40rpx 0;
    width: 100%;
    border-bottom: 2rpx solid #e9e9e9;
    overflow: hidden;
}

.detail_img {
    width: 100%;
    height: 400rpx;
    background-color: #ff0;
}

.detail_bd {
    font-size: 28rpx;
    line-height: 40rpx;
    margin-top: 14rpx;
    letter-spacing: 2px;
    color: #d5d5d5;
}

.Unsubscribe {
    padding: 40rpx 0 40rpx 0;
    width: 100%;
    border-bottom: 2rpx solid #e9e9e9;
}

.banner {
    padding: 40rpx 0 40rpx 0;
    width: 100%;
    border-bottom: 2rpx solid #e9e9e9;
    margin-bottom: 100rpx;
}

.banner_wrap {
    width: 100%;
    margin-top: 20rpx;
    overflow: scroll-x;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.banner_one {
    margin-right: 20rpx;
    width: 200rpx;
    overflow: hidden;
    display: inline-block;
}

.banner_img {
    width: 200rpx;
    height: 200rpx;
    background-color: #fff;
}

.banner_title {
    font-size: 28rpx;
    letter-spacing: 2rpx;
    margin: 10rpx 0 10rpx 0;
    overflow: hidden;
}

.banner_detail {
    font-size: 26rpx;
    color: #d5d5d5;
    letter-spacing: 2rpx;
    overflow: hidden;
}

.footer {
    height: 100rpx;
    background-color: #fcfcfc;
    width: 100%;
    display: flex;
    flex-direction: row;
    position: fixed;
    bottom: 0;
    z-index: 999;
    border-top: 1rpx solid #e9e9e9;
}

.contact,.reservation {
    width: 50%;
    text-align: center;
    line-height: 100rpx;
    font-size: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.tip {
    width: 38rpx;
    height: 35rpx;
    position: absolute;
    top: 36rpx;
    left: 75rpx;
}

.line {
    width: 2rpx;
    background-color: #e0e0e0;
    height: 50%;
    margin-top: 25rpx;
}

.reservation_detail {
    width: 100%;
    background-color: #ef5a3c;
    line-height: 100rpx;
    color: #fff;
    background: #ff9a11;
}

.diy_contact {
    width: 100%;
    opacity: 0;
    position: absolute;
    z-index: 9;
    height: 100%;
}