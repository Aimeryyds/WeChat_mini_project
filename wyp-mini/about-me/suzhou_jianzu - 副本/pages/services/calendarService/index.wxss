.weeks {
    height: 60rpx;
    line-height: 60rpx;
    background: #eeeff4;
    color: #4a4f53;
    font-size: 24rpx;
    border-bottom: 1rpx solid #e2e3e5;
}

.monthTitle {
    width: 100%;
    height: 80rpx;
    background: #fff;
    line-height: 80rpx;
    text-align: center;
    color: #333;
    font-size: 26rpx;
}

.week {
    text-align: center;
}

.days {
    height: 500rpx;
}

.grid {
    width: 107.1428571429rpx;
}

.day {
    width: 60rpx;
    height: 60rpx;
    color: #333;
    font-size: 26rpx;
    font-weight: 200;
}

.border-radius {
    border-radius: 50%;
    position: relative;
    left: 0;
    top: 0;
    color: #fff;
}

.pink-bg {
    background-color: #ff9a11;
}

.page {
    position: absolute;
    top: 0;
    width: 100%;
    height: 1000rpx;
    background-color: #fff;
    z-index: 2;
}

.box {
    display: flex;
}

.box-lr {
    flex-direction: row;
}

.box-rl {
    flex-direction: row-reverse;
}

.box-tb {
    flex-direction: column;
}

.box-bt {
    flex-direction: column-reverse;
}

.box-pack-center {
    justify-content: center;
}

.box-pack-start {
    justify-content: flex-start;
}

.box-pack-end {
    justify-content: flex-end;
}

.box-pack-between {
    justify-content: space-between;
}

.box-pack-around {
    justify-content: space-around;
}

.box-align-center {
    align-items: center;
}

.box-align-start {
    align-items: flex-start;
}

.box-align-end {
    align-items: flex-end;
}

.self-align-center {
    align-self: center;
    margin: 0 auto;
}

.self-align-start {
    align-self: flex-start;
}

.self-align-end {
    align-self: flex-end;
}

.self-align-stretch {
    align-self: stretch;
}

.box-wrap {
    flex-wrap: wrap;
}

.box-nowrap {
    flex-wrap: nowrap;
}

.flex {
    flex-grow: 1;
}

.shrink {
    flex-shrink: 1;
}

.bg {
    background-image: linear-gradient(to bottom,#faefe7,#ffcbd7);
    overflow: hidden;
}

.brown-color {
    color: #784344;
}

.pink-color {
    color: #000;
    padding-bottom: 30rpx;
}

.color-bg {
    color: #d5d5d5;
}

.white-color {
    color: #fff;
}

.fs24 {
    font-size: 24rpx;
}

.fs28 {
    font-size: 28rpx;
}

.fs32 {
    font-size: 32rpx;
}

.fs36 {
    font-size: 36rpx;
}

.section {
    font-size: 28rpx;
    margin-top: 20rpx;
    text-align: center;
    height: 70rpx;
}

.picker {
    color: #333;
    letter-spacing: 4rpx;
    font-size: 28rpx;
}