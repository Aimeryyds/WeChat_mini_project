.concent {
    width: 95%;
    margin: 0 auto 20rpx;
    background: red;
}

.concentLeft {
    width: 20%;
    height: auto;
    float: left;
}

.concentLeft .day {
    width: 50%;
    height: 80rpx;
    line-height: 80rpx;
    float: left;
    font-size: 50rpx;
}

.concentLeft .month {
    width: 50%;
    height: 80rpx;
    float: left;
    font-size: 28rpx;
    line-height: 98rpx;
    color: #666;
}

.concentLeft .year {
    display: block;
    width: 90%;
    text-align: center;
    font-size: 28rpx;
    line-height: 60rpx;
    color: #FBBE48;
}

.concentLeft .type {
    display: block;
    width: 90%;
    text-align: center;
    font-size: 28rpx;
    color: #30BFBC;
}

.concentRight {
    width: 75%;
    height: 100%;
    float: left;
    margin-left: 30rpx;
}

.concent .title {
    padding-top: 30rpx;
    width: 70%;
    height: auto;
    font-size: 30rpx;
    font-weight: bolder;
    line-height: 30rpx;
    padding-bottom: 20rpx;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.concent .introduce {
    width: 90%;
    height: 36rpx;
    padding-bottom: 50rpx;
    font-size: 30rpx;
    color: #666;
    line-height: 40rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.couponNum {
    width: auto;
    height: auto;
    overflow: scroll-x;
    white-space: nowrap;
}

.channelImageList {
    display: inline-block;
}

.channelImage {
    display: inline-block;
    margin-right: 20rpx;
    margin-top: 10rpx;
}

.concent .showImages {
    width: 100%;
}

.concent .showImages image {
    display: inline-block;
}

.concent .person {
    width: 100%;
    height: auto;
}

.concent .person .personImages {
    display: inline-block;
    height: 100%;
    width: 80%;
    vertical-align: middle;
    line-height: 100rpx;
    overflow: hidden;
}

.concent .person .personImages image {
    vertical-align: middle;
    border-radius: 50%;
    margin-right: 10rpx;
    width: 70rpx;
    height: 70rpx;
}

.concent .person .personNum {
    color: #D95940;
    display: inline-block;
    width: 20%;
    vertical-align: middle;
    line-height: 60rpx;
    text-align: right;
    font-size: 34rpx;
}

.concent .person .personNum text {
    font-size: 28rpx;
    color: #000;
    margin-right: 10rpx;
}

.noActivity {
    width: 77%;
    height: auto;
    margin: 200rpx auto 0;
    text-align: center;
    font-size: 32rpx;
    color: #000;
    line-height: 60rpx;
}

.noActivity text {
    color: #24B2F2;
}

text.creatActi {
    display: block;
    margin: 20rpx auto 0;
    width: 200rpx;
    line-height: 62rpx;
    height: 70rpx;
    background: #24B2F2;
    border-radius: 10rpx;
    color: #fff;
    font-size: 32rpx;
}