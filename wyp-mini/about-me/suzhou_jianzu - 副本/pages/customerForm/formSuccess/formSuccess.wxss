.formList {
    margin: 40rpx auto 40rpx;
    width: 95%;
    border-bottom: 1rpx solid #ccc;
    height: 93rpx;
    padding-bottom: 40rpx;
}

.detialInfo {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    color: #000;
    font-size: 32rpx;
    text-align: center;
}

.everyInputhbit {
    position: relative;
    margin: 20rpx auto;
    width: 85%;
    height: 80rpx;
}

.title {
    display: inline-block;
    width: 25%;
    height: 80rpx;
    font-size: 28rpx;
    text-align: center;
    background: #fff;
    border: 1px solid #ccc;
    border-right: none;
    border-bottom-left-radius: 10rpx;
    border-top-left-radius: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    line-height: 80rpx;
}

.everyInputhbit .cont {
    display: inline-block;
    width: 71%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #666;
    border: 1px solid #ccc;
    background: #fafafa;
    text-align: center;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.bgtypeList {
    position: fixed;
    z-index: 3;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.cardAgainType {
    padding: 10rpx 20rpx 15rpx 20rpx;
    border-radius: 15rpx;
    width: 455rpx;
    height: 314rpx;
    line-height: 90rpx;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #d2d3d5;
    margin: 0 auto;
    margin-top: 250rpx;
    color: #666;
    font-size: 30rpx;
}

.determineAgin {
    margin: 30rpx auto 0;
    width: 200rpx;
    line-height: 62rpx;
    height: 70rpx;
    border-top: 1px solid #ccc;
    background: #A5CA25;
    border-radius: 10rpx;
    color: #fff;
    font-size: 30rpx;
}