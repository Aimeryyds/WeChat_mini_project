scroll-view {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 100%;
    background-size: 100% 100%;
}

.formList {
    width: 100%;
    height: auto;
    padding: 20rpx 0;
    border-bottom: 1rpx solid #000;
}

.detialInfo {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    color: #000;
    font-size: 32rpx;
    text-align: center;
}

.everyInputhbit {
    position: relative;
    margin: 0 auto;
    width: 85%;
    height: auto;
    min-height: 80rpx;
    margin-bottom: 15rpx;
}

.must {
    position: absolute;
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    line-height: 0;
    top: 20rpx;
    left: 78rpx;
    color: red;
    font-size: 28rpx;
}

.title {
    display: inline-block;
    width: 25%;
    height: 80rpx;
    line-height: 80rpx;
    vertical-align: top;
    font-size: 28rpx;
    text-align: center;
    background: #fff;
    border: 2rpx solid #ccc;
    border-right: none;
    border-bottom-left-radius: 10rpx;
    border-top-left-radius: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.everyInputhbit .cont {
    vertical-align: top;
    display: inline-block;
    padding: 0rpx 20rpx;
    width: 65%;
    min-height: 78rpx;
    line-height: 78rpx;
    height: auto;
    font-size: 28rpx;
    color: #666;
    border: 2rpx solid #ccc;
    background: #fafafa;
    text-align: center;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.everyInputhbit .cont .time {
    display: inline-block;
    font-size: 28rpx;
    color: #666;
    min-height: 75rpx;
    height: auto;
    width: 48%;
}

.everyInputhbit .cont .time:nth-child(1) {
    display: inline-block;
    font-size: 28rpx;
    color: #666;
    border-right: 1px solid #ccc;
}

.contImg {
    display: inline-block;
    width: 71%;
    min-height: 80rpx;
    height: auto;
    border: 1px solid #ccc;
    background: #fafafa;
}

.everyInputhbit .contImg image {
    display: inline-block;
    width: 45%;
    vertical-align: middle;
    margin-left: 10rpx;
    height: auto;
}

.bgtypeList {
    position: fixed;
    z-index: 3;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
}

.cardAgainType {
    padding: 10rpx 20rpx 15rpx 20rpx;
    border-radius: 15rpx;
    width: 455rpx;
    height: 314rpx;
    line-height: 90rpx;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #d2d3d5;
    margin: 0 auto;
    margin-top: 250rpx;
    color: #666;
    font-size: 30rpx;
}

.determineAgin {
    margin: 30rpx auto 0;
    width: 200rpx;
    line-height: 62rpx;
    height: 70rpx;
    border-top: 1px solid #ccc;
    background: #A5CA25;
    border-radius: 10rpx;
    color: #fff;
    font-size: 30rpx;
}