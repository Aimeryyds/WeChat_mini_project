page {
    background-size: 100% 100%;
    background: #E6E6E6;
}

.scroll {
    min-height: 1230rpx;
    height: auto;
    background-size: cover;
}

.again {
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    width: 100%;
    height: 100%;
    background-size: cover;
}

.again .cont {
    position: absolute;
    top: 0rpx;
    left: 0rpx;
    bottom: 0rpx;
    right: 0rpx;
    width: 400rpx;
    height: 100rpx;
}

.formList {
    width: 100%;
    height: auto;
    padding-top: 20rpx;
}

.titleAll {
    width: 100%;
    height: 80rpx;
    font-size: 30rpx;
    color: #000;
    text-align: center;
    padding-top: 60rpx;
}

.titleAll text {
    width: auto;
    height: 50rpx;
    line-height: 17rpx;
    font-size: 30rpx;
    text-align: center;
    padding: 0 20rpx;
    background: #fff;
    border-radius: 8rpx;
    display: inline-block;
}

.everyInput {
    position: relative;
    margin: 0 auto;
    width: 85%;
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 30rpx;
}

.everyInputhbit {
    position: relative;
    margin: 0 auto;
    width: 85%;
    height: auto;
    line-height: 80rpx;
    margin-bottom: 30rpx;
    display: flex;
    flex-direction: row;
}

.everyInputIndu {
    position: relative;
    margin: 0 auto;
    width: 85%;
    height: auto;
    line-height: 80rpx;
    margin-bottom: 30rpx;
}

.must {
    position: absolute;
    display: inline-block;
    width: 20rpx;
    height: 20rpx;
    line-height: 0;
    top: 20rpx;
    left: 78rpx;
    color: red;
    font-size: 28rpx;
}

.titleBlock {
    position: absolute;
    z-index: 100;
    width: 95%;
    min-height: 85rpx;
    height: auto;
    top: -5rpx;
    left: -5rpx;
    font-size: 28rpx;
    text-align: center;
    background: #fff;
    border: 1px solid #ccc;
    border-radius: 8rpx;
    padding: 0 10rpx;
}

.title {
    display: inline-block;
    width: 25%;
    height: 80rpx;
    vertical-align: top;
    font-size: 28rpx;
    text-align: center;
    background: #fff;
    border: 1px solid #ccc;
    border-right: none;
    border-bottom-left-radius: 10rpx;
    border-top-left-radius: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.everyInput input {
    display: inline-block;
    width: 71%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #666;
    border: 1px solid #ccc;
    background: #fafafa;
    text-align: center;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.everyInputIndu textarea {
    display: inline-block;
    width: 65%;
    padding: 20rpx 20rpx;
    height: 150rpx;
    line-height: 40rpx;
    vertical-align: top;
    font-size: 28rpx;
    color: #666;
    border: 1px solid #ccc;
    background: #fafafa;
    text-align: center;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.everyInputhbit checkbox-group {
    display: inline-block;
    padding: 0rpx 20rpx;
    width: 71%;
    height: auto;
    line-height: 75rpx;
    font-size: 28rpx;
    box-sizing: border-box;
    color: #666;
    border: 1px solid #ccc;
    background: #fafafa;
    text-align: left;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.everyInputhbit checkbox-group .checkbox {
    display: inline-block;
    margin-right: 30rpx;
}

.everyInputhbit radio-group {
    display: inline-block;
    padding: 0rpx 20rpx;
    width: 65%;
    height: auto;
    line-height: 75rpx;
    font-size: 28rpx;
    color: #666;
    border: 1px solid #ccc;
    background: #fafafa;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

radio-group label {
    display: inline-block;
    padding: 0rpx 20rpx;
    width: 95%;
    height: 75rpx;
    line-height: 75rpx;
    font-size: 28rpx;
    color: #666;
    border-top: 1px solid #000;
}

radio-group label:nth-child(1) {
    border-top: none;
}

.everyInput .formDateValue {
    display: inline-block;
    width: 71%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    border: 1px solid #ccc;
    background: #fafafa;
    text-align: center;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.everyInput .formDateValueTime {
    display: inline-block;
    width: 35%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    background: #fafafa;
    text-align: center;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    border: 1px solid #ccc;
}

.everyInput .endTime {
    border: 1px solid #ccc;
    border-left: none;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
}

.formValue {
    display: inline-block;
    width: 71%;
    height: 80rpx;
    line-height: 80rpx;
    border-bottom-right-radius: 10rpx;
    border-top-right-radius: 10rpx;
    border: 1px solid #ccc;
    background: #fafafa;
}

.item {
    width: 80rpx;
    height: 80rpx;
    float: left;
    position: relative;
    margin: 0rpx 0 10rpx 10rpx;
}

.delete {
    position: absolute;
    height: 15px;
    bottom: 0;
    width: 85rpx;
    left: -1rpx;
    background: #ccc;
    opacity: .8;
    text-align: center;
}

.delete image {
    vertical-align: top;
    width: 20px;
    height: 15px;
}

.createSubmit {
    margin: 60rpx auto 0rpx;
    width: 70%;
    background: #A5CA25;
    color: #fff;
    font-size: 30rpx;
}

