.bottom-back {
    overflow: hidden;
    text-align: center;
    padding-top: 60rpx;
    margin-bottom: 150rpx;
}

.bottom-back .process text,.bottom-back .done text {
    display: inline-block;
    font-size: 24rpx;
    color: #999999;
    height: 30rpx;
    line-height: 30rpx;
    line-height: 1;
}

.bottom-back .process .icon,.bottom-back .done .icon {
    display: inline-block;
    margin-right: 10rpx;
    width: 30rpx;
    height: 30rpx;
    vertical-align: middle;
}