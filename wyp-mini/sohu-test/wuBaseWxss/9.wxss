@-webkit-keyframes rotate {
    0%,100% {
        -webkit-transform: rotate(0deg);
    }

    100% {
        -webkit-transform: rotate(360deg);
    }
}

@keyframes rotate {
    0%,100% {
        -webkit-transform: rotate(0deg);
    }

    100% {
        -webkit-transform: rotate(360deg);
    }
}

.roRotate {
    -webkit-animation: rotate 1s linear infinite;
}

