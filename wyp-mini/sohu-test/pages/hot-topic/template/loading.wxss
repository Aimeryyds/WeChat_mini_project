.loading-coverage {
    margin: 30rpx;
    margin-bottom: 0;
    background-color: #fff;
    border-radius: 7rpx;
    overflow: hidden;
    width: 696rpx;
}

.loading-coverage .chunk>view {
    display: inline-block;
    background-color: #f7f7f7;
}

.loading-coverage .top {
    padding: 22rpx;
    padding-bottom: 0;
}

.loading-coverage .top>view:nth-child(1) {
    width: 26rpx;
    height: 26rpx;
    border-radius: 100%;
}

.loading-coverage .top>view:nth-child(2) {
    width: 63rpx;
    height: 20rpx;
    margin-left: 15rpx;
}

.loading-coverage .top>view:nth-child(3) {
    display: block;
    height: 35rpx;
    width: 650rpx;
    margin-top: 25rpx;
}

.loading-coverage .top>view:nth-child(4) {
    display: block;
    margin-top: 10rpx;
    width: 300rpx;
    height: 35rpx;
}

.loading-coverage .top>view:nth-child(5) {
    display: block;
    height: 25rpx;
    width: 650rpx;
    margin-top: 25rpx;
}

.loading-coverage .top>view:nth-child(6) {
    display: block;
    height: 25rpx;
    width: 650rpx;
    margin-top: 10rpx;
}

.loading-coverage .top>view:nth-child(7) {
    display: block;
    height: 25rpx;
    width: 550rpx;
    margin-top: 10rpx;
}

.loading-coverage .bottom>view:nth-child(1) {
    width: 100%;
    height: 285rpx;
    background-color: #f2f2f2;
    margin-top: 14rpx;
    display: block;
}