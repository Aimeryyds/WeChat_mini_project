.loading-coverage {
    overflow: hidden;
}

.loading-coverage .chunk {
    margin-top: 50rpx;
    position: relative;
}

.loading-coverage .chunk>view {
    display: inline-block;
    background-color: #f7f7f7;
    position: absolute;
}

.loading-coverage .image-text {
    height: 158rpx;
}

.loading-coverage .image-text>view:nth-child(1) {
    width: 245rpx;
    height: 158rpx;
    left: 0;
    top: 0;
}

.loading-coverage .image-text>view:nth-child(2) {
    width: 456rpx;
    height: 35rpx;
    left: 270rpx;
    top: 0;
}

.loading-coverage .image-text>view:nth-child(3) {
    width: 256rpx;
    height: 35rpx;
    left: 270rpx;
    top: 40rpx;
}

.loading-coverage .image-text>view:nth-child(4) {
    width: 94rpx;
    height: 27rpx;
    left: 270rpx;
    bottom: 0;
}

.loading-coverage .image-text>view:nth-child(5) {
    width: 23rpx;
    height: 25rpx;
    right: 32rpx;
    bottom: 0;
}

.loading-coverage .image-group {
    height: 295rpx;
}

.loading-coverage .image-group>view:nth-child(1),.loading-coverage .image-group>view:nth-child(2),.loading-coverage .image-group>view:nth-child(3) {
    width: 245rpx;
    height: 158rpx;
    left: 0;
}

.loading-coverage .image-group>view:nth-child(2) {
    left: 50%;
    transform: translate(-50%,0);
}

.loading-coverage .image-group>view:nth-child(3) {
    right: 0;
    left: auto;
}

.loading-coverage .image-group>view:nth-child(4) {
    height: 35rpx;
    left: 30rpx;
    right: 30rpx;
    top: 172rpx;
}

.loading-coverage .image-group>view:nth-child(5) {
    height: 35rpx;
    left: 30rpx;
    width: 469rpx;
    top: 216rpx;
}

.loading-coverage .image-group>view:nth-child(6) {
    bottom: 0;
    height: 27rpx;
    left: 40rpx;
    width: 100rpx;
}

.loading-coverage .image-group>view:nth-child(7) {
    width: 23rpx;
    height: 25rpx;
    right: 32rpx;
    bottom: 0;
}