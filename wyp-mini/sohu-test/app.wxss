page {
    height: 100%;
}

.container {
    min-height: 1280rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
    flex-direction: column;
    -webkit-box-align: center;
    align-items: center;
    box-sizing: border-box;
    background: #fff;
    font-family: SourceHanSansCN Normal,PingFang SC,"STHeitiSC","黑体-简",Avenir,"微软雅黑",sans-serif,"宋体";
}

image {
    display: block;
}

.navigator-hover {
    background-color: rgba(0,0,0,0);
    opacity: 0.7;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.wx-video-cover-duration {
    display: none;
}

button::after {
    border: none;
    display: none;
}

button {
    text-align: left;
    border-top-style: none;
    border-right-style: none;
    border-bottom-style: none;
    border-left-style: none;
    padding-left: 0;
    padding-right: 0;
    border-radius: 0;
    background-color: transparent;
    overflow: visible;
}