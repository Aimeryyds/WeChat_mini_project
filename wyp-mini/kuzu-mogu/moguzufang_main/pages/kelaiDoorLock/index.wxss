.bg {
    border-radius: 50%;
    width: 50%;
    height: 50%;
    top: 25%;
    left: 25%;
    position: absolute;
    opacity: 0;
}

.bg1 {
    animation: bgkey1 ease 2.8s infinite;
}

.bg2 {
    animation: bgkey1 ease 2.8s infinite;
    animation-delay: -0.7s;
}

.bg3 {
    animation: bgkey1 ease 2.8s infinite;
    animation-delay: -1.4s;
}

.bg4 {
    animation: bgkey1 ease 2.8s infinite;
    animation-delay: -2.1s;
}

@-webkit-keyframes bgkey1 {
    0% {
        opacity: 0;
        transform: scale(1);
    }

    5% {
        opacity: 0;
        transform: scale(1);
    }

    6% {
        opacity: 1;
        transform: scale(1);
    }

    95% {
        opacity: 0;
        transform: scale(2);
    }

    100% {
        opacity: 0;
        transform: scale(1);
    }
}

@keyframes bgkey1 {
    0% {
        opacity: 0;
        transform: scale(1);
    }

    5% {
        opacity: 0;
        transform: scale(1);
    }

    6% {
        opacity: 1;
        transform: scale(1);
    }

    95% {
        opacity: 0;
        transform: scale(2);
    }

    100% {
        opacity: 0;
        transform: scale(1);
    }
}

