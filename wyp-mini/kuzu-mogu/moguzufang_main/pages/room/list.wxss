.search-header {
    height: 100rpx;
    margin: 0 40rpx;
    background-color: #fff;
}

.search-header .search-block {
    padding: 0 24rpx;
    background-color: #F3F5F7;
    height: 68rpx;
    line-height: 68rpx;
    border-radius: 34rpx;
    font-size: 28rpx;
    color: #333;
}

.search-header .search-ph {
    display: inline-block;
    vertical-align: middle;
    color: #BCC1C4;
}

.search-header .mgic-search {
    display: inline-block;
    vertical-align: middle;
    font-size: 32rpx;
    color: #BCC1C4;
    margin-right: 16rpx;
}

.room-list {
    padding-left: 0;
    padding-right: 0;
    padding-top: 300rpx;
}

.content-fix {
    z-index: 9;
    top: 0;
    left: 0;
    position: fixed;
    width: 100%;
    background-color: #fff;
}

.content-fix-bg {
    z-index: 8;
    top: 0;
    left: 0;
    position: fixed;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.2);
}

.empty {
    margin-top: 130rpx;
    text-align: center;
    padding-bottom: 50rpx;
}

.empty image {
    width: 276rpx;
    height: 276rpx;
}