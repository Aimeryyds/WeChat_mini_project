.mg-loading-indicator {
    position: relative;
}

.mg-loading-indicator .mg-loading-item {
    width: 20rpx;
    height: 20rpx;
    margin: 0;
}

.mg-loading-indicator .mg-loading-item:before {
    content: ' ';
    position: absolute;
    top: 50%;
    left: 50%;
    margin-left: -12rpx;
    margin-top: -19rpx;
    width: 25rpx;
    height: 38rpx;
    transform: skew(-0.46rad) scale(0.5);
    transform-origin: center center;
    border-radius: 2rpx 4rpx 4rpx 6rpx;
}

.mg-loading-indicator .mg-loading-item:before {
    background-color: #ff763f;
}

.mg-loading-indicator .mg-loading-item {
    display: inline-block;
    position: relative;
    transform: scale(0);
    opacity: 0;
    animation: AULoadingScaleColorBoth 0.766s cubic-bezier(0.42,0,0.58,1) infinite alternate;
}

.mg-loading-indicator .mg-loading-item:nth-child(1) {
    animation-delay: -2.2s;
}

.mg-loading-indicator .mg-loading-item:nth-child(2) {
    animation-delay: -1.966s;
}

.mg-loading-indicator .mg-loading-item:nth-child(3) {
    animation-delay: -1.666s;
}

