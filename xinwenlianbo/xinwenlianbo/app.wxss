.nonet {
    width: 100%;
    height: 100%;
    position: fixed;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.nonet image {
    width: 160rpx;
    height: 160rpx;
    margin-bottom: 55rpx;
}

.nonet view {
    height: 40rpx;
    font-size: 30rpx;
    margin-bottom: 30rpx;
    color: #666666;
    line-height: 45rpx;
}

.nonet .rush {
    color: #ed4137;
    width: 200rpx;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    border: 1px solid #ed4137;
    border-radius: 50rpx;
    margin-top: 40rpx;
}

.refresh-btn-wrapper {
    position: fixed;
    width: 50px;
    height: 50px;
    bottom: 20px;
    right: 10px;
}

.refresh-btn-wrapper .refresh-btn {
    display: block;
    width: 100%;
    height: 100%;
    background: url(http://s3a.pstatp.com/resource/neihan_wap/static/image/refreshicon_0e30755.png) no-repeat center center;
    background-size: 100%;
}

.refresh-btn-wrapper .refresh-btn.spinner {
    animation: rotator-anticlock .5s linear infinite;
}

.scrollto-btn-wrapper {
    position: fixed;
    width: 92rpx;
    height: 92rpx;
    bottom: 170rpx;
    right: 19rpx;
    z-index: 100;
}

.scrollto-btn {
    display: block;
    width: 100%;
    height: 100%;
    background: url(data:image/png;base64,) no-repeat center center;
    background-size: 100%;
}

@-webkit-keyframes rotator-anticlock {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(-360deg);
    }
}

@keyframes rotator-anticlock {
    0% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(-360deg);
    }
}

.hide {
    display: none!important;
}