@import "../../__wuBaseWxss__/5.wxss";

@import "../../templates/audio/audio.wxss";

.hide {
    display: none;
}

.show {
    display: block;
}

.audiobox {
    z-index: 5;
    position: fixed;
    bottom: 1rpx;
    height: 100rpx;
    border: 1rpx solid #ddd;
    border-bottom: 0rpx;
    width: 100%;
    background: #fff;
}

.tab_title {
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    white-space: nowrap;
}

.tab_title .guanzhu_tit {
    margin: 0 40rpx;
    display: inline-block;
    color: #bbb;
}

.tab_title .guanzhu_tit.active {
    color: #ef4d44;
}

.tab_title .guanzhu_tit.getmore {
    color: #467af0;
}

.guanzhuSel {
    width: 100%;
    position: fixed;
    top: 80rpx;
    z-index: 10;
    display: none;
    border-bottom: 1rpx solid #ccc;
    background: rgba(0,0,0,0.5);
}

.guanzhuSel .Selscroll {
    background: #fff;
}

.guanzhuSel .Sel_li {
    width: 90%;
    margin: 0 auto;
    border-bottom: 1px solid #ccc;
    height: 40rpx;
    line-height: 40rpx;
    font-size: 30rpx;
    color: #000;
    padding: 20rpx 0;
}

.guanzhuSel .Sel_li1.Sel_li {
    color: blue;
}

.guanzhuSel .addGZ {
    height: 150rpx;
    line-height: 150rpx;
    font-size: 32rpx;
    color: blue;
    text-align: center;
    background-color: #fff;
    border-top: 1rpx solid #ccc;
}

.conlist {
    height: 1000rpx;
}

.conlist .list_li {
    padding: 30rpx 20rpx;
    border-bottom: 10rpx solid #ddd;
}

.conlist .list_li .li_tit {
    font-size: 30rpx;
    color: #000;
    line-height: 40rpx;
    height: 80rpx;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}

.conlist .list_li .li_time {
    font-size: 26rpx;
    color: #666;
    margin-top: 10rpx;
    margin-bottom: 10rpx;
}

.conlist .list_li .li_gjc {
    font-size: 24rpx;
    color: #333;
    float: left;
    border: 1rpx solid #ccc;
    border-radius: 6rpx;
    padding: 6rpx 10rpx;
    margin-right: 10rpx;
}

.conlist .list_li .li_gjc:last-child {
    margin-right: 0;
}

.conlist .list_li .li_timelength {
    text-align: right;
    font-size: 26rpx;
    color: #666;
    padding: 6rpx 0;
}

.show {
    display: block;
}

.hide {
    display: none;
}

.search_result {
    width: 300rpx;
    height: 300rpx;
    margin: 236rpx auto 0;
}

.search_result image {
    width: 300rpx;
    height: 300rpx;
    display: inline-block;
}

.search_result .text {
    font-size: 36rpx;
    color: #666;
    text-align: center;
    line-height: 46rpx;
    margin-top: 20rpx;
}

.articlelist_lw {
    width: 100%;
    height: 72rpx;
    text-align: center;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    background-color: #f5f5fd;
}

.articlelist_lw image {
    width: 54rpx;
    height: 100%;
}

.articlelist_loading {
    text-align: center;
}

.contentover {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 24rpx;
    color: #999;
    background: url(data:image/jpeg;base64,) no-repeat;
    background-size: contain;
}

.addguanzhu {
    position: fixed;
    top: 0;
    right: 30rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    color: #467af0;
}