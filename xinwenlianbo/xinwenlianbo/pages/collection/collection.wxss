@import "../../templates/audio/audio.wxss";

.hide {
    display: none;
}

.show {
    display: block;
}

.audiobox {
    z-index: 5;
    position: fixed;
    bottom: 101rpx;
    height: 100rpx;
    border: 1rpx solid #ddd;
    border-left: 0;
    border-right: 0;
    width: 100%;
    background: #fff;
}

.jjsx {
    width: 300rpx;
    height: 300rpx;
    margin: 333rpx auto 0;
}

.jjsx image {
    width: 300rpx;
    height: 300rpx;
    display: inline-block;
}

.jjsx .text {
    font-size: 36rpx;
    color: #666;
    text-align: center;
    line-height: 46rpx;
    margin-top: 20rpx;
}

page {
    height: 100%;
}

.swiper-tab {
    width: 100%;
    text-align: center;
    position: fixed;
    bottom: 0;
    padding: 0 10rpx 0 10rpx;
    z-index: 999;
    height: 100rpx;
    background-color: #fff;
    border-top: 1rpx solid #dddddd;
    box-shadow: 0 0 30rpx #ddd;
}

.swiper-tab-item {
    height: 92rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    font-weight: bold;
    font-family: PingFangSC-Semibold,sans-serif;
    display: inline-block;
    width: 135rpx;
    margin-right: 50rpx;
    color: #999999;
}

.swiper-tab-item:last-of-type {
    margin-right: 0rpx;
}

.on {
    color: #333;
    border-bottom: 8rpx solid #ed4137;
}

.swiper-box {
    background-color: #f5f5fc;
    display: block;
    width: 100%;
    overflow: hidden;
}

.hide {
    display: none;
}

.show {
    display: block;
}

.dataZQbox {
    width: 100%;
    text-align: center;
    color: black;
}

.boxnodata {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    margin-top: -80rpx;
}

.boxnodata image {
    width: 300rpx;
    height: 300rpx;
    margin-bottom: 20rpx;
}

.boxnodata text {
    font-size: 36rpx;
    display: block;
    color: #666666;
}

.list {
    z-index: 88;
}

.list_li {
    border-top: 1px solid #eaecf6;
}

.list_li_zq {
    background-color: #fff;
    border: 1rpx solid #e2e8f4;
    border-radius: 12rpx 12rpx;
    margin: 20rpx;
    min-height: 300rpx;
    position: relative;
}

.list_zq_left {
    float: left;
    width: 90rpx;
    position: absolute;
    top: -2rpx;
    text-align: center;
    margin: 0 40rpx;
}

.list_zq_left .zq_year {
    background: #db291f;
    width: 90rpx;
    height: 30rpx;
    color: #ffffff;
    font-size: 25rpx;
    font-family: PingFangSC-Semibold,sans-serif;
    padding: 10rpx;
}

.list_zq_left .zq_month {
    background: #ed4137;
    width: 90rpx;
    height: 30rpx;
    color: #ffffff;
    font-size: 25rpx;
    font-family: PingFangSC-Semibold,sans-serif;
    padding: 10rpx;
    border-bottom-right-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
}

.list_zq_left .zq_day {
    color: #000000;
    font-size: 90rpx;
    width: 90rpx;
    line-height: 115rpx;
    font-family: PingFangSC-Medium,sans-serif;
}

.list_zq_left .zq_data {
    width: 90rpx;
    color: #999999;
    font-size: 24rpx;
    font-family: PingFangSC-Semibold,sans-serif;
    padding: 15rpx 0;
    font-weight: bold;
}

.zq_line {
    width: 42rpx;
    border-top: 4rpx solid #dddddd;
    margin-left: 25rpx;
}

.list_zq_right {
    width: 490rpx;
    text-align: left;
    margin-left: 40rpx;
    position: relative;
    left: 145rpx;
}

.zq_title {
    width: 100%;
    font-size: 32rpx;
    color: #666666;
    margin: 30rpx 0;
    font-family: PingFangSC-Medium,sans-serif;
}

.zq_timeleng {
    width: 100%;
}

.zq_timeleng_img {
    padding-left: 30rpx;
    line-height: 45rpx;
    font-size: 22rpx;
    color: #999999;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 25rpx 25rpx;
    background-position: left center;
}

.zq_tag {
    margin-top: 55rpx;
    width: 100%;
}

.zq_tag_con {
    line-height: 45rpx;
    float: left;
    margin-right: 10rpx;
    border: 1rpx solid #e6e7f3;
    border-radius: 14rpx;
    color: #747ba7;
    font-size: 20rpx;
    font-family: PingFangSC-Medium,sans-serif;
    background: #eeeff9;
    padding: 0 15rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    max-width: 120rpx;
}

.woguanzhude {
    padding: 20rpx;
    background-color: #f5f5fd;
}

.woguanzhude .guanzhu_list {
    overflow: hidden;
    border-radius: 10rpx;
    background-color: #fff;
    padding: 15rpx 0rpx 20rpx 0rpx;
    border: 1rpx solid #e2e8f4;
    margin-bottom: 20rpx;
}

.woguanzhude .guanzhu_list:last-of-type {
    margin-bottom: 0;
}

.woguanzhude .guanzhu_list .shang {
