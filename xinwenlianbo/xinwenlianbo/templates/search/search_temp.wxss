@import "../share/share_temp.wxss";

.search-bar {
    position: relative;
    padding: 0px 0 20rpx;
    margin: 0 40rpx;
    display: flex;
    box-sizing: border-box;
    background-color: #ffffff;
}

.search-wrap {
    position: relative;
    flex: auto;
    border-radius: 30rpx;
    background: #fff;
}

.search-box {
    position: relative;
    width: 640rpx;
    height: 60rpx;
    padding-left: 30rpx;
    background-color: #f0f0f0;
    border-radius: 50rpx;
    font-size: 22rpx;
    color: #999;
}

.search-box .search_icon {
    width: 36rpx;
    height: 60rpx;
    float: left;
    background: #f0f0f0 url(data:image/png;base64,) no-repeat;
    background-size: 28rpx 30rpx;
    background-position: center center;
}

.icon-search {
    margin: 16rpx 16rpx 16rpx 20rpx;
}

.search-input {
    float: left;
    width: 478rpx;
    font-size: 30rpx;
    height: 60rpx;
    line-height: 30rpx;
    padding-left: 10rpx;
    color: #333333;
    caret-color: #ed4137;
}

input::-webkit-input-placeholder {
    color: rgb(117,117,117);
}

.icon-clear {
    position: absolute;
    top: 12rpx;
    right: 12rpx;
    width: 36rpx;
    height: 36rpx;
    z-index: 99;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}

.cancel-btn {
    line-height: 60rpx;
    color: #666666;
    font-size: 30rpx;
    height: 60rpx;
}