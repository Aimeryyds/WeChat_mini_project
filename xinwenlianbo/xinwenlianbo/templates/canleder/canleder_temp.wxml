<template name="canleder">
    <view class="rili">
        <view class="rilibox">
            <view class="monthDate">
                <view class="time">{{rl.month}}</view>
                <view class="danwei">月</view>
                <view class="time">{{rl.getDate}}</view>
                <view class="danwei">日</view>
            </view>
            <view class="weeknongli">
                <view class="year">{{rl.year}}</view>
                <view class="weekbox">
                    <view class="week">{{rl.xingqi}}</view>
                    <view class="nongli">农历{{rl.nongli}}</view>
                </view>
            </view>
        </view>
    </view>
</template>
