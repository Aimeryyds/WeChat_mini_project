.search .input-container {
    position: relative;
    height: 86rpx;
    border-bottom: 1rpx solid #d8d8d8;
}

.search .input-container .input {
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto;
    left: 4%;
    right: 16%;
    height: 58rpx;
    line-height: 58rpx;
    border-radius: 29rpx;
    background-color: #f4f5f6;
    color: #333;
    font-size: 28rpx;
    padding: 9rpx 50rpx;
    box-sizing: border-box;
}

.search .input-container .input::before {
    content: '';
    top: 0;
    bottom: 0;
    margin: auto;
    left: 16rpx;
    display: inline-block;
    position: absolute;
    width: 26rpx;
    height: 26rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}

.search .input-container .input-placeholder {
    color: #999;
}

.search .input-container .delete {
    position: absolute;
    top: 0;
    bottom: 0;
    right: 18%;
    margin: auto;
    width: 28rpx;
    height: 28rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}

.search .input-container .cancel {
    position: absolute;
    right: 4%;
    line-height: 86rpx;
    font-size: 32rpx;
    color: #2a90d7;
    margin: auto;
}

.search .banner-container {
    margin: 40rpx 4% 32rpx 4%;
    height: 32rpx;
    position: relative;
}

.search .banner-container .logo {
    position: absolute;
    top: 0;
    bottom: 0;
    width: 136rpx;
    height: 32rpx;
}

.search .banner-container .update-time {
    display: inline;
    float: right;
    font-size: 26rpx;
    line-height: 32rpx;
    color: #999999;
}

.search .rank-list .rank-item {
    position: relative;
    display: inline-block;
    width: 50%;
    height: 45rpx;
    padding: 0 4%;
    margin-bottom: 31rpx;
    box-sizing: border-box;
}

.search .rank-list .rank-item .rank-num {
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto;
    display: inline-block;
    font-family: DINAlternate;
    font-weight: bold;
    line-height: 45rpx;
    color: #999;
    font-size: 32rpx;
}

.search .rank-list .rank-item .rank-content {
    position: absolute;
    left: 16%;
    right: 16%;
    top: 0;
    bottom: 0;
    margin: auto;
    color: #222222;
    font-size: 32rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.search .rank-list .rank-item .rank-content:last-child {
    right: 8%;
}

.search .rank-list .rank-item .tag {
    position: absolute;
    right: 8%;
    top: 0;
    bottom: 0;
    margin: auto;
    display: inline-block;
    height: 28rpx;
    object-fit: cover;
    object-position: right;
}

.search .rank-list .rank-item:last-child .rank-content::after {
    content: '';
    display: inline-block;
    margin-left: 8rpx;
    width: 12rpx;
    height: 20rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: contain;
}

.search .search-data-container {
    margin: 8rpx 30rpx;
    box-shadow: 0 1rpx 0 0 #e8e8e8;
}

.search .search-data-container .search-data-item {
    margin: 20rpx 0;
    display: inline-block;
    box-sizing: border-box;
    width: 50%;
    font-size: 32rpx;
    font-weight: 400;
    color: #222;
    line-height: 1.1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search .search-data-container .search-data-item:nth-child(2n+1) {
    border-right: 1rpx solid #d8d8d8;
}

.search .search-data-container .search-data-item:nth-child(2n) {
    padding-left: 30rpx;
}

.search .search-history-container {
    margin: 30rpx;
    box-shadow: 0 1rpx 0 0 #e8e8e8;
    position: relative;
}

.search .search-history-container .search-history-title {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    color: #999;
    font-size: 28rpx;
    line-height: 40rpx;
    font-weight: 400;
}

.search .search-history-container .search-history-title::after {
    content: '';
    display: inline-block;
    width: 32rpx;
    height: 28rpx;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: contain;
}

.search .search-history-container .delete {
    position: absolute;
    top: 0;
    right: 0;
    width: 32rpx;
    height: 32rpx;
}

.search .search-history-container .search-history-item {
    margin: 20rpx 0;
    display: inline-block;
    box-sizing: border-box;
    width: 50%;
    font-size: 32rpx;
    font-weight: 400;
    color: #222;
    line-height: 1.1;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search .search-history-container .search-history-item:nth-child(2n+1) {
    border-right: 1rpx solid #d8d8d8;
}

.search .search-history-container .search-history-item:nth-child(2n) {
    padding-left: 30rpx;
}

.search .hideSearchHistory {
    box-shadow: none;
}

.search .hideSearchHistory .search-history-title::after {
    transform: rotate(180deg);
    content: '';
    display: inline-block;
    width: 32rpx;
    height: 28rpx;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: contain;
}

.search .sug-list .sug-item {
    font-size: 32rpx;
    color: #222;
    line-height: 1.1;
    padding: 26rpx 70rpx;
    border-bottom: 1rpx solid #f3f3f3;
}

.search .feedlist-container .feedlist-item {
    margin: 0 30rpx;
    padding: 26rpx 0;
    box-shadow: inset 0 0.5rpx 0 0 #e8e8e8;
}

.search .feedlist-container .feedlist-item:first-child {
    box-shadow: none;
}

.search .feedlist-container .feedlist-item .title {
    display: inline;
    font-size: 38rpx;
    line-height: 1.32;
    color: #222;
}

.search .feedlist-container .feedlist-item .highlight {
    color: #f85959;
}

.search .feedlist-container .feedlist-item .info {
    margin-top: 12rpx;
    font-size: 24rpx;
    color: #999;
    line-height: 1.17;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.search .feedlist-container .feedlist-item .info text {
    margin-right: 16rpx;
}

.search .feedlist-container .feedlist-item .info .tag-stick {
    width: 54rpx;
    height: 28rpx;
    margin-right: 8rpx;
}

.search .feedlist-container .feedlist-item .info .tag-hot {
    width: 32rpx;
    height: 28rpx;
    margin-right: 8rpx;
}

.search .feedlist-container .feedlist-item .middle_mode {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.search .feedlist-container .feedlist-item .middle_mode .left {
    margin-right: 24rpx;
    width: 440rpx;
    display: inline-block;
}

.search .feedlist-container .feedlist-item .middle_mode .left .source {
    max-width: 200rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search .feedlist-container .feedlist-item .middle_mode .right {
    position: relative;
    line-height: 0;
}

.search .feedlist-container .feedlist-item .middle_mode .right image {
    width: 226rpx;
    height: 150rpx;
    border-radius: 6rpx;
}

.search .feedlist-container .feedlist-item .middle_mode .right .video-duration {
    display: inline-block;
    position: absolute;
    right: 8rpx;
    bottom: 8rpx;
    padding: 0 12rpx;
    height: 40rpx;
    background-color: rgba(0,0,0,0.4);
    border-radius: 20rpx;
    line-height: 40rpx;
    text-align: center;
    color: #fff;
    font-size: 20rpx;
}

.search .feedlist-container .feedlist-item .middle_mode .right .video-duration::before {
    content: '';
    display: inline-block;
    width: 10rpx;
    height: 14rpx;
    margin-right: 4rpx;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: contain;
}

.search .feedlist-container .feedlist-item .large_mode .img-container {
    position: relative;
}

.search .feedlist-container .feedlist-item .large_mode .img-container .img {
    width: 100%;
    height: 392rpx;
    margin-top: 12rpx;
    border-radius: 6rpx;
}

.search .feedlist-container .feedlist-item .large_mode .img-container .play-btn {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    margin: auto;
    width: 88rpx;
    height: 88rpx;
}

.search .feedlist-container .feedlist-item .more_mode .img-container {
    margin-top: 12rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
}

.search .feedlist-container .feedlist-item .more_mode .img-container .img {
    width: 226rpx;
    height: 150rpx;
    box-sizing: border-box;
    border-radius: 6rpx;
}

.search .feedlist-container .loading {
    color: #999;
    font-size: 24rpx;
    text-align: center;
    line-height: 80rpx;
}