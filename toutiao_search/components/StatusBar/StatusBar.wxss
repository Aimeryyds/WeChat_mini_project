.banner-container {
    position: fixed;
    width: 100%;
    background-color: #d43d3d;
    z-index: 9999;
}

.title {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    vertical-align: bottom;
    font-size: 37rpx;
    font-weight: 500;
    color: #fff;
    text-align: center;
}

.search {
    position: absolute;
    left: 15rpx;
    width: 70%;
    background-color: #fff;
    color: #222;
    font-size: 28rpx;
    border-radius: 32rpx;
    padding: 0 48rpx 0 64rpx;
    box-sizing: border-box;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.text-container {
    transition: 300ms ease-in;
}

.text {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.search::before {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 16rpx;
    margin: auto;
    content: '';
    display: inline-block;
    width: 48rpx;
    height: 48rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: cover;
}

.back-container {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 86rpx;
    height: 87rpx;
}

.back {
    position: absolute;
    left: 23rpx;
    bottom: 26rpx;
    width: 40rpx;
    height: 35rpx;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: contain;
}

.home-container {
    position: absolute;
    left: 86rpx;
    bottom: 0;
    width: 86rpx;
    height: 87rpx;
}

.home {
    position: absolute;
    left: 23rpx;
    bottom: 24rpx;
    width: 40rpx;
    height: 40rpx;
    background: url('data:image/png;base64,') no-repeat center;
    background-size: contain;
}

.split-line {
    position: absolute;
    left: 86rpx;
    bottom: 27rpx;
    margin: auto;
    width: 1rpx;
    height: 36rpx;
    background: #fff;
    opacity: 0.27;
}